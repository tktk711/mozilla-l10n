#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Agiki sync: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr ""

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Akaunti: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Pe okube"

#: connecting.label
msgid "Connecting…"
msgstr "Kubo…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Sync me Firefor mamegi ki kwanyo woko"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Dok cen"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 pe rwate kwede cik ma koni ni pi Sync me Firefox.Tim ber imi ngec "
"idwok i cik ma okati."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 pe rwate kwede cik me Sync pa Firefox macon. Tim be i mi ngec pa "
"Firefox ikom kompiuta mukene mamegi."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Nong ngec Mapol"

#: sync.update.close
msgid "Close"
msgstr "Lor"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Pe romo tero Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Intanet pe okube"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S pe onongo romo kubo i Sync. Onyo imito temo doki?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Tem doki"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Tera me cing"
