msgid "Toggle Sidebar"
msgstr "Sypaneel aan/af"

#: toggle_sidebar_label
msgctxt "toggle_sidebar_label"
msgid "Toggle Sidebar"
msgstr "Sypaneel aan/af"

#: outline.title
msgid "Show Document Outline"
msgstr "Wys dokumentoorsig"

#: outline_label
msgid "Document Outline"
msgstr "Dokumentoorsig"

#: thumbs.title
msgid "Show Thumbnails"
msgstr "Wys duimnaels"

#: thumbs_label
msgid "Thumbnails"
msgstr "Duimnaels"

#: findbar.title
msgid "Find in Document"
msgstr "Soek in dokument"

#: findbar_label
msgid "Find"
msgstr "Vind"

#. # Thumbnails panel item (tooltip and alt text for images)
#. # LOCALIZATION NOTE (thumb_page_title): "{{page}}" will be replaced by the page
#. # number.
#: thumb_page_title
msgid "Page {{page}}"
msgstr "Bladsy {{page}}"

#. # LOCALIZATION NOTE (thumb_page_canvas): "{{page}}" will be replaced by the page
#. # number.
#: thumb_page_canvas
msgid "Thumbnail of Page {{page}}"
msgstr "Duimnael van bladsy {{page}}"

#. # Context menu
#: first_page.label
msgid "Go to First Page"
msgstr "Gaan na eerste bladsy"

#: last_page.label
msgid "Go to Last Page"
msgstr "Gaan na laaste bladsy"

#: page_rotate_cw.label
msgid "Rotate Clockwise"
msgstr "Roteer kloksgewys"

#: page_rotate_ccw.label
msgid "Rotate Counterclockwise"
msgstr "Roteer anti-kloksgewys"

#. # Find panel button title and messages
#: find_label
msgid "Find:"
msgstr "Vind:"

#: find_previous.title
msgid "Find the previous occurrence of the phrase"
msgstr "Vind die vorige voorkoms van die frase"

#: find_previous_label
msgctxt "find_previous_label"
msgid "Previous"
msgstr "Vorige"

#: find_next.title
msgid "Find the next occurrence of the phrase"
msgstr "Vind die volgende voorkoms van die frase"

#: find_next_label
msgctxt "find_next_label"
msgid "Next"
msgstr "Volgende"

#: find_highlight
msgid "Highlight all"
msgstr "Verlig alle"

#: find_match_case_label
msgid "Match case"
msgstr "Kassensitief"

#: find_reached_top
msgid "Reached top of document, continued from bottom"
msgstr "Bokant van dokument is bereik; gaan voort van onder af"

#: find_reached_bottom
msgid "Reached end of document, continued from top"
msgstr "Einde van dokument is bereik; gaan voort van bo af"

#: find_not_found
msgid "Phrase not found"
msgstr "Frase nie gevind nie"

#. # Error panel labels
#: error_more_info
msgid "More Information"
msgstr "Meer inligting"

#: error_less_info
msgid "Less Information"
msgstr "Minder inligting"

#: error_close
msgid "Close"
msgstr "Sluit"

#. # LOCALIZATION NOTE (error_version_info): "{{version}}" and "{{build}}" will be
#. # replaced by the PDF.JS version and build ID.
#: error_version_info
msgid "PDF.js v{{version}} (build: {{build}})"
msgstr "PDF.js v{{version}} (ID: {{build}})"

#. # LOCALIZATION NOTE (error_message): "{{message}}" will be replaced by an
#. # english string describing the error.
#: error_message
msgid "Message: {{message}}"
msgstr "Boodskap: {{message}}"

#. # LOCALIZATION NOTE (error_stack): "{{stack}}" will be replaced with a stack
#. # trace.
#: error_stack
msgid "Stack: {{stack}}"
msgstr "Stapel: {{stack}}"

#. # LOCALIZATION NOTE (error_file): "{{file}}" will be replaced with a filename
#: error_file
msgid "File: {{file}}"
msgstr "Lêer: {{file}}"

#. # LOCALIZATION NOTE (error_line): "{{line}}" will be replaced with a line number
#: error_line
msgid "Line: {{line}}"
msgstr "Lyn: {{line}}"

#: rendering_error
msgid "An error occurred while rendering the page."
msgstr "'n Fout het voorgekom toe die bladsy weergegee is."

#. # Predefined zoom values
#: page_scale_width
msgid "Page Width"
msgstr "Bladsywydte"

#: page_scale_fit
msgid "Page Fit"
msgstr "Pas bladsy"

#: page_scale_auto
msgid "Automatic Zoom"
msgstr "Outomatiese zoem"

#: page_scale_actual
msgid "Actual Size"
msgstr "Werklike grootte"

#. # Loading indicator messages
#: loading_error_indicator
msgid "Error"
msgstr "Fout"

#: loading_error
msgid "An error occurred while loading the PDF."
msgstr "'n Fout het voorgekom met die laai van die PDF."

#: invalid_file_error
msgid "Invalid or corrupted PDF file."
msgstr "Ongeldige of korrupte PDF-lêer."

#: missing_file_error
msgid "Missing PDF file."
msgstr "PDF-lêer is weg."

#. # LOCALIZATION NOTE (text_annotation_type.alt): This is used as a tooltip.
#. # "{{type}}" will be replaced with an annotation type from a list defined in
#. # the PDF spec (32000-1:2008 Table 169 – Annotation types).
#. # Some common types are e.g.: "Check", "Text", "Comment", "Note"
#: text_annotation_type.alt
msgid "[{{type}} Annotation]"
msgstr "[{{type}}-annotasie"

#: request_password
msgid "PDF is protected by a password:"
msgstr "PDF word deur 'n wagwoord beskerm:"

#: invalid_password
msgid "Invalid Password."
msgstr "Ongeldige wagwoord."

#: printing_not_supported
msgid "Warning: Printing is not fully supported by this browser."
msgstr "Waarskuwing: Dié blaaier ondersteun nie drukwerk ten volle nie."

#: printing_not_ready
msgid "Warning: The PDF is not fully loaded for printing."
msgstr "Waarskuwing: Die PDF is nog nie volledig gelaai vir drukwerk nie."

#: web_fonts_disabled
msgid "Web fonts are disabled: unable to use embedded PDF fonts."
msgstr ""
"Webfonte is gedeaktiveer: kan nie PDF-fonte wat ingebed is, gebruik nie."

#: document_colors_disabled
msgid ""
"PDF documents are not allowed to use their own colors: 'Allow pages to "
"choose their own colors' is deactivated in the browser."
msgstr ""
"PDF-dokumente word nie toegelaat om hul eie kleure te gebruik nie:  'Laat "
"bladsye toe om hul eie kleure te kies' is gedeaktiveer in die blaaier."

