#: error.login.reason.network
msgid "Failed to connect to the server"
msgstr "Enntummi annkɔnɛkte annkɔ sɛɛva ne so"

#: error.login.reason.recoverykey
msgid "Wrong Recovery Key"
msgstr "Nsa-san-ka safoa a ɛnndi mu"

#: error.login.reason.account
msgid "Incorrect account name or password"
msgstr "Wo akawnt din anaaso ehintasɛmfua nndi mu"

#: error.login.reason.no_username
msgid "Missing account name"
msgstr "Ayera akawnt din"

#: error.login.reason.no_password2
msgid "Missing password"
msgstr "Ayera ehintasɛmfua"

#: error.login.reason.no_recoverykey
msgid "No saved Recovery Key to use"
msgstr "Nsa-san-ka safoa biara nni hɔ a wɔde esie na wɔ de bɛ di dwuma"

#: error.login.reason.server
msgid "Server incorrectly configured"
msgstr "Wɔnnhyehyɛɛ sɛɛva no yie"

#: error.sync.failed_partial
msgid "One or more data types could not be synced"
msgstr "Enntumi ammfa data sɛso binom ennsie"

#. # LOCALIZATION NOTE (error.sync.reason.serverMaintenance): We removed the extraneous period from this string
#: error.sync.reason.serverMaintenance
msgid ""
"Firefox Sync server maintenance is underway, syncing will resume "
"automatically"
msgstr "Wɔresiesie Firefox Sync sɛɛva, ɛbɛsan ahyɛ ase asynke otomatikmu"

#: invalid-captcha
msgid "Incorrect words, try again"
msgstr "Nsɛmfua no nyɛ papa, san sɔ hwɛ"

#: weak-password
msgid "Use a stronger password"
msgstr "Fa ehintasɛmfua ɛyɛ den yɛ"

#. # this is the fallback, if we hit an error we didn't bother to localize
#: error.reason.unknown
msgid "Unknown error"
msgstr "Nnim mfomso yi"

#: change.password.pwSameAsPassword
msgid "Password can't match current password"
msgstr "Ehintasɛmfua no ne ehintasɛmfua yi bɔ abira"

#: change.password.pwSameAsUsername
msgid "Password can't match your user name"
msgstr "Ehintasɛmfua no ne dinyi bɔ abira"

#: change.password.pwSameAsEmail
msgid "Password can't match your email address"
msgstr "Ehintasɛmfua no ne emel adrɛs yi bɔ abira"

#: change.password.mismatch
msgid "The passwords entered do not match"
msgstr "Ahintasɛmfua no bɔ abira"

#: change.password.tooShort
msgid "The password entered is too short"
msgstr "Ehintasɛmfua no yɛ tiaa dodow"

#~ msgid "Password can't match your Recovery Key"
#~ msgstr "Ehintasɛmfua renntumi mmfa nsa-san-ka safoa nnyɛ pɛ"

#~ msgid "Wrong Sync Key"
#~ msgstr "Sync safoa a ɛnnyɛ nokware"

#~ msgid "No saved Sync Key to use"
#~ msgstr "Wo nnsiee Sync safoa biara"

#~ msgid "The new Sync Key cannot be the same as your Sync Key"
#~ msgstr "Sync Key foforo no ne Sync Key yi ɛbɔ abira"

#~ msgid "The Sync Key cannot be the same as your password"
#~ msgstr "Sync Key no ne ehintasɛmfua no bɔ abira"

#~ msgid "The Sync Key cannot be the same as your user name"
#~ msgstr "Sync Key no ne din no bɔ abira"

#~ msgid "The Sync Key cannot be the same as your email address"
#~ msgstr "Sync Key no ne emel adrɛs no bɔ abira"

#~ msgid "The Sync Key entered is too short"
#~ msgstr "Sync Key no yɛ tiaa dodow"

#~ msgid "No saved password to use"
#~ msgstr "Wo nnsiee ehintasɛmfua biara"

#~ msgid "Incorrect username or password"
#~ msgstr "Agodin anaaso ehintasɛmfua a enndi mu"
