#. # LOCALIZATION NOTE (commands):
#. #  %S is a comma separated list of command names.
#: commands
msgid ""
"Commands: %S.\n"
"Use /help &lt;command&gt; for more information."
msgstr ""
"Ordens: %S.\n"
"Faiga servir /help &lt;orden&gt; ta saber-ne mas."

#. # LOCALIZATION NOTE (noCommand, noHelp):
#. #  %S is the command name the user typed.
#: noCommand
msgid "No '%S' command."
msgstr "No s'ha trobau a orden '%S'."

#: noHelp
msgid "No help message for the '%S' command, sorry!"
msgstr "No i ha garra mensache d'aduya t'a orden '%S'!"

#: sayHelpString
msgid "say &lt;message&gt;: send a message without processing commands."
msgstr ""
"say &lt;missatge&gt;: ninvía un mensache sinde ordens de procesamiento."

#: rawHelpString
msgid "raw &lt;message&gt;: send a message without escaping HTML entities."
msgstr ""
"raw &lt;missatge&gt;: ninvía un mensache sinde escapar as entidats HTML."

#: helpHelpString
msgid ""
"help &lt;name&gt;: show the help message for the &lt;name&gt; command, or "
"the list of possible commands when used without parameter."
msgstr ""
"help &lt;nom&gt;: amostra o mensache d'aduya d'a orden &lt;nom&gt;, u a "
"lista d'ordens posibles quan s'emplega sinde parametros."

#. # LOCALIZATION NOTE (statusCommand):
#. #  %1$S is replaced with a status command name
#. #   (one of "back", "away", "busy", "dnd", or "offline").
#. #  %2$S is replaced with the localized version of that status type
#. #   (one of the 5 strings below).
#: statusCommand
msgid ""
"%1$S &lt;status message&gt;: set the status to %2$S with an optional status "
"message."
msgstr ""
"%1$S &lt;mensache d'estau&gt;: define o estau de %2$S con un mensache "
"d'estau opcional."

#: back
msgid "available"
msgstr "disponible"

#: away
msgid "away"
msgstr "ausent"

#: busy
msgctxt "busy"
msgid "unavailable"
msgstr "no disponible"

#: dnd
msgctxt "dnd"
msgid "unavailable"
msgstr "no disponible"

#: offline
msgid "offline"
msgstr "desconnectau"
