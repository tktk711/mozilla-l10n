#. %brandDTD;
#: loadError.label
msgid "Problem loading page"
msgstr "S'ha produciu un problema en cargar a pachina"

#: retry.label
msgid "Try Again"
msgstr "Torna-lo a probar"

#. Specific error messages
#: connectionFailure.title
msgid "Unable to connect"
msgstr "No s'ha puesto connectar"

#: connectionFailure.longDesc2
msgctxt "connectionFailure.longDesc2"
msgid "&sharedLongDesc3;"
msgstr "&sharedLongDesc3;"

#: deniedPortAccess.title
msgid "This address is restricted"
msgstr "L'adreza ye restrinchida"

#: deniedPortAccess.longDesc
msgctxt "deniedPortAccess.longDesc"
msgid ""
msgstr ""

#: dnsNotFound.title
msgid "Server not found"
msgstr "No s'ha trobau o servidor"

#. <!-- LOCALIZATION NOTE (dnsNotFound.longDesc4) This string contains markup including widgets for searching
#. or enabling wifi connections. The text inside tags should be localized.  Do not change the ids.
#: dnsNotFound.longDesc4
msgid ""
"<ul> <li>Check the address for typing errors such as <strong>ww</strong>."
"example.com instead of <strong>www</strong>.example.com</li> <div "
"id='searchbox'> <input id='searchtext' type='search'></input> <button "
"id='searchbutton'>Search</button> </div> <li>If you are unable to load any "
"pages, check your device’s data or Wi-Fi connection. <button "
"id='wifi'>Enable Wi-Fi</button> </li> </ul>"
msgstr ""
"<ul> <li>Comprebe que l'adreza no tien errors como <strong>ww</strong>."
"eixemplo.com instead of <strong>www</strong>.eixemplo.com</li> <div "
"id='searchbox'> <input id='searchtext' type='search'></input> <button "
"id='searchbutton'>Buscar</button> </div> <li>Si no puet cargar garra "
"pachina, comprebe a connexión de datos u Wi-Fi d'o suyo dispositivo. <button "
"id='wifi'>Activar Wi-Fi</button> </li> </ul>"

#: fileNotFound.title
msgid "File not found"
msgstr "No s'ha trobau o fichero"

#: fileNotFound.longDesc
msgid ""
"<ul> <li>Check the file name for capitalization or other typing errors.</li> "
"<li>Check to see if the file was moved, renamed or deleted.</li> </ul>"
msgstr ""
"<ul> <li>Comprebe a sintaxi d'o nombre d'o fichero, incluindo-ie l'uso de "
"mayusclas y minusclas.</li> <li>Comprebe que o fichero no siga estau moviu, "
"renombrau u eliminau.</li> </ul>"

#: fileAccessDenied.title
msgid "Access to the file was denied"
msgstr "S'ha denegau l'acceso a lo fichero"

#: fileAccessDenied.longDesc
msgid ""
"<ul> <li>It may have been removed, moved, or file permissions may be "
"preventing access.</li> </ul>"
msgstr ""
"<ul> <li>Talment s'haiga borrau, moviu u l'acceso ye vedau por los permisos "
"d'o fichero.</li> </ul>"

#: generic.title
msgid "Oops."
msgstr "Ups."

#: generic.longDesc
msgid "<p>&brandShortName; can’t load this page for some reason.</p>"
msgstr "<p>&brandShortName; no puet cargar la pachina por bella razón.</p>"

#: malformedURI.title
msgid "The address isn’t valid"
msgstr "L'adreza no ye valida"

#. <!-- LOCALIZATION NOTE (malformedURI.longDesc2) This string contains markup including widgets for searching
#. or enabling wifi connections. The text inside the tags should be localized. Do not touch the ids.
#: malformedURI.longDesc2
msgid ""
"<ul> <li>Web addresses are usually written like <strong>http://www.example."
"com/</strong></li> <div id='searchbox'> <input id='searchtext' "
"type='search'></input> <button id='searchbutton'>Search</button> </div> "
"<li>Make sure that you’re using forward slashes (i.e. <strong>/</strong>).</"
"li> </ul>"
msgstr ""
"<ul> <li>As adrezas web gosan escribir-se asinas: <strong>http://www."
"eixemplo.com/</strong></li> <div id='searchbox'> <input id='searchtext' "
"type='search'></input> <button id='searchbutton'>Buscar</button> </"
"div> <li>Compreba que no has usau barras enta debant (i.e. <strong>/</"
"strong>).</li> </ul>"

#: netInterrupt.title
msgid "The connection was interrupted"
msgstr "S'ha interrumpiu a connexión"

#: netInterrupt.longDesc2
msgctxt "netInterrupt.longDesc2"
msgid "&sharedLongDesc3;"
msgstr "&sharedLongDesc3;"

#: notCached.title
msgid "Document Expired"
msgstr "O documento ha caducau"

#: notCached.longDesc
msgid ""
"<p>The requested document is not available in &brandShortName;’s cache.</"
"p><ul><li>As a security precaution, &brandShortName; does not automatically "
"re-request sensitive documents.</li><li>Click Try Again to re-request the "
"document from the website.</li></ul>"
msgstr ""
"<p>O documento solicitau ya no ye disponible en a caché de &brandShortName;."
"</p><ul><li>Por precaución, &brandShortName; no tornará a solicitar "
"automaticament os documentos sensibles.</li><li>Faiga clic en Prebar-lo de "
"nuevas ta tornar a solicitar o documento a o puesto web.</li></ul>"

#: netOffline.title
msgid "Offline mode"
msgstr "Modo sin connexión"

#. <!-- LOCALIZATION NOTE (netOffline.longDesc3) This string contains markup including widgets enabling wifi connections.
#. The text inside the tags should be localized. Do not touch the ids.
#: netOffline.longDesc3
msgid ""
"<ul> <li>Try again. &brandShortName; will attempt to open a connection and "
"reload the page. <button id='wifi'>Enable Wi-Fi</button> </li> </ul>"
msgstr ""
"<ul> <li>Torne-lo a prebar. &brandShortName; mirará d'ubrir una connexión y "
"recargar a pachina. <button id='wifi'>Activar a Wi-Fi</button> </li> </ul>"

#: contentEncodingError.title
msgid "Content Encoding Error"
msgstr "Error de codificación d'o conteniu"

#: contentEncodingError.longDesc
msgctxt "contentEncodingError.longDesc"
msgid ""
"<ul> <li>Please contact the website owners to inform them of this problem.</"
"li> </ul>"
msgstr ""
"<ul> <li>Meta-se en contacto con os propietarios d'o puesto web ta informar-"
"les d'iste problema.</li> </ul>"

#: unsafeContentType.title
msgid "Unsafe File Type"
msgstr "Tipo de fichero inseguro"

#: unsafeContentType.longDesc
msgctxt "unsafeContentType.longDesc"
msgid ""
"<ul> <li>Please contact the website owners to inform them of this problem.</"
"li> </ul>"
msgstr ""
"<ul> <li>Meta-se en contacto con os propietarios d'o puesto web ta informar-"
"les d'iste problema.</li> </ul>"

#: netReset.title
msgid "The connection was reset"
msgstr "S'ha reiniciau a connexión"

#: netReset.longDesc2
msgctxt "netReset.longDesc2"
msgid "&sharedLongDesc3;"
msgstr "&sharedLongDesc3;"

#: netTimeout.title
msgid "The connection has timed out"
msgstr "S'ha acotolau o tiempo d'aspera d'a connexión"

#: netTimeout.longDesc2
msgctxt "netTimeout.longDesc2"
msgid "&sharedLongDesc3;"
msgstr "&sharedLongDesc3;"

#: unknownProtocolFound.title
msgid "The address wasn’t understood"
msgstr "No s'ha entendiu l'adreza"

#: unknownProtocolFound.longDesc
msgid ""
"<ul> <li>You might need to install other software to open this address.</li> "
"</ul>"
msgstr ""
"<ul> <li>Podría estar que calese instalar unatro software ta ubrir l'adreza."
"</li> </ul>"

#: proxyConnectFailure.title
msgid "The proxy server is refusing connections"
msgstr "O servidor proxy ye refusando as connexions"

#: proxyConnectFailure.longDesc
msgid ""
"<ul> <li>Check the proxy settings to make sure that they are correct.</li> "
"<li>Contact your network administrator to make sure the proxy server is "
"working.</li> </ul>"
msgstr ""
"<ul> <li>Comprebe a configuración d'o servidor proxy ta asegurar-se de que "
"ye correcta.</li> <li>Meta-se en contacto con l'administrador d'o ret ta "
"asegurar-se de que o servidor proxy ye funcionando.</li> </ul>"

#: proxyResolveFailure.title
msgid "Unable to find the proxy server"
msgstr "No s'ha puesto trobar o servidor proxy"

#. <!-- LOCALIZATION NOTE (proxyResolveFailure.longDesc3) This string contains markup including widgets for enabling wifi connections.
#. The text inside the tags should be localized. Do not touch the ids.
#: proxyResolveFailure.longDesc3
msgid ""
"<ul> <li>Check the proxy settings to make sure that they are correct.</li> "
"<li>Check to make sure your device has a working data or Wi-Fi connection. "
"<button id='wifi'>Enable Wi-Fi</button> </li> </ul>"
msgstr ""
"<ul> <li>Comprebe os achustes d'o proxy ta asegurar-se que son correctos.</"
"li> <li>Comprebe que o suyo dispositivo tien una connexión de datos u Wi-Fi "
"que funciona. <button id='wifi'>Activar a Wi-Fi</button> </li> </ul>"

#: redirectLoop.title
msgid "The page isn’t redirecting properly"
msgstr "A pachina no ye reendrezando adequadament"

#: redirectLoop.longDesc
msgid ""
"<ul> <li>This problem can sometimes be caused by disabling or refusing to "
"accept cookies.</li> </ul>"
msgstr ""
"<ul> <li>Ixo podría pasar en haber desactivau u refusau l'acceptación de "
"cookies.</li> </ul>"

#: unknownSocketType.title
msgid "Unexpected response from server"
msgstr "Respuesta inasperada d'o servidor"

#: unknownSocketType.longDesc
msgid ""
"<ul> <li>Check to make sure your system has the Personal Security Manager "
"installed.</li> <li>This might be due to a non-standard configuration on the "
"server.</li> </ul>"
msgstr ""
"<ul> <li>Asegure-se de que o suyo sistema tiene un chestor de seguranza "
"personal instalau.</li> <li>Puestar se tracte de una configuración no "
"estandar en o servidor.</li> </ul>"

#: nssFailure2.title
msgctxt "nssFailure2.title"
msgid "Secure Connection Failed"
msgstr "Ha fallau a connexión segura"

#: nssFailure2.longDesc2
msgid ""
"<ul> <li>The page you are trying to view cannot be shown because the "
"authenticity of the received data could not be verified.</li> <li>Please "
"contact the website owners to inform them of this problem.</li> </ul>"
msgstr ""
"<ul> <li>A pachina que ye mirando de veyer no puet amostrar-se porque no ye "
"posible verificar l’autenticidat d'os datos recibius.</li> <li>Meta-se en "
"contacto con o responsable d'o puesto web ta informar-le-ne.</li> </ul>"

#: nssBadCert.title
msgctxt "nssBadCert.title"
msgid "Secure Connection Failed"
msgstr "A connexión segura ha fallau"

#: nssBadCert.longDesc2
msgid ""
"<ul> <li>This could be a problem with the server’s configuration, or it "
"could be someone trying to impersonate the server.</li> <li>If you have "
"connected to this server successfully in the past, the error may be "
"temporary, and you can try again later.</li> </ul>"
msgstr ""
"<ul> <li>Isto podría estar un problema con a configuración d'o servidor, u "
"bien podría estar belún que ye mirando de fer-se pasar por o servidor.</li> "
"<li>Si se i ha connectau sin garra problema bella atra vegada, a error "
"podría estar temporal, y puede tornar-lo a prebar mas entabant.</li> </ul>"

#. <!-- LOCALIZATION NOTE (sharedLongDesc3) This string contains markup including widgets for enabling wifi connections.
#. The text inside the tags should be localized. Do not touch the ids.
#: sharedLongDesc3
msgid ""
"<ul> <li>The site could be temporarily unavailable or too busy. Try again in "
"a few moments.</li> <li>If you are unable to load any pages, check your "
"mobile device’s data or Wi-Fi connection. <button id='wifi'>Enable Wi-Fi</"
"button> </li> </ul>"
msgstr ""
"<ul> <li>Talment o puesto siga temporalment no disponible u masiau ocupau. "
"Torne a intentar-lo en bells momentos.</li> <li>Si no puet cargar garra "
"pachina, comprebe a connexión de datos u Wi-Fi d'o suyo dispoisitivo mobil. "
"<button id='wifi'>Activar a Wi-Fi</button> </li> </ul>"

#: cspBlocked.title
msgid "Blocked by Content Security Policy"
msgstr "S'ha bloquiau por a politica de seguranza de conteniu"

#: cspBlocked.longDesc
msgid ""
"<p>&brandShortName; prevented this page from loading in this way because the "
"page has a content security policy that disallows it.</p>"
msgstr ""
"<p>&brandShortName; ha privau que a pachina se cargue d'ista traza porque "
"tiene una politica de seguranza de conteniu que no lo permite.</p>"

#: corruptedContentErrorv2.title
msgid "Corrupted Content Error"
msgstr "Error en o conteniu d'a pachina"

#: corruptedContentErrorv2.longDesc
msgid ""
"<p>The page you are trying to view cannot be shown because an error in the "
"data transmission was detected.</p><ul><li>Please contact the website owners "
"to inform them of this problem.</li></ul>"
msgstr ""
"<p>A pachina que ye mirando de veyer no se puede amostrar porque s'ha "
"produciu una error en a transmisión d'os datos.</p><ul><li>Meta-se en "
"contacto con os propietarios d'o puesto web ta informar-les d'iste problema</"
"li></ul>"

#: securityOverride.linkText
msgid "Or you can add an exception…"
msgstr "U puede adhibir-ie una excepción…"

#: securityOverride.getMeOutOfHereButton
msgid "Get me out of here!"
msgstr "Quita-me d'aquí!"

#: securityOverride.exceptionButtonLabel
msgid "Add Exception…"
msgstr "Adhibir una excepción…"

#. <!-- LOCALIZATION NOTE (securityOverride.warningContent) - Do not translate the
#. contents of the <xul:button> tags.  The only language content is the label= field,
#. which uses strings already defined above. The button is included here (instead of
#. netError.xhtml) because it exposes functionality specific to firefox.
#: securityOverride.warningContent
msgid ""
"<p>You should not add an exception if you are using an internet connection "
"that you do not trust completely or if you are not used to seeing a warning "
"for this server.</p>  <button id='getMeOutOfHereButton'>&securityOverride."
"getMeOutOfHereButton;</button> <button "
"id='exceptionDialogButton'>&securityOverride.exceptionButtonLabel;</button>"
msgstr ""
"<p>No habría d'adhibir una excepción si ye emplegando una connexión "
"d'Internet que no i confía plenament u si no gosaba veyer una alvertencia en "
"iste servidor.</p>  <button id='getMeOutOfHereButton'>&securityOverride."
"getMeOutOfHereButton;</button> <button "
"id='exceptionDialogButton'>&securityOverride.exceptionButtonLabel;</button>"

#: remoteXUL.title
msgid "Remote XUL"
msgstr "XUL remoto"

#: remoteXUL.longDesc
msgid ""
"<p><ul><li>Please contact the website owners to inform them of this problem."
"</li></ul></p>"
msgstr ""
"<p><ul><li>Meta-se en contacto con os propietarios d'o puesto web ta "
"informar-les d'iste problema.</li></ul></p>"

#: sslv3Used.title
msgid "Unable to Connect Securely"
msgstr "No s'ha puesto connectar de traza segura"

#. <!-- LOCALIZATION NOTE (sslv3Used.longDesc) - Do not translate
#. "SSL_ERROR_UNSUPPORTED_VERSION".
#: sslv3Used.longDesc
msgid "Advanced info: SSL_ERROR_UNSUPPORTED_VERSION"
msgstr "Información abanzada: SSL_ERROR_UNSUPPORTED_VERSION"

#: weakCryptoUsed.title
msgctxt "weakCryptoUsed.title"
msgid "Your connection is not secure"
msgstr "La suya connexión no ye segura"

#. <!-- LOCALIZATION NOTE (weakCryptoUsed.longDesc) - Do not translate
#. "SSL_ERROR_NO_CYPHER_OVERLAP".
#: weakCryptoUsed.longDesc
msgid "Advanced info: SSL_ERROR_NO_CYPHER_OVERLAP"
msgstr "Información Abanzada: SSL_ERROR_NO_CYPHER_OVERLAP"

#: inadequateSecurityError.title
msgctxt "inadequateSecurityError.title"
msgid "Your connection is not secure"
msgstr "La suya connexión no ye segura"

#. <!-- LOCALIZATION NOTE (inadequateSecurityError.longDesc) - Do not translate
#. "NS_ERROR_NET_INADEQUATE_SECURITY".
#: inadequateSecurityError.longDesc
msgid ""
"<p><span class='hostname'></span> uses security technology that is outdated "
"and vulnerable to attack. An attacker could easily reveal information which "
"you thought to be safe. The website administrator will need to fix the "
"server first before you can visit the site.</p><p>Error code: "
"NS_ERROR_NET_INADEQUATE_SECURITY</p>"
msgstr ""
"<p><span class='hostname'></span> fa servir technolochía de seguranza que ye "
"obsoleta y vulnerable debant d'un ataque. Un atacant podría revelar "
"información que vusté creyeba segura. L'administrador d'o puesto web habrá "
"de fixar lo servidor antes que vusté pueda vesitar lo puesto.</p><p>Codigo "
"d'error:  NS_ERROR_NET_INADEQUATE_SECURITY</p>"
