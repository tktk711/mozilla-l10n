#. LOCALIZATION NOTE (reloadPlugin.post): include a starting space as needed
#: reloadPlugin.pre
msgctxt "reloadPlugin.pre"
msgid ""
msgstr ""

#: reloadPlugin.middle
msgid "Reload the page"
msgstr "أعِد تحميل الصفحة"

#: reloadPlugin.post
msgid " to try again."
msgstr " للمحاولة من جديد."

#. This and the other report.* strings should be as short as possible, ideally 2-3 words.
#: report.please
msgid "Send crash report"
msgstr "أرسِل بلاغ انهيار"

#: report.submitting
msgid "Sending report…"
msgstr "يرسل البلاغ…"

#: report.submitted
msgid "Crash report sent."
msgstr "أُرسل البلاغ."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "التبليغ بالانهيار معطل."

#: report.failed
msgid "Submission failed."
msgstr "فشل الإرسال."

#: report.unavailable
msgid "No report available."
msgstr "لا بلاغ متاح."

#: report.comment
msgid "Add a comment (comments are publicly visible)"
msgstr "أضِف تعليقًا. ملحوظة: التعليقات متاحة للعموم"

#: report.pageURL
msgid "Include the page's URL"
msgstr "ضمِّن مسار الصفحة"

#: plugin.file
msgid "File"
msgstr "الملف"

#: plugin.mimeTypes
msgid "MIME Types"
msgstr "أنواع MIME"

