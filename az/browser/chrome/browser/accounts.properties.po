#~ msgid "&Sign In"
#~ msgstr "&Daxil Ol"

#~ msgid "%S not verified"
#~ msgstr "%S təsdiqlənməyib"

#~ msgid "Please click the verification link in the email sent to %S"
#~ msgstr ""
#~ "Lütfən %S ünvanına göndərilən e-poçtdakı təsdiqləmə keçidinə klikləyin"

#~ msgid "&Resend"
#~ msgstr "&Təkrar göndər"

