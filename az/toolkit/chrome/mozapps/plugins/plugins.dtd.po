#: report.please
msgid "Send crash report"
msgstr "Çökmə hesabatını göndər"

#: report.submitting
msgid "Sending report…"
msgstr "Hesabat göndərilir..."

#: report.submitted
msgid "Crash report sent."
msgstr "Çökmə hesabatı göndərildi."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "Çökmə hesabatı göndərilməsi söndürülüb."

#: report.failed
msgid "Submission failed."
msgstr "Göndərmə uğursuz oldu."

#: report.unavailable
msgid "No report available."
msgstr "Hesabat yoxdur."

