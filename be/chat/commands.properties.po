#. # LOCALIZATION NOTE (commands):
#. #  %S is a comma separated list of command names.
#: commands
msgid ""
"Commands: %S.\n"
"Use /help &lt;command&gt; for more information."
msgstr ""
"Загады: %S.\n"
"Ужывайце &lt;загад&gt; /help для атрымання даведкі."

#. # LOCALIZATION NOTE (noCommand, noHelp):
#. #  %S is the command name the user typed.
#: noCommand
msgid "No '%S' command."
msgstr "Няма загаду '%S'."

#: noHelp
msgid "No help message for the '%S' command, sorry!"
msgstr "Прабачце! Няма даведкі для загаду '%S'."

#: sayHelpString
msgid "say &lt;message&gt;: send a message without processing commands."
msgstr "say &lt;паведамленне&gt;: даслаць паведамленне без апрацоўкі загадаў."

#: rawHelpString
msgid "raw &lt;message&gt;: send a message without escaping HTML entities."
msgstr ""
"raw &lt;паведамленне&gt;: даслаць паведамленне без маскавання адзінак HTML."

#: helpHelpString
msgid ""
"help &lt;name&gt;: show the help message for the &lt;name&gt; command, or "
"the list of possible commands when used without parameter."
msgstr ""
"help &lt;назва&gt;: паказаць правілы выкарыстання загаду &lt;назва&gt; або "
"спіс спіс дступных загадаў (без параметра)."

#. # LOCALIZATION NOTE (statusCommand):
#. #  %1$S is replaced with a status command name
#. #   (one of "back", "away", "busy", "dnd", or "offline").
#. #  %2$S is replaced with the localized version of that status type
#. #   (one of the 5 strings below).
#: statusCommand
msgid ""
"%1$S &lt;status message&gt;: set the status to %2$S with an optional status "
"message."
msgstr ""
"&lt;паведамленне&gt; %1$S: прызначыць статус %2$S з неабавязковым "
"паведамленнем."

#: back
msgid "available"
msgstr "даступны"

#: away
msgid "away"
msgstr "зніклы"

#: busy
msgctxt "busy"
msgid "unavailable"
msgstr "не даступны"

#: dnd
msgctxt "dnd"
msgid "unavailable"
msgstr "не даступны"

#: offline
msgid "offline"
msgstr "па-за сеткай"
