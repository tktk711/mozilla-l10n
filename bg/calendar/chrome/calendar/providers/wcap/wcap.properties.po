#. # args: host
#: accessingServerFailedError.text
msgid "Cannot access server %1$S!"
msgstr "Няма достъп до сървър %1$S!"

#: loginFailed.text
msgid "Login failed or invalid session Id."
msgstr "Провален вход или невалидна сесия."

#: accessDenied.text
msgid "The user is denied access."
msgstr "Достъпа на потребителя е отказан."

#. # args: host
#: noHttpsConfirmation.text
msgid ""
"Insecure login on %1$S!\n"
"Continue?"
msgstr ""
"Несигурен достъп до %1$S!\n"
"Продължаване?"

#: noHttpsConfirmation.check.text
msgid "Don't ask again."
msgstr "Без повече питане."

#: noHttpsConfirmation.label
msgid "Warning!"
msgstr "Внимание!"

#. # args: host, prodId, serverVersion, wcapVersion
#: insufficientWcapVersionConfirmation.text
msgid ""
"Server %1$S (%2$S, v%3$S, WCAP v%4$S) doesn't support a sufficient WCAP "
"version! The required version is at least 3.0.0.\n"
"Continue?"
msgstr ""
"Сървър %1$S (%2$S, v%3$S, WCAP v%4$S) не поддържа нужната WCAP версия! "
"Изисква се поне версия 3.0.0.\n"
"Продължаване?"

#: insufficientWcapVersionConfirmation.label
msgid "Insufficient WCAP version!"
msgstr "Недостатъчна WCAP версия!"

#: loginDialog.label
msgid "Calendar Server Password Required"
msgstr "Изисква се парола за календарния сървър"

#: privateItem.title.text
msgid "Private"
msgstr "Личен"

#: confidentialItem.title.text
msgid "Confidential"
msgstr "Поверителен"

#: busyItem.title.text
msgid "Busy"
msgstr "Зает"
