#. # Html event display in message
#: imipHtml.header
msgid "Event Invitation"
msgstr "Покана за събитие"

#: imipHtml.summary
msgid "Title:"
msgstr "Заглавие:"

#: imipHtml.location
msgid "Location:"
msgstr "Местоположение:"

#: imipHtml.when
msgid "When:"
msgstr "Кога:"

#: imipHtml.organizer
msgid "Organizer:"
msgstr "Организатор:"

#: imipHtml.description
msgid "Description:"
msgstr "Описание:"

#: imipHtml.comment
msgid "Comment:"
msgstr "Коментар:"

#: imipHtml.attendees
msgid "Attendees:"
msgstr "Присъстващи:"

#: imipHtml.canceledOccurrences
msgid "Canceled Occurrences:"
msgstr "Отменени  събития:"

#: imipHtml.modifiedOccurrences
msgid "Modified Occurrences:"
msgstr "Променени събития:"

#: imipHtml.newLocation
msgid "New Location: %1$S"
msgstr "Ново местоположение: %1$S"

#: imipAddToCalendar.label
msgid "Add To Calendar"
msgstr "Добавяне към календар"

#: imipAddedItemToCal
msgid "Event Added to Calendar"
msgstr "Събитието е добавено към календар"

#: imipCanceledItem
msgid "Event has been deleted"
msgstr "Събитието е изтрито"

#: imipUpdatedItem
msgid "Event has been updated"
msgstr "Събитието е обновено"

#: imipBarCancelText
msgid "This message contains an event cancellation."
msgstr "Това писмо съдържа информация за отмяна на събитие."

#: imipBarRefreshText
msgid "This message asks for an event update."
msgstr "Това писмо изисква обновяване на събитие."

#: imipBarPublishText
msgid "This message contains an event."
msgstr "Това писмо съдържа информация за събитие."

#: imipBarRequestText
msgid "This message contains an invitation to an event."
msgstr "Това писмо съдържа покана за събитие."

#: imipBarUpdateText
msgid "This message contains an update to an existing event."
msgstr "Това писмо съдържа промяна за съществуващо събитие."

#: imipBarAlreadyProcessedText
msgid "This message contains an event that has already been processed."
msgstr "Това писмо съдържа информация за събитие, коете вече е изминало."

#: imipBarProcessedNeedsAction
msgid "This message contains an event that you have not yet responded to."
msgstr ""
"Това съобщение съдържа информация за събитие, на което все още не сте "
"отговорили"

#: imipBarReplyText
msgid "This message contains a reply to an invitation."
msgstr "Това писмо съдържа отговор на покана."

#: imipBarUnsupportedText
msgid ""
"This message contains an event that this version of Lightning cannot process."
msgstr ""
"Това писмо съдържа информация за събитие, което тази версия на Lightning не "
"може да обработи."

#: imipBarProcessingFailed
msgid "Processing message failed. Status: %1$S."
msgstr "Неуспешна обработка на писмо. Статус: %1$S."

#: imipBarNotWritable
msgid ""
"No writable calendars are configured for invitations, please check the "
"calendar properties."
msgstr ""
"Няма календари, които да са конфигурирани за записване на събития. Моля, "
"проверете настойките."

#: imipSendMail.title
msgid "E-Mail Notification"
msgstr "Известяване по мейл"

#: imipSendMail.text
msgid "Would you like to send out notification E-Mail now?"
msgstr "Искате ли да изпратите уведомителен мейл сега?"

#: imipSendMail.Outlook2000CompatMode.text
msgid "Support Outlook 2000 and Outlook 2002/XP"
msgstr "Поддръжка на Outlook 2000 и Outlook 2002/XP"

#: imipNoIdentity
msgid "None"
msgstr "Никой"

#: imipNoCalendarAvailable
msgid "There are no writable calendars available."
msgstr "Няма достъпни календари, които могат да бъдат променяни."

#: itipReplySubject
msgid "Event Invitation Reply: %1$S"
msgstr "Отговор на покана за събитие: %1$S"

#: itipReplyBodyAccept
msgid "%1$S has accepted your event invitation."
msgstr "Вашата покана беше приета от %1$S."

#: itipReplyBodyDecline
msgid "%1$S has declined your event invitation."
msgstr "Вашата покана беше отказана от %1$S."

#: itipReplySubjectAccept
msgid "Event Invitation Reply (Accepted): %1$S"
msgstr "Отговор на покана за събитие (прието): %1$S"

#: itipReplySubjectDecline
msgid "Event Invitation Reply (Declined): %1$S"
msgstr "Отговор на покана за събитие (оказано): %1$S"

#: itipReplySubjectTentative
msgid "Event Invitation Reply (Tentative): %1$S"
msgstr "Отговор на покана за събитие (колебливо): %1$S"

#: itipRequestSubject
msgid "Event Invitation: %1$S"
msgstr "Покана за събитие: %1$S"

#: itipRequestUpdatedSubject
msgid "Updated Event Invitation: %1$S"
msgstr "Обновена покана за събитие: %1$S"

#: itipRequestBody
msgid "%1$S has invited you to %2$S"
msgstr "%1$S ви покани на %2$S"

#: itipCancelSubject
msgid "Event Canceled: %1$S"
msgstr "Отменено събитие: %1$S"

#: itipCancelBody
msgid "%1$S has canceled this event: « %2$S »"
msgstr "%1$S отмени това събитие: « %2$S »"

#: confirmProcessInvitation
msgid ""
"You have recently deleted this item, are you sure you want to process this "
"invitation?"
msgstr ""
"Това събитие беше наскоро изтрито, сигурен ли сте, че искате да обработите "
"поканата?"

#: confirmProcessInvitationTitle
msgid "Process Invitation?"
msgstr "Обработване на поканата?"

#: invitationsLink.label
msgid "Invitations: %1$S"
msgstr "Покани: %1$S"
