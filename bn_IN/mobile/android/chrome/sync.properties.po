#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "সর্বশেষ সিঙ্ক: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "সর্বশেষ সিঙ্ক: চলছে…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "অ্যাকাউন্ট: %S"

#: notconnected.label
msgid "Not connected"
msgstr "সংযুক্ত নয়"

#: connecting.label
msgid "Connecting…"
msgstr "সংযোগ স্থাপন করা হচ্ছে…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "আপনার ফায়ারফক্স সিঙ্ক অ্যাকাউন্ট সরিয়ে ফেলা হয়েছে"

#: notificationDisconnect.button
msgid "Undo"
msgstr "পূর্বাবস্থা"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 ফায়ারফক্স সিঙ্কের সর্বশেষ সংস্করণের সাথে সামঞ্জস্যপূর্ণ নয়। দয়া করে সর্বশেষ "
"সংস্করণে আপডেট করুন।"

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 ফায়ারফক্স সিঙ্কের পুরানো সংস্করণের সাথে সামঞ্জস্যপূর্ণ নয়। দয়া করে ফায়ারফক্স "
"আপডেট করুন আপনার অন্য কম্পিউটারে(কম্পিউটারগুলিতে)।"

#: sync.update.title
msgid "Firefox Sync"
msgstr "ফায়ারফক্স সিঙ্ক"

#: sync.update.button
msgid "Learn More"
msgstr "আরও জানুন"

#: sync.update.close
msgid "Close"
msgstr "বন্ধ করুন"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "সিঙ্ক সেটাপ করা গেল না"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "ইন্টারনেট কানেকশান নেই"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S সিঙ্ক এর সাথে যোগাযোগ করা গেল না। আবার চেষ্টা করতে চান?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "পুনরায় চেষ্টা করুন"

#: sync.setup.manual
msgid "Manual setup"
msgstr "স্বনির্বাচিত সেটাপ"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "আপনার অন্য কম্পিউটারগুলিতে কোন ট্যাব নেই।"

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "সিঙ্ক এখন আগের থেকে আরও নতুন এবং সহজতর।"

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "আরও জানতে এখানে টিপুন"
