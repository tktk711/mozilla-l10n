#. # String will be replaced by brandShortName.
#: saveLogin
msgid "Would you like %S to remember this login?"
msgstr "আপনি কি %S কে এই লগইনটি মনে রাখাতে চান?"

#: rememberButton
msgid "Remember"
msgstr "মনে রাখুন"

#: neverButton
msgid "Never"
msgstr "কখনো না"

#. # String is the login's hostname
#: updatePassword
msgid "Update saved password for %S?"
msgstr "%S এর পাসওয়ার্ড আপডেট সংরক্ষণ করা হবে কি?"

#: updatePasswordNoUser
msgid "Update saved password for this login?"
msgstr "সংরক্ষণ করা পাসওয়ার্ড এই লগিনের জন্যে আপডেট করবেন?"

#: updateButton
msgid "Update"
msgstr "আপডেট করুন"

#: dontUpdateButton
msgid "Don't update"
msgstr "আপডেট করবেন না"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "আপনি কোন ব্যবহারকারীর জন্য পাসওয়ার্ড পরিবর্তন করছেন তা নিশ্চিত করুন"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "পাসওয়ার্ড পরিবর্তন নিশ্চিত করুন"

#. # Strings used by PromptService.js
#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "এই পাসওয়ার্ড মনে রাখার জন্য পাসওয়ার্ড পরিচালন ব্যবস্থা ব্যবহার করুন।"

#: username
msgid "Username"
msgstr "ব্যবহারকারীর নাম"

#: password
msgid "Password"
msgstr "পাসওয়ার্ড"
