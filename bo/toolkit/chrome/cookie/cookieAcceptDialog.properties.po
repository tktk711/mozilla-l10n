#: hostColon
msgid "Host:"
msgstr "རྩ་འཛུགས་གནས:"

#: domainColon
msgid "Domain:"
msgstr "དྲ་ཁོངས:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "གསང་རྒྱ་བྱས་པའི་སྦྲེལ་མཐུད་ཁོ་ན"

#: forAnyConnection
msgid "Any type of connection"
msgstr "སྦྲེལ་མཐུད་གང་རུང་ཞིག"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "ལེ་ཚན་གྱི་མཐར་ལ"

#: showDetails
msgid "Show Details"
msgstr "གསལ་བཤད་མངོན་པ"

#: hideDetails
msgid "Hide Details"
msgstr "གསལ་བཤད་ཡིབ"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "དྲ་ཚིགས་%S་ཡིས་དྲ་ཤུལ་སྒྲིག་འཛུགས་འདོད་པ"

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "དྲ་ཚིགས་%S་ཡིས་རིམ་པ་གཉིས་པའི་དྲ་ཤུལ་སྒྲིག་འཛུགས་འདོད་པ"

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"%S་དྲ་ཚིགས་ཀྱིས་དྲ་ཤུལ་གཞན་ཞིག་སྒྲིག་འཛུགས་བྱེད་འདོད་པ།\n"
"ཁྱོད་ཀྱིས་དྲ་ཤོག་འདི་ནས་དྲ་ཤུན་%S་ཐོབ་ཡོད་པ"

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "དྲ་ཚིགས་%S་ཡིས་གནས་ཡོད་པའི་དྲ་ཤུལ་བཟོ་བཅོས་འདོད་པ"
