msgstr "ལག་ཆའི་གནས་གསར་པ་ཁ་སྣོན"

#: saveChanges.label
msgid "Done"
msgstr "བཟོས་ཚར"

#: undoChanges.label
msgid "Undo Changes"
msgstr "བསྒྱུར་བཅོས་མི་བཟོ་བ"
