msgid "Customize Toolbar"
msgstr "ལག་ཆའི་གནས་སྒྲིག་བཟོ"

#: dialog.dimensions
msgid "width: 92ch; height: 36em;"
msgstr "width: 92ch; height: 36em;"

#: instructions.description
msgid "You can add or remove items by dragging to or from the toolbars."
msgstr "ཁྱོད་ཀྱིས་ལག་ཆའི་གནས་ནང་གི་དོན་ཚན་རྣམས་དྲུད་ནས་ཁ་སྣོན་དང་བསུབ་ཆོག"

#: show.label
msgid "Show:"
msgstr "སྟོན་པ:"

#: iconsAndText.label
msgid "Icons and Text"
msgstr "མཚོན་རྟགས་དང་ཡི་གེ"

#: icons.label
msgid "Icons"
msgstr "མཚོན་རྟགས"

#: text.label
msgid "Text"
msgstr "ཡི་གེ"

#: useSmallIcons.label
msgid "Use Small Icons"
