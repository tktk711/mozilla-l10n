#~ msgid "&Sign In"
#~ msgstr "&En em gennaskañ"

#~ msgid "%S not verified"
#~ msgstr "n'eo ket gwiriet %S"

#~ msgid "Please click the verification link in the email sent to %S"
#~ msgstr "Klikit war an ere gwiriañ er postel bet kaset da %S"

#~ msgid "&Resend"
#~ msgstr "&Kas en-dro"

#~ msgid "&Forget"
#~ msgstr "&Ankouaat"

#~ msgid "Un&link Sync"
#~ msgstr "Di&liammañ Sync"

#~ msgid "Unlink old version of Sync?"
#~ msgstr "Diliammañ an handelv kozh eus Sync ?"

