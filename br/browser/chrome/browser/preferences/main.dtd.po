#: startup.label
msgid "Startup"
msgstr "Loc'hañ"

#: startupPage.label startupPage.accesskey
msgid "When &brandShortName; &starts:"
msgstr "&Pa loc'h &brandShortName; :"

#: startupHomePage.label
msgid "Show my home page"
msgstr "Diskouez ma fennbajennad"

#: startupBlankPage.label
msgid "Show a blank page"
msgstr "Diskouez ur bajenn wenn"

#: startupLastSession.label
msgid "Show my windows and tabs from last time"
msgstr "Diskouez ma frenestroù hag ivinelloù evel ar wech diwezhañ"

#: homepage.label homepage.accesskey
msgid "Home &Page:"
msgstr "P&ennbajennad :"

#: useCurrentPage.label useCurrentPage.accesskey
msgid "Use &Current Page"
msgstr "&Arverañ ar bajenn vremanel"

#: useMultiple.label
msgid "Use Current Pages"
msgstr "Arverañ ar pajennoù bremanel"

#: chooseBookmark.label chooseBookmark.accesskey
msgid "Use &Bookmark…"
msgstr "A&rverañ ur sined…"

#: restoreDefault.label restoreDefault.accesskey
msgid "&Restore to Default"
msgstr "Assav an arventennoù dre &ziouer"

#: downloads.label
msgid "Downloads"
msgstr "Pellgargadurioù"

#: saveTo.label saveTo.accesskey
msgid "Sa&ve files to"
msgstr "Enr&ollañ restroù e-barzh"

#: chooseFolderWin.label chooseFolderWin.accesskey
msgid "Br&owse…"
msgstr "&Furchal…"

#: chooseFolderMac.label chooseFolderMac.accesskey
msgid "Choos&e…"
msgstr "&Diuzañ…"

#: alwaysAsk.label alwaysAsk.accesskey
msgid "&Always ask me where to save files"
msgstr "A&tav goulenn diganin pelec'h lakaat restroù"

#: alwaysCheckDefault.label
msgid ""
"Always check to see if &brandShortName; is the default browser on startup"
msgstr ""
"Gwiriañ atav ma'z eo &brandShortName; ar merdeer dre ziouer e-pad al loc'hañ"

#: alwaysCheckDefault2.label alwaysCheckDefault2.accesskey
msgid "Al&ways check if &brandShortName; is your default browser"
msgstr "G&wiriañ atav ma'z eo &brandShortName; ho merdeer dre ziouer"

#: setAsMyDefaultBrowser.label setAsMyDefaultBrowser.accesskey
msgid "Make &brandShortName; My &Default Browser"
msgstr "Arverañ &brandShortName; da verdeer dre z&iouer"

#: setAsMyDefaultBrowser2.label setAsMyDefaultBrowser2.accesskey
msgid "Make &Default"
msgstr "&Lakaat dre ziouer"

#: isDefault.label
msgid "&brandShortName; is currently your default browser"
msgstr "Ho merdeer dre ziouer eo &brandShortName;"

#: isNotDefault.label
msgid "&brandShortName; is not your default browser"
msgstr "N'eo ket &brandShortName; ho merdeer dre ziouer"

#: separateProfileMode.label
msgid "Allow &brandShortName; and Firefox to run at the same time"
msgstr "Aotren &brandShortName; ha Firefox da erounit war un dro"

#: useFirefoxSync.label
msgid "Tip: This uses separate profiles. Use Sync to share data between them."
msgstr ""
"Tun : arveret e vo aeladoù disheñvel. Arverit Sync evit rannañ roadennoù "
"etrezo."

#: getStarted.label
msgid "Start using Sync…"
msgstr "Arverañ Sync…"

