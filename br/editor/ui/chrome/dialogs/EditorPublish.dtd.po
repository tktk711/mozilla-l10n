#. Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "Embann ar bajenn"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "Arventennoù ar brudañ"

#: publishTab.label
msgctxt "publishTab.label"
msgid "Publish"
msgstr "Brudañ"

#: settingsTab.label
msgid "Settings"
msgstr "Arventennoù"

#: publishButton.label
msgctxt "publishButton.label"
msgid "Publish"
msgstr "Brudañ"

#. Publish Tab Panel
#: siteList.label siteList.accesskey
msgctxt "siteList.label siteList.accesskey"
msgid "Sit&e Name:"
msgstr "&Anv al lec'hienn:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "Dibabit da beseurt lec'hienn e fell deoc'h brudañ"

#: newSiteButton.label newSiteButton.accesskey
msgid "&New Site"
msgstr "&Lec'hienn nevez"

#: docDirList.label docDirList.accesskey
msgid "&Site subdirectory for this page:"
msgstr "&Kavlec'h al lec'hienn evit ar bajenn-mañ:"

#: docDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory for this page"
msgstr "Dibabit pe bizskrivit anv ar c'havlec'h a-bell evit ar bajenn-mañ"

#: publishImgCheckbox.label publishImgCheckbox.accesskey
msgid "Include images and &other files"
msgstr "&Enderc'hel ar skeudennoù hag ar restroù all"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr "Brudañ ar skeudennoù hag ar restroù all meneget er bajenn-mañ"

#: sameLocationRadio.label sameLocationRadio.accesskey
msgid "&Use same location as page"
msgstr "&Implijout ar memes lec'hiadur hag ar bajenn"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "Embann ar restroù er memes lec'hiadur hag ar bajenn"

#: useSubdirRadio.label useSubdirRadio.accesskey
msgid "Use this site sub&directory:"
msgstr "I&mplijout iskavlec'h-mañ al lec'hienn:"

#: useSubdirRadio.tooltip
msgid "Publish files to the selected remote subdirectory"
msgstr "Embann ar restroù en iskavlec'h a-bell diuzet"

#: otherDirList.tooltip
msgid ""
"Choose or enter name of remote subdirectory where files will be published"
msgstr ""
"Dibabit pe bizskrivit anv ar c'havlec'h a-bell da belec'h e vo brudet ar "
"restroù"

#: pageTitle.label pageTitle.accesskey
msgid "Page &Title:"
msgstr "&Titl ar bajenn:"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr ""
"Bizskrivit un titl evit anavezout ar bajenn e-barzh ar prenestr pe ar sinedoù"

#: pageTitleExample.label
msgid "e.g.: \"My Web Page\""
msgstr "skouer: \"Ma fajenn Web\""

#: filename.label filename.accesskey
msgid "&Filename:"
msgstr "A&nv ar restr:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr ""
"Bizskrivit un anv evit ar restr-mañ, en ur enderc'hel '.html' evit ur bajenn "
"web"

#: filenameExample.label
msgid "e.g,: \"mypage.html\""
msgstr "skouer: \"mafajenn.html\""

#: setDefaultButton.label setDefaultButton.accesskey
msgid "Set as &Default"
msgstr "A&rverañ dre ziouer"

#: removeButton.label removeButton.accesskey
msgid "&Remove Site"
msgstr "Di&lemel al lec'hienn"

#. Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "Brudañ lec'hiennoù"

#: serverInfo.label
msgid "Server Information"
msgstr "Stlennoù an dafariad"

#: loginInfo.label
msgid "Login Information"
msgstr "Stlennoù kennask"

#: siteName.label siteName.accesskey
msgctxt "siteName.label siteName.accesskey"
msgid "Sit&e Name:"
msgstr "&Anv al lec'hienn:"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr "Ul lesanv evit anavezout al lec'hienn-mañ (skouer: 'MaLec'hienn')"

#: siteUrl.label siteUrl.accesskey
msgid "Publishing &address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr ""
"&Chomlec'h evit ar brudañ (skouer: 'ftp://ftp.maisp.com/maanvarveriad'):"

#: siteUrl.tooltip
msgid ""
"The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr ""
"Ar chomlec'h FTP:// pe HTTP:// roet gant ho pourvezer servijoù ar genrouedad "
"(ISP) pe ho herberc'hier lec'hienn"

#: browseUrl.label browseUrl.accesskey
msgid ""
"H&TTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr ""
"Chom&lec'h HTTP ho pennbajenn (skouer: 'http://www.maisp.com/maanvarveriad'):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr ""
"Ar chomlec'h HTTP:// ho penngavlec'h (n'endalc'hit ket an anvioù restr)"

#: username.label username.accesskey
msgid "&User name:"
msgstr "A&nv arveriad:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr ""
"An anv arveriad a implijit evit kennaskañ ouzh ho pourvezer servijoù ar "
"genrouedad (ISP) pe ho herberc'hier lec'hienn"

#: password.label password.accesskey
msgid "Pass&word:"
msgstr "&Ger-tremen:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "Ar ger-tremen kevredet gant hoc'h anv arveriad"

#: savePassword.label savePassword.accesskey
msgid "&Save Password"
msgstr "&Enrollañ ar ger-tremen"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr ""
"Diuzit an dra-mañ evit enrollañ diarvar ho ker-tremen gant an ardoer gerioù-"
"tremen"
