#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Goubredañ diwezhañ : %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Goubredañ diwezhañ : o vont war-raok..."

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Kont : %S"

#: notconnected.label
msgid "Not connected"
msgstr "N'eo ket kennasket"

#: connecting.label
msgid "Connecting…"
msgstr "O kennaskañ…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Dilamet eo bet ho kont evit Firefox Sync"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Dizober"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 n'eo ket keverlec'h gant an handelv nevesañ eus Firefox Sync. Mar "
"plij, hizivait gant an handelv diwezhañ."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 n'eo ket keverlec'h gant an handelv koshoc'h eus Firefox Sync. Mar "
"plij, hizivait Firefox war hoc'h urzhiataer(ioù) all."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Gouzout hiroc'h"

#: sync.update.close
msgid "Close"
msgstr "Serriñ"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "N'haller ket kefluniañ Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "N'eus kennask hegerz ebet ouzh internet"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr ""
"%S n'eo ket bet gouest da gennaskañ ouzh Sync. Ha fellout a ra deoc'h klask "
"en-dro ?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Klask en-dro"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Kefluniañ dre zorn"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Ivinell ebet diouzh hoc'h urzhiataerioù all."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Nevez flamm eo Sync hag aesoc'h eget biskoazh."

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Stokit amañ da c'houzout hiroc'h"
