msgstr "I"

#: toggleToolboxF12.keycode
msgid "VK_F12"
msgstr "VK_F12"

#: toggleToolboxF12.keytext
msgid "F12"
msgstr "F12"

#: toolboxCloseButton.tooltip
msgid "Close Developer Tools"
msgstr "Zatvori razvojne alate"

#: toolboxOptionsButton.key
msgid "O"
msgstr "O"

#: toolboxNextTool.key
msgid "]"
msgstr "]"

#: toolboxPreviousTool.key
msgid "["
msgstr "["

