#: 1
msgid "Parsing an XSLT stylesheet failed."
msgstr "Parsiranje XSLT stila nije uspjelo."

#: 2
msgid "Parsing an XPath expression failed."
msgstr "Parsiranje XPath izraza nije uspjelo."

#: 3
msgctxt "3"
msgid ""
msgstr ""

#: 4
msgid "XSLT transformation failed."
msgstr "XSLT transformacija nije uspjela."

#: 5
msgid "Invalid XSLT/XPath function."
msgstr "Neispravna XSLT/XPath funkcija."

#: 6
msgid "XSLT Stylesheet (possibly) contains a recursion."
msgstr "XSLT stil (vjerovatno) sadrži rekurziju."

#: 7
msgid "Attribute value illegal in XSLT 1.0."
msgstr "Vrijednost atributa je neispravna u XSLT 1.0."

#: 8
msgid "An XPath expression was expected to return a NodeSet."
msgstr "Od XPath izraza je očekivano da vrati NodeSet."

#: 9
msgid "XSLT transformation was terminated by <xsl:message>."
msgstr "XSLT transformacija je prekinuta od <xsl:message>."

#: 10
msgid "A network error occurred loading an XSLT stylesheet:"
msgstr "Došlo je do greške u mreži prilikom učitavanja XSLT stila:"

#: 11
msgid "An XSLT stylesheet does not have an XML mimetype:"
msgstr "XSLT stil ne sadrži XML mimetype:"

#: 12
msgid "An XSLT stylesheet directly or indirectly imports or includes itself:"
msgstr "XSLT stil direktno ili indirektno uvozi ili uključuje samog sebe:"

#: 13
msgid "An XPath function was called with the wrong number of arguments."
msgstr "XPath funkcija je pozvana sa pogrešnim brojem argumenata."

#: 14
msgid "An unknown XPath extension function was called."
msgstr "Nepoznata funkcija XPath ekstenzije je bila pozvana."

#: 15
msgid "XPath parse failure: ‘)’ expected:"
msgstr "Greška u XPath parsiranju: očekivano ')':"

#: 16
msgid "XPath parse failure: invalid axis:"
msgstr "Greška u XPath parsiranju: pogrešna osa:"

#: 17
msgid "XPath parse failure: Name or Nodetype test expected:"
msgstr "Greška u XPath parsiranju: očekivana provjera imena ili Nodetypea:"

#: 18
msgid "XPath parse failure: ‘]’ expected:"
msgstr "Greška u XPath parsiranju: očekivano ']':"

#: 19
msgid "XPath parse failure: invalid variable name:"
msgstr "Greška u XPath parsiranju: neispravno ime varijable:"

#: 20
msgid "XPath parse failure: unexpected end of expression:"
msgstr "Greška u XPath parsiranju: neočekivan kraj izraza:"

#: 21
msgid "XPath parse failure: operator expected:"
msgstr "Greška u XPath parsiranju: očekivan operator:"

#: 22
msgid "XPath parse failure: unclosed literal:"
msgstr "Greška u XPath parsiranju: nezatvoren literal:"

#: 23
msgid "XPath parse failure: ‘:’ unexpected:"
msgstr "Greška u XPath parsiranju: ':' nije očekivano:"

#: 24
msgid "XPath parse failure: ‘!’ unexpected, negation is not():"
msgstr "Greška u XPath parsiranju: '!' nije očekivano, negacija je not():"

#: 25
msgid "XPath parse failure: illegal character found:"
msgstr "Greška u XPath parsiranju: pronađen je neodgovarajući znak:"

#: 26
msgid "XPath parse failure: binary operator expected:"
msgstr "Greška u XPath parsiranju: očekivan je binarni operator:"

#: 27
msgid "An XSLT stylesheet load was blocked for security reasons."
msgstr "Učitavanje XSLT stila je blokirano iz sigurnosnih razloga."

#: 28
msgid "Evaluating an invalid expression."
msgstr "Procjenjivanje neispravnog izraza."

#: 29
msgid "Unbalanced curly brace."
msgstr "Neuravnotežene vitičaste zagrade."

#: 30
msgid "Creating an element with an invalid QName."
msgstr "Kreiranje elementa sa neispravnim QName."

#: 31
msgid "Variable binding shadows variable binding within the same template."
msgstr ""
"Dodjela promjenjive zasjenjuje dodjelu promjenjive unutar istog obrasca."

#: 32
msgid "Call to the key function not allowed."
msgstr "Poziv ključne funkcije nije dopušten."

#: LoadingError
msgid "Error loading stylesheet: %S"
msgstr "Greška pri učitavanju stila: %S"

#: TransformError
msgid "Error during XSLT transformation: %S"
msgstr "Greška prilikom XSLT transformacije: %S"
