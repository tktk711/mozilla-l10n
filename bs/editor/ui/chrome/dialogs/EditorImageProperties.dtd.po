#. These strings are for use specifically in the editor's image and form image dialogs.
#. Window title
#: windowTitle.label
msgid "Image Properties"
msgstr "Postavke slike"

#: pixelsPopup.value
msgid "pixels"
msgstr "piksela"

#. These are in the Location tab panel
#: locationEditField.label locationEditField.accessKey
msgid "Image &Location:"
msgstr "&Lokacija slike:"

#: locationEditField.tooltip
msgid "Type the image's filename or location"
msgstr "Upišite naziv fajla ili lokaciju"

#: locationEditField.shortenedDataURI
msgid "Shortened data URI (copy will place the full URI onto the clipboard)"
msgstr "Skraćeni URI podataka (kopiranje će staviti cijeli URI na clipboard)"

#: title.label title.accessKey
msgid "&Tooltip:"
msgstr "&Tooltip:"

#: title.tooltip
msgid "The html 'title' attribute that displays as a tooltip"
msgstr "Html 'title' atribut koji se prikazuje kao tooltip"

#: altText.label altText.accessKey
msgid "&Alternate text:"
msgstr "&Alternativni tekst:"

#: altTextEditField.tooltip
msgid "Type text to display in place of the image"
msgstr "Upišite tekst koji će se prikazati na mjestu slike"

#: noAltText.label noAltText.accessKey
msgid "&Don't use alternate text"
msgstr "&Ne koristi alternativni tekst"

#: previewBox.label
msgid "Image Preview"
msgstr "Pregled slike"

#. These controls are in the Dimensions tab panel
#. actualSize.label should be same as actualSizeRadio.label + ":"
#: actualSize.label
msgid "Actual Size:"
msgstr "Stvarna veličina:"

#: actualSizeRadio.label actualSizeRadio.accessKey
msgid "&Actual Size"
msgstr "Stv&arna veličina"

#: actualSizeRadio.tooltip
msgid "Revert to the image's actual size"
msgstr "Vrati sliku na stvarnu veličinu"

#: customSizeRadio.label customSizeRadio.accessKey
msgid "Custom &Size"
msgstr "&Prilagođena veličina"

#: customSizeRadio.tooltip
msgid "Change the image's size as displayed in the page"
msgstr "Promijeni veličinu slike na onnu prikazanu na stranici"

#: heightEditField.label heightEditField.accessKey
msgid "Hei&ght:"
msgstr "&Visina:"

#: widthEditField.label widthEditField.accessKey
msgid "&Width:"
msgstr "&Širina:"

#: constrainCheckbox.label constrainCheckbox.accessKey
msgid "&Constrain"
msgstr "&Ograniči"

#: constrainCheckbox.tooltip
msgid "Maintain the image's aspect ratio"
msgstr "Zadrži omjer slike"

#. These controls are in the Image Map box of the expanded area
#: imagemapBox.label
msgid "Image Map"
msgstr "Mapa slike"

#: removeImageMapButton.label removeImageMapButton.accessKey
msgid "&Remove"
msgstr "&Ukloni"

#. These are the options for image alignment
#: alignment.label
msgid "Align Text to Image"
msgstr "Poravnaj tekst uz sliku"

#: bottomPopup.value
msgid "At the bottom"
msgstr "Na dno"

#: topPopup.value
msgid "At the top"
msgstr "Na vrh"

#: centerPopup.value
msgid "In the center"
msgstr "U sredinu"

#: wrapRightPopup.value
msgid "Wrap to the right"
msgstr "Omotaj na desno"

#: wrapLeftPopup.value
msgid "Wrap to the left"
msgstr "Omotaj na lijevo"

#. These controls are in the Spacing Box
#: spacingBox.label
msgid "Spacing"
msgstr "Razmak"

#: leftRightEditField.label leftRightEditField.accessKey
msgid "&Left and Right:"
msgstr "&Lijevo i desno:"

#: topBottomEditField.label topBottomEditField.accessKey
msgid "&Top and Bottom:"
msgstr "&Vrh i dno:"

#: borderEditField.label borderEditField.accessKey
msgid "Solid &Border:"
msgstr "Puni &okvir:"

#. These controls are in the Link Box
#: showImageLinkBorder.label showImageLinkBorder.accessKey
msgid "Show &border around linked image"
msgstr "Prikaži &okvir oko povezane slike s linkom"

#: LinkAdvancedEditButton.label LinkAdvancedEditButton.accessKey
msgid "&Link Advanced Edit…"
msgstr "Napredno uređivanje &linka…"

#: LinkAdvancedEditButton.tooltip
msgid "Add or modify HTML attributes, style attributes, and JavaScript"
msgstr "Dodaj ili uredi HTML atribute, atribute stila i JavaScript"

#. These tabs are currently used in the image input dialog
#: imageInputTab.label
msgid "Form"
msgstr "Obrazac"

#: imageLocationTab.label
msgid "Location"
msgstr "Lokacija"

#: imageDimensionsTab.label
msgid "Dimensions"
msgstr "Dimenzije"

#: imageAppearanceTab.label
msgid "Appearance"
msgstr "Izgled"

#: imageLinkTab.label
msgid "Link"
msgstr "Link"
