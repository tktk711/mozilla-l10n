#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Zadnja sinhronizacija: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Zadnja sinkronizacija: u toku…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Račun: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Nije povezan"

#: connecting.label
msgid "Connecting…"
msgstr "Povezivanje…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Vaš Firefox Sync račun je uklonjen."

#: notificationDisconnect.button
msgid "Undo"
msgstr "Poništi"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 nije kompatibilan sa zadnjom Firefox Sync verzijom. Nadogradite na "
"najnoviju verziju."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 nije kompatibilan sa starijim Firefox Sync verzijama. Nadogradite "
"Firefox na ostalim vašim računarima."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Saznajte više"

#: sync.update.close
msgid "Close"
msgstr "Zatvori"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Ne mogu podesiti Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Nema dostupne internet konekcije"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S se ne može povezati na Sync. Želite li pokušati ponovo?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Pokušaj"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Ručno podešavanje"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Nema tabova s vaših drugih računara."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Sync je potpuno novi i lakši nego ikad prije. "

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Dodirnite ovdje da saznate više"
