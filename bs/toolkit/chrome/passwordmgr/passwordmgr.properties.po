#: rememberValue
msgid "Use Password Manager to remember this value."
msgstr "Koristi Upravnik Lozinkama za pamćenje ove vrijednosti."

#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "Koristi Upravnik Lozinkama za pamćenje ove lozinke."

#: savePasswordTitle
msgid "Confirm"
msgstr "Potvrdi"

#. # LOCALIZATION NOTE (saveLoginMsg, saveLoginMsgNoUser):
#. # %1$S is brandShortName, %2$S is the login's hostname.
#: saveLoginMsg
msgid "Would you like %1$S to save this login for %2$S?"
msgstr "Želite li da %1$S spasi ovu prijavu za %2$S?"

#: saveLoginMsgNoUser
msgid "Would you like %1$S to save this password for %2$S?"
msgstr "Želite li da %1$S spasi ovu lozinku za %2$S?"

#: saveLoginButtonAllow.label saveLoginButtonAllow.accesskey
msgid "&Save"
msgstr "&Spasi"

#: saveLoginButtonDeny.label saveLoginButtonDeny.accesskey
msgid "&Don’t Save"
msgstr "Nemoj sp&asiti"

#: updateLoginMsg
msgid "Would you like to update this login?"
msgstr "Da li želite ažurirati ovu prijavu?"

#: updateLoginMsgNoUser
msgid "Would you like to update this password?"
msgstr "Da li biste željeli ažurirati ovu lozinku?"

#: updateLoginButtonText
msgid "Update"
msgstr "Ažuriraj"

#: updateLoginButtonAccessKey
msgctxt "updateLoginButtonAccessKey"
msgid "U"
msgstr "U"

#: updateLoginButtonDeny.label updateLoginButtonDeny.accesskey
msgid "&Don’t Update"
msgstr "Nemoj až&urirati"

#. # LOCALIZATION NOTE (rememberPasswordMsg):
#. # 1st string is the username for the login, 2nd is the login's hostname.
#. # Note that long usernames may be truncated.
#: rememberPasswordMsg
msgid "Would you like to remember the password for “%1$S” on %2$S?"
msgstr "Da li biste željeli zapamtiti lozinku za \"%1$S\" na %2$S?"

#. # LOCALIZATION NOTE (rememberPasswordMsgNoUsername):
#. # String is the login's hostname.
#: rememberPasswordMsgNoUsername
msgid "Would you like to remember the password on %S?"
msgstr "Da li biste željeli zapamtiti lozinku na %S?"

#. # LOCALIZATION NOTE (noUsernamePlaceholder):
#. # This is displayed in place of the username when it is missing.
#: noUsernamePlaceholder
msgctxt "noUsernamePlaceholder"
msgid "No username"
msgstr "Nema korisničkog imena"

#: togglePasswordLabel
msgid "Show password"
msgstr "Prikaži lozinku"

#: togglePasswordAccessKey2
msgid "h"
msgstr "h"

#: notNowButtonText
msgid "&Not Now"
msgstr "&Ne sada"

#: notifyBarNotNowButtonText
msgid "Not Now"
msgstr "Ne sada"

#: notifyBarNotNowButtonAccessKey
msgid "N"
msgstr "N"

#: neverForSiteButtonText
msgid "Ne&ver for This Site"
msgstr "&Nikada za ovu stranicu"

#: notifyBarNeverRememberButtonText2
msgid "Never Save"
msgstr "Nikad ne spašavaj"

#: notifyBarNeverRememberButtonAccessKey2
msgid "e"
msgstr "e"

#: rememberButtonText
msgid "&Remember"
msgstr "&Zapamti"

#: notifyBarRememberPasswordButtonText
msgid "Remember Password"
msgstr "Zapamti lozinku"

#: notifyBarRememberPasswordButtonAccessKey
msgid "R"
msgstr "R"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "Potvrdi promjenu lozinke"

#. # LOCALIZATION NOTE (updatePasswordMsg):
#. # String is the username for the login.
#: updatePasswordMsg
msgid "Would you like to update the saved password for “%S”?"
msgstr "Da li biste željeli ažurirati spašenu lozinku za \"%S\"?"

#: updatePasswordMsgNoUser
msgid "Would you like to update the saved password?"
msgstr "Da li biste željeli ažurirati spašenu lozinku?"

#: notifyBarUpdateButtonText
msgid "Update Password"
msgstr "Ažuriraj lozinku"

#: notifyBarUpdateButtonAccessKey
msgctxt "notifyBarUpdateButtonAccessKey"
msgid "U"
msgstr "U"

#: notifyBarDontChangeButtonText
msgid "Don’t Change"
msgstr "Ne mijenjaj"

#: notifyBarDontChangeButtonAccessKey
msgid "D"
msgstr "D"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "Molimo da potvrdite za kojeg korisnika mijenjate lozinku"

#: hidePasswords
msgid "Hide Passwords"
msgstr "Sakrij lozinke"

#: hidePasswordsAccessKey
msgctxt "hidePasswordsAccessKey"
msgid "P"
msgstr "P"

#: showPasswords
msgid "Show Passwords"
msgstr "Prikaži lozinke"

#: showPasswordsAccessKey
msgctxt "showPasswordsAccessKey"
msgid "P"
msgstr "P"

#: noMasterPasswordPrompt
msgid "Are you sure you wish to show your passwords?"
msgstr "Da li ste sigurni da želite prikazati vaše lozinke?"

#: removeAllPasswordsPrompt
msgid "Are you sure you wish to remove all passwords?"
msgstr "Da li ste sigurni da želite ukloniti sve lozinke?"

#: removeAllPasswordsTitle
msgid "Remove all passwords"
msgstr "Ukloni sve lozinke"

#: removeLoginPrompt
msgid "Are you sure you wish to remove this login?"
msgstr "Da li ste sigurni da želite ukloniti ovu prijavu?"

#: removeLoginTitle
msgid "Remove login"
msgstr "Ukloni prijavu"

#: loginsDescriptionAll
msgid "Logins for the following sites are stored on your computer:"
msgstr "Lozinke za sljedeće stranice su pohranjene na vašem računaru:"

#: loginsDescriptionFiltered
msgid "The following logins match your search:"
msgstr "Navedene lozinke se podudaraju sa pretragom:"

#. # LOCALIZATION NOTE (loginHostAge):
#. # This is used to show the context menu login items with their age.
#. # 1st string is the username for the login, 2nd is the login's age.
#: loginHostAge
msgid "%1$S (%2$S)"
msgstr "%1$S (%2$S)"

#. # LOCALIZATION NOTE (noUsername):
#. # String is used on the context menu when a login doesn't have a username.
#: noUsername
msgctxt "noUsername"
msgid "No username"
msgstr "Nema korisničkog imena"

#: duplicateLoginTitle
msgid "Login already exists"
msgstr "Prijava već postoji"

#: duplicateLogin
msgid "A duplicate login already exists."
msgstr "Duplikat prijave već postoji."

#. # LOCALIZATION NOTE (insecureFieldWarningDescription2, insecureFieldWarningDescription3):
#. # %1$S will contain insecureFieldWarningLearnMore and look like a link to indicate that clicking will open a tab with support information.
#: insecureFieldWarningDescription2
msgid ""
"This connection is not secure. Logins entered here could be compromised. %1$S"
msgstr ""
"Ova konekcija nije sigurna. Podaci za prijavu unešeni ovdje mogu biti "
"kompromitovani. %1$S"

#: insecureFieldWarningDescription3
msgid "Logins entered here could be compromised. %1$S"
msgstr "Podaci za prijavu unešeni ovdje mogu biti kompromitovani. %1$S"

#: insecureFieldWarningLearnMore
#, fuzzy
msgid "Learn More"
msgstr "Saznajte više"

