msgid "OK"
msgstr "D'acord"

#: nssdialogs.cancel.label
msgid "Cancel"
msgstr "Cancel·la"

#. # These strings are stolen from security/manager/locales/en-US/chrome/pippki/pippki.dtd
#: downloadCert.title
msgid "Downloading Certificate"
msgstr "S'està baixant el certificat"

#: downloadCert.message1
msgid "You have been asked to trust a new Certificate Authority (CA)."
msgstr "S'ha sol·licitat confiar en una entitat certificadora nova."

#: downloadCert.viewCert.label
msgctxt "downloadCert.viewCert.label"
msgid "View"
msgstr "Mostra"

#: downloadCert.trustSSL
msgid "Trust to identify websites."
msgstr "Confia-hi per identificar llocs web."

#: downloadCert.trustEmail
msgid "Trust to identify email users."
msgstr "Confia-hi per identificar usuaris de correu."

#: downloadCert.trustObjSign
msgid "Trust to identify software developers."
msgstr "Confia-hi per identificar desenvolupadors de programari."

#: pkcs12.getpassword.title
msgid "Password Entry Dialog"
msgstr "Diàleg Introducció de la contrasenya"

#: pkcs12.getpassword.message
msgid ""
"Please enter the password that was used to encrypt this certificate backup."
msgstr ""
"Introduïu la contrasenya que es va utilitzar per xifrar la còpia de "
"seguretat d'aquest certificat."

#: clientAuthAsk.title
msgid "User Identification Request"
msgstr "Sol·licitud d'identificació de l'usuari"

#: clientAuthAsk.message1
msgid "This site has requested that you identify yourself with a certificate:"
msgstr "Aquest lloc ha sol·licitat que us identifiqueu amb un certificat:"

#: clientAuthAsk.message2
msgid "Choose a certificate to present as identification:"
msgstr "Trieu un certificat per presentar-lo com a identificació:"

#: clientAuthAsk.message3
msgid "Details of selected certificate:"
msgstr "Detalls del certificat seleccionat:"

#: clientAuthAsk.remember.label
msgid "Remember this decision"
msgstr "Recorda aquesta decisió"

#. # LOCALIZATION NOTE(clientAuthAsk.nickAndSerial): Represents a single cert when
#. # the user is choosing from a list of certificates.
#. # %1$S is the nickname of the cert.
#. # %2$S is the serial number of the cert in AA:BB:CC hex format.
#: clientAuthAsk.nickAndSerial
msgid "%1$S [%2$S]"
msgstr "%1$S [%2$S]"

#. # LOCALIZATION NOTE(clientAuthAsk.hostnameAndPort):
#. # %1$S is the hostname of the server.
#. # %2$S is the port of the server.
#: clientAuthAsk.hostnameAndPort
msgid "%1$S:%2$S"
msgstr "%1$S:%2$S"

#. # LOCALIZATION NOTE(clientAuthAsk.organization): %S is the Organization of the
#. # server cert.
#: clientAuthAsk.organization
msgid "Organization: \"%S\""
msgstr "Organització: «%S»"

#. # LOCALIZATION NOTE(clientAuthAsk.issuer): %S is the Organization of the
#. # issuer cert of the server cert.
#: clientAuthAsk.issuer
msgid "Issued Under: \"%S\""
msgstr "Emès sota: «%S»"

#. # LOCALIZATION NOTE(clientAuthAsk.issuedTo): %1$S is the Distinguished Name of
#. # the currently selected client cert, such as "CN=John Doe,OU=Example" (without
#. # quotes).
#: clientAuthAsk.issuedTo
msgid "Issued to: %1$S"
msgstr "Emès per a: %1$S"

#. # LOCALIZATION NOTE(clientAuthAsk.serial): %1$S is the serial number of the
#. # selected cert in AA:BB:CC hex format.
#: clientAuthAsk.serial
msgid "Serial number: %1$S"
msgstr "Número de sèrie: %1$S"

#. # LOCALIZATION NOTE(clientAuthAsk.validityPeriod):
#. # %1$S is the already localized notBefore date of the selected cert.
#. # %2$S is the already localized notAfter date of the selected cert.
#: clientAuthAsk.validityPeriod
msgid "Valid from %1$S to %2$S"
msgstr "Vàlid des de %1$S fins a %2$S"

#. # LOCALIZATION NOTE(clientAuthAsk.keyUsages): %1$S is a comma separated list of
#. # already localized key usages the selected cert is valid for.
#: clientAuthAsk.keyUsages
msgid "Key Usages: %1$S"
msgstr "Usos de la clau: %1$S"

#. # LOCALIZATION NOTE(clientAuthAsk.emailAddresses): %1$S is a comma separated
#. # list of e-mail addresses the selected cert is valid for.
#: clientAuthAsk.emailAddresses
msgid "Email addresses: %1$S"
msgstr "Adreces electròniques: %1$S"

#. # LOCALIZATION NOTE(clientAuthAsk.issuedBy): %1$S is the Distinguished Name of
#. # the cert which issued the selected cert.
#: clientAuthAsk.issuedBy
msgid "Issued by: %1$S"
msgstr "Emès per: %1$S"

#. # LOCALIZATION NOTE(clientAuthAsk.storedOn): %1$S is the name of the PKCS #11
#. # token the selected cert is stored on.
#: clientAuthAsk.storedOn
msgid "Stored on: %1$S"
msgstr "Emmagatzemat a: %1$S"

#: clientAuthAsk.viewCert.label
msgctxt "clientAuthAsk.viewCert.label"
msgid "View"
msgstr "Mostra"

#: certmgr.title
msgid "Certificate Details"
msgstr "Detalls del certificat"

#. # These strings are stolen from security/manager/locales/en-US/chrome/pippki/certManager.dtd
#: certmgr.subjectinfo.label
msgid "Issued To"
msgstr "Emès per a"

#: certmgr.issuerinfo.label
msgid "Issued By"
msgstr "Emès per"

#: certmgr.periodofvalidity.label
msgid "Period of Validity"
msgstr "Període de validesa"

#: certmgr.fingerprints.label
msgid "Fingerprints"
msgstr "Empremtes digitals"

#: certdetail.cn
msgid "Common Name (CN): %1$S"
msgstr "Nom comú (CN): %1$S"

#: certdetail.o
msgid "Organization (O): %1$S"
msgstr "Organització (O): %1$S"

#: certdetail.ou
msgid "Organizational Unit (OU): %1$S"
msgstr "Unitat organitzativa (OU): %1$S"

#. # LOCALIZATION NOTE(certdetail.serialnumber): %1$S is the serial number of the
#. # cert being viewed in AA:BB:CC hex format.
#: certdetail.serialnumber
msgid "Serial Number: %1$S"
msgstr "Número de sèrie: %1$S"

#. # LOCALIZATION NOTE(certdetail.sha256fingerprint): %1$S is the SHA-256
#. # Fingerprint of the cert being viewed in AA:BB:CC hex format.
#: certdetail.sha256fingerprint
msgid "SHA-256 Fingerprint: %1$S"
msgstr "Empremta digital SHA-256: %1$S"

#. # LOCALIZATION NOTE(certdetail.sha1fingerprint): %1$S is the SHA-1 Fingerprint
#. # of the cert being viewed in AA:BB:CC hex format.
#: certdetail.sha1fingerprint
msgid "SHA1 Fingerprint: %1$S"
msgstr "Empremta digital SHA1: %1$S"

#. # LOCALIZATION NOTE(certdetail.notBefore): %1$S is the already localized
#. # notBefore date of the cert being viewed.
#: certdetail.notBefore
msgid "Begins On: %1$S"
msgstr "Data d'inici: %1$S"

#. # LOCALIZATION NOTE(certdetail.notAfter): %1$S is the already localized notAfter
#. # date of the cert being viewed.
#: certdetail.notAfter
msgid "Expires On: %1$S"
msgstr "Data de venciment: %1$S"

