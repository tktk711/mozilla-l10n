#. # 1st string is the username for the login, 2nd is the login's hostname.
#. # Note that long usernames may be truncated.
#: savePassword
msgid "Save password for \"%1$S\" on %2$S?"
msgstr "Voleu desar la contrasenya per «%1$S» per «%2$S»?"

#. # String is the login's hostname
#: savePasswordNoUser
msgid "Save password on %S?"
msgstr "Voleu desar la contrasenya per «%S»?"

#: saveButton
msgid "Save"
msgstr "Desa"

#: dontSaveButton
msgid "Don't save"
msgstr "No desis"

#. # String is the login's hostname
#: updatePassword
msgid "Update saved password for %S?"
msgstr "Voleu actualitzar la contrasenya desada per %S?"

#: updatePasswordNoUser
msgid "Update saved password for this login?"
msgstr "Voleu actualitzar la contrasenya desada d'aquest d'inici de sessió?"

#: updateButton
msgid "Update"
msgstr "Actualitza"

#: dontUpdateButton
msgid "Don't update"
msgstr "No actualitzis"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "Confirmeu a quin usuari esteu canviant-li la contrasenya"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "Confirmeu el canvi de contrasenya"

#. # Strings used by PromptService.js
#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "Utilitza el gestor de contrasenyes per recordar aquesta contrasenya."

#: username
msgid "Username"
msgstr "Nom d'usuari"

#: password
msgid "Password"
msgstr "Contrasenya"
