#: muteButton.muteLabel
msgid "Mute"
msgstr "Sense so"

#: muteButton.unmuteLabel
msgid "Unmute"
msgstr "Amb so"

#: fullscreenButton.enterfullscreenlabel
msgid "Full Screen"
msgstr "Pantalla completa"

#: fullscreenButton.exitfullscreenlabel
msgid "Exit Full Screen"
msgstr "Surt de la pantalla completa"

#: castingButton.castingLabel
msgid "Cast to Screen"
msgstr "Transmet a la pantalla"

#: stats.media
msgid "Media"
msgstr "Suports"

#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: stats.size
msgid "Size"
msgstr "Mida"

#: stats.activity
msgid "Activity"
msgstr "Activitat"

#: stats.activityPaused
msgid "Paused"
msgstr "En pausa"

#: stats.activityPlaying
msgid "Playing"
msgstr "En reproducció"

#: stats.activityEnded
msgid "Ended"
msgstr "Finalitzat"

#: stats.activitySeeking
msgid "(seeking)"
msgstr "(s'està posicionant)"

#: stats.volume
msgid "Volume"
msgstr "Volum"

#: stats.framesParsed
msgid "Frames parsed"
msgstr "Fotogrames analitzats"

#: stats.framesDecoded
msgid "Frames decoded"
msgstr "Fotogrames descodificats"

#: stats.framesPresented
msgid "Frames presented"
msgstr "Fotogrames presentats"

#: stats.framesPainted
msgid "Frames painted"
msgstr "Fotogrames pintats"

#: error.aborted
msgid "Video loading stopped."
msgstr "S'ha aturat la càrrega del vídeo."

#: error.network
msgid "Video playback aborted due to a network error."
msgstr ""
"S'ha interromput la reproducció del vídeo a causa d'un problema de la xarxa."

#: error.decode
msgid "Video can't be played because the file is corrupt."
msgstr "El vídeo no es pot reproduir perquè el fitxer està malament."

#: error.srcNotSupported
msgid "Video format or MIME type is not supported."
msgstr "El format del vídeo o el tipus MIME no és compatible."

#: error.noSource2
msgid "No video with supported format and MIME type found."
msgstr "No s'ha trobat cap vídeo amb un format o un tipus MIME compatibles."

#: error.generic
msgid "Video playback aborted due to an unknown error."
msgstr ""
"S'ha interromput la reproducció del vídeo a causa d'un problema desconegut."

#. the #1 string is the current
#. media position, and the #2 string is the total duration. For example, when at
#. the 5 minute mark in a 6 hour long video, #1 would be "5:00" and #2 would be
#. "6:00:00", result string would be "5:00 of 6:00:00 elapsed".
#: scrubberScale.nameFormat
msgid "#1 of #2 elapsed"
msgstr "S'ha reproduït #1 de #2"
