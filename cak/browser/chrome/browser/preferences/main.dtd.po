#: startup.label
msgid "Startup"
msgstr "Tikirisab'äl"

#: startupPage2.label startupPage2.accesskey
msgid "When &brandShortName; &starts"
msgstr "Toq &brandShortName; &tatikirisaj"

#: startupUserHomePage.label
msgid "Show your home page"
msgstr "Tik'ut pe ri tikirib'äl ruxaq ruk'amaya'l"

#: startupBlankPage.label
msgid "Show a blank page"
msgstr "Tik'ut pe jun kowöl ruxaq"

#: startupPrevSession.label
msgid "Show your windows and tabs from last time"
msgstr "Kek'ut ri taq rutzuwäch chuqa' taq ruwi' ruk'isib'äl rumolojri'ïl"

#: homepage2.label homepage2.accesskey
msgid "Home &Page"
msgstr "Tikirib'äl &ruxaq"

#: useCurrentPage.label useCurrentPage.accesskey
msgid "Use &Current Page"
msgstr "Tokisäx ri ruxaq k'amaya'l k'o &wakami"

#: useMultiple.label
msgid "Use Current Pages"
msgstr "Ke'okisäx ri taq ruxaq k'amaya'l e k'o wakami"

#: chooseBookmark.label chooseBookmark.accesskey
msgid "Use &Bookmark…"
msgstr "Tokisäx &yaketal…"

#: restoreDefault.label restoreDefault.accesskey
msgid "&Restore to Default"
msgstr "&Titzolïx ruwäch ri rajil achi'el ruk'amon pe"

#: downloads.label
msgid "Downloads"
msgstr "Taq qasa'n"

#: saveTo.label saveTo.accesskey
msgid "Sa&ve files to"
msgstr "Ti&yak pa ruyonil"

#: chooseFolderWin.label chooseFolderWin.accesskey
msgid "Br&owse…"
msgstr "Ti&nik'öx…"

#: chooseFolderMac.label chooseFolderMac.accesskey
msgid "Choos&e…"
msgstr "&Ticha'…"

#: alwaysAskWhere.label alwaysAskWhere.accesskey
msgid "&Always ask you where to save files"
msgstr "&Jantape' tik'utüx pe akuchi' yeyak wi kan ri taq yakb'äl"

#: alwaysCheckDefault2.label alwaysCheckDefault2.accesskey
msgid "Alwa&ys check if &brandShortName; is your default browser"
msgstr "Jun&elïk tinik'öx we &brandShortName; ja ri' ri awokik'amaya'l"

#: setAsMyDefaultBrowser2.label setAsMyDefaultBrowser2.accesskey
msgid "Make &Default"
msgstr "Tib'an &kan richin wi chi re"

#: isDefault.label
msgid "&brandShortName; is currently your default browser"
msgstr "&brandShortName; ja awokik'amaya'l kan k'o wi"

#: isNotDefault.label
msgid "&brandShortName; is not your default browser"
msgstr "&brandShortName; man ja ta ri awokik'amaya'l kan k'o wi"

#: separateProfileMode.label
msgid "Allow &brandShortName; and Firefox to run at the same time"
msgstr "Tiya' q'ij chi ri &brandShortName; chuqa' Firefox ketzije' junam"

#: useFirefoxSync.label
msgid "Tip: This uses separate profiles. Use Sync to share data between them."
msgstr ""
"Pixa': Re re' nrokisaj jachon taq ruwäch b'i'aj. Tawokisaj Ximoj richin "
"nakomonij na'oj chi kikojol."

#: getStarted.notloggedin.label
msgid "Sign in to &syncBrand.shortName.label;…"
msgstr "Tatz'ib'aj ab'i' pa &syncBrand.shortName.label;…"

#: getStarted.configured.label
msgid "Open &syncBrand.shortName.label; preferences"
msgstr "Tijaq &syncBrand.shortName.label; taq rajowab'al"

#: e10sEnabled.label
msgid "Enable multi-process &brandShortName;"
msgstr "Titzij ri k'ïy tajin richin &brandShortName;"

#~ msgid "Start using Sync…"
#~ msgstr "Tichap rokisaxik Ximojri'ïl…"
