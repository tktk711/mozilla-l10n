#: loadError.label
msgid "Problem loading page"
msgstr "K'o k'ayewal toq nitikirisäx ri ruxaq k'amaya'l"

#: retry.label
msgid "Try Again"
msgstr "Titojtob'ëx chik"

#: returnToPreviousPage.label
msgid "Go Back"
msgstr "Titzolin"

#: advanced.label
