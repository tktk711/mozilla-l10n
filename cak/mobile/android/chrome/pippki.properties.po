#~ msgstr "SHA1 Retal ruwi' q'ab'aj"

#~ msgid "Begins On"
#~ msgstr "Tatikirisaj pa"

#~ msgid "Expires On"
#~ msgstr "Nik'is ruq'ijul pa"

#~ msgid "Certificate Exists"
#~ msgstr "Ri ruwujil b'i'aj man k'o ta"

#~ msgid "The Certificate already exists."
#~ msgstr "Ri Ruwujil b'i'aj k'o chik."

