msgstr "Moloj (MO)"

#: certmgr.certdetail.ou
msgid "Organizational Unit (OU)"
msgstr "Ruq'a' moloj (OU)"

#: certmgr.certdetail.serialnumber
msgid "Serial Number"
msgstr "Rucholajem rajilab'al"

#: certmgr.certdetail.sha256fingerprint
msgid "SHA-256 Fingerprint"
msgstr "SHA-256 Retal ruwi' q'ab'aj"

#: certmgr.certdetail.sha1fingerprint
