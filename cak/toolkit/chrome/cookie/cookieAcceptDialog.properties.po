#: hostColon
msgid "Host:"
msgstr "Ruk'u'x Samaj:"

#: domainColon
msgid "Domain:"
msgstr "Ruk'ojlem b'ey:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Xa xe man etaman ta taq chuq'unem "

#: forAnyConnection
msgid "Any type of connection"
msgstr "Xa b'a achike ruwäch chi chuq'unem"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "Pa ruk'isib'al ri moloj"

#: showDetails
msgid "Show Details"
msgstr "Kek'ut pe ri taq rub'anikil"

#: hideDetails
msgid "Hide Details"
msgstr "Tewäx ri taq rub'anikil"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Ri ruxaq k'amaya'l %S tajin nutojtob'ej nuyäk jun kuki."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "Ri ruxaq k'amaya'l %S tajin nutojtob'ej nuyäk jun ruka'n kuki."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"Ri ruxaq k'amaya'l %S nuk'utuj q'ij richin nuyäk jun chik kuki.\n"
"Eyakon %S taq kukis pa re ruxaq k'amaya'l re'."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "Ri ruxaq k'amaya'l %S nrajo' nujalwachij jun kuki ri k'o chupam."
