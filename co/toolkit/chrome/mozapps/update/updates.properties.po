msgstr "Stallatu: %S"

#: statusFailed
msgid "Install Failed"
msgstr "Stallazione micca riisciuta"

#: pauseButtonPause
msgid "Pause"
msgstr "Arretta"

#: pauseButtonResume
msgid "Resume"
msgstr "Ripiglià"

