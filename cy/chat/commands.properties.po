#. # LOCALIZATION NOTE (commands):
#. #  %S is a comma separated list of command names.
#: commands
msgid ""
"Commands: %S.\n"
"Use /help &lt;command&gt; for more information."
msgstr ""
"Gorchmynion: %S.\n"
"Defnyddiwch /help &lt;command&gt; am rhagor o wybodaeth."

#. # LOCALIZATION NOTE (noCommand, noHelp):
#. #  %S is the command name the user typed.
#: noCommand
msgid "No '%S' command."
msgstr "Dim gorchymyn '%S'."

#: noHelp
msgid "No help message for the '%S' command, sorry!"
msgstr "Dim neges cymorth ar gyfer y gorchymyn '%S', ymddiheuriadau!"

#: sayHelpString
msgid "say &lt;message&gt;: send a message without processing commands."
msgstr "say &lt;message&gt;: anfon neges heb brosesu gorchmynion."

#: rawHelpString
msgid "raw &lt;message&gt;: send a message without escaping HTML entities."
msgstr "raw &lt;message&gt;: anfon neges heb ryddhau endidau HTML."

#: helpHelpString
msgid ""
"help &lt;name&gt;: show the help message for the &lt;name&gt; command, or "
"the list of possible commands when used without parameter."
msgstr ""
"help &lt;name&gt;: dangos neges cymorth ar gyfer gorchymyn &lt;name&gt; , "
"neu'r rhestr o orchmynion posib pan yn cael eu defnyddio heb baramedr."

#. # LOCALIZATION NOTE (statusCommand):
#. #  %1$S is replaced with a status command name
#. #   (one of "back", "away", "busy", "dnd", or "offline").
#. #  %2$S is replaced with the localized version of that status type
#. #   (one of the 5 strings below).
#: statusCommand
msgid ""
"%1$S &lt;status message&gt;: set the status to %2$S with an optional status "
"message."
msgstr ""
"%1$S &lt;status message&gt;: gosod y statws i %2$S gyda neges statws dewisol."

#: back
msgid "available"
msgstr "ar gael"

#: away
msgid "away"
msgstr "i ffwrdd"

#: busy
msgctxt "busy"
msgid "unavailable"
msgstr "ddim ar gael"

#: dnd
msgctxt "dnd"
msgid "unavailable"
msgstr "ddim ar gael"

#: offline
msgid "offline"
msgstr "all-lein"
