#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Slědna synchronizacija: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Slědna synchronizacija: běžy…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Konto: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Njezwězany"

#: connecting.label
msgid "Connecting…"
msgstr "Zwězujo se…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Wašo konto Firefox Sync jo se wótpórało"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Anulěrowaś"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 njejo kompatibelny z nejnowšeju wersiju Firefox Sync. Pšosym "
"zaktualizěrujśo na nejnowšu wersiju."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 njejo kompatibelny ze staršymi wersijami Firefox Sync. Pšosym "
"zaktualizěrujśo Firefox na swójich drugich licadłach."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Dalšne informacije"

#: sync.update.close
msgid "Close"
msgstr "Zacyniś"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Sync njedo se konfigurěrowaś"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Internetny zwisk njestoj k dispoziciji"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S njedajo se ze Sync zwězaś. Cośo hyšći raz wopytaś?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Hyšći raz wopytaś"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Manuelne konfigurěrowanje"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Žedne rejtarki z wašych drugich licadłow."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Synchronizacija jo nowučka a lažčej ako do togo. "

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Pótusniśo how, aby wěcej zgónił"
