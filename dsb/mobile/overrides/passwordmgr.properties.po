#. # 1st string is the username for the login, 2nd is the login's hostname.
#. # Note that long usernames may be truncated.
#: savePassword
msgid "Save password for \"%1$S\" on %2$S?"
msgstr "Gronidło za \"%1$S\" na %2$S składowaś?"

#. # String is the login's hostname
#: savePasswordNoUser
msgid "Save password on %S?"
msgstr "Gronidło na %S składowaś?"

#: saveButton
msgid "Save"
msgstr "Składowaś"

#: dontSaveButton
msgid "Don't save"
msgstr "Njeskładowaś"

#. # String is the login's hostname
#: updatePassword
msgid "Update saved password for %S?"
msgstr "Składowane gronidło za %S aktualizěrowaś?"

#: updatePasswordNoUser
msgid "Update saved password for this login?"
msgstr "Składowane gronidło za toś to pśizjawjenje aktualizěrowaś?"

#: updateButton
msgid "Update"
msgstr "Aktualizěrowaś"

#: dontUpdateButton
msgid "Don't update"
msgstr "Njeaktualizěrowaś"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "Pšosym wobkšuśćo, za kótaregož wužywarja gronidło změnijośo"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "Změnjenje gronidła wobkšuśiś"

#. # Strings used by PromptService.js
#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "Zastojnik gronidłow wužywaś, aby se toś to gronidło spomnjeło."

#: username
msgid "Username"
msgstr "Wužywarske mě"

#: password
msgid "Password"
msgstr "Gronidło"
