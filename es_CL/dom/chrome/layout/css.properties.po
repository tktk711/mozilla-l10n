#: MimeNotCss
msgid ""
"The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not "
"\"text/css\"."
msgstr ""
"La hoja de estilos %1$S no se ha cargado porque su tipo MIME, \"%2$S\", no "
"es \"text/css\"."

#: MimeNotCssWarn
msgid ""
"The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", "
"is not \"text/css\"."
msgstr ""
"La hoja de estilos %1$S se ha cargado como CSS a pesar de que su tipo MIME, "
"\"%2$S\", no es \"text/css\"."

#: PEUnexpEOF2
msgid "Unexpected end of file while searching for %1$S."
msgstr "Inesperado fin de archivo mientras se buscaba %1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Se ha pasado una cadena de espacios en blanco como regla."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Declaración rechazada."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Ignorado hasta la siguiente declaración."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Propiedad desconocida '%1$S'."

#: PEValueParsingError
msgid "Error in parsing value for '%1$S'."
msgstr "Error al interpretar el valor para '%1$S'."

#: PEExpectEndValue
msgid "Expected end of value but found '%1$S'."
msgstr "Se esperaba el final del valor pero se encontró '%1$S'."

#: PESkipAtRuleEOF2
msgid "end of at-rule"
msgstr ""

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "No se reconoce la regla at o error al leer regla at '%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "cadena de caracteres en regla @charset"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Se esperaba cadena de caracteres, pero se encontró '%1$S'."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "fin de la lista de medios en regla @import o @media"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Se esperaba ',' en la lista de medios, pero se encontró '%1$S'."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr ""
"Se esperaba un identificador en la lista de medios, pero se encontró '%1$S'."

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "Se esperaba una URI en regla @import, pero se encontró '%1$S'."

#: PEImportBadURI
msgid "Invalid URI in @import rule: '%1$S'."
msgstr "URI no válida en regla @import: '%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Se encontró un inesperado '%1$S' dentro de @import."

#: PEGroupRuleEOF2
msgid "end of @media, @supports or @-moz-document rule"
msgstr "final de regla @media, @supports o @-moz-document"

#: PEGroupRuleNestedAtRule
msgid "%1$S rule not allowed within @media or @-moz-document rule."
msgstr "Regla %1$S no permitida dentro de @media o @-moz-document."

#: PEMozDocRuleBadFunc
msgid ""
"Expected url(), url-prefix(), or domain() in @-moz-document rule but found "
"'%1$S'."
msgstr ""
"Se esperaba url(), url-prefix() o domain() en regla @-moz-document, pero se "
"encontró '%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "Se esperaba URI en regla @-moz-document, pero se encontró '%1$S'."

#: PEMozDocRuleNotString
msgid ""
"Expected string in @-moz-document rule regexp() function but found '%1$S'."
msgstr ""
"Cadena esperada en regla @-moz-document en función regexp() pero se econtró "
"'%1$S'."

#: PEMozDocRuleEOF
msgid "next URI in @-moz-document rule"
msgstr "próxima URI en regla @-moz-document"

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "prefijo de espacio de nombre en regla @namespace"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "URI de espacio de nombre en regla @namespace"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Token inesperado dentro de @namespace: '%1$S'."

#: PEKeyframeNameEOF
msgid "name of @keyframes rule."
msgstr "nombre de la regla @keyframes."

#: PEKeyframeBadName
msgid "Expected identifier for name of @keyframes rule."
msgstr "Identificador esperado para el nombre de una regla @keyframes."

#: PEKeyframeBrace
msgid "Expected opening { of @keyframes rule."
msgstr "Se esperaba una apertura { de una regla @keyframes."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "} de cierre de bloque de declaración"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "} de cierre de juego de reglas no válidas"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Juego de reglas ignoradas debido a un mal selector."

#: PEBadSelectorKeyframeRuleIgnored
msgid "Keyframe rule ignored due to bad selector."
msgstr "Regla keyframe ignorada debido a un selector erróneo."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "',' o '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "Se esperó ',' o '{' pero se encontró '%1$S'."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Se espera selector."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Combinador balanceante."

#: PEClassSelEOF
msgid "class name"
msgstr "nombre de clase"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "Se esperó identificador del selector de clase pero se encontró '%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "tipo de elemento"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Se esperó nombre de elemento o '*' pero se encontró '%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Prefijo de espacio de nombre desconocido '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "nombre de atributo"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr ""
"Se esperó identificador del nombre de atributo pero se encontró '%1$S'."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr ""
"Se esperó nombre de atributo o espacio de nombre pero se encontró '%1$S'."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "Se esperó '|' pero se encontró '%1$S'."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "parte de selector de atributo"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Token inesperado en selector de atributo: '%1$S'."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "valor de atributo"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "']' para finalizar selector de atributo"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr ""
"Se esperó ']' para finalizar el selector de atributo pero se encontró '%1$S'."

#: PEAttSelBadValue
msgid ""
"Expected identifier or string for value in attribute selector but found "
"'%1$S'."
msgstr ""
"Se esperó identificador o cadena para el valor en selector de atributo pero "
"se encontró '%1$S'."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "nombre de la pseudo-clase o pseudo-elemento"

#: PEPseudoSelBadName
msgid ""
"Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr ""
"Se esperó identificador de pseudo-clase o pseudo-elemento pero se encontró "
"'%1$S'."

#: PEPseudoSelNonFunc
msgid ""
"Function token for non-function pseudo-class or pseudo-element, or the other "
"way around, when reading '%1$S'."
msgstr ""
"Token de función pseudo-clase o pseudo-elemento que no es función, o de la "
"otra forma, al leer '%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Se esperó pseudo-elemento pero se encontró '%1$S'."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "La pseudo-clase de negación no se puede negar '%1$S'."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Pseudo-elementos no pueden ser negados '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Este pseudo-elemento debe usar la forma \"::\": '%1$S'."

#: PEPseudoSelTrailing
msgid ""
"Found trailing token after pseudo-element, which must be the last part of a "
"selector:  '%1$S'."
msgstr ""
"Se encontró token al final después de pseudo-elemento, que debe ser la "
"última parte de un selector:  '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Pseudo-elemento extra '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Pseudo-clase o pseudo-elemento desconocido '%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "selector dentro de negación"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr ""
"Selector simple mal formado como argumento de negación de la pseudo-clase "
"'%1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "')' de cierre faltante en pseudo-clase de negación '%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Falta argumento en pseudo-clase de negación '%1$S'."

#: PEPseudoClassArgEOF
msgid "argument to pseudo-class selector"
msgstr "argumento a selector de pseudo-clase"

#: PEPseudoClassArgNotIdent
msgid "Expected identifier for pseudo-class parameter but found '%1$S'."
msgstr ""
"Se esperó identificador del parámetro de la pseudo-clase pero se encontró "
"'%1$S'."

#: PEPseudoClassArgNotNth
msgid "Expected part of argument to pseudo-class but found '%1$S'."
msgstr ""
"Se esperaba parte del argumento a una pseudo-clase, pero se encontró '%1$S'."

#: PEPseudoClassNoClose
msgid "Missing closing ')' in pseudo-class, found '%1$S' instead."
msgstr ""
"Falta ')' de cierre en pseudo-clase, se ha encontrado '%1$S' en su lugar."

#: PEPseudoClassNoArg
msgid "Missing argument in pseudo-class '%1$S'."
msgstr "Falta argumento en pseudo-clase '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "selector"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr ""
"Se esperó '{' para iniciar el bloque de declaración pero se encontró '%1$S'."

#: PEColorEOF
msgid "color"
msgstr "color"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Se esperó color pero se encontró '%1$S'."

#: PEColorComponentEOF
msgid "color component"
msgstr "componente de color"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "Se esperó un porcentaje pero se encontró '%1$S'."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Se esperó un entero pero se encontró '%1$S'."

#: PEColorBadRGBContents
