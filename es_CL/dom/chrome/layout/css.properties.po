msgstr "S esperó '%2$S' pero se encontró '%1$S'."

#: PEColorHueEOF
msgid "hue"
msgstr "tinte"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "Se esperó ',' pero se encontró '%1$S'."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "saturación"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "luminosidad"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "opacidad en valor de color"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Se esperó un numero pero se encontró '%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "Se esperó ')' pero se encontró '%1$S'."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "';' o '}' para finalizar declaración"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "Se esperó ':' pero se encontró '%1$S'."

#: PEParseDeclarationDeclExpected
