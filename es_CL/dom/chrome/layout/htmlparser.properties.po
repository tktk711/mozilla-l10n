#. # which is available under the MIT license.
#. # Tokenizer errors
#: errGarbageAfterLtSlash
msgid "Garbage after “</”."
msgstr "Hay basura después de “</”."

#: errLtSlashGt
msgid ""
"Saw “</>”. Probable causes: Unescaped “<” (escape as “&lt;”) or mistyped end "
"tag."
msgstr ""
"Se encontró un “</>”. Las posibles causas son: falta escapar un “<” (se "
"puede escapar como “&lt;”) o hay un etiqueta de cierre mal escrita."

#: errCharRefLacksSemicolon
msgid "Character reference was not terminated by a semicolon."
msgstr "La referencia a caracter no fue terminada con punto y coma."

#: errNoDigitsInNCR
msgid "No digits in numeric character reference."
msgstr "No hay dígitos en la referencia a caracter numérica."

#: errGtInSystemId
msgid "“>” in system identifier."
msgstr "“>” en identificador de sistema."

#: errGtInPublicId
msgid "“>” in public identifier."
msgstr "“>” en identificador público."

#: errNamelessDoctype
msgid "Nameless doctype."
msgstr "Doctype sin nombre."

#: errConsecutiveHyphens
msgid ""
"Consecutive hyphens did not terminate a comment. “--” is not permitted "
"inside a comment, but e.g. “- -” is."
msgstr ""
"Guiones consecutivos que no terminan un comentario. No se permite “--” "
"dentro de un comentario, pero si por ej. “- -”."

#: errPrematureEndOfComment
msgid "Premature end of comment. Use “-->” to end a comment properly."
msgstr ""
"Finalización prematura de comentario. Use “-->” para cerrar el comentario "
"correctamente."

#: errBogusComment
msgid "Bogus comment."
msgstr "Comentario falso."

#: errUnquotedAttributeLt
msgid ""
"“<” in an unquoted attribute value. Probable cause: Missing “>” immediately "
"before."
msgstr ""
"“<” en un valor de atributo sin comillas. Causa probable: falta un “>” "
"inmediatamente antes."

#: errUnquotedAttributeGrave
msgid ""
"“`” in an unquoted attribute value. Probable cause: Using the wrong "
"character as a quote."
msgstr ""
"“`” en un valor de atributo sin comillas. Causa probable: se usa un caracter "
"incorrecto para las comillas."

#: errUnquotedAttributeQuote
msgid ""
"Quote in an unquoted attribute value. Probable causes: Attributes running "
"together or a URL query string in an unquoted attribute value."
msgstr ""
"Comillas en un valor de atributo sin comillas. Causas probables: hay "
"atributos contiguos pegados o hay una cadena de consulta URL en un valor de "
"atributo sin comillas."

#: errUnquotedAttributeEquals
msgid ""
"“=” in an unquoted attribute value. Probable causes: Attributes running "
"together or a URL query string in an unquoted attribute value."
msgstr ""
"“=” en un valor de atributo sin comillas. Causas probables: hay atributos "
"contiguos pegados o hay una cadena de consulta URL en un valor de atributo "
"sin comillas."

#: errSlashNotFollowedByGt
msgid "A slash was not immediately followed by “>”."
msgstr "Una barra inclinada no está seguida inmediatamente por un “>”."

#: errNoSpaceBetweenAttributes
msgid "No space between attributes."
msgstr "No hay espacio entre los atributos."

#: errUnquotedAttributeStartLt
msgid ""
"“<” at the start of an unquoted attribute value. Probable cause: Missing “>” "
"immediately before."
msgstr ""
"“<” en el comienzo de un valor de atributo sin comillas. Causa probable: "
"falta un “>” inmediatamente antes."

#: errUnquotedAttributeStartGrave
msgid ""
"“`” at the start of an unquoted attribute value. Probable cause: Using the "
"wrong character as a quote."
msgstr ""
"“`” en el comienzo de un valor de atributo sin comillas. Causa probable: se "
"usa un caracter incorrecto como comillas."

#: errUnquotedAttributeStartEquals
msgid ""
"“=” at the start of an unquoted attribute value. Probable cause: Stray "
"duplicate equals sign."
msgstr ""
"“=” en el comienzo de un valor de atributo sin comillas. Causa probable: hay "
"un símbolo de igualdad duplicado."

#: errAttributeValueMissing
msgid "Attribute value missing."
msgstr "Falta un valor de atributo."

#: errBadCharBeforeAttributeNameLt
msgid ""
"Saw “<” when expecting an attribute name. Probable cause: Missing “>” "
"immediately before."
msgstr ""
"Se encontró un “<” cuando se esperaba un nombre de atributo. Causa probable: "
"falta un “>” inmediatamente antes."

#: errEqualsSignBeforeAttributeName
msgid ""
"Saw “=” when expecting an attribute name. Probable cause: Attribute name "
"missing."
msgstr ""
"Se encontró un “=” cuando se esperaba un nombre de atributo. Causa probable: "
"falta un nombre de atributo."

#: errBadCharAfterLt
msgid ""
"Bad character after “<”. Probable cause: Unescaped “<”. Try escaping it as "
"“&lt;”."
msgstr ""
"Caracter inválido después de “<”. Causa probable: hay un “<” sin escapar. "
"Intente escaparlo como “&lt;”."

#: errLtGt
msgid ""
"Saw “<>”. Probable causes: Unescaped “<” (escape as “&lt;”) or mistyped "
"start tag."
msgstr ""
"Se encontró un “<>”. Causas probables: hay un “<” sin escapar (puede "
"escarparlo como “&lt;”) o hay una etiqueta de comienzo mal escrita."

#: errProcessingInstruction
msgid ""
"Saw “<?”. Probable cause: Attempt to use an XML processing instruction in "
"HTML. (XML processing instructions are not supported in HTML.)"
msgstr ""
"Se encontró un “<?”. Causa probable: se intentó usar una instrucción de "
"procesamiento XML en el HTML. (Las instrucciones de procesamiento XML no "
"están soportadas en HTML)."

#: errUnescapedAmpersandInterpretedAsCharacterReference
msgid ""
"The string following “&” was interpreted as a character reference. (“&” "
"probably should have been escaped as “&amp;”.)"
msgstr ""
"La cadena que sigue a “&” fue interpretada como una referencia de caracter. "
"(Probablemente el “&” debe ser escapado como en “&amp;”)."

#: errNotSemicolonTerminated
msgid ""
"Named character reference was not terminated by a semicolon. (Or “&” should "
"have been escaped as “&amp;”.)"
msgstr ""
"Una referencia de caracter nombrada no fue terminada por un punto y coma. "
"(El el “&” debería haber sido escapado como en “&amp;”)."

#: errNoNamedCharacterMatch
msgid ""
"“&” did not start a character reference. (“&” probably should have been "
"escaped as “&amp;”.)"
msgstr ""
"“&” no comienza como una referencia a caracter. (Probablemente el “&” "
"debería ser escapado como en “&amp;”)."

#: errQuoteBeforeAttributeName
msgid ""
"Saw a quote when expecting an attribute name. Probable cause: “=” missing "
"immediately before."
msgstr ""
"Se encontró una comilla cuando se esperaba un nombre de atributo. Causa "
"probable: hay un “=” faltante inmediatamente antes."

#: errLtInAttributeName
msgid "“<” in attribute name. Probable cause: “>” missing immediately before."
msgstr ""
"Hay un “<” en un nombre de atributo. Causa probable: hay un “>” faltante "
"inmediatamente antes."

#: errQuoteInAttributeName
msgid ""
"Quote in attribute name. Probable cause: Matching quote missing somewhere "
"earlier."
msgstr ""
"Hay una comilla en un nombre de atributo. Causa probable: falta una comilla "
"de cierre en algún punto anterior."

#: errExpectedPublicId
msgid "Expected a public identifier but the doctype ended."
msgstr ""
"Se esperaba un identificador público pero se encontró el fin del doctype."

#: errBogusDoctype
msgid "Bogus doctype."
msgstr "Doctype falso."

#: maybeErrAttributesOnEndTag
msgid "End tag had attributes."
msgstr "Hay atributos en etiqueta de cierre."

#: maybeErrSlashInEndTag
msgid "Stray “/” at the end of an end tag."
msgstr "Se extravió un “/” al final de una etiqueta de cierre."

#: errNcrNonCharacter
msgid "Character reference expands to a non-character."
msgstr "Referencia a caracter se expande a un no-caracter."

#: errNcrSurrogate
msgid "Character reference expands to a surrogate."
msgstr "Referencia a caracter se expande a un sustituto."

#: errNcrControlChar
msgid "Character reference expands to a control character."
msgstr "La referencia de caracter se expande a un caracter de control."

#: errNcrCr
msgid "A numeric character reference expanded to carriage return."
msgstr "Una referencia a caracter numérica se expande a retorno de carro."

#: errNcrInC1Range
msgid "A numeric character reference expanded to the C1 controls range."
msgstr ""
"Una referencia a caracter numérica se expande al rango de controles C1."

#: errEofInPublicId
msgid "End of file inside public identifier."
msgstr "Fin de archivo dentro de un identificador público."

#: errEofInComment
msgid "End of file inside comment."
msgstr "Fin de archivo dentro de un comentario."

#: errEofInDoctype
msgid "End of file inside doctype."
msgstr "Fin de archivo dentro del doctype."

#: errEofInAttributeValue
msgid "End of file reached when inside an attribute value. Ignoring tag."
msgstr ""
"Se alcanzó el fin de archivo mientras estaba dentro de un valor de atributo. "
"Ignorando la etiqueta."

#: errEofInAttributeName
msgid "End of file occurred in an attribute name. Ignoring tag."
msgstr ""
"Ocurrió un fin de archivo en un nombre de atributo. Ignorando la etiqueta."

#: errEofWithoutGt
msgid "Saw end of file without the previous tag ending with “>”. Ignoring tag."
msgstr ""
"Se encontró el fin de archivo sin que termine la etiqueta previa con un “>”. "
"Ignorando la etiqueta."

#: errEofInTagName
msgid "End of file seen when looking for tag name. Ignoring tag."
msgstr ""
"Se encontró el fin del archivo cuando se buscaba el nombre de la etiqueta. "
"Ignorando la etiqueta."

#: errEofInEndTag
msgid "End of file inside end tag. Ignoring tag."
msgstr ""
"Fin de archivo dentro de una etiqueta de cierre. Ignorando la etiqueta."

#: errEofAfterLt
msgid "End of file after “<”."
msgstr "Fin de archivo después de “<”."

#: errNcrOutOfRange
msgid "Character reference outside the permissible Unicode range."
msgstr "La referencia de caracter está fuera del rango Unicode permisible."

#: errNcrUnassigned
msgid "Character reference expands to a permanently unassigned code point."
msgstr ""
"La referencia de caracter se expande a un punto de código sin asignación "
"permanente."

#: errDuplicateAttribute
msgid "Duplicate attribute."
msgstr "Atributo duplicado."

#: errEofInSystemId
msgid "End of file inside system identifier."
msgstr "Fin de archivo dentro de un identificador del sistema."

#: errExpectedSystemId
msgid "Expected a system identifier but the doctype ended."
msgstr "Se esperaba un identificador del sistema, pero finalizó el doctype."

#: errMissingSpaceBeforeDoctypeName
msgid "Missing space before doctype name."
msgstr "Un espacio faltante antes del nombre del doctype."

#: errHyphenHyphenBang
msgid "“--!” found in comment."
msgstr "Se encontró “--!” en un comentario."

#: errNcrZero
msgid "Character reference expands to zero."
msgstr "La referencia de caracter se expande a cero."

#: errNoSpaceBetweenDoctypeSystemKeywordAndQuote
msgid "No space between the doctype “SYSTEM” keyword and the quote."
msgstr ""
"No hay espacio entre la palabra clave “SYSTEM” del doctype y la comilla."

#: errNoSpaceBetweenPublicAndSystemIds
msgid "No space between the doctype public and system identifiers."
msgstr ""
"No hay espacio entre los identficadores públicos y de sistema del doctype."

#: errNoSpaceBetweenDoctypePublicKeywordAndQuote
msgid "No space between the doctype “PUBLIC” keyword and the quote."
msgstr ""
"No hay espacio entre la palabra clave “PUBLIC” del doctype y la comilla."

#. # Tree builder errors
#: errStrayStartTag2
msgid "Stray start tag “%1$S”."
msgstr "Etiqueta de comienzo “%1$S” extraviada."

#: errStrayEndTag
msgid "Stray end tag “%1$S”."
msgstr "Etiqueta de cierre “%1$S” extraviada."

#: errUnclosedElements
msgid "End tag “%1$S” seen, but there were open elements."
msgstr ""
"Se encontró una etiqueta de cierre “%1$S”, pero no había elementos abiertos."

#: errUnclosedElementsImplied
msgid "End tag “%1$S” implied, but there were open elements."
msgstr ""
"Hay una etiqueta de cierre “%1$S” implícita, pero había elementos abiertos."

#: errUnclosedElementsCell
msgid "A table cell was implicitly closed, but there were open elements."
msgstr ""
"Fue cerrada implícitamente una celda de tabla, pero había elementos abiertos."

#: errStrayDoctype
msgid "Stray doctype."
msgstr "Doctype extraviado."

#: errAlmostStandardsDoctype
msgid "Almost standards mode doctype. Expected “<!DOCTYPE html>”."
msgstr "Doctype de modo casi estándar. Se esperaba un “<!DOCTYPE html>”."

#: errQuirkyDoctype
msgid "Quirky doctype. Expected “<!DOCTYPE html>”."
msgstr "Doctype de modo raro (quirky). Se esperaba “<!DOCTYPE html>”."

#: errNonSpaceInTrailer
msgid "Non-space character in page trailer."
msgstr "Hay un caracter de no-espacio luego del fin de página."

#: errNonSpaceAfterFrameset
msgid "Non-space after “frameset”."
msgstr "Hay un caracter de no-espacio después de “frameset”."

#: errNonSpaceInFrameset
msgid "Non-space in “frameset”."
msgstr "Hay un caracter de no-espacio in “frameset”."

#: errNonSpaceAfterBody
msgid "Non-space character after body."
msgstr "Hay un caracter de no-espacio después de body."

#: errNonSpaceInColgroupInFragment
msgid "Non-space in “colgroup” when parsing fragment."
msgstr ""
"Hay un caracter de no-espacio en “colgroup” cuando se analizaba "
"sintácticamente el fragmento."

#: errNonSpaceInNoscriptInHead
msgid "Non-space character inside “noscript” inside “head”."
msgstr "Hay un caracter de no-espacio dentro de “noscript” dentro de “head”."

#: errFooBetweenHeadAndBody
msgid "“%1$S” element between “head” and “body”."
msgstr "Hay un elemento “%1$S” entre “head” y “body”."

#: errStartTagWithoutDoctype
msgid ""
"Start tag seen without seeing a doctype first. Expected “<!DOCTYPE html>”."
msgstr ""
"Se encontró una etiqueta de comienzo sin ver un doctype primero. Se esperaba "
"un “<!DOCTYPE html>”."

#: errNoSelectInTableScope
msgid "No “select” in table scope."
msgstr "No hay “select” en el alcance de la tabla."

#: errStartSelectWhereEndSelectExpected
msgid "“select” start tag where end tag expected."
msgstr ""
"Hay una etiqueta de comienzo “select”, cuando se esperaba una etiqueta de "
"cierre."

#: errStartTagWithSelectOpen
msgid "“%1$S” start tag with “select” open."
msgstr ""
"Hay una etiqueta de comienzo “%1$S”, con una etiqueta “select” abierta."

#: errBadStartTagInHead2
msgid "Bad start tag “%1$S” in “head”."
msgstr "Etiqueta de comienzo incorrecta “%1$S” en “head”."

#: errImage
msgid "Saw a start tag “image”."
msgstr "Se encontró una etiqueta de comienzo “image”."

#: errIsindex
msgid "“isindex” seen."
msgstr "Se encontró “isindex”."

#: errFooSeenWhenFooOpen
msgid ""
"An “%1$S” start tag seen but an element of the same type was already open."
msgstr ""
"Se encontró una etiqueta de comienzo “%1$S”, pero ya había un elemento "
"abierto del mismo tipo."

#: errHeadingWhenHeadingOpen
msgid "Heading cannot be a child of another heading."
msgstr "Los encabezados no pueden ser hijos de otros encabezados."

#: errFramesetStart
msgid "“frameset” start tag seen."
msgstr "Se encontró una etiqueta de comienzo “frameset”."

#: errNoCellToClose
msgid "No cell to close."
msgstr "No hay celda para cerrar."

#: errStartTagInTable
msgid "Start tag “%1$S” seen in “table”."
msgstr "Se encontró una etiqueta de comienzo “%1$S” en “table”."

#: errFormWhenFormOpen
msgid ""
"Saw a “form” start tag, but there was already an active “form” element. "
"Nested forms are not allowed. Ignoring the tag."
msgstr ""
"Se encontró una etiqueta de comienzo “form”, pero ya había un elemento "
"“form” activo. No son permitidos los formularios anidados. Se ignora la "
"etiqueta."

#: errTableSeenWhileTableOpen
msgid "Start tag for “table” seen but the previous “table” is still open."
msgstr ""
"Se encontró una etiqueta de comienzo “table”, pero el “table” anterior sigue "
"abierto."

#: errStartTagInTableBody
msgid "“%1$S” start tag in table body."
msgstr "Hay una etiqueta de comienzo “%1$S” en el cuerpo de una tabla."

#: errEndTagSeenWithoutDoctype
msgid ""
"End tag seen without seeing a doctype first. Expected “<!DOCTYPE html>”."
msgstr ""
"Se encontró una etiqueta de cierre sin ver un doctype primero. Se esperaba "
"un “<!DOCTYPE html>”."

#: errEndTagAfterBody
msgid "Saw an end tag after “body” had been closed."
msgstr ""
"Se encontró una etiqueta de cierre después que “body” había sido cerrado."

#: errEndTagSeenWithSelectOpen
msgid "“%1$S” end tag with “select” open."
msgstr "Hay un cierre de la etiqueta “%1$S” con un “select” abierto."

#: errGarbageInColgroup
msgid "Garbage in “colgroup” fragment."
msgstr "Hay basura en el fragmento “colgroup”."

#: errEndTagBr
msgid "End tag “br”."
msgstr "Cierre de etiqueta “br”."

#: errNoElementToCloseButEndTagSeen
msgid "No “%1$S” element in scope but a “%1$S” end tag seen."
msgstr ""
"No hay un elemento “%1$S” en el alcance, pero se encontró un cierre de la "
"etiqueta “%1$S”."

#: errHtmlStartTagInForeignContext
msgid "HTML start tag “%1$S” in a foreign namespace context."
msgstr ""
"Hay una etiqueta de comienzo HTML “%1$S” en el contexto de un espacio de "
"nombres extraño."

#: errTableClosedWhileCaptionOpen
msgid "“table” closed but “caption” was still open."
msgstr "Fue cerrada la etiqueta “table”, pero hay un “caption” aún abierto."

#: errNoTableRowToClose
msgid "No table row to close."
msgstr "No hay fila de tabla para cerrar."

#: errNonSpaceInTable
msgid "Misplaced non-space characters inside a table."
msgstr "Dentro de una tabla, hay caracteres de no-espacio fuera de lugar."

#: errUnclosedChildrenInRuby
msgid "Unclosed children in “ruby”."
msgstr "Hijo sin cerrar en “ruby”."

#: errStartTagSeenWithoutRuby
msgid "Start tag “%1$S” seen without a “ruby” element being open."
msgstr ""
"Se encontró la etiqueta de comienzo “%1$S” sin un elemento “ruby” abierto."

#: errSelfClosing
msgid ""
"Self-closing syntax (“/>”) used on a non-void HTML element. Ignoring the "
"slash and treating as a start tag."
msgstr ""
"Se usó la sintaxis de auto-cierre (“/>”) en un elemento HTML no vacío. Se "
"ignorará la barra inclinada y se interpretará como una etiqueta de comienzo."

#: errNoCheckUnclosedElementsOnStack
