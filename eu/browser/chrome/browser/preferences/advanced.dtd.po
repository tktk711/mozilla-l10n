#. Please check for possible accesskey conflicts.
#: updateApplication.label
msgid "&brandShortName; Updates"
msgstr "&brandShortName; eguneraketak"

#. LOCALIZATION NOTE (updateApplication.version.*): updateApplication.version.pre
#. # is followed by a version number, keep the trailing space or replace it with a
#. # different character as needed. updateApplication.version.post is displayed
#. # after the version number, and is empty on purpose for English. You can use it
#. # if required by your language.
#: updateApplicationDescription.label
msgid ""
"Keep &brandShortName; up to date for the best performance, stability, and "
"security."
msgstr ""
"Mantendu &brandShortName; eguneratuta errendimendu, egonkortasun eta "
"segurtasun onena bermatzeko."

#: updateApplication.version.pre
msgid "Version "
msgstr "​"

#: updateApplication.version.post
msgctxt "updateApplication.version.post"
msgid ""
msgstr "bertsioa"

#: updateApplication.description
msgid "Allow &brandShortName; to"
msgstr "Baimendu &brandShortName;(r)i"

#: updateAuto3.label updateAuto3.accesskey
msgid "&Automatically install updates (recommended)"
msgstr "Instalatu eguneraketak &automatikoki (gomendatua)"

#: updateCheckChoose2.label updateCheckChoose2.accesskey
msgid "&Check for updates but let you choose to install them"
msgstr "Eguneraketak &bilatzen baina utzi aukeratzen instalatuko diren edo ez"

#: updateManual2.label updateManual2.accesskey
msgid "&Never check for updates (not recommended)"
msgstr "Ez egiaztatu i&noiz eguneraketarik dagoen (ez gomendatua)"

