msgid "Pocket"
msgstr "Pocket"

#: pocket-button.tooltiptext
msgid "Save to Pocket"
msgstr "Gorde Pocket-en"

#: saveToPocketCmd.label saveToPocketCmd.accesskey
msgid "Save Page to Poc&ket"
msgstr "Gorde orria Poc&ket-en"

#: saveLinkToPocketCmd.label saveLinkToPocketCmd.accesskey
msgid "Save Link t&o Pocket"
msgstr "G&orde lotura Pocket-en"

#: pocketMenuitem.label
