msgid "When:"
msgstr "Noiz:"

#: imipHtml.organizer
msgid "Organizer:"
msgstr "Antolatzailea:"

#: imipHtml.description
msgid "Description:"
msgstr "Azalpena:"

#: imipHtml.comment
msgid "Comment:"
msgstr "Iruzkina:"

#: imipHtml.attendees
msgid "Attendees:"
msgstr "Partaideak:"

#: imipHtml.canceledOccurrences
msgid "Canceled Occurrences:"
msgstr "Utzitako errepikapenak:"

#: imipHtml.modifiedOccurrences
msgid "Modified Occurrences:"
msgstr "Aldatutako errepikapenak:"

#: imipHtml.newLocation
msgid "New Location: %1$S"
msgstr "Kokapen berria: %1$S"

#: imipAddToCalendar.label
msgid "Add To Calendar"
msgstr "Egutegira gehitu"

#: imipAddedItemToCal
msgid "Event Added to Calendar"
msgstr "Gertaera egutegira gehitu da"

#: imipCanceledItem
msgid "Event has been deleted"
msgstr "Gertaera ezabatua izan da"

#: imipUpdatedItem
msgid "Event has been updated"
msgstr "Gertaera eguneratu izan da"

#: imipBarCancelText
msgid "This message contains an event cancellation."
msgstr "Mezu honek gertaera ezeztapen bat du."

#: imipBarRefreshText
msgid "This message asks for an event update."
msgstr "Mezu honek gertaera baten eguneratzeaz galdetzen du"

#: imipBarPublishText
msgid "This message contains an event."
msgstr "Mezu honek gertaera bat dauka"

#: imipBarRequestText
msgid "This message contains an invitation to an event."
msgstr "Mezu honek gertaera baterako gonbidapena dauka."

#: imipBarUpdateText
msgid "This message contains an update to an existing event."
msgstr "Mezu honek dagoen gertaera baten eguneratzea dakar."

#: imipBarAlreadyProcessedText
msgid "This message contains an event that has already been processed."
msgstr "Mezu honek jadanik prozesatu den gertaera dauka."

#: imipBarProcessedNeedsAction
msgid "This message contains an event that you have not yet responded to."
msgstr "Oraindik erantzun ez duzun gertaera bat dauka mezu honek."

#: imipBarReplyText
msgid "This message contains a reply to an invitation."
msgstr "Mezu honek gonbidapen bati egindako erantzuna dauka."

#: imipBarUnsupportedText
msgid ""
"This message contains an event that this version of Lightning cannot process."
msgstr ""
"Mezu honek Lightning bertsio honek kudeatu ezin duen gertaera bat dauka."

#: imipBarProcessingFailed
msgid "Processing message failed. Status: %1$S."
msgstr "Mezua prozesatzean huts egin du. Egoera: %1$S."

#: imipBarNotWritable
msgid ""
"No writable calendars are configured for invitations, please check the "
"calendar properties."
msgstr ""
"Ez da egutegi idazgarririk konfiguratu gonbidapenetarako, egiaztatu "
"egutegiaren propietateak."

#: imipSendMail.title
msgid "E-Mail Notification"
msgstr "Posta elektroniko bidezko jakinarazpena"

#: imipSendMail.text
msgid "Would you like to send out notification E-Mail now?"
msgstr "Mezu elektronikoa bidaltzerik nahi jakinarazteko?"

#: imipSendMail.Outlook2000CompatMode.text
msgid "Support Outlook 2000 and Outlook 2002/XP"
msgstr "Outlook 2000 eta Outlook 2002/XP laguntza"

#: imipNoIdentity
msgid "None"
msgstr "Bat ere ez"

#: imipNoCalendarAvailable
msgid "There are no writable calendars available."
msgstr "Ez dago egutegi idazkorrik erabilgarri."

#: itipReplySubject
msgid "Event Invitation Reply: %1$S"
msgstr "Gertaera gonbidapenari erantzuna: %1$S"

#: itipReplyBodyAccept
msgid "%1$S has accepted your event invitation."
msgstr "%1$S(e)k zure gertaera gonbidapena onartu du."

#: itipReplyBodyDecline
msgid "%1$S has declined your event invitation."
msgstr "%1$S(e)k zure gertaera gonbidapena ukatu du."

#: itipReplySubjectAccept
msgid "Event Invitation Reply (Accepted): %1$S"
msgstr "Gertaera gonbidapenari erantzuna (onartua): %1$S"

#: itipReplySubjectDecline
msgid "Event Invitation Reply (Declined): %1$S"
msgstr "Gertaera gonbidapen erantzuna (Atzera botata): %1$S"

#: itipReplySubjectTentative
msgid "Event Invitation Reply (Tentative): %1$S"
msgstr "Gertaera gonbidapen erantzuna (Zalantzatan): %1$S"

#: itipRequestSubject
msgid "Event Invitation: %1$S"
msgstr "Gertaera gonbidapena: %1$S"

#: itipRequestUpdatedSubject
msgid "Updated Event Invitation: %1$S"
msgstr "Gertaera gonbidapen eguneratua: %1$S"

#: itipRequestBody
msgid "%1$S has invited you to %2$S"
msgstr "%1$S(e)k %2$S(e)ra gonbidatu zaitu"

#: itipCancelSubject
msgid "Event Canceled: %1$S"
msgstr "Gertaera ezeztatua: %1$S"

#: itipCancelBody
msgid "%1$S has canceled this event: « %2$S »"
msgstr "%1$S(e)k gertaera hau ezeztatu du: « %2$S »"

#: confirmProcessInvitation
msgid ""
"You have recently deleted this item, are you sure you want to process this "
"invitation?"
msgstr ""
"Duela gutxi ezabatu duzu elementu hau, ziur zaude gonbidapena prozesatu nahi "
"duzula?"

#: confirmProcessInvitationTitle
msgid "Process Invitation?"
msgstr "Prozesatu gonbidapena?"

#: invitationsLink.label
msgid "Invitations: %1$S"
msgstr "Gonbidapenak: %1$S"
