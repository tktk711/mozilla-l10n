msgid "System Integration"
msgstr "Sistemaren integrazioa"

#: acceptIntegration.label
msgid "Set as Default"
msgstr "Ezarri lehenetsi gisa"

#: cancelIntegration.label
msgid "Skip Integration"
msgstr "Saltatu integrazioa"

#: cancelIntegration2.label
msgid "Cancel"
msgstr "Utzi"

#: defaultClient.intro
msgid "Use &brandShortName; as the default client for:"
msgstr "Erabili &brandShortName; lehenetsitako bezero gisa honentzat:"

#: email.label
msgid "E-Mail"
msgstr "E-posta"

#: newsgroups.label
msgid "Newsgroups"
msgstr "Berri-taldeak"

#: feeds.label
msgid "Feeds"
msgstr "Jarioak"

#: unsetDefault.tooltip
msgid ""
"It is not possible to unset &brandShortName; as the default client within "
"&brandShortName;. To make another application the default you must use its "
"'Set as default' dialog."
msgstr ""
"Ezin da &brandShortName; barruan &brandShortName; lehenetsitako bezero gisa "
"desezarri. Beste aplikazio bat lehenesteko erabili bere 'Ezarri "
"lehenetsitako gisa' elkarrizketa-koadroa."

#: checkOnStartup.label checkOnStartup.accesskey
msgid "&Always perform this check when starting &brandShortName;"
msgstr "Egi&aztatu beti &brandShortName; abiaraztean"

#: searchIntegration.label searchIntegration.accesskey
msgid "Allow &searchIntegration.engineName; to &search messages"
msgstr "&Baimendu &searchIntegration.engineName;(r)i mezuak bilatzea"
