#: hostColon
msgid "Host:"
msgstr "Ostalaria:"

#: domainColon
msgid "Domain:"
msgstr "Domeinua:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Soilik zifratutako konexioak"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Edozein konexio mota"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "Saio amaieran"

#: showDetails
msgid "Show Details"
msgstr "Erakutsi xehetasunak"

#: hideDetails
msgid "Hide Details"
msgstr "Ezkutatu xehetasunak"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "%S guneak cookie bat ezarri nahi du."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "%S guneak bigarren cookie bat ezarri nahi du."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"%S guneak beste cookie bat ezarri nahi du.\n"
"Dagoeneko gune honetako %S cookie dituzu."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "%S guneak dagoen cookie bat aldatu nahi du."
