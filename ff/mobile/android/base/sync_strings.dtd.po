msgid "Firefox Sync"
msgstr "Firefox Sync"

#: syncBrand.shortName.label
msgid "Sync"
msgstr "Sanngoɗin"

#. Main titles.
#: sync.app.name.label
msgctxt "sync.app.name.label"
msgid "&syncBrand.fullName.label;"
msgstr "&syncBrand.fullName.label;"

#: sync.title.connect.label
msgid "Connect to &syncBrand.shortName.label;"
msgstr "Seŋo e &syncBrand.shortName.label;"

#: sync.title.adddevice.label
msgid "Add a &syncBrand.fullName.label; Account"
msgstr "Ɓeydu Konte &syncBrand.fullName.label;"

#: sync.title.pair.label
msgid "Pair a Device"
msgstr "Jokkondir Kaɓirɗi"

#. J-PAKE Key Screen
#: sync.subtitle.connect.label
msgid ""
"To activate your new device, select “Set up &syncBrand.shortName.label;” on "
"the device."
msgstr ""
"Ngam hurminde kaɓirgol maa kesol ngol, labo “Set up &syncBrand.shortName."
"label;” e kaɓirgol ngol."

#: sync.subtitle.header2.label
msgid "Enter this code on your other device"
msgstr "Naatnu ngol dokkol e kaɓirgol maa ngola"

#: sync.subtitle.connectlocation2.label
msgid ""
"Select “&sync.title.pair.label;” in the &syncBrand.shortName.label; section "
"of your other device\\'s Firefox options."
msgstr ""
"Labo “&sync.title.pair.label;” e rogere &syncBrand.shortName.label; e nder "
"cuɓe maa Firefox e kaɓirgol maa ngola."

#: sync.subtitle.pair.label
msgid "To activate, select “Pair a device” on your other device."
msgstr "Ngam hurminde, labo “Jokkondir Kaɓirɗi” e kaɓirgol maa ngola."

#: sync.pin.default.label
msgid "...\\n...\\n...\\n"
msgstr "...\\n...\\n...\\n"

#: sync.pin.oneline.label
msgid "..."
msgstr "..."

#: sync.link.show.label
msgid "Show me how."
msgstr "Hollu no waɗirtee."

#: sync.link.advancedsetup.label
msgid "Advanced setup…"
msgstr "Teelte ceeɓtore…"

#: sync.link.nodevice.label
msgid "I don\\'t have the device with me…"
msgstr "Mi jogaaki kaɓirgol ngol jooni…"

#. J-PAKE Waiting Screen
#: sync.jpake.subtitle.waiting.label
msgid "Waiting for other device…"
msgstr "Nana sabbii kabirgol goɗngol ngol…"

#. Account Login Screen
#: sync.subtitle.account.label
msgid "Enter your &syncBrand.fullName.label; account information"
msgstr "Naatnu humpito konte maa &syncBrand.fullName.label;"

#: sync.input.username.label
msgid "Account Name"
msgstr "Innde Konte"

#: sync.input.password.label
msgid "Password"
msgstr "Finnde"

#: sync.input.key.label
msgid "Recovery Key"
msgstr "Kelal jiytirgal"

#: sync.checkbox.server.label
msgid "Use custom server"
msgstr "Huutoro sarworde feŋtorde"

#: sync.input.server.label
msgid "Server URL"
msgstr "URL Sarworde"

#. Setup Fail
#: sync.title.fail.label
msgid "Cannot Set Up &syncBrand.shortName.label;"
msgstr "Horiima teeltaade set-up &syncBrand.shortName.label;"

#: sync.subtitle.fail.label
msgid ""
"&syncBrand.fullName.label; could not connect to the server. Would you like "
"to try again?"
msgstr ""
"&syncBrand.fullName.label; horiima seŋaade e sarworde ndee. Aɗa yiɗi etaade "
"goɗngol?"

#: sync.button.tryagain.label
msgid "Try again"
msgstr "Eto goɗngol"

#: sync.button.manual.label
msgid "Manual Setup"
msgstr "Teelte junngol"

#: sync.subtitle.nointernet.label
msgid "No internet connection available."
msgstr "Alaa ceŋol enternet woodi."

#: sync.subtitle.failaccount.label
msgid "Account creation on your device failed."
msgstr "Cosgol konte e kaɓirgol maa woorii."

#: sync.subtitle.failmultiple.label
msgid ""
"Do you have more than one Firefox installed? Currently, &syncBrand.fullName."
"label; only supports one Firefox installation at a time. Please uninstall "
"other instances to use &syncBrand.shortName.label;."
msgstr ""
"Aɗa jogii ko ɓuri aafannde wootere Firefox? Oo sahaa, &syncBrand.fullName."
"label; jaɓata tan ko aafannde Firefox wootere laawol gootol. Tiiɗno aaftu "
"cili goɗɗi ɗii ngam huutoraade &syncBrand.shortName.label;."

#. Setup Success
#: sync.title.success.label
msgid "Setup Complete"
msgstr "Teeltagol Gasii"

#: sync.subtitle.success.label1
msgid ""
"Your data is now being downloaded in the background. You can go to Settings "
"to manage your account, or start browsing with &brandShortName;."
msgstr ""
"Keɓe maa ngoni jooni ko e aawteede e cakkitol. Aɗa waawi yahde to Teelte "
"ngam toppitaade konte maa, walla fuɗɗo wanngoraade &brandShortName;."

#: sync.settings.label
msgid "Settings"
msgstr "Teelte"

#: sync.subtitle.manage.label1
msgid ""
"Your &syncBrand.fullName.label; account is already set up. You can go to "
"Settings to manage your account, or launch &brandShortName;."
msgstr ""
"Konte maa &syncBrand.fullName.label; teeltanooma kisa. Yah to Teelte ngam "
"toppitaade konte maa walla hurmin &brandShortName;."

#. Pair Device
#: sync.pair.tryagain.label
msgid "Please try again."
msgstr "Tiiɗno fuɗɗito."

#: sync.pair.connectlocation.label
msgid ""
"To activate your new device, select “Set up &syncBrand.shortName.label;” on "
"the device, and then select “I Have an Account.”"
msgstr ""
"Ngam hurminde kaɓirgol maa kesol ngol, labo “Set up &syncBrand.shortName."
"label;” e kaɓirgol ngol, kisa labo-ɗaa “Miɗo Jogii Konte.”"

#. Firefox SyncAdapter Settings Screen
#: sync.settings.options.label
msgid "Options"
msgstr "Cuɓe"

#: sync.summary.pair.label
msgid "Link another device to your &syncBrand.shortName.label; account"
msgstr "Seŋ kaɓirgol goɗngol e konte maa &syncBrand.shortName.label;"

#. Configure Engines
#: sync.configure.engines.title.label
msgid "What to sync"
msgstr "Ko Syncetee"

#: sync.configure.engines.sync.my.title.label
msgid "Sync your…"
msgstr "Sync…"

#: sync.configure.engines.title.bookmarks
msgid "Bookmarks"
msgstr "Maantore maa"

#: sync.configure.engines.title.passwords
msgid "Passwords"
msgstr "Finndeeji"

#: sync.configure.engines.title.history
msgid "History"
msgstr "Aslol"

#: sync.configure.engines.title.tabs
msgid "Tabs"
msgstr "Tabbe"

