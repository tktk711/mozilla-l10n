#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Sync cakkitiiɗo: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Sync cakkitiinɗo: ko heen woni…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Konte: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Seŋaaki"

#: connecting.label
msgid "Connecting…"
msgstr "Nana seŋoo…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Konte maa Sync Firefox momtaama"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Firtu"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 jaaɓdaani e yamre sakkitiinde Firefox Sync. Tiiɗno hesɗitin to yamre "
"sakkitiinde."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 jaaɓdaani e yamre sakkitiinde Firefox Sync. Tiiɗno hesɗitin Firefox e "
"ordinateer maa goɗɗo oo."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Ɓeydu Humpito"

#: sync.update.close
msgid "Close"
msgstr "Uddu"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Horiima Teeltaade Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Alaa ceŋol enternet woodi"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S horiima seŋaade e Sync. Aɗa yiɗi etaade goɗngol?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Eto goɗngol"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Teelte junngol"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Alaa tabbe ngoni e ordinateeruuji goɗɗi ɗii."

