#: migrationWizard.title
msgid "Import Wizard"
msgstr "Treoraí Iompórtála"

#: importFromWin.label
msgid ""
"Import Options, Account Settings, Address Book, Filters and other data from:"
msgstr ""
"Iompórtáil Roghanna, Socruithe Cuntais, Leabhar Seoltaí, Scagairí agus "
"sonraí eile ó:"

#: importFromNonWin.label
msgid ""
"Import Preferences, Account Settings, Address Book, Filters, and other data "
"from:"
msgstr ""
"Iompórtáil Roghanna, Socruithe Cuntais, Leabhar Seoltaí, Scagairí, agus "
"sonraí eile ó:"

#: importFromNothing.label importFromNothing.accesskey
msgid "&Don't import anything"
msgstr "&Ná hiompórtáil rud ar bith"

#: importFromSeamonkey3.label importFromSeamonkey3.accesskey
msgid "&SeaMonkey 2 or later"
msgstr "&SeaMonkey 2 nó níos déanaí"

#: importFromOExpress.label importFromOExpress.accesskey
msgid "O&utlook Express"
msgstr "O&utlook Express"

#: importFromOutlook.label importFromOutlook.accesskey
msgid "&Outlook"
msgstr "&Outlook"

#: importSource.title
msgid "Import Settings and Mail Folders"
msgstr "Iompórtáil Socruithe agus Fillteáin Ríomhphoist"

#: importItems.title
msgid "Items to Import"
msgstr "Rudaí le hIompórtáil"

#: importItems.label
msgid "Select which items to import:"
msgstr "Roghnaigh na rudaí le hiompórtáil:"

#: migrating.title
msgid "Importing…"
msgstr "Á nIompórtáil…"

#: migrating.label
msgid "The following items are currently being imported…"
msgstr "Tá na rudaí seo á n-iompórtáil faoi láthair…"

#: selectProfile.title
msgid "Select Profile"
msgstr "Roghnaigh Próifíl"

#: selectProfile.label
msgid "The following profiles are available to import from:"
msgstr "Próifílí le fáil ónar féidir a iompórtáil:"

#: done.title
msgid "Import Complete"
msgstr "Iompórtáil curtha i gcrích"

#: done.label
msgid "The following items were successfully imported:"
msgstr "Iompórtáladh na rudaí seo go rathúil:"

#~ msgid "&Eudora"
#~ msgstr "&Eudora"
