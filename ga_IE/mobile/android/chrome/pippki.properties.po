#~ msgstr "Ainm Coiteann (Common Name, CN)"

#~ msgid "Organization (O)"
#~ msgstr "Eagras (Organization, O)"

#~ msgid "Serial Number"
#~ msgstr "Sraithuimhir"

#~ msgid "SHA1 Fingerprint"
#~ msgstr "Méarlorg SHA1"

#~ msgid "Begins On"
#~ msgstr "Le tosú"

#~ msgid "Expires On"
#~ msgstr "Le síothlú"

