#: migrationWizard.title
msgid "Import Wizard"
msgstr "Draoidh an ion-phortaidh"

#: importFrom.label
msgid "Import Options, Bookmarks, History, Passwords and other data from:"
msgstr ""
"Ion-phortaich roghainnean, comharran-lìn, eachdraidh, faclan-faire 's dàta "
"eile o:"

#: importFromUnix.label
msgid "Import Preferences, Bookmarks, History, Passwords and other data from:"
msgstr ""
"Ion-phortaich roghainnean, comharran-lìn, eachdraidh, faclan-faire 's dàta "
"eile o:"

#: importFromBookmarks.label
msgid "Import Bookmarks from:"
msgstr "Ion-phortaich comharran-lìn o:"

#: importFromIE.label importFromIE.accesskey
msgid "&Microsoft Internet Explorer"
msgstr "&Microsoft Internet Explorer"

#: importFromNothing.label importFromNothing.accesskey
msgid "&Don't import anything"
msgstr "Na ion-phortaich ru&d sam bith"

#: importFromSafari.label importFromSafari.accesskey
msgid "&Safari"
msgstr "&Safari"

#: importFromChrome.label importFromChrome.accesskey
msgid "&Chrome"
msgstr "&Chrome"

#: importFromFirefox.label importFromFirefox.accesskey
msgid "Firefo&x"
msgstr "Firefo&x"

#: noMigrationSources.label
msgid ""
"No programs that contain bookmarks, history or password data could be found."
msgstr ""
"Cha deach prògram a lorg sa bheil comharran-lìn, eachdraidh no faclan-faire."

#: importSource.title
msgid "Import Settings and Data"
msgstr "Ion-phortaich roghainnean is dàta"

#: importItems.title
msgid "Items to Import"
msgstr "Rudan ri an ion-phortadh"

#: importItems.label
msgid "Select which items to import:"
msgstr "Tagh na rudan a thèid ion-phortadh:"

#: migrating.title
msgid "Importing…"
msgstr "Ag ion-phortadh…"

#: migrating.label
msgid "The following items are currently being imported…"
msgstr "Tha na rudan a leanas 'gan ion-phortadh an-dràsta…"

#: selectProfile.title
msgid "Select Profile"
msgstr "Tagh pròifil"

#: selectProfile.label
msgid "The following profiles are available to import from:"
msgstr "'S urrainn dhut ion-phortadh o na pròifilean a leanas:"

#: done.title
msgid "Import Complete"
msgstr "Ion-phortadh deiseil"

#: done.label
msgid "The following items were successfully imported:"
msgstr "Chaidh na rudan a leanas ion-phortadh gu soirbheachail:"

