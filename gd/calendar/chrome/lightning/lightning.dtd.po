#. iMIP Bar (meeting support)
#: lightning.imipbar.btnAccept.label
msgid "Accept"
msgstr "Gabh ris"

#: lightning.imipbar.btnAccept2.tooltiptext
msgid "Accept event invitation"
msgstr "Gabh ri cuireadh an tachartais"

#: lightning.imipbar.btnAcceptRecurrences.label
msgid "Accept all"
msgstr "Gabh ris a h-uile"

#: lightning.imipbar.btnAcceptRecurrences2.tooltiptext
msgid "Accept event invitation for all occurrences of the event"
msgstr "Gabh ris a' chuireadh airson gach aon tachartas dheth"

#: lightning.imipbar.btnAdd.label
msgid "Add"
msgstr "Cuir ris"

#: lightning.imipbar.btnAdd.tooltiptext
msgid "Add the event to the calendar"
msgstr "Cuir an tachartas ris a' mhìosachan"

#: lightning.imipbar.btnDecline.label
msgid "Decline"
msgstr "Diùlt"

#: lightning.imipbar.btnDecline2.tooltiptext
msgid "Decline event invitation"
msgstr "Diùlt cuireadh an tachartais"

#: lightning.imipbar.btnDeclineRecurrences.label
msgid "Decline all"
msgstr "Diùlt na h-uile"

#: lightning.imipbar.btnDeclineRecurrences2.tooltiptext
msgid "Decline event invitation for all occurrences of the event"
msgstr "Diùlt gach cuireadh gun tachartas"

#: lightning.imipbar.btnDelete.label
msgid "Delete"
msgstr "Sguab às"

#: lightning.imipbar.btnDelete.tooltiptext
msgid "Delete from calendar"
msgstr "Sguab às a' mhìosachan"

#: lightning.imipbar.btnDetails.label
msgid "Details…"
msgstr "Mion-fhiosrachadh…"

#: lightning.imipbar.btnDetails.tooltiptext
msgid "Show event details"
msgstr "Seall mion-fhiosrachadh an tachartais"

#: lightning.imipbar.btnMore.label
msgid "More"
msgstr "Barrachd"

#: lightning.imipbar.btnMore.tooltiptext
msgid "Click to show more options"
msgstr "Dèan briogadh airson barrachd roghainnean"

#: lightning.imipbar.btnReconfirm2.label
msgid "Reconfirm"
msgstr "Ath-dhearbh"

#: lightning.imipbar.btnReconfirm.tooltiptext
msgid "Sends a reconfirmation to the organizer"
msgstr "Cuiridh seo ath-dhearbhadh gun eagraiche"

#: lightning.imipbar.btnSaveCopy.label
msgid "Save a copy"
msgstr "Sàbhail lethbhreac dheth"

#: lightning.imipbar.btnSaveCopy.tooltiptext
msgid ""
"Save a copy of the event to the calendar independently of replying to the "
"organizer. The list of attendees will be cleared."
msgstr ""
"Sàbhail lethbhreac dhen tachartas sa mhìosachan, fiù mur am freagair thu an "
"t-eagraiche. Thèid liosta nam freastalaichean fhalamhachadh."

#: lightning.imipbar.btnTentative.label
msgid "Tentative"
msgstr "An dòchas"

#: lightning.imipbar.btnTentative2.tooltiptext
msgid "Accept event invitation tentatively"
msgstr "Gabh ri cuireadh an tachartais gun chinnt"

#: lightning.imipbar.btnTentativeRecurrences.label
msgid "Tentative all"
msgstr "Gach aon gun chinnt"

#: lightning.imipbar.btnTentativeRecurrences2.tooltiptext
msgid "Accept event invitation tentatively for all occurrences of the event"
msgstr "Gabh ri gach cuireadh an tachartais gun chinnt"

#: lightning.imipbar.btnUpdate.label
msgid "Update"
msgstr "Ùraich"

#: lightning.imipbar.btnUpdate.tooltiptext
msgid "Update event in calendar"
msgstr "Ùraich an tachartas sa mhìosachan"

#: lightning.imipbar.description
msgid "This message contains an invitation to an event."
msgstr "Tha cuireadh gu tachartas san teachdaireachd seo."

