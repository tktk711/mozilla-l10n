#. # Publishing error strings:
#. # LOCALIZATION NOTE Don't translate %dir% or %file% in the Publishing error strings:
#: FileNotFound
msgid "%file% not found."
msgstr "Cha deach am faidhle %file% a lorg."

#: SubdirDoesNotExist
msgid ""
"The subdirectory \"%dir%\" doesn't exist on this site or the filename \"%file"
"%\" is already in use by another subdirectory."
msgstr ""
"Chan eil am fo-phasgan \"%dir%\" ann air an làrach seo no tha an t-ainm "
"\"%file%\" 'ga chleachdadh ann am fo-phasgan eile mu thràth."

#: FilenameIsSubdir
msgid "The filename \"%file%\" is already in use by another subdirectory."
msgstr ""
"Tha an t-ainm \"%file%\" 'ga chleachdadh ann am fo-phasgan eile mu thràth."

#: ServerNotAvailable
msgid "The server is not available. Check your connection and try again later."
msgstr ""
"Chan eil am frithealaiche ri fhaighinn. Cuir sùil air a' cheangal agad 's "
"feuch ris a-rithist an ceann tamaill."

#: Offline
msgid ""
"You are currently offline. Click the icon near the lower-right corner of any "
"window to go online."
msgstr ""
"Tha thu far loidhne an-dràsta. Briog air an ìomhaigheag air an taobh dheis "
"aig bun uinneig sam bith gus a dhol air loidhne."

#: DiskFull
msgid "There is not enough disk space available to save the file \"%file%.\""
msgstr ""
"Chan eil àite gu leòr air an diosg gus am faidhle \"%file%\" a shàbhaladh."

#: NameTooLong
msgid "The filename or subdirectory name is too long."
msgstr "Tha an t-ainm airson an fhaidhle no an fho-phasgan ro fhada."

#: AccessDenied
msgid "You do not have permission to publish to this location."
msgstr "Chan eil cead foillseachaidh agad san àite seo."

#: UnknownPublishError
msgid "Unknown publishing error occurred."
msgstr "Thachair mearachd foillseachaidh neo-aithnichte."

#: PublishFailed
msgid "Publishing failed."
msgstr "Dh'fhàillig am foillseachadh."

#: PublishCompleted
msgid "Publishing completed."
msgstr "Deiseil leis an fhoillseachadh."

#: AllFilesPublished
