msgid "&Name:"
msgstr "Ai&nm:"

#: folderProps.location.label folderProps.location.accesskey
msgid "&Location:"
msgstr "Seò&ladh:"

#: folderSharingTab.label
msgid "Sharing"
msgstr "Co-roinneadh"

#: privileges.button.label privileges.button.accesskey
msgid "&Privileges…"
msgstr "&Pribhleidean…"

#: permissionsDesc.label
msgid "You have the following permissions:"
msgstr "Tha cead agad na leanas a dhèanamh:"

#: folderType.label
msgid "Folder Type:"
msgstr "Seòrsa a' phasgain:"

#: folderQuotaTab.label
msgid "Quota"
msgstr "Cuota"

#: folderQuotaRoot.label
msgid "Quota Root:"
msgstr "Freumh a' chuota:"

#: folderQuotaUsage.label
msgid "Usage:"
msgstr "Cleachdadh:"

#: folderQuotaStatus.label
msgid "Status:"
msgstr "Cor:"

#: numberOfMessages.label
msgid "Number of messages:"
msgstr "Àireamh de theachdaireachdan:"

#. LOCALIZATION NOTE: When the number of messages can't be determined, this string is displayed as the number
#: numberUnknown.label
msgctxt "numberUnknown.label"
msgid "unknown"
msgstr "chan eil fhios"

#: sizeOnDisk.label
msgid "Size on disk:"
msgstr "Meud air an diosg:"

#. LOCALIZATION NOTE: When the size can't be determined, this string is displayed as the size
#: sizeUnknown.label
msgctxt "sizeUnknown.label"
msgid "unknown"
msgstr "chan eil fhios"

