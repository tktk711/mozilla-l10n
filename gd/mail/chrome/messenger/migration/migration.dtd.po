#: migrationWizard.title
msgid "Import Wizard"
msgstr "Draoidh an ion-phortaidh"

#: importFromWin.label
msgid ""
"Import Options, Account Settings, Address Book, Filters and other data from:"
msgstr ""
"Ion-phortaich roghainnean, roghainnean cunntais, leabhar nan seòlaidh, "
"criathragan is dàta eile o:"

#: importFromNonWin.label
msgid ""
"Import Preferences, Account Settings, Address Book, Filters, and other data "
"from:"
msgstr ""
"Ion-phortaich roghainnean, roghainnean cunntais, leabhar nan seòlaidh, "
"criathragan is dàta eile o:"

#: importFromNothing.label importFromNothing.accesskey
msgid "&Don't import anything"
msgstr "Na ion-phortaich ru&d sam bith"

#: importFromSeamonkey3.label importFromSeamonkey3.accesskey
msgid "&SeaMonkey 2 or later"
msgstr "&SeaMonkey 2 no nas ùire"

#: importFromOExpress.label importFromOExpress.accesskey
msgid "O&utlook Express"
msgstr "O&utlook Express"

#: importFromOutlook.label importFromOutlook.accesskey
msgid "&Outlook"
msgstr "&Outlook"

#: importFromEudora.label importFromEudora.accesskey
msgid "&Eudora"
msgstr "&Eudora"

#: importSource.title
msgid "Import Settings and Mail Folders"
msgstr "Ion-phortaich na roghainnean is pasgain puist"

#: importItems.title
msgid "Items to Import"
msgstr "Rudan ri an ion-phortadh"

#: importItems.label
msgid "Select which items to import:"
msgstr "Tagh na rudan a thèid ion-phortadh:"

#: migrating.title
msgid "Importing…"
msgstr "Ag ion-phortadh…"

#: migrating.label
msgid "The following items are currently being imported…"
msgstr "Tha na rudan a leanas 'gan ion-phortadh an-dràsta…"

#: selectProfile.title
msgid "Select Profile"
msgstr "Tagh pròifil"

#: selectProfile.label
msgid "The following profiles are available to import from:"
msgstr "'S urrainn dhut ion-phortadh o na pròifilean a leanas:"

#: done.title
msgid "Import Complete"
msgstr "Ion-phortadh deiseil"

#: done.label
msgid "The following items were successfully imported:"
msgstr "Chaidh na rudan a leanas ion-phortadh gu soirbheachail:"
