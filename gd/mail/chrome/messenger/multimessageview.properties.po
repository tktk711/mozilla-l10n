#. # LOCALIZATION NOTE (selectedNMessages, acrossNThreads): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/Localization_and_Plurals
#. # #1 number of threads or solitary messages selected
#. # example: 4 conversations
#: NConversations
msgid "#1 conversation; #1 conversations"
msgstr "#1 chòmhradh;#1 chòmhradh;#1 còmhraidhean;#1 còmhradh"

#: numMsgs
msgid "#1 message;#1 messages"
msgstr ""
"#1 teachdaireachd;#1 theachdaireachd;#1 teachdaireachdan;#1 teachdaireachd"

#: countUnread
msgid ", #1 unread;, #1 unread"
msgstr ", #1 gun leughadh;, #1 gun leughadh"

#: ignoredCount
msgid "#1 ignored;#1 ignored"
msgstr ""
"#1 teachdaireachd air leigeil seachad;#1 theachdaireachd air leigeil seachad;"
"#1 teachdaireachdan air leigeil seachad;#1 teachdaireachd air leigeil seachad"

#: noSubject
msgid "(no subject)"
msgstr "(gun chuspair)"

#. # thread and multiple message selection summaries
#: messagesSize
msgid "These messages take up: #1 #2."
msgstr "Tha na teachdaireachdan seo a' lìonadh: #1 #2."

#: noticeText
msgid "(Note: #1 messages are selected, the first #2 are shown)"
msgstr ""
"(Aire: Tha #1 teachdaireachd(an) air an taghadh, tha a' chiad #2 dhiubh 'gan "
"sealltainn)"
