msgid "&Return Receipts…"
msgstr "&Bannan-cuidhteis…"

#. Display and Reading Settings
#: reading.caption
msgid "Reading"
msgstr "Leughadh"

#: display.caption
msgid "Display"
msgstr "Taisbeanadh"

#: showCondensedAddresses.label showCondensedAddresses.accesskey
msgid "&Show only display name for people in my address book"
msgstr ""
"Na &seall ach an t-ainm-taisbeanaidh airson nan daoine ann an leabhar nan "
"seòladh agam"

#: autoMarkAsRead.label autoMarkAsRead.accesskey
msgid "&Automatically mark messages as read"
msgstr "Cuire&ar comharra gun deach teachdaireachd a leughadh leis fhèin"

#: markAsReadNoDelay.label markAsReadNoDelay.accesskey
msgid "Immediately &on display"
msgstr "Sa bhad nuair a ch&oimheadas mi air"

#. This will concatenate to
#. "After displaying for [___] seconds",
#. using (markAsReadDelay.label) and a number (secondsLabel.label).
#: markAsReadDelay.label markAsReadDelay.accesskey
msgid "After &displaying for"
msgstr "Nuair a bhios mi air coimhea&d air fad"

#: secondsLabel.label
msgid "seconds"
msgstr "diog(an)"

#: openMsgIn.label
msgid "Open messages in:"
msgstr "Fosgail teachdaireachdan ann an:"

#: openMsgInNewTab.label openMsgInNewTab.accesskey
msgid "A new &tab"
msgstr "&Taba ùr"

#: reuseExpRadio0.label reuseExpRadio0.accesskey
msgid "A &new message window"
msgstr "Ui&nneag teachdaireachd ùr"

#: reuseExpRadio1.label reuseExpRadio1.accesskey
msgid "An &existing message window"
msgstr "Uinn&eag teachdaireachd a tha fosgailte mu thràth"

#: closeMsgOnMoveOrDelete.label closeMsgOnMoveOrDelete.accesskey
msgid "&Close message window/tab on move or delete"
msgstr ""
"Dùin uinneag/taba na tea&chdaireachd nuair a sguabas mi às fear no ma "
"ghluaiseas mi fear"

#. Update
#: autoCheck.label
msgid "Automatically check for updates to:"
msgstr "Lorgar ùrachaidhean airson na leanas leis fhèin:"

#: enableAppUpdate.label
msgid "&brandShortName;"
msgstr "&brandShortName;"

#: enableAppUpdate.accesskey
msgid "h"
msgstr "h"

#: enableAddonsUpdate.label enableAddonsUpdate.accesskey
msgid "Installed &Add-ons"
msgstr "Tuille&adain a chaidh a stàladh"

#: whenUpdatesFound.label
msgid "When updates to &brandShortName; are found,"
msgstr "Nuair a thèid ùrachadh a lorg airson &brandShortName;,"

#: modeAskMe.label modeAskMe.accesskey
msgid "Ask &me what I want to do"
msgstr "Faighnich dhìo&m na tha mi airson dèanamh"

#: modeAutomatic.label modeAutomatic.accesskey
msgid "Automatically &download and install the update"
msgstr "Luch&daichear a-nuas is stàlaichear an t-ùrachadh leis fhèin"

#: modeAutoAddonWarn.label modeAutoAddonWarn.accesskey
msgid "&Warn me if this will disable any of my add-ons"
msgstr ""
"&Thoir rabhadh dhomh ma bhios gin dhe na tuilleadan agam à comas 'na chois"

#: showUpdates.label showUpdates.accesskey
msgid "&Show Update History"
msgstr "&Seall eachdraidh nan ùrachaidhean"

#: useService.label useService.accesskey
msgid "Use a &background service to install updates"
msgstr "Cleachd seir&bheis a stàlaicheas ùrachaidhean sa chùlaibh"

