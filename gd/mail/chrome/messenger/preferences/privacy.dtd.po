#. Web Content
#: captionWebContent.label
msgid "Web Content"
msgstr "Susbaint-lìn"

#: keepHistory.label keepHistory.accesskey
msgid "&Remember websites and links I've visited"
msgstr ""

#: acceptCookies.label acceptCookies.accesskey
msgid "&Accept cookies from sites"
msgstr "Gabh ri briosgaidean o làrai&chean"

#: acceptThirdParty.pre.label acceptThirdParty.pre.accesskey
msgid "A&ccept third-party cookies:"
msgstr "&Gabh ri briosgaidean treas pàrtaidh:"

#: acceptThirdParty.always.label
msgid "Always"
msgstr "An-còmhnaidh"

#: acceptThirdParty.never.label
msgid "Never"
msgstr "Chan ann idir"

#: acceptThirdParty.visited.label
msgid "From visited"
msgstr "O fheadhainn air an deach tadhal"

#: keepUntil.label keepUntil.accesskey
msgid "&Keep until:"
msgstr "&Cum gus:"

#: expire.label
msgid "they expire"
msgstr "am falbh an ùine orra"

#: close.label
msgid "I close &brandShortName;"
msgstr "an dùin mi &brandShortName;"

#: askEachTime.label
msgid "ask me every time"
msgstr "faighnich dhìom gach turas"

#: cookieExceptions.label cookieExceptions.accesskey
msgid "&Exceptions…"
msgstr "&Eisgeachdan…"

#: showCookies.label showCookies.accesskey
msgid "&Show Cookies…"
msgstr "&Seall na briosgaidean…"

#: dntTrackingNopref.label2
msgid "Do not tell sites anything about my tracking preferences"
msgstr "Na innis do làraichean dè na roghainnean tracaidh a tha agam"

#: dntTrackingNopref.accesskey
msgid "o"
msgstr "o"

#: dntTrackingNotOkay.label2
msgid "Tell sites that I do not want to be tracked"
msgstr "Innis do làraichean-lìn nach bu toigh leam gun dèan iad tracadh orm"

#: dntTrackingNotOkay.accesskey
msgid "n"
msgstr "n"

#: dntTrackingOkay.label2
msgid "Tell sites that I want to be tracked"
msgstr "Innis do làraichean-lìn gum faod iad mo thracadh"

#: dntTrackingOkay.accesskey
msgid "t"
msgstr "t"

#: doNotTrackInfo.label
msgid "Learn more about Do Not Track"
msgstr "Barrachd fiosrachaidh air a' ghleus \"Na tracaich orm\""
