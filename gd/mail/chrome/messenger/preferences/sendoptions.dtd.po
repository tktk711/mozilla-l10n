msgstr "&Cuir ris…"

#: DeleteHtmlDomain.label DeleteHtmlDomain.accesskey
msgctxt "DeleteHtmlDomain.label DeleteHtmlDomain.accesskey"
msgid "&Delete"
msgstr "&Sguab às"

#: DeletePlainText.label DeletePlainText.accesskey
msgctxt "DeletePlainText.label DeletePlainText.accesskey"
msgid "&Delete"
msgstr "&Sguab às"

