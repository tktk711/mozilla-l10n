msgstr "Ceart ma-tha"

#: nssdialogs.cancel.label
msgid "Cancel"
msgstr "Sguir dheth"

#. # These strings are stolen from security/manager/locales/en-US/chrome/pippki/pippki.dtd
#: downloadCert.title
msgid "Downloading Certificate"
msgstr "A' luchdadh a-nuas teisteanas"

#: downloadCert.message1
msgid "You have been asked to trust a new Certificate Authority (CA)."
msgstr ""
"Chaidh iarraidh ort earbsa a chur ann an ùghdarras teisteanachaidh (ÙT) ùr."

#: downloadCert.viewCert.label
msgid "View"
msgstr "Seall"

#: downloadCert.trustSSL
msgid "Trust to identify websites."
msgstr "Cuir earbsa ann gus làraichean-lìn aithneachadh."

#: downloadCert.trustEmail
msgid "Trust to identify email users."
msgstr "Cuir earbsa ann gus cleachdaichean puist-dhealain aithneachadh."

#: downloadCert.trustObjSign
msgid "Trust to identify software developers."
msgstr "Cuir earbsa ann gus luchd-leasachaidh bathar-bog aithneachadh."

#: caCertExists.title
msgid "Certificate Exists"
msgstr "Tha teisteanas ann"

#: caCertExists.message
msgid "The Certificate already exists."
msgstr "Tha an teisteanas seo ann mu thràth."

#: certmgr.title
msgid "Certificate Details"
msgstr "Mion-fhiosrachadh an teisteanais"

#. # These strings are stolen from security/manager/locales/en-US/chrome/pippki/certManager.dtd
#: certmgr.subjectinfo.label
msgid "Issued To"
msgstr "Air fhoillseachadh do"

#: certmgr.issuerinfo.label
msgid "Issued By"
msgstr "Air fhoillseachadh le"

#: certmgr.validity.label
msgid "Validity"
msgstr "Dligheachd"

#: certmgr.fingerprints.label
msgid "Fingerprints"
msgstr "Lorgan-meur"

#: certmgr.certdetail.cn
msgid "Common Name (CN)"
msgstr "Ainm Coitcheann (Common Name, CN)"

#: certmgr.certdetail.o
msgid "Organization (O)"
msgstr "Buidheann (Organisation, O)"

#: certmgr.certdetail.ou
msgid "Organizational Unit (OU)"
msgstr "Aonad na buidhne (Organisational Unit, OU)"

#: certmgr.certdetail.serialnumber
msgid "Serial Number"
msgstr "Àireamh shreathach"

#: certmgr.certdetail.sha1fingerprint
msgid "SHA1 Fingerprint"
msgstr "Lorg-meur SHA1"

#: certmgr.certdetail.md5fingerprint
msgid "MD5 Fingerprint"
msgstr "Lorg-meur MD5"

#: certmgr.issued
msgid "Issued On"
msgstr "Air fhoillseachadh"

#: certmgr.expires
msgid "Expires On"
msgstr "Falbhaidh an ùine air"
