#: hostColon
msgid "Host:"
msgstr "Oyeokuai vae:"

#: domainColon
msgid "Domain:"
msgstr "Oporomoamɨri:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Jaeño ñomoïru oyepapagüe reta"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Oipotagüe oñemoïru vae"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "Sesion iyapɨ pe"

#: showDetails
msgid "Show Details"
msgstr "Oechauka mbegüe pe kavi"

#: hideDetails
msgid "Hide Details"
msgstr "Oñomi oechauka kavi vae"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Sitio %S oeka omboatɨ vaerä metei cookie."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "Sitio %S oeka omboatɨ vaerä mokoía cookie."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"Sitio %S oiporu iya pe omboatɨ vaerä ïru cookie.\n"
" Oime oyemboatɨgüe reta %S cookies kuae sitio pe."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "Sitio %S oipota ombopɨau metei cookie oime vae."
