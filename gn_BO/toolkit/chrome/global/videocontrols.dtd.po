msgid "Play"
msgstr "Mbarɨvo yeye"

#: playButton.pauseLabel
msgid "Pause"
msgstr "Epɨta räri"

#: muteButton.muteLabel
msgid "Mute"
msgstr "Emokïrii"

#: muteButton.unmuteLabel
msgid "Unmute"
msgstr "Eyeapɨsaka"

#: fullscreenButton.enterfullscreenlabel
msgid "Full Screen"
msgstr "pantalla jokoreve"

#: fullscreenButton.exitfullscreenlabel
msgid "Exit Full Screen"
msgstr "Eë pantalla jokoreve vae güi"

#: stats.media
msgid "Media"
msgstr "Multimedia"

#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: stats.size
msgid "Size"
msgstr "Tuichagüe ani michigüe"

#: stats.activity
msgid "Activity"
msgstr "Tembiapo"

#: stats.activityPaused
msgid "Paused"
msgstr "Opɨta michi pegua"

#: stats.activityPlaying
msgid "Playing"
msgstr "Omoñemuña güɨnoi"

#: stats.activityEnded
msgid "Ended"
msgstr "Tɨnɨe kavi"

#: stats.activitySeeking
msgid "(seeking)"
msgstr "(Oeka güɨnoi)"

#: stats.volume
msgid "Volume"
msgstr "Jeta kavi"

#: stats.framesParsed
msgid "Frames parsed"
msgstr "Ñamae kavi jese vae japɨpe reta"

#: stats.framesDecoded
msgid "Frames decoded"
msgstr "Oyembocoficagüe japɨpe"

#: stats.framesPresented
msgid "Frames presented"
msgstr "Oechauka vae reta japɨpe"

#: stats.framesPainted
msgid "Frames painted"
msgstr "Omopirague reta japɨpe"

#: error.aborted
msgid "Video loading stopped."
msgstr "Video ombogüeyɨ vae opɨta"

#: error.network
msgid "Video playback aborted due to a network error."
msgstr "Video oyapo vae oikomegua mbaeavɨ oime red pe jeko pegua."

#: error.decode
msgid "Video can't be played because the file is corrupt."
msgstr "Video mbaetɨ ipuere oyapo metei archivo jaekvimbae rupi."

#: error.srcNotSupported
msgid "Video format or MIME type is not supported."
msgstr "Video kërai oyeapo vaerä ani tipo MIME mbaetɨ güɨropɨa."

#: error.noSource2
msgid "No video with supported format and MIME type found."
msgstr "Mbaetɨ oñeväe video güɨropɨa vae a ni tipo MIME. "

#: error.generic
msgid "Video playback aborted due to an unknown error."
msgstr "Video oyapo vae oikomegua mbaeavɨ yaikuaambae jeko pegua."

#. the #1 string is the current
#. media position, and the #2 string is the total duration. For example, when at
#. the 5 minute mark in a 6 hour long video, #1 would be "5:00" and #2 would be
#. "6:00:00", result string would be "5:00 of 6:00:00 elapsed".
#: scrubberScale.nameFormat
msgid "#1 of #2 elapsed"
msgstr "#1 pe #2 oasa"

