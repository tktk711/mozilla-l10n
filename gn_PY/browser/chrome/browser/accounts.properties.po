#. # LOCALIZATION NOTE (reconnectDescription) - %S = Email address of user's Firefox Account
#: reconnectDescription
msgid "Reconnect %S"
msgstr "Eikejey %S"

#. # LOCALIZATION NOTE (verifyDescription) - %S = Email address of user's Firefox Account
#: verifyDescription
msgid "Verify %S"
msgstr "Jehecha %S"

#. # These strings are shown in a desktop notification after the
#. # user requests we resend a verification email.
#: verificationSentTitle
msgid "Verification Sent"
msgstr "Jehechajey ñemondo"

#. # LOCALIZATION NOTE (verificationSentBody) - %S = Email address of user's Firefox Account
#: verificationSentBody
msgid "A verification link has been sent to %S."
msgstr "Joajuha jehechajey rehegua oñemondo %S-pe."

#: verificationNotSentTitle
msgid "Unable to Send Verification"
msgstr "Ndaikatúi oñemondo jehechajey"

#: verificationNotSentBody
msgid ""
"We are unable to send a verification mail at this time, please try again "
"later."
msgstr ""
"Ndaikatúi roguerahauka ñanduti veve jehechajey rehegua ko'ag̃aite, ikatúpiko "
"eha'ãjey ag̃amieve."

#. # LOCALIZATION NOTE (deviceConnectedTitle, deviceConnectedBody, deviceConnectedBody.noDeviceName)
#. # These strings are used in a notification shown when a new device joins the Sync account.
#. # deviceConnectedBody.noDeviceName is shown instead of deviceConnectedBody when we
#. # could not get the device name that joined
#: deviceConnectedTitle
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: deviceConnectedBody
msgid "This computer is now syncing with %S."
msgstr "Ko mohendaha oñembojuehehína %S ndive."

#: deviceConnectedBody.noDeviceName
msgid "This computer is now syncing with a new device."
msgstr "Ko mohendaha oñembojuehehína mba'e'oka pyahu ndive."

#. # LOCALIZATION NOTE (syncStartNotification.title, syncStartNotification.body)
#. # These strings are used in a notification shown after Sync is connected.
#: syncStartNotification.title
msgid "Sync Enabled"
msgstr "Mbojueheha myendypyre"

#. # %S is brandShortName
#: syncStartNotification.body2
msgid "%S will begin syncing momentarily."
msgstr "%S oñepyrũta iñembojuehe sapy'aiterãicha."

#. # LOCALIZATION NOTE (deviceDisconnectedNotification.title, deviceDisconnectedNotification.body)
#. # These strings are used in a notification shown after Sync was disconnected remotely.
#: deviceDisconnectedNotification.title
msgid "Sync Disconnected"
msgstr "Ñembojuehe jehejapyre"

#: deviceDisconnectedNotification.body
msgid "This computer has been successfully disconnected from Firefox Sync."
msgstr "Ko mohendaha ojei hekopete Firefox Sync-gui."

#. # LOCALIZATION NOTE (sendToAllDevices.menuitem)
#. # Displayed in the Send Tab/Page/Link to Device context menu when right clicking a tab, a page or a link.
#: sendToAllDevices.menuitem
msgid "Send to All Devices"
msgstr "Emondo opaite mba'e'oka"

#. # LOCALIZATION NOTE (tabArrivingNotification.title, tabArrivingNotificationWithDevice.title,
#. # multipleTabsArrivingNotification.title, unnamedTabsArrivingNotification2.body,
#. # unnamedTabsArrivingNotificationMultiple2.body, unnamedTabsArrivingNotificationNoDevice.body)
#. # These strings are used in a notification shown when we're opening tab(s) another device sent us to display.
#. # LOCALIZATION NOTE (tabArrivingNotification.title, tabArrivingNotificationWithDevice.title)
#. # The body for these is the URL of the tab recieved
#: tabArrivingNotification.title
msgid "Tab Received"
msgstr "Tendayke jejapyhy"

#. # LOCALIZATION NOTE (tabArrivingNotificationWithDevice.title) %S is the device name
#: tabArrivingNotificationWithDevice.title
msgid "Tab from %S"
msgstr "Tendayke %S mba'éva"

#: multipleTabsArrivingNotification.title
msgid "Tabs Received"
msgstr "Tendayke jejapyhy"

#. # LOCALIZATION NOTE (unnamedTabsArrivingNotification2.body):
#. # Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 is the number of tabs received and #2 is the device name.
#: unnamedTabsArrivingNotification2.body
msgid "#1 tab has arrived from #2;#1 tabs have arrived from #2"
msgstr "Tendayke #1 og̃uahẽma #2 guive;tendayke #1 og̃uahẽma #2 guive"

#. # LOCALIZATION NOTE (unnamedTabsArrivingNotificationMultiple2.body):
#. # Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 is the number of tabs received.
#: unnamedTabsArrivingNotificationMultiple2.body
msgid ""
"#1 tab has arrived from your connected devices;#1 tabs have arrived from "
"your connected devices"
msgstr ""
"Tendayke #1 og̃uahẽma mba'e'oka hendýva guive;#1 tendayke og̃uahẽma mba'e'oka "
"hendýva guive"

#. # LOCALIZATION NOTE (unnamedTabsArrivingNotificationNoDevice.body):
#. # Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 is the number of tabs received
#. # This version is used when we don't know any device names.
#: unnamedTabsArrivingNotificationNoDevice.body
msgid "#1 tab has arrived;#1 tabs have arrived"
msgstr "Tendayke #1 og̃uahẽma;tendayke #1 og̃uahẽma"

#~ msgid "We’ve rebuilt Sync to make it easier for everyone."
#~ msgstr "Romyatyrõjey Sync hasyve'ỹ hag̃uáicha opavavetépe."

#~ msgid "&Sign in to Sync"
#~ msgstr "Emoñepyrũ tembiapo embojuehe hag̃ua"

#~ msgid ""
#~ "A verification link has been sent to %S. Please check your email and "
#~ "click the link to begin syncing."
#~ msgstr ""
#~ "Peteĩ jehechajey joajuha ojeguerahuaka %S-pe. Ikatúpiko ehechami "
#~ "neñanduti veve ha eikutu pe juajuhápe eñepyrũ hag̃ua ñembojuehe. "

#~ msgid "%S required for sync"
#~ msgstr "%S oikotevẽ ñembojueherã"

#~ msgid ""
#~ "We've rebuilt Sync to make it easier for everyone. Please upgrade to a "
