#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Ñembojuehe ramovéva: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Ñembojuehe ramovéva: oikohína…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Mba'ete: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Jeike'ỹ"

#: connecting.label
msgid "Connecting…"
msgstr "Oikehína…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Ne mba'ete Firefox Sync pegua oñemboguéma"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Mbojevy"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 ndojokupytýi Firefox Sync apopyre ipyahuvéva ndive. Ikatúpiko, "
"embohekopyahu apopyre ipyahuvévare."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 ndojokupytýi Firefox Sync apopyre itujavéva ndive. Ikatúpiko, "
"embohekopyahu Firefox Sync ambue ne mba'e'okápe."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Eikuaave"

#: sync.update.close
msgid "Close"
msgstr "Mboty"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Ndaikatúi emboheko Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Ndaikatúi ko'ág̃a eike ñandutípe"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S ko'ág̃a ndaikatúi oike Sync-pe. Ehasejeýpa ag̃amieve?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Eha'ãjey"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Ñemboheko popeguáva"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Ndaipóri tendayke ne mohendaha ambuévape."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Sync ipyahupaite ha avei ndahasyive araka'eve guarégui. "

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Epoko ko'ápe eikuaave hag̃ua"
