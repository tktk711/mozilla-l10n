#. # String will be replaced by brandShortName.
#: saveLogin
msgid "Would you like %S to remember this login?"
msgstr "Eipotápa %S nemandu'a hag̃ua marandu jeikeha rehegua"

#: rememberButton
msgid "Remember"
msgstr "Nemandu'ápa"

#: neverButton
msgid "Never"
msgstr "Araka'eve"

#. # String is the login's hostname
#: updatePassword
msgid "Update saved password for %S?"
msgstr "Ñembohekopyahúpa oñongatúta ñe'ẽñemi %S-pe g̃uarã"

#: updatePasswordNoUser
msgid "Update saved password for this login?"
msgstr "Ñembohekopyahúpa oñongatúta ñe'ẽñemi ko jeikehápe g̃uarã"

#: updateButton
msgid "Update"
msgstr "Ñembohekopyahu"

#: dontUpdateButton
msgid "Don't update"
msgstr "Ani embohekopyahu"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "Emoneĩ mavaite puruhárape g̃uarã emoambue pe ñe'ẽñemi"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "Emoneĩ ñe'ẽñemi moambue"

#. # Strings used by PromptService.js
#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "Eipuru ñe'ẽñemi ñangarekoha nemandu'a hag̃ua ko ñe'ẽñemíre."

#: username
msgid "Username"
msgstr "Puruhára réra"

#: password
msgid "Password"
msgstr "Ñe'ẽñemi"
