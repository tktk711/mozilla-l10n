#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "છેલ્લું સીંક: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "છેલ્લું સીંક: પ્રગતિમાં છે…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "ખાતું: %S"

#: notconnected.label
msgid "Not connected"
msgstr "જોડાયેલ નથી"

#: connecting.label
msgid "Connecting…"
msgstr "જોડાઇ રહ્યા છીએ..."

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "તમારું Firefox સીંક ખાતું દૂર કરવામાં આવ્યુ છે"

#: notificationDisconnect.button
msgid "Undo"
msgstr "રદ કરો"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 એ Firefox સીંકની તાજેતરની આવૃત્તિ સાથે સુસંગત નથી. મહેરબાની કરીને તાજેતરની "
"આવૃત્તિમાં અપડેટ કરો."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 એ Firefox સીંકની જૂની આવૃત્તિ સાથે સુસંગત નથી. મહેરબાની કરીને Firefox ને તમારા "
"અન્ય કમ્પ્યૂટર(ઓ) પર અપડેટ કરો."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox સીંક"

#: sync.update.button
msgid "Learn More"
msgstr "વધુ શીખો"

#: sync.update.close
msgid "Close"
msgstr "બંધ કરો"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "સીંક સેટઅપ કરી શકતા નથી"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "કોઇ ઇન્ટરનેટ જોડાણ ઉપલબ્ધ નથી"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S સીંક સાથે જોડાઇ શક્યુ નહિ. શું તમે ફરી પ્રયાસ કરવા ઇચ્છો છો?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "ફરી પ્રયાસ કરો"

#: sync.setup.manual
msgid "Manual setup"
msgstr "સ્વયં સેટઅપ"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "તમારા અન્ય કમ્પ્યૂટરોમાં કોઇ ટૅબ નથી."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "સીંક હંમેશ કરતાં એકદમ નવું અને સરળ છે."

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "વધુ શીખવા માટે અહીં ઠપકારો"
