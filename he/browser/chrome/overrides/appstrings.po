#: malformedURI
msgid "הכתובת אינה חוקית ולא ניתנת לטעינה."
msgstr ""

#: fileNotFound
msgid "Firefox לא מצא את הקובץ ב־%S."
msgstr ""

#: dnsNotFound
msgid "Firefox לא מצא את השרת בכתובת %S."
msgstr ""

#: protocolNotFound
msgid ""
"Firefox אינו יודע כיצד לפתוח כתובת זו, מכיוון שהפרוטוקול (%S) לא מקושר עם אף "
"תוכנה."
msgstr ""

#: connectionFailure
msgid "Firefox לא יכול ליצור קשר עם השרת ב־%S."
msgstr ""

#: netInterrupt
msgid "החיבור אל %S נקטע תוך כדי שהדף נטען."
msgstr ""

#: netTimeout
msgid "לשרת ב־%S לוקח יותר מדי זמן להגיב."
msgstr ""

#: redirectLoop
msgid ""
"Firefox זיהה שהשרת מכוון מחדש את הבקשה עבור כתובת זו בצורה שלא תושלם לעולם."
msgstr ""

#. ## LOCALIZATION NOTE (confirmRepostPrompt): In this item, don't translate "%S"
#: confirmRepostPrompt
msgid ""
"כדי להציג את דף זה, %S צריך לשלוח מידע שיבצע שוב כל פעולה (כגון חיפוש או "
"אישור הזמנה) שבוצעה מוקדם יותר."
msgstr ""

#: resendButton.label
msgid "שלח שוב"
msgstr ""

#: unknownSocketType
msgid "Firefox אינו יודע כיצר לתקשר עם השרת."
msgstr ""

#: netReset
msgid "החיבור לשרת אותחל בזמן שהדף נטען."
msgstr ""

#: notCached
msgid "מסמך זה לא זמין יותר."
msgstr ""

#: netOffline
msgid "Firefox נמצא כעת במצב לא־מקוון ואינו יכול לגלוש באינטרנט."
msgstr ""

#: isprinting
msgid "לא ניתן לשנות את המסמך בזמן הדפסה או בזמן תצוגה לפני הדפסה."
msgstr ""

#: deniedPortAccess
msgid ""
"כתובת זו משתמשת בשער רשת המיעד בדרך כלל לשימוש אחר מאשר גלישה באינטרנט. "
"הדפדפן ביטל את הבקשה עבור ההגנה והאבטחה שלך."
msgstr ""

#: proxyResolveFailure
msgid "Firefox מוגדר להשתמש בשרת מתווך שלא ניתן למצוא אותו."
msgstr ""

#: proxyConnectFailure
msgid "Firefox מוגדר להשתמש בשרת מתווך המסרב לחיבורים."
msgstr ""

#: contentEncodingError
msgid ""
"לא ניתן להציג את הדף שאתה מנסה לראות מכיוון שהוא משתמש בסוג דחיסה לא חוקי או "
"לא ידוע. אנא צור קשר עם בעלי האתר כדי ליידע אותם על בעיה זו."
msgstr ""

#: unsafeContentType
msgid ""
"הדף בו הנך מנסה לצפות בו לא ניתן להצגה מאחר שהוא מכיל סוג קובץ שעלול להיות "
"מסוכן לפתיחה. בבקשה צור קשר עם בעלי האתר כדי ליידע אותם על בעיה זו."
msgstr ""

#: externalProtocolTitle
msgid "בקשה לפרוטוקול חיצוני"
msgstr ""

#: externalProtocolPrompt
msgid ""
"נדרשת הפעלה של יישום חיצוני כדי לטפל ב-%1$S: קישורים.\n"
"\n"
"\n"
"הקישור הנדרש:\n"
"\n"
"%2$S\n"
"\n"
"יישום: %3$S\n"
"\n"
"\n"
"אם לא ציפית לבקשה זו יתכן שזהו ניסיון לנצל נקודת תורפה בתוכנה ההיא. בטל "
"דרישה זו אלא אם כן אתה בטוח שאיננה זדונית.\n"
msgstr ""

#. #LOCALIZATION NOTE (externalProtocolUnknown): The following string is shown if the application name can't be determined
#: externalProtocolUnknown
msgid "<לא ידוע>"
msgstr ""

#: externalProtocolChkMsg
msgid "זכור את הבחירה שלי לכל הקישורים מסוג זה"
msgstr ""

#: externalProtocolLaunchBtn
msgid "הפעל יישום"
msgstr ""

#: malwareBlocked
msgid ""
"האתר בכתובת %S דווח בעבר בתור אתר תקיפה ולכן נחסם בהתאם להגדרות האבטחה שלך."
msgstr ""

#: phishingBlocked
msgid ""
"האתר בכתובת %S דווח בתור אתר הונאה שנועד כדי להפיל משתמשים למלכודות שיגרמו "
"להם לשתף את הפרטים האישיים שלהם או מידע פיננסי."
msgstr ""

#: cspFrameAncestorBlocked
msgid "מדיניות אבטחת התוכן בדף זהה מונעת ממנו להיות מוטבע בצורה זו."
msgstr ""

#: corruptedContentError
msgid ""
"הדף שאתה מנסה לצפות בו אינו יכול להיות מוצג בגלל שהתגלתה שגיאה בהעברת "
"הנתונים."
msgstr ""

#: remoteXUL
msgid ""
"עמוד זה משתמש בטכנולוגיה לא נתמכת שאינה זמינה יותר ב־Firefox כבררת מחדל."
msgstr ""
