#: malformedURI
msgid "הכתובת אינה חוקית ולא ניתנת לטעינה."
msgstr ""

#: fileNotFound
msgid "לא ניתן למצוא את הקובץ %S. אנא בדוק את המיקום ונסה שוב."
msgstr ""

#: dnsNotFound
msgid "לא ניתן למצוא את %S. אנא בדוק את השם ונסה שוב."
msgstr ""

#: protocolNotFound
msgid "%S אינו פרוטוקול רשום."
msgstr ""

#: connectionFailure
msgid "הקשר נדחה בעת ניסיון להתחבר עם %S."
msgstr ""

#: netInterrupt
msgid "החיבור אל %S נותק באופן לא צפוי. יתכן וחלק מן הנתונים הועברו."
msgstr ""

#: netTimeout
msgid "תם הזמן המוקצב בעת ניסיון להתחבר עם %S."
msgstr ""

#: redirectLoop
msgid ""
"נעבר גבול ההכוונה מחדש עבור כתובת זו. לא ניתן לטעון את הדף המבוקש. יתכן "
"והדבר נגרם מעוגיות שנחסמות."
msgstr ""

#: confirmRepostPrompt
msgid ""
"כדי להציג את דף זה, %S צריך לשלוח מידע שיבצע שוב כל פעולה (כגון חיפוש או "
"אישור הזמנה) שבוצעה מוקדם יותר."
msgstr ""

#: resendButton.label
msgid "שלח שוב"
msgstr ""

#: unknownSocketType
msgid ""
"לא ניתן להציג מסמך זה אלא אם תתקין את מנהל האבטחה האישי (PSM). הורד והתקן את "
"PSM ונסה שוב, או צור קשר עם מנהל המערכת שלך."
msgstr ""

#: netReset
msgid "המסמך אינו מכיל נתונים."
msgstr ""

#: notCached
msgid "מסמך זה לא זמין יותר."
msgstr ""

#: netOffline
msgid ""
"לא ניתן להציג מסמך זה במצב לא־מקוון. כדי לעבור למצב מקוון, בטל את הסימון של "
"\"עבוד לא־מקוון\" מהתפריט קובץ."
msgstr ""

#: isprinting
msgid "לא ניתן לשנות את המסמך בזמן הדפסה או בזמן תצוגה לפני הדפסה."
msgstr ""

#: deniedPortAccess
msgid "הגישה למספר השער שניתן נחסמה מסיבות אבטחה."
msgstr ""

#: proxyResolveFailure
msgid ""
"לא ניתן למצוא את השרת המתווך שהגדרת. אנא בדוק את הגדרות השרת המתווך שלך ונסה "
"שוב."
msgstr ""

#: proxyConnectFailure
msgid ""
"ההתחברות נדחתה בניסיון להתחבר לשרת המתווך שהגדרת. אנא בדוק את הגדרות השרת "
"המתווך שלך ונסה שוב."
msgstr ""

#: contentEncodingError
msgid ""
"לא ניתן להציג את הדף שאתה מנסה לראות מכיוון שהוא משתמש בסוג דחיסה לא חוקי או "
"לא ידוע. אנא צור קשר עם בעלי האתר כדי ליידע אותם על בעיה זו."
msgstr ""

#: unsafeContentType
msgid ""
"הדף בו הנך מנסה לצפות בו לא ניתן להצגה מאחר שהוא מכיל סוג קובץ שעלול להיות "
"מסוכן לפתיחה. בבקשה צור קשר עם בעלי האתר כדי ליידע אותם על בעיה זו."
msgstr ""

#: externalProtocolTitle
msgid "בקשת פרוטוקול חיצוני"
msgstr ""

#: externalProtocolPrompt
msgid ""
"נדרשת הפעלה של יישום חיצוני כדי לטפל ב-%1$S: קישורים.\n"
"\n"
"\n"
"הקישור הנדרש:\n"
"\n"
"%2$S\n"
"\n"
"יישום: %3$S\n"
"\n"
"\n"
"אם לא ציפית לבקשה זו יתכן שזהו ניסיון לנצל נקודת תורפה בתוכנה ההיא. בטל "
"דרישה זו אלא אם כן אתה בטוח שאיננה זדונית.\n"
msgstr ""

#. #LOCALIZATION NOTE (externalProtocolUnknown): The following string is shown if the application name can't be determined
#: externalProtocolUnknown
msgid "<לא מוכר>"
msgstr ""

#: externalProtocolChkMsg
msgid "זכור את הבחירה שלי לכל הקישורים מסוג זה"
msgstr ""

#: externalProtocolLaunchBtn
msgid "הפעל יישום"
msgstr ""

#: malwareBlocked
msgid ""
"האתר בכתובת %S דווח בעבר בתור אתר תקיפה ולכן נחסם בהתאם להגדרות האבטחה שלך."
msgstr ""

#: phishingBlocked
msgid ""
"האתר בכתובת %S דווח בתור אתר הונאה שנועד כדי להפיל משתמשים למלכודות שיגרמו "
"להם לשתף את הפרטים האישיים שלהם או מידע פיננסי."
msgstr ""

#: cspFrameAncestorBlocked
msgid "מדיניות אבטחת התוכן בדף זהה מונעת ממנו להיות מוטבע בצורה זו."
msgstr ""

#: corruptedContentError
msgid ""
"הדף שאתה מנסה לצפות בו אינו יכול להיות מוצג בגלל שהתגלתה שגיאה בהעברת "
"הנתונים."
msgstr ""

#: remoteXUL
msgid "עמוד זה משתמש בטכנולוגיה לא נתמכת שאינה זמינה יותר כבררת מחדל."
msgstr ""
