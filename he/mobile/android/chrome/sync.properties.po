#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "סנכרון אחרון: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "סנכרון אחרון: בביצוע…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "חשבון: %S"

#: notconnected.label
msgid "Not connected"
msgstr "לא מחובר"

#: connecting.label
msgid "Connecting…"
msgstr "מתחבר…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "חשבונך ב־Firefox Sync נמחק"

#: notificationDisconnect.button
msgid "Undo"
msgstr "ביטול"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 אינה תואמת עם הגרסה האחרונה של Firefox Sync. בבקשה לעדכן לגרסה החדשה "
"ביותר."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 אינו תואם עם גרסאות ישנות יותר של Firefox Sync. בבקשה לעדכן את Firefox "
"על המחשב או על המחשבים האחרים שלך."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "מידע נוסף"

#: sync.update.close
msgid "Close"
msgstr "סגירה"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "לא ניתן להגדיר את Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "לא קיים חיבור לרשת"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S לא הצליח להתחבר ל־Sync. האם לנסות שוב?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "נסה שוב"

#: sync.setup.manual
msgid "Manual setup"
msgstr "הגדרה ידנית"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "אין לשוניות ממחשבים אחרים."
