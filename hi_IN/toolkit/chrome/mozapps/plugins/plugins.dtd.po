#. LOCALIZATION NOTE (reloadPlugin.post): include a starting space as needed
#: reloadPlugin.pre
msgctxt "reloadPlugin.pre"
msgid ""
msgstr ""

#: reloadPlugin.middle
msgid "Reload the page"
msgstr "पृष्ठ फिर लोड करें"

#: reloadPlugin.post
msgid " to try again."
msgstr " फिर आजमाने के लिए."

#. This and the other report.* strings should be as short as possible, ideally 2-3 words.
#: report.please
msgid "Send crash report"
msgstr "क्रैश रिपोर्ट भेजें"

#: report.submitting
msgid "Sending report…"
msgstr "रिपोर्ट भेज रहा है…"

#: report.submitted
msgid "Crash report sent."
msgstr "क्रैश रिपोर्ट भेजा गया."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "क्रैश रिपोर्टिंग निष्क्रिय किया गया."

#: report.failed
msgid "Submission failed."
msgstr "सुपुर्दगी विफल."

#: report.unavailable
msgid "No report available."
msgstr "कोई रिपोर्ट उपलब्ध नहीं."

#: report.comment
msgid "Add a comment (comments are publicly visible)"
msgstr "Add a comment (comments are publicly visible)"

#: report.pageURL
msgid "Include the page's URL"
msgstr "पृष्ठ यूआरएल सम्मिलित करें"

#: plugin.file
msgid "File"
msgstr "फ़ाइल"

#: plugin.mimeTypes
msgid "MIME Types"
msgstr "माइम क़िस्म"

