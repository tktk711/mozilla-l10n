msgstr "&Ne uvozi ništa"

#: importFromSafari.label importFromSafari.accesskey
msgid "&Safari"
msgstr "&Safari"

#: importFromCanary.label importFromCanary.accesskey
msgid "Chrome Ca&nary"
msgstr "Chrome Ca&nary"

#: importFromChrome.label importFromChrome.accesskey
msgid "&Chrome"
msgstr "&Chrome"

#: importFromChromium.label importFromChromium.accesskey
msgid "Chromi&um"
