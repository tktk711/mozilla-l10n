#: loadError.label
msgid "Problem loading page"
msgstr "Problem pri učitavanju stranice"

#: retry.label
msgid "Try Again"
msgstr "Pokušaj ponovno"

#: returnToPreviousPage.label
msgid "Go Back"
msgstr "Idi natrag"

#: advanced.label
