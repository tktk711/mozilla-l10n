msgctxt "error_close"
msgid "Close"
msgstr "Zatvori"

#. # LOCALIZATION NOTE (error_version_info): "{{version}}" and "{{build}}" will be
#. # replaced by the PDF.JS version and build ID.
#: error_version_info
msgid "PDF.js v{{version}} (build: {{build}})"
msgstr "PDF.js v{{version}} (build: {{build}})"

#. # LOCALIZATION NOTE (error_message): "{{message}}" will be replaced by an
#. # english string describing the error.
#: error_message
msgid "Message: {{message}}"
msgstr "Poruka: {{message}}"

#. # LOCALIZATION NOTE (error_stack): "{{stack}}" will be replaced with a stack
#. # trace.
#: error_stack
msgid "Stack: {{stack}}"
msgstr "Stog: {{stack}}"

#. # LOCALIZATION NOTE (error_file): "{{file}}" will be replaced with a filename
#: error_file
msgid "File: {{file}}"
msgstr "Datoteka: {{file}}"

#. # LOCALIZATION NOTE (error_line): "{{line}}" will be replaced with a line number
#: error_line
msgid "Line: {{line}}"
msgstr "Redak: {{line}}"

#: rendering_error
msgid "An error occurred while rendering the page."
msgstr "Došlo je do greške prilikom iscrtavanja stranice."

#. # Predefined zoom values
#: page_scale_width
msgid "Page Width"
msgstr "Širina stranice"

#: page_scale_fit
msgid "Page Fit"
msgstr "Pristajanje stranici"

#: page_scale_auto
msgid "Automatic Zoom"
msgstr "Automatsko uvećanje"

#: page_scale_actual
msgid "Actual Size"
msgstr "Prava veličina"

#. # LOCALIZATION NOTE (page_scale_percent): "{{scale}}" will be replaced by a
#. # numerical scale value.
#: page_scale_percent
msgid "{{scale}}%"
msgstr "{{scale}}%"

#. # Loading indicator messages
#: loading_error_indicator
msgid "Error"
msgstr "Greška"

#: loading_error
msgid "An error occurred while loading the PDF."
msgstr "Došlo je do greške pri učitavanju PDF-a."

#: invalid_file_error
msgid "Invalid or corrupted PDF file."
msgstr "Kriva ili oštećena PDF datoteka."

#: missing_file_error
msgid "Missing PDF file."
msgstr "Nedostaje PDF datoteka."

#: unexpected_response_error
msgid "Unexpected server response."
msgstr "Neočekivani odgovor poslužitelja."

#. # LOCALIZATION NOTE (text_annotation_type.alt): This is used as a tooltip.
#. # "{{type}}" will be replaced with an annotation type from a list defined in
#. # the PDF spec (32000-1:2008 Table 169 – Annotation types).
#. # Some common types are e.g.: "Check", "Text", "Comment", "Note"
#: text_annotation_type.alt
msgid "[{{type}} Annotation]"
msgstr "[{{type}} Bilješka]"

#: password_label
msgid "Enter the password to open this PDF file."
msgstr "Upišite lozinku da biste otvorili ovu PDF datoteku."

#: password_invalid
msgid "Invalid password. Please try again."
msgstr "Neispravna lozinka. Pokušajte ponovo."

#: password_ok
msgid "OK"
msgstr "U redu"

#: password_cancel
msgid "Cancel"
msgstr "Odustani"

#: printing_not_supported
msgid "Warning: Printing is not fully supported by this browser."
msgstr "Upozorenje: Ispisivanje nije potpuno podržano u ovom pregledniku."

#: printing_not_ready
msgid "Warning: The PDF is not fully loaded for printing."
msgstr "Upozorenje: PDF nije u potpunosti učitan za ispis."

#: web_fonts_disabled
msgid "Web fonts are disabled: unable to use embedded PDF fonts."
msgstr ""
"Web fontovi su onemogućeni: nije moguće koristiti umetnute PDF fontove."

#: document_colors_not_allowed
msgid ""
"PDF documents are not allowed to use their own colors: “Allow pages to "
"choose their own colors” is deactivated in the browser."
msgstr ""
"PDF dokumenti nemaju dopuštene koristiti vlastite boje: opcija 'Dopusti "
"stranicama da koriste vlastite boje' je deaktivirana."

