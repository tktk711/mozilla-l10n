#. # args: host
#: accessingServerFailedError.text
msgid "Cannot access server %1$S!"
msgstr "Nije moguće pristupiti poslužitelju %1$S!"

#: loginFailed.text
msgid "Login failed or invalid session Id."
msgstr "Prijava nije uspjela ili nevažeći ID sesije."

#: accessDenied.text
msgid "The user is denied access."
msgstr "Korisniku nije odobren pristup."

#. # args: host
#: noHttpsConfirmation.text
msgid ""
"Insecure login on %1$S!\n"
"Continue?"
msgstr ""
"Nesigurna prijava na %1$S!\n"
"Nastaviti?"

#: noHttpsConfirmation.check.text
msgid "Don't ask again."
msgstr "Ne pitaj ponovno."

#: noHttpsConfirmation.label
msgid "Warning!"
msgstr "Upozorenje!"

#. # args: host, prodId, serverVersion, wcapVersion
#: insufficientWcapVersionConfirmation.text
msgid ""
"Server %1$S (%2$S, v%3$S, WCAP v%4$S) doesn't support a sufficient WCAP "
"version! The required version is at least 3.0.0.\n"
"Continue?"
msgstr ""
"Poslužitelj %1$S (%2$S, v%3$S, WCAP v%4$S) ne podržava potrebnu WCAP "
"inačicu! Potrebna je najmanje 3.0.0 inačica.\n"
"Nastaviti?"

#: insufficientWcapVersionConfirmation.label
msgid "Insufficient WCAP version!"
msgstr "Nepodržana WCAP inačica!"

#: loginDialog.label
msgid "Calendar Server Password Required"
msgstr "Potrebna lozinka za poslužitelj kalendara"

#: privateItem.title.text
msgid "Private"
msgstr "Privatno"

#: confidentialItem.title.text
msgid "Confidential"
msgstr "Povjerljivo"

#: busyItem.title.text
msgid "Busy"
msgstr "Zauzet"
