#: imipHtml.newLocation
msgid "New Location: %1$S"
msgstr "Nova lokacija: %1$S"

#: imipAddToCalendar.label
msgid "Add To Calendar"
msgstr "Dodaj u kalendar"

#: imipAddedItemToCal
msgid "Event Added to Calendar"
msgstr "Događaj dodan u kalendar"

#: imipCanceledItem
msgid "Event has been deleted"
msgstr "Događaj je izbrisan"

#: imipUpdatedItem
msgid "Event has been updated"
msgstr "Događaj je ažuriran"

#: imipBarCancelText
msgid "This message contains an event cancellation."
msgstr "Poruka sadrži otkazivanje događaja."

#: imipBarRefreshText
msgid "This message asks for an event update."
msgstr "Poruka sadrži upit za ažuriranje događaja."

#: imipBarPublishText
msgid "This message contains an event."
msgstr "Poruka sadrži događaj."

#: imipBarRequestText
msgid "This message contains an invitation to an event."
msgstr "Poruka sadrži pozivnicu na događaj."

#: imipBarUpdateText
msgid "This message contains an update to an existing event."
msgstr "Poruka sadrži ažuriranje postojećeg događaja."

#: imipBarAlreadyProcessedText
msgid "This message contains an event that has already been processed."
msgstr "Poruka sadrži događaj koji je već obrađen."

#: imipBarProcessedNeedsAction
msgid "This message contains an event that you have not yet responded to."
msgstr "Ova poruka sadrži događaj na koji još niste odgovorili."

#: imipBarReplyText
msgid "This message contains a reply to an invitation."
msgstr "Poruka sadrži odgovor na pozivnicu."

#: imipBarUnsupportedText
msgid ""
"This message contains an event that this version of Lightning cannot process."
msgstr ""
"Poruka sadrži događaj koji ova inačica Lightning kalendara ne može obraditi."

#: imipBarProcessingFailed
msgid "Processing message failed. Status: %1$S."
msgstr "Obrada poruke neuspješna. Status: %1$S."

#: imipBarNotWritable
msgid ""
"No writable calendars are configured for invitations, please check the "
"calendar properties."
msgstr ""
"Nije postavljen niti jedan kalendar za pozivnice sa mogućnosti pisanja, "
"provjerite postavke kalendara."

#: imipSendMail.title
msgid "E-Mail Notification"
msgstr "Obavjest preko e-pošte"

#: imipSendMail.text
msgid "Would you like to send out notification E-Mail now?"
msgstr "Želite li sada poslati obavjest preko e-pošte?"

#: imipSendMail.Outlook2000CompatMode.text
msgid "Support Outlook 2000 and Outlook 2002/XP"
msgstr "Podrži Outlook 2000 i Outlook 2002/XP"

#: imipNoIdentity
msgid "None"
msgstr "Nijedan"

#: imipNoCalendarAvailable
msgid "There are no writable calendars available."
msgstr "Nisu dostupni kalendari u koje se može pisati."

#: itipReplySubject
msgid "Event Invitation Reply: %1$S"
msgstr "Odgovor na pozivnicu na događaj: %1$S"

#: itipReplyBodyAccept
msgid "%1$S has accepted your event invitation."
msgstr "%1$S je prihvatio vaš poziv na događaj."

#: itipReplyBodyDecline
msgid "%1$S has declined your event invitation."
msgstr "%1$S je odbio vaš poziv na događaj."

#: itipReplySubjectAccept
msgid "Event Invitation Reply (Accepted): %1$S"
msgstr "Odgovor na pozivnicu na događaj (Prihvaćen): %1$S"

#: itipReplySubjectDecline
msgid "Event Invitation Reply (Declined): %1$S"
msgstr "Odgovor na pozivnicu na događaj (Odbijen): %1$S"

#: itipReplySubjectTentative
msgid "Event Invitation Reply (Tentative): %1$S"
msgstr "Odgovor na pozivnicu na događaj (Privremen): %1$S"

#: itipRequestSubject
msgid "Event Invitation: %1$S"
msgstr "Pozivnica na događaj: %1$S"

#: itipRequestUpdatedSubject
msgid "Updated Event Invitation: %1$S"
msgstr "Ažurirana pozivnica na događaj: %1$S"

#: itipRequestBody
msgid "%1$S has invited you to %2$S"
msgstr "%1$S vas je pozvao na %2$S"

#: itipCancelSubject
msgid "Event Canceled: %1$S"
msgstr "Događaj otkazan: %1$S"

#: itipCancelBody
msgid "%1$S has canceled this event: « %2$S »"
msgstr "%1$S je otkazao ovaj događaj: « %2$S »"

#: confirmProcessInvitation
msgid ""
"You have recently deleted this item, are you sure you want to process this "
"invitation?"
msgstr ""
"Nedavno ste obrisali ovu stavku, jeste li sigurni da želite obraditi ovaj "
"poziv?"

#: confirmProcessInvitationTitle
msgid "Process Invitation?"
msgstr "Obradi poziv?"

#: invitationsLink.label
msgid "Invitations: %1$S"
msgstr "Pozivnice: %1$S"

