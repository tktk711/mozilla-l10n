#: 1
msgid "Parsing an XSLT stylesheet failed."
msgstr "Obrada XSLT liste stilova nije uspjela."

#: 2
msgid "Parsing an XPath expression failed."
msgstr "Obrada XPath izraza nije uspjela."

# Ignore.
#: 3
msgctxt "3"
msgid ""
msgstr " "

#: 4
msgid "XSLT transformation failed."
msgstr "XSLT pretvorba nije uspjela."

#: 5
msgid "Invalid XSLT/XPath function."
msgstr "Kriva XLST/XPath funkcija."

#: 6
msgid "XSLT Stylesheet (possibly) contains a recursion."
msgstr "XSLT lista stilova (možda) sadrži rekurziju."

#: 7
msgid "Attribute value illegal in XSLT 1.0."
msgstr "Nedopuštena vrijednost atributa u XSLT 1.0."

#: 8
msgid "An XPath expression was expected to return a NodeSet."
msgstr "XPath izraz je trebao vratiti NodeSet."

#: 9
msgid "XSLT transformation was terminated by <xsl:message>."
msgstr "XSLT pretvorba je prekinuta s <xsl:message>."

#: 10
msgid "A network error occurred loading an XSLT stylesheet:"
msgstr "Došlo je do mrežne greške pri učitavanju XSLT liste stilova:"

#: 11
msgid "An XSLT stylesheet does not have an XML mimetype:"
msgstr "XSLT lista stilova nema XML mimetype:"

#: 12
msgid "An XSLT stylesheet directly or indirectly imports or includes itself:"
msgstr "XSLT lista stilova izravno ili neizravno uvozi ili uvrštava samu sebe:"

#: 13
msgid "An XPath function was called with the wrong number of arguments."
msgstr "XPath funkcija je bila pozvana s krivim brojem argumenata."

#: 14
msgid "An unknown XPath extension function was called."
msgstr "Pozvano je nepoznato XPath proširenje."

#: 15
msgid "XPath parse failure: ')' expected:"
msgstr "Neuspješna obrada XPatha: ')' očekivan:"

#: 16
msgid "XPath parse failure: invalid axis:"
msgstr "Neuspješna obrada XPatha: kriva os:"

#: 17
msgid "XPath parse failure: Name or Nodetype test expected:"
msgstr "Neuspješna obrada XPatha: Očekivan naziv ili nodetype test:"

#: 18
msgid "XPath parse failure: ']' expected:"
msgstr "Neuspješna obrada XPatha: ']' očekivan:"

#: 19
msgid "XPath parse failure: invalid variable name:"
msgstr "Neuspješna obrada XPatha: krivi naziv varijable:"

#: 20
msgid "XPath parse failure: unexpected end of expression:"
msgstr "Neuspješna obrada XPatha: neočekivani kraj izraza:"

#: 21
msgid "XPath parse failure: operator expected:"
msgstr "Neuspješna obrada XPatha: očekivan operator:"

#: 22
msgid "XPath parse failure: unclosed literal:"
msgstr "Neuspješna obrada XPatha: nezatvoren doslijed:"

#: 23
msgid "XPath parse failure: ':' unexpected:"
msgstr "Neuspješna obrada XPatha: ':' očekivan:"

#: 24
msgid "XPath parse failure: '!' unexpected, negation is not():"
msgstr "Neuspješna obrada XPatha: očekivan '!', negacija je not():"

#: 25
msgid "XPath parse failure: illegal character found:"
msgstr "Neuspješna obrada XPatha: pronađen je krivi znak:"

#: 26
msgid "XPath parse failure: binary operator expected:"
msgstr "Neuspješna obrada XPatha: očekivan binarni operator:"

#: 27
msgid "An XSLT stylesheet load was blocked for security reasons."
msgstr "Učitavanje XSLT liste stilove je blokirano zbog sigurnosnih razloga."

#: 28
msgid "Evaluating an invalid expression."
msgstr "Ocjenjivanje neispravnog izraza."

#: 29
msgid "Unbalanced curly brace."
msgstr "Nezatvorena vitičasta zagrada."

#: 30
msgid "Creating an element with an invalid QName."
msgstr "Izrada elementa s neispravnim QName."

#: 31
msgid "Variable binding shadows variable binding within the same template."
msgstr ""
"Varijabilno povezivanje prekriva varijabilno povezivanje unutar istog "
"predloška."

#: 32
msgid "Call to the key function not allowed."
msgstr ""

#: LoadingError
msgid "Error loading stylesheet: %S"
msgstr "Greška pri učitavanju liste stilova: %S"

#: TransformError
msgid "Error during XSLT transformation: %S"
msgstr "Greška tijekom XSLT transformacije: %S"
