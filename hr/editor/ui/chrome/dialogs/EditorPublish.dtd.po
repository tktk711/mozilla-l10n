#. Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "Objavi stranicu"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "Postavke objavljivanja"

#: publishTab.label
msgctxt "publishTab.label"
msgid "Publish"
msgstr "Objavi"

#: settingsTab.label
msgid "Settings"
msgstr "Postavke"

#: publishButton.label
msgctxt "publishButton.label"
msgid "Publish"
msgstr "Objavi"

#. Publish Tab Panel
#: siteList.label siteList.accesskey
msgctxt "siteList.label siteList.accesskey"
msgid "Sit&e Name:"
msgstr "Naziv stranic&e:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "Odaberite stranicu na koju želite objaviti"

#: newSiteButton.label newSiteButton.accesskey
msgid "&New Site"
msgstr "&Nova stranica"

#: docDirList.label docDirList.accesskey
msgid "&Site subdirectory for this page:"
msgstr "Poddirektorij &stranice za ovu stranicu:"

#: docDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory for this page"
msgstr "Odaberite ili upišite naziv vanjskog poddirektorija za ovu stranicu"

#: publishImgCheckbox.label publishImgCheckbox.accesskey
msgid "Include images and &other files"
msgstr "&Obuhvati slike i druge datoteke"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr "Objavi slike i druge datoteke povezane s ovom stranicom"

#: sameLocationRadio.label sameLocationRadio.accesskey
msgid "&Use same location as page"
msgstr "&Koristi istu lokaciju kao i stranica"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "Objavi datoteke na istoj lokaciji kao i stranicu"

#: useSubdirRadio.label useSubdirRadio.accesskey
msgid "Use this site sub&directory:"
msgstr "Koristi po&ddirektorij ove stranice:"

#: useSubdirRadio.tooltip
msgid "Publish files to the selected remote subdirectory"
msgstr "Objavi datoteke u odabrani vanjski poddirektorij"

#: otherDirList.tooltip
msgid ""
"Choose or enter name of remote subdirectory where files will be published"
msgstr ""
"Odaberite ili upišite naziv vanjskog poddirektorija gdje će datoteke biti "
"objavljene"

#: pageTitle.label pageTitle.accesskey
msgid "Page &Title:"
msgstr "Naslov s&tranice:"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr ""
"Upišite naslov kako bi stranica bila prepoznatljiva u prozoru i zabilješkama"

#: pageTitleExample.label
msgid "e.g.: \"My Web Page\""
msgstr "npr.,: \"Moja Web Stranica\""

#: filename.label filename.accesskey
msgid "&Filename:"
msgstr "N&aziv datoteke:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr "Upišite naziv za ovu datoteku, uključujući '.html' za web stranicu"

#: filenameExample.label
msgid "e.g,: \"mypage.html\""
msgstr "npr.,: \"mojastranica.html\""

#: setDefaultButton.label setDefaultButton.accesskey
msgid "Set as &Default"
msgstr "&Postavi kao zadano"

#: removeButton.label removeButton.accesskey
msgid "&Remove Site"
msgstr "&Ukloni stranicu"

#. Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "Objavljivanje stranica"

#: serverInfo.label
msgid "Server Information"
msgstr "Informacije o poslužitelju"

#: loginInfo.label
msgid "Login Information"
msgstr "Informacije za prijavu"

#: siteName.label siteName.accesskey
msgctxt "siteName.label siteName.accesskey"
msgid "Sit&e Name:"
msgstr "Naziv stranic&e:"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr ""
"Nadimak koji označava ovu stranicu prilikom objavljivanja (npr.: "
"'MojaStranica')"

#: siteUrl.label siteUrl.accesskey
msgid "Publishing &address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr ""
"&Adresa za objavljivanje (npr.: 'ftp://ftp.mojisp.com/mojekorisnickoime'):"

#: siteUrl.tooltip
msgid ""
"The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr ""
"FTP:// ili HTTP:// adresa dobivena od vašeg ISP-a ili pružatelja usluge web "
"hostinga"

#: browseUrl.label browseUrl.accesskey
msgid ""
"H&TTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr ""
"H&TTP adresa vaše početne stranice (npr.: 'http://www.mojisp.com/"
"mojekorisnickoime'):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr ""
"HTTP:// adresa direktorija vaše početne stranice (ne upisujte nazive "
"datoteka)"

#: username.label username.accesskey
msgid "&User name:"
msgstr "&Korisničko ime:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr ""
"Korisničko ime koje koristite kako bi se prijavili na svoj ISP ili uslugu "
"web hostinga"

#: password.label password.accesskey
msgid "Pass&word:"
msgstr "&Lozinka:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "Lozinka koja je povezana s vašim korisničkim imenom"

#: savePassword.label savePassword.accesskey
msgid "&Save Password"
msgstr "&Spremi lozinku"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr ""
"Odaberite ovo kako bi sigurno spremili vašu lozinku koristeći upravitelja "
"lozinki"
