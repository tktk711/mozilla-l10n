#. #
#. # The following are used by the outlook express import code to display status/error 
#. # and informational messages
#. #
#. # Short name of import module
#. ## @name TEXTIMPORT_NAME
#. ## @loc None
#: 2000
msgid "Text file (LDIF, .tab, .csv, .txt)"
msgstr "Tekst datoteka (LDIF, .tab, .csv, .txt)"

#. # Description of import module
#. ## @name TEXTIMPORT_DESCRIPTION
#. ## @loc None
#: 2001
msgid ""
"Import an address book from a text file, including:  LDIF (.ldif, .ldi), tab-"
"delimited (.tab, .txt) or comma-separated (.csv) formats."
msgstr ""
"Uvezite adresar iz tekst datoteke, uključujući formate:  LDIF (.ldif, .ldi), "
"odijeljeno tabulatorom (.tab, .txt) ili odijeljeno točkom-zarez (.csv)."

#. # Description of import module
#. ## @name TEXTIMPORT_ADDRESS_NAME
#. ## @loc None
#: 2002
msgid "Text Address Book"
msgstr "Tekst adresar"

#. # Description
#. ## @name TEXTIMPORT_ADDRESS_SUCCESS
#. ## @loc None
#: 2003
msgid "Imported address book %S"
msgstr "Uvezen adresar %S"

#. # Error message
#. ## @name TEXTIMPORT_ADDRESS_BADPARAM
#. ## @loc None
#: 2004
msgid "Bad parameter passed to import address book."
msgstr "Loš parametar je proslijeđen za uvoz adresara."

#. # Error message
#. ## @name TEXTIMPORT_ADDRESS_BADSOURCEFILE
#. ## @loc None
#: 2005
msgid "Error accessing file for address book %S."
msgstr "Greška prilikom pristupanja datoteci za adresar %S."

#. # Error message
#. ## @name TEXTIMPORT_ADDRESS_CONVERTERROR
#. ## @loc None
#: 2006
msgid ""
"Error importing address book %S, all addresses may not have been imported."
msgstr "Greška prilikom uvoza adresara %S, možda nisu uvezene sve adrese."
