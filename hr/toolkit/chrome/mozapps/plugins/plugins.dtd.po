#. LOCALIZATION NOTE (reloadPlugin.post): include a starting space as needed
#: reloadPlugin.pre
msgctxt "reloadPlugin.pre"
msgid ""
msgstr " "

#: reloadPlugin.middle
msgid "Reload the page"
msgstr "Osvježi stranicu"

#: reloadPlugin.post
msgid " to try again."
msgstr " za ponovni pokušaj."

#. This and the other report.* strings should be as short as possible, ideally 2-3 words.
#: report.please
msgid "Send crash report"
msgstr "Pošalji izvješće o rušenju"

#: report.submitting
msgid "Sending report…"
msgstr "Slanje izvješća…"

#: report.submitted
msgid "Crash report sent."
msgstr "Izvještaj poslan."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "Prijavljivanje rušenja je onemogućeno."

#: report.failed
msgid "Submission failed."
msgstr "Slanje nije uspjelo."

#: report.unavailable
msgid "No report available."
msgstr "Nema dostupnog izvješća."

#: report.comment
msgid "Add a comment (comments are publicly visible)"
msgstr "Dodajte komentar (komentari su javno vidljivi)"

#: report.pageURL
msgid "Include the page's URL"
msgstr "Uključite URL stranice"

#: plugin.file
msgid "File"
msgstr "Datoteka"

#: plugin.mimeTypes
msgid "MIME Types"
msgstr "MIME vrste"
