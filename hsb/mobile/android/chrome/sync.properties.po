#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Poslednja synchronizacija: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Poslednja synchronizacija: běži…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Konto: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Njezwjazany"

#: connecting.label
msgid "Connecting…"
msgstr "Zwjazuje so…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Waše konto Firefox Sync je so wotstroniło"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Cofnyć"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 z najnowšej wersiju Firefox Sync kompatibelny njeje. Prošu "
"zaktualizujće na najnowšu wersiju."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 ze staršimi wersijemi Firefox Sync kompatibelny njeje. Prošu "
"zaktualizujće Firefox na swojich druhich ličakach."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Dalše informacije"

#: sync.update.close
msgid "Close"
msgstr "Začinić"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Sync njeda so konfigurować"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Internetny zwisk k dispoziciji njesteji"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S njeda so ze Sync zwjazać. Chceće hišće raz spytać?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Hišće raz spytać"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Manuelne konfigurowanje"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Žane rajtarki z wašich druhich ličakow."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Synchronizacija je čišće nowa a lóša hač do toho"

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Dótkńće so tu, zo byšće wjace zhonił"
