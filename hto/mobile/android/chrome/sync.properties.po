#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Dáamakɨ odama: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Íraɨfue dáamakɨ fɨnófɨnode..."

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Fákara: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Nítaoinede"

#: connecting.label
msgid "Connecting…"
msgstr "Nítaoɨde"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Ó fakara Firefox Sync já óni óga"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Fúita"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 jóbiñede kómue version íe Firefox Sync.Maí kómue version ñue jóne."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 jóbiñede jáiai version  íe Firefox Sync.Maí kómue version ñue jóne "
"Firefox Sync jɨaɨe jóriaɨmo."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Aiyo onóde"

#: sync.update.close
msgid "Close"
msgstr "Ɨbai"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Ñúe fɨnónide Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Íñede nitaioyena  internet"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S nɨtanide Sync. ¿Abɨdo fɨinoakadɨo?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Meíne fɨnori"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Ónoɨdo fɨno"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Íñede úitɨarɨ jɨaɨe ó ráamo"

