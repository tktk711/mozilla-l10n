msgid "Comment:"
msgstr "Megjegyzés:"

#: imipHtml.attendees
msgid "Attendees:"
msgstr "Résztvevők:"

#: imipHtml.canceledOccurrences
msgid "Canceled Occurrences:"
msgstr "Lemondott alkalmak:"

#: imipHtml.modifiedOccurrences
msgid "Modified Occurrences:"
msgstr "Módosított alkalmak:"

#: imipHtml.newLocation
msgid "New Location: %1$S"
msgstr "Új hely: %1$S"

#: imipAddToCalendar.label
msgid "Add To Calendar"
msgstr "Hozzáadás a naptárhoz"

#: imipAddedItemToCal
msgid "Event Added to Calendar"
msgstr "Esemény hozzáadva a naptárhoz"

#: imipCanceledItem
msgid "Event has been deleted"
msgstr "Az esemény törölve lett"

#: imipUpdatedItem
msgid "Event has been updated"
msgstr "Az esemény frissítve lett"

#: imipBarCancelText
msgid "This message contains an event cancellation."
msgstr "Ez az üzenet egy esemény törlését tartalmazza."

#: imipBarRefreshText
msgid "This message asks for an event update."
msgstr "Ez az üzenet egy esemény frissítését kéri."

#: imipBarPublishText
msgid "This message contains an event."
msgstr "Ez az üzenet egy eseményt tartalmaz."

#: imipBarRequestText
msgid "This message contains an invitation to an event."
msgstr "Ez az üzenet egy eseményre szóló meghívót tartalmaz."

#: imipBarUpdateText
msgid "This message contains an update to an existing event."
msgstr "Ez az üzenet egy létező esemény frissítését tartalmazza."

#: imipBarAlreadyProcessedText
msgid "This message contains an event that has already been processed."
msgstr "Ez az üzenet egy olyan eseményt tartalmaz, amely már fel van dolgozva."

#: imipBarProcessedNeedsAction
msgid "This message contains an event that you have not yet responded to."
msgstr "Ez az üzenet megválaszolatlan eseményt tartalmaz."

#: imipBarReplyText
msgid "This message contains a reply to an invitation."
msgstr "Ez az üzenet egy meghívóra küldött választ tartalmaz."

#: imipBarUnsupportedText
msgid ""
"This message contains an event that this version of Lightning cannot process."
msgstr ""
"Ez az üzenet egy olyan eseményt tartalmaz, amelyet a Lightning ezen verziója "
"nem tud feldolgozni."

#: imipBarProcessingFailed
msgid "Processing message failed. Status: %1$S."
msgstr "Az üzenet feldolgozása sikertelen. Állapot: %1$S."

#: imipBarNotWritable
msgid ""
"No writable calendars are configured for invitations, please check the "
"calendar properties."
msgstr ""
"Nincsenek beállítva írható naptárak a meghívókhoz, ellenőrizze a naptár "
"beállításait."

#: imipSendMail.title
msgid "E-Mail Notification"
msgstr "E-mail értesítés"

#: imipSendMail.text
msgid "Would you like to send out notification E-Mail now?"
msgstr "Szeretné most szétküldeni az értesítő e-mailt?"

#: imipSendMail.Outlook2000CompatMode.text
msgid "Support Outlook 2000 and Outlook 2002/XP"
msgstr "Az Outlook 2000 és az Outlook 2002/XP támogatása"

#: imipNoIdentity
msgid "None"
msgstr "Nincs"

#: imipNoCalendarAvailable
msgid "There are no writable calendars available."
msgstr "Nincsenek írható naptárak."

#: itipReplySubject
msgid "Event Invitation Reply: %1$S"
msgstr "Válasz eseményre meghívóra: %1$S"

#: itipReplyBodyAccept
msgid "%1$S has accepted your event invitation."
msgstr "%1$S elfogadta a meghívót az eseményre."

#: itipReplyBodyDecline
msgid "%1$S has declined your event invitation."
msgstr "%1$S nem fogadta el a meghívót az eseményre."

#: itipReplySubjectAccept
msgid "Event Invitation Reply (Accepted): %1$S"
msgstr "Válasz eseménymeghívóra (elfogadva): %1$S"

#: itipReplySubjectDecline
msgid "Event Invitation Reply (Declined): %1$S"
msgstr "Válasz eseménymeghívóra (elutasítva): %1$S"

#: itipReplySubjectTentative
msgid "Event Invitation Reply (Tentative): %1$S"
msgstr "Válasz eseménymeghívóra (valószínű): %1$S"

#: itipRequestSubject
msgid "Event Invitation: %1$S"
msgstr "Meghívó eseményre: %1$S"

#: itipRequestUpdatedSubject
msgid "Updated Event Invitation: %1$S"
msgstr "Frissített eseménymeghívó: %1$S"

#: itipRequestBody
msgid "%1$S has invited you to %2$S"
msgstr "%1$S meghívta Önt egy %2$S eseményre"

#: itipCancelSubject
msgid "Event Canceled: %1$S"
msgstr "Esemény törölve: %1$S"

#: itipCancelBody
msgid "%1$S has canceled this event: « %2$S »"
msgstr "%1$S törölte ezt az eseményt: « %2$S »"

#: confirmProcessInvitation
msgid ""
"You have recently deleted this item, are you sure you want to process this "
"invitation?"
msgstr "Nemrég törölte ezt az elemet, valóban feldolgozza ezt a meghívást?"

#: confirmProcessInvitationTitle
msgid "Process Invitation?"
msgstr "Feldolgozza a meghívást?"

#: invitationsLink.label
msgid "Invitations: %1$S"
msgstr "Meghívók: %1$S"
