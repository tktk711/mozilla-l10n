msgid "Title:"
msgstr "Cím:"

#: imipHtml.location
msgid "Location:"
msgstr "Hely:"

#: imipHtml.when
msgid "When:"
msgstr "Mikor:"

#: imipHtml.organizer
msgid "Organizer:"
msgstr "Szervező:"

