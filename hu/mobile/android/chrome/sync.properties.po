#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Utolsó szinkronizálás: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Utolsó szinkronizálás: folyamatban…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Fiók: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Nincs kapcsolódva"

#: connecting.label
msgid "Connecting…"
msgstr "Csatlakozás…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "A Firefox Sync-fiókja el lett távolítva"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Visszavonás"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"A #1 #2 nem kompatibilis a Firefox Sync legújabb verziójával. Frissítsen a "
"legújabb verzióra."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"A #1 #2 nem kompatibilis a Firefox Sync régebbi verzióival. Frissítse a "
"Firefoxot a többi számítógépén."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "További tudnivalók"

#: sync.update.close
msgid "Close"
msgstr "Bezárás"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "A Sync beállítása sikertelen"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Nincs internetkapcsolat"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "A %S nem tud kapcsolódni a Synchez. Megpróbálja újra?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Próbálja újra"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Manuális beállítása"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Ne legyenek lapok más számítógépről"

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "A Sync vadonatúj és egyszerűbb, mint valaha."

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Koppintson további információkért"
