#. # 1st string is the username for the login, 2nd is the login's hostname.
#. # Note that long usernames may be truncated.
#: savePassword
msgid "Save password for \"%1$S\" on %2$S?"
msgstr "Menti „%1$S” jelszavát ezen: %2$S?"

#. # String is the login's hostname
#: savePasswordNoUser
msgid "Save password on %S?"
msgstr "Menti a jelszót ezen: %S?"

#: saveButton
msgid "Save"
msgstr "Mentés"

#: dontSaveButton
msgid "Don't save"
msgstr "Ne mentse"

#. # String is the login's hostname
#: updatePassword
msgid "Update saved password for %S?"
msgstr "Frissíti %S mentett jelszavát?"

#: updatePasswordNoUser
msgid "Update saved password for this login?"
msgstr "Frissíti a mentett jelszót ehhez a bejelentkezéshez?"

#: updateButton
msgid "Update"
msgstr "Frissítés"

#: dontUpdateButton
msgid "Don't update"
msgstr "Ne frissítse"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "Erősítse meg, hogy melyik felhasználó jelszavát változtatja meg"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "Jelszóváltoztatás megerősítése"

#. # Strings used by PromptService.js
#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "Jelszókezelő használata a jelszó megjegyzésére."

#: username
msgid "Username"
msgstr "Felhasználónév"

#: password
msgid "Password"
msgstr "Jelszó"
