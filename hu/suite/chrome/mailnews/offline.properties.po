#. #   LOCALIZATION NOTE :
#. #   do not localize "\n".  use "\n" to make the text fit nicely in the dialog.
#. # 
#. # Download Messages Prompt
#. #
#: downloadMessagesWindowTitle
msgid "Work Offline "
msgstr "Kapcsolat nélküli munka"

#. #   LOCALIZATION NOTE :
#. #   do not localize "\n".  use "\n" to make the text fit nicely in the dialog.
#: downloadMessagesLabel
msgid ""
"Do you want to download messages \n"
"for offline use before you go offline?\n"
"\n"
msgstr ""
"Szeretné letölteni az üzeneteket,\n"
"mielőtt a kapcsolat nélküli üzemmódra vált?\n"
"\n"

#: downloadMessagesCheckboxLabel
msgid "Always ask me when I go offline"
msgstr "Kérdés a kapcsolat megszakítása előtt minden alkalommal"

#: downloadMessagesDownloadButtonLabel
msgid "Download"
msgstr "Letöltés"

#: downloadMessagesNoDownloadButtonLabel
msgid "Don't Download"
msgstr "Letöltés nélkül"

#: downloadMessagesCancelButtonLabel
msgctxt "downloadMessagesCancelButtonLabel"
msgid "Cancel"
msgstr "Mégse"

#. # 
#. # Send Messages Prompt
#. #
#: sendMessagesWindowTitle
msgid "Work Online "
msgstr "Online munka"

#: sendMessagesLabel2
msgid "Would you like to send your unsent messages now?"
msgstr "Would you like to send your unsent messages now?"

#: sendMessagesCheckboxLabel
msgid "Always ask me when I go online"
msgstr "Kérdés a csatlakozás előtt minden alkalommal"

#: sendMessagesSendButtonLabel
msgid "Send"
msgstr "Küldés"

#: sendMessagesNoSendButtonLabel
msgid "Don't Send"
msgstr "Nincs küldés"

#: sendMessagesCancelButtonLabel
msgctxt "sendMessagesCancelButtonLabel"
msgid "Cancel"
msgstr "Mégse"

#. #
#. # GetMessages Offline Prompt 
#. #
#: getMessagesOfflineWindowTitle
msgid "Get Messages "
msgstr "Üzenetek letöltése"

#. #   LOCALIZATION NOTE :
#. #   do not localize "\n".  use "\n" to make the text fit nicely in the dialog.
#: getMessagesOfflineLabel
msgid ""
"You are currently offline. Would you like \n"
"to go online to get your new messages?\n"
"\n"
msgstr ""
"Jelenleg kapcsolat nélküli módban van. Szeretne kapcsolódni a hálózathoz, \n"
"és letölteni az új üzeneteket?\n"
"\n"

#: getMessagesOfflineGoButtonLabel
msgctxt "getMessagesOfflineGoButtonLabel"
msgid "Go online"
msgstr "Csatlakozás a hálózathoz"

#. # 
#. # Send Messages Offline Prompt
#. #
#: sendMessagesOfflineWindowTitle
msgid "Send Messages "
msgstr "Üzenetek küldése"

#. #   LOCALIZATION NOTE :
#. #   do not localize "\n".  use "\n" to make the text fit nicely in the dialog.
#: sendMessagesOfflineLabel
msgid ""
"You are currently offline. Would you like \n"
"to go online and send your unsent messages?\n"
"\n"
msgstr ""
"Jelenleg kapcsolat nélküli módban van. Szeretne kapcsolódni a hálózathoz,\n"
"és elküldeni a postázandó üzeneteket?\n"
"\n"

#: sendMessagesOfflineGoButtonLabel
msgctxt "sendMessagesOfflineGoButtonLabel"
msgid "Go online"
msgstr "Csatlakozás a hálózathoz"
