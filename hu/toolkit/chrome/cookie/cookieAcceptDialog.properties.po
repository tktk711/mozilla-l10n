#: hostColon
msgid "Host:"
msgstr "Gép:"

#: domainColon
msgid "Domain:"
msgstr "Tartomány:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Csak titkosított kapcsolatok"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Bármilyen kapcsolat"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "A munkamenet végén"

#: showDetails
msgid "Show Details"
msgstr "Részletek megjelenítése"

#: hideDetails
msgid "Hide Details"
msgstr "Részletek elrejtése"

#: detailsAccessKey
msgid "T"
msgstr "R"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "A(z) %S webhely sütit szeretne elhelyezni."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "A(z) %S webhely egy második sütit is el szeretne helyezni."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"A(z) %S webhely egy újabb sütit szeretne elhelyezni. \n"
"Már %S sütije van erről a webhelyről."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "A(z) %S webhely módosítani szeretne egy korábbi sütit."
