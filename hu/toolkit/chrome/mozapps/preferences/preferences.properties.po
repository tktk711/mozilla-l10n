#. #### Master Password
#: password_not_set
msgid "(not set)"
msgstr "(nincs beállítva)"

#: failed_pw_change
msgid "Unable to change Master Password."
msgstr "Nem sikerült megváltoztatni a mesterjelszót!"

#: incorrect_pw
msgid ""
"You did not enter the correct current Master Password. Please try again."
msgstr "A megadott mesterjelszó nem helyes. Próbálja újra."

#: pw_change_ok
msgid "Master Password successfully changed."
msgstr "A mesterjelszó megváltoztatása sikerült."

#: pw_erased_ok
msgid "You have deleted your Master Password. "
msgstr "Törölte a mesterjelszót."

#: pw_not_wanted
msgid "Warning! You have decided not to use a Master Password."
msgstr "Vigyázat! Úgy döntött, hogy nem használ mesterjelszót."

#: pw_empty_warning
msgid ""
"Your stored web and email passwords, form data, and private keys will not be "
"protected."
msgstr ""
"A weboldalakhoz, űrlapokhoz és az e-mailhez tárolt jelszavakat nem védi "
"semmi."

#: pw_change2empty_in_fips_mode
msgid ""
"You are currently in FIPS mode. FIPS requires a non-empty Master Password."
msgstr ""
"Jelenleg FIPS-módban van. A FIPS-hez kötelező nem üres mesterjelszót megadni."

#: pw_change_success_title
msgid "Password Change Succeeded"
msgstr "Sikeres jelszóváltoztatás"

#: pw_change_failed_title
msgid "Password Change Failed"
msgstr "Sikertelen jelszóváltoztatás"

#: pw_remove_button
msgid "Remove"
msgstr "Eltávolítás"
