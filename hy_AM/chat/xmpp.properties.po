#. # LOCALIZATION NOTE (connection.*)
#. #   These will be displayed in the account manager in order to show the progress
#. #   of the connection.
#. #   (These will be displayed in account.connection.progress from
#. #    accounts.properties, which adds … at the end, so do not include
#. #    periods at the end of these messages.)
#: connection.initializingStream
msgid "Initializing stream"
msgstr "Հոսքի որոշում"

#: connection.initializingEncryption
msgid "Initializing encryption"
msgstr "Կոդավորման որոշում"

#: connection.authenticating
msgid "Authenticating"
msgstr "Վավերացում"

#: connection.gettingResource
msgid "Getting resource"
msgstr "Աղբյուրի ստացում"

#: connection.downloadingRoster
msgid "Downloading contact list"
msgstr "Կապերի ցանկի բեռնում"

#. # LOCALIZATION NOTE (connection.error.*)
#. #   These will show in the account manager if an error occurs during the
#. #   connection attempt.
#: connection.error.invalidUsername
msgid "Invalid username (your username should contain an '@' character)"
msgstr "Invalid username (your username should contain an '@' character)"

#: connection.error.failedToCreateASocket
msgid "Failed to create a socket (Are you offline?)"
msgstr "socket ստեղծելու սխալ (Դուք անցանց ե՞ք)"

#: connection.error.serverClosedConnection
msgid "The server closed the connection"
msgstr "Սպասարկիչը փակել է կապը"

#: connection.error.resetByPeer
msgid "Connection reset by peer"
msgstr "Միացումը ետարկվել է պիռի կողմից"

#: connection.error.timedOut
msgid "The connection timed out"
msgstr "Միացման ժամանակը լրացել է"

#: connection.error.receivedUnexpectedData
msgid "Received unexpected data"
msgstr "Ստացել է անորոշ տվյալ"

#: connection.error.incorrectResponse
msgid "Received an incorrect response"
msgstr "Ստացել է սխալ պատասխան"

#: connection.error.startTLSRequired
msgid "The server requires encryption but you disabled it"
msgstr "Սպասարկիչը պահանջում է կոդավորում, որը Դուք անջատել եք"

#: connection.error.startTLSNotSupported
msgid ""
"The server doesn't support encryption but your configuration requires it"
msgstr ""
"Սպասարկիչը չի աջակցում կոդավորում, բայց Ձեր կարգավորումները ակնկալում են"

#: connection.error.failedToStartTLS
msgid "Failed to start encryption"
msgstr "Հնարավոր չէ սկսել կոդավորումը"

#: connection.error.noAuthMec
msgid "No authentication mechanism offered by the server"
msgstr "Սպասարկիչը չի տրամադրում վավերացում"

#: connection.error.noCompatibleAuthMec
msgid ""
"None of the authentication mechanisms offered by the server are supported"
msgstr "Ոչ մի վավերացման մեխանիզմ չի առաջարկվում սպասարկիչի կողմից"

#: connection.error.notSendingPasswordInClear
msgid ""
"The server only supports authentication by sending the password in cleartext"
msgstr ""
"The server only supports authentication by sending the password in cleartext"

#: connection.error.authenticationFailure
msgid "Authentication failure"
msgstr "Ներկայացումը ձախողվեց"

#: connection.error.notAuthorized
msgid "Not authorized (Did you enter the wrong password?)"
msgstr "Ներկայացված չէ (հնարավոր է գաղտնաբառը սխալ է)։"

#: connection.error.failedToGetAResource
msgid "Failed to get a resource"
msgstr "Ռեսուրսը հասանելի չէ"

#. # LOCALIZATION NOTE (conversation.error.notDelivered):
#. #   This is displayed in a conversation as an error message when a message
#. #   the user has sent wasn't delivered.
#. #   %S is replaced by the text of the message that wasn't delivered.
#: conversation.error.notDelivered
msgid "This message could not be delivered: %S"
msgstr "This message could not be delivered: %S"

#. # LOCALIZATION NOTE (tooltip.*):
#. #   These are the titles of lines of information that will appear in
#. #   the tooltip showing details about a contact or conversation.
#. # LOCALIZATION NOTE (tooltip.status):
#. #   %S will be replaced by the XMPP resource identifier
#: tooltip.status
msgid "Status (%S)"
msgstr "Վիճակը (%S)"

#: tooltip.statusNoResource
msgid "Status"
msgstr "Վիճակը"

#: tooltip.subscription
msgid "Subscription"
msgstr "Բաժանորդագրություն"

#. # LOCALIZATION NOTE (chatRoomField.*):
#. #   These are the name of fields displayed in the 'Join Chat' dialog
#. #   for XMPP accounts.
#. #   The _ character won't be displayed; it indicates the next
#. #   character of the string should be used as the access key for this
#. #   field.
#: chatRoomField.room
msgid "_Room"
msgstr "_Room"

#: chatRoomField.server
msgid "_Server"
msgstr "_Server"

#: chatRoomField.nick
msgid "_Nick"
msgstr "_Nick"

#: chatRoomField.password
msgid "_Password"
msgstr "_Password"

#. # LOCALIZATION NOTE
#. #  Buddies that aren't in any group on the server will appear in this group.
#. #  Try to use the same translation as for defaultGroup in instantbird.properties
#: defaultGroup
msgid "Contacts"
msgstr "Կոնտակտներ"

#. # LOCALIZATION NOTE (options.*):
#. #   These are the protocol specific options shown in the account manager and
#. #   account wizard windows.
#: options.resource
msgid "Resource"
msgstr "Աղբյուրը"

#: options.priority
msgid "Priority"
msgstr "Առաջնահերթ-ը"

#: options.connectionSecurity
msgid "Connection security"
msgstr "Կապուղու անվտանգությունը"

#: options.connectionSecurity.requireEncryption
msgid "Require encryption"
msgstr "Պահանջում է կոդավորում"

#: options.connectionSecurity.opportunisticTLS
msgid "Use encryption if available"
msgstr "Օգտ. կոդավորում, եթե հասանելի է"

#: options.connectionSecurity.allowUnencryptedAuth
msgid "Allow sending the password unencrypted"
msgstr "Թույլատրել գաղտնաբառի ուղարկումը ոչ կոդավորված"

#: options.connectServer
msgid "Server"
msgstr "Սպասարկիչը"

#: options.connectPort
msgid "Port"
msgstr "Պորտը"

