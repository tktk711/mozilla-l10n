#. #   account wizard windows.
#: options.resource
msgid "Resource"
msgstr "Աղբյուրը"

#: options.priority
msgid "Priority"
msgstr "Առաջնահերթ-ը"

#: options.connectionSecurity
msgid "Connection security"
msgstr "Կապուղու անվտանգությունը"

#: options.connectionSecurity.requireEncryption
msgid "Require encryption"
msgstr "Պահանջում է կոդավորում"

#: options.connectionSecurity.opportunisticTLS
msgid "Use encryption if available"
msgstr "Օգտ. կոդավորում, եթե հասանելի է"

#: options.connectionSecurity.allowUnencryptedAuth
msgid "Allow sending the password unencrypted"
msgstr "Թույլատրել գաղտնաբառի ուղարկումը ոչ կոդավորված"

#: options.connectServer
msgid "Server"
msgstr "Սպասարկիչը"

#: options.connectPort
msgid "Port"
msgstr "Պորտը"

#: options.domain
msgid "Domain"
msgstr "Տիրույթ"

#. # LOCALIZATION NOTE (*.protocolName)
#. #  This name is used whenever the name of the protocol is shown.
#: gtalk.protocolName
msgid "Google Talk"
msgstr "Google Talk"

#: odnoklassniki.protocolName
msgid "Odnoklassniki"
msgstr "Odnoklassniki"

#. # LOCALIZATION NOTE (gtalk.usernameHint):
#. #  This is displayed inside the accountUsernameInfoWithDescription
#. #  string defined in imAccounts.properties when the user is
#. #  configuring a Google Talk account.
#: gtalk.usernameHint
msgid "email address"
msgstr "էլ. հասցեներ"

#. # LOCALIZATION NOTE (odnoklassniki.usernameHint):
#. #  This is displayed inside the accountUsernameInfoWithDescription
#. #  string defined in imAccounts.properties when the user is
#. #  configuring a Odnoklassniki account.
#: odnoklassniki.usernameHint
