#. # CSP Warnings:
#. # LOCALIZATION NOTE (CSPViolation):
#. # %1$S is the reason why the resource has not been loaded.
#: CSPViolation
msgid "The page's settings blocked the loading of a resource: %1$S"
msgstr ""

#. # LOCALIZATION NOTE (CSPViolationWithURI):
#. # %1$S is the directive that has been violated.
#. # %2$S is the URI of the resource which violated the directive.
#: CSPViolationWithURI
msgid ""
"The page's settings blocked the loading of a resource at %2$S (\"%1$S\")."
msgstr ""

#. # LOCALIZATION NOTE (triedToSendReport):
#. # %1$S is the URI we attempted to send a report to.
#: triedToSendReport
msgid "Tried to send report to invalid URI: \"%1$S\""
msgstr "Անվավեր URI-ին զեկույց ուղարկելու փորձ.\"%1$S\""

#. # LOCALIZATION NOTE (errorWas):
#. # %1$S is the error resulting from attempting to send the report
#: errorWas
msgid "error was: \"%1$S\""
msgstr "Սխալմունք.\"%1$S\""

#. # LOCALIZATION NOTE (couldNotParseReportURI):
#. # %1$S is the report URI that could not be parsed
#: couldNotParseReportURI
msgid "couldn't parse report URI: %1$S"
msgstr "Անհնար է  վերլուծել URI զեկույցը.%1$S"

#. # LOCALIZATION NOTE (couldNotProcessUnknownDirective):
#. # %1$S is the unknown directive
#: couldNotProcessUnknownDirective
msgid "Couldn't process unknown directive '%1$S'"
msgstr " '%1$S' անհայտ հրահանգը անհնար է մշակել"

#. # LOCALIZATION NOTE (ignoringUnknownOption):
#. # %1$S is the option that could not be understood
#: ignoringUnknownOption
msgid "Ignoring unknown option %1$S"
msgstr ""

#. # LOCALIZATION NOTE (reportURInotETLDPlus1):
#. # %1$S is the ETLD of the report URI that could not be used
#: reportURInotETLDPlus1
msgid "The report URI (%1$S) must be from the same eTLD+1."
msgstr ""

#. # LOCALIZATION NOTE (reportURInotSameSchemeAsSelf, reportURInotSamePortAsSelf):
#. # %1$S is the report URI that could not be used
#: reportURInotSameSchemeAsSelf
msgid ""
"The report URI (%1$S) must use the same scheme as the originating document."
msgstr ""

#: reportURInotSamePortAsSelf
msgid ""
"The report URI (%1$S) must use the same port as the originating document."
msgstr ""

#. # LOCALIZATION NOTE (pageCannotSendReportsTo):
#. # %1$S is the URI of the page with the policy
#. # %2$S is the report URI that could not be used
#: pageCannotSendReportsTo
msgid "page on %1$S cannot send reports to %2$S"
msgstr "%1$S-ում գտնվող էջը չի կարողանում ուղարկել զեկույցներ %2$S-ին"

#: allowOrDefaultSrcRequired
msgid ""
"'allow' or 'default-src' directive required but not present.  Reverting to "
"\"default-src 'none'\""
msgstr ""
"Պահանջվում են բացակայող 'allow' or 'default-src' հրահանգները:Վերադարձ է "
"կատարվել \"default-src 'none'\"-ին"

#. # LOCALIZATION NOTE (failedToParseUnrecognizedSource):
#. # %1$S is the CSP Source that could not be parsed
#: failedToParseUnrecognizedSource
msgid "Failed to parse unrecognized source %1$S"
msgstr "Հնարավոր չեղավ վերլուծել չճանաչված %1$S աղբյուրը "

#. # LOCALIZATION NOTE (reportPostRedirect):
#. # %1$S is the specified report URI before redirect
#: reportPostRedirect
msgid "Post of violation report to %1$S failed, as a redirect occurred"
msgstr ""
"                  Վերուղղորդման պատճառով  խախտման զեկույցի մասին "
"հաղորդագրության ուղարկումը %1$S-ին ձախողվեց                "

#. # LOCALIZATION NOTE (allowDirectiveIsDeprecated):
#. # Don't translate "allow" and "default-src" as they are keywords and part of
#. # the CSP protocol syntax.
#: allowDirectiveIsDeprecated
msgid ""
"allow directive is deprecated, use the equivalent default-src directive "
"instead"
msgstr ""
"Թույլատրման հրահանգը հնացել է:Փոխարենը օգտագործեք համարժեք սկզբնադիր-src  "
"հրահանգը"

#. # LOCALIZATION NOTE (inlineScriptBlocked):
#. # inline script refers to JavaScript code that is embedded into the HTML document.
#: inlineScriptBlocked
msgid "An attempt to execute inline scripts has been blocked"
msgstr ""

#. # LOCALIZATION NOTE (inlineStyleBlocked):
#. # inline style refers to CSS code that is embedded into the HTML document.
#: inlineStyleBlocked
msgid "An attempt to apply inline style sheets has been blocked"
msgstr ""

#. # LOCALIZATION NOTE (scriptFromStringBlocked):
#. # eval is a name and should not be localized.
#: scriptFromStringBlocked
msgid ""
"An attempt to call JavaScript from a string (by calling a function like "
"eval) has been blocked"
msgstr ""

#. # CSP Errors:
#: policyURINotAlone
msgid "policy-uri directive can only appear alone"
msgstr "policy-uri հրահանգը կարող է միայն մենակ հայտնվել"

#: noParentRequest
msgid "The policy-uri cannot be fetched without a parent request and a CSP."
msgstr ""
"policy-uri-ն հնարավոր չէ դուրս բերել առանց ծնողական հարցման և CSP-"
"ի:                "

#. # LOCALIZATION NOTE (policyURIParseError):
#. # %1$S is the URI that could not be parsed
#: policyURIParseError
msgid "could not parse URI in policy URI: %1$S"
msgstr "URI քաղաքականության մեջ հնարավոր չեղավ վերլուծել URI-ն.%1$S"

#. # LOCALIZATION NOTE (nonMatchingHost):
#. # %1$S is the URI host that does not match
#: nonMatchingHost
msgid "can't fetch policy uri from non-matching hostname: %1$S"
msgstr ""
"                  Չհամընկնող հոսթի (սերվերի) անվանումից չհաջողվեց ստանալ uri "
"քաղաքականությունը.%1$S                "

#. # LOCALIZATION NOTE (nonMatchingPort):
#. # %1$S is the URI port that does not match
#: nonMatchingPort
msgid "can't fetch policy uri from non-matching port: %1$S"
msgstr ""
"Չհաջողվեց ստանալ uri քաղաքականությունը չհամընկնող միացքից."
"%1$S                "

#. # LOCALIZATION NOTE (nonMatchingScheme):
#. # %1$S is the URI scheme that does not match
#: nonMatchingScheme
msgid "can't fetch policy uri from non-matching scheme: %1$S"
msgstr ""
"Չի հաջողվում ստանալ uri քաղաքականությունը չհամընկնող սխեմայից."
"%1$S                "

#. # LOCALIZATION NOTE (errorFetchingPolicy):
#. # %1$S is the error that caused fetching to fail
#: errorFetchingPolicy
msgid "Error fetching policy-uri: %1$S"
msgstr "policy-uri-ի ստացման սխալ.%1$S"

#: cspSourceNotURI
msgid "Provided argument is not an nsIURI"
msgstr "Տրամադրված փաստարկը nsIURI-ը չէ"

#: argumentIsNotString
msgid "Provided argument is not a string"
msgstr "Տրամադրված փաստարկը չի հանդիսանում տող"

#: selfDataNotProvided
msgid "Can't use 'self' if self data is not provided"
msgstr ""
"Հնարավոր չէ օգտագործել 'self',եթե սեփական (ինքնակառավարման) տվյալները  չեն "
"տրամադրվում                "

#. # LOCALIZATION NOTE (uriWithoutScheme):
#. # %1$S is the URI without a scheme
#: uriWithoutScheme
msgid "can't parse a URI without a scheme: %1$S"
msgstr "Առանց սխեմայի չի հաջողվում վերլուծել URI-ն.%1$S       "

#: selfKeywordNoSelfData
msgid "self keyword used, but no self data specified"
msgstr ""
"Օգտագործվել է ինքնակառավարման (self) հիմնաբառը,բայց չի նշված ինքնակառավարման "
"(self) տվյալները"

#. # LOCALIZATION NOTE (couldntParseInvalidSource):
#. # %1$S is the source that could not be parsed
#: couldntParseInvalidSource
msgid "Couldn't parse invalid source %1$S"
msgstr "Չհաջողվեց վերլուծել անվավեր %1$S աղբյուրը"

#. # LOCALIZATION NOTE (hostSourceWithoutData):
#. # %1$S is the source
#: hostSourceWithoutData
msgid "Can't create host-only source %1$S without 'self' data"
msgstr "Հնարավոր չէ ստեղծել միայն-հոսթ %1$S  աղբյուրը առանց 'self' տվյալների"

#. # LOCALIZATION NOTE (sourceWithoutData):
#. # %1$S is the source
#: sourceWithoutData
msgid "Can't create source %1$S without 'self' data"
msgstr ""
"Հնարավոր չէ ստեղծել %1$S աղբյուրը առանց 'self' տվյալների                "

#. # LOCALIZATION NOTE (couldntParseInvalidHost):
#. # %1$S is the host that's invalid
#: couldntParseInvalidHost
msgid "Couldn't parse invalid host %1$S"
msgstr "Չհաջողվեց վերլուծել անվավեր %1$S հոսթը"

#. # LOCALIZATION NOTE (couldntParseScheme):
#. # %1$S is the string source
#: couldntParseScheme
msgid "Couldn't parse scheme in %1$S"
msgstr "%1$S-ում չհաջողվեց վերլուծել սխեման"

#. # LOCALIZATION NOTE (couldntParsePort):
#. # %1$S is the string source
#: couldntParsePort
msgid "Couldn't parse port in %1$S"
msgstr "                  Չհաջողվեց վերլուծել միացքը %1$S-ում                "

#. # LOCALIZATION NOTE (notIntersectPort):
#. # %1$S is one source we tried to intersect
#. # %2$S is the other
#: notIntersectPort
msgid "Could not intersect %1$S with %2$S due to port problems."
msgstr ""
"Միացքի խնդիրների պատճառով չհաջողվեց հատել %1$S-ը %2$S-ի հետ:                "

#. # LOCALIZATION NOTE (notIntersectScheme):
#. # %1$S is one source we tried to intersect
#. # %2$S is the other
#: notIntersectScheme
msgid "Could not intersect %1$S with %2$S due to scheme problems."
msgstr "Սխեմայի խնդիրների պատճառով չհաջողվեց հատել %1$S-ը %2$S-ի հետ:"

#. # LOCALIZATION NOTE (intersectingSourceWithUndefinedHost):
#. # %1$S is the source
#: intersectingSourceWithUndefinedHost
msgid "intersecting source with undefined host: %1$S"
msgstr ""
"                                    Սկզբնաղբյուրի հատում չորոշված հոսթերի "
"հետ.%1$S                                "

#. # LOCALIZATION NOTE (intersectingSourcesWithUndefinedHosts):
#. # %1$S is the first source
#. # %2$S is the second source
#: intersectingSourcesWithUndefinedHosts
msgid "intersecting two sources with undefined hosts: %1$S and %2$S"
msgstr "Չորոշված հոսթերի հետ երկու սկզբնաղբյուրների հատում.%1$S և %2$S"

#. # LOCALIZATION NOTE (duplicateDirective):
#. # %1$S is the name of the duplicate directive
#: duplicateDirective
msgid ""
"Duplicate %1$S directives detected.  All but the first instance will be "
"ignored."
msgstr ""
"Հայտնաբերվել է կրկնօրինակվող %1$S հրահանգներ:Բացի առաջինից բոլոր նմուշները  "
"կմերժվեն:"

#~ msgid "Directive %1$S violated"
#~ msgstr "%1$S հրահանգը խախտված է"

#~ msgid "Directive %1$S violated by %2$S"
#~ msgstr "%1$S հրահանգը խախտված է %2$S-ի կողմից                "

#~ msgid "don't understand option '%1$S'.  Ignoring it."
#~ msgstr "'%1$S' ընտրանքի անըմբռնում: Այն կմերժվի:                "

#~ msgid "can't use report URI from non-matching eTLD+1: %1$S"
#~ msgstr ""
#~ "Անհամապատասխան  eTLD+1-ից URI զեկույցի օգտագործման անհնարինություն."
#~ "%1$S                "

#~ msgid ""
#~ "can't use report URI with different scheme from originating document: %1$S"
#~ msgstr ""
#~ "Օրիգինալ փաստաթղթի սխեմայից տարբերվող  URI զեկույցի օգտագործման "
#~ "անհնարինություն.%1$S"

#~ msgid ""
#~ "can't use report URI with different port from originating document: %1$S"
#~ msgstr ""
#~ "Օրիգինալ փաստաթղթի միացքից տարբերվող  URI զեկույցի օգտագործման "
#~ "անհնարինություն.%1$S"
