#: accountsWindow.title
msgid "Instant messaging status"
msgstr "Գրության կարգը"

#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: accountsWindow2.style
msgid "width: 41em; height: 27em;"
msgstr "width: 41em; height: 27em;"

#: accountManager.newAccount.label accountManager.newAccount.accesskey
msgid "&New Account"
msgstr "&Նոր հաշիվ"

#: accountManager.close.label accountManager.close.accesskey
msgid "C&lose"
msgstr "&Փակել"

#. This should match account.commandkey in instantbird.dtd
#: accountManager.close.commandkey
msgid "a"
msgstr "Փ"

#. This title must be short, displayed with a big font size
#: accountManager.noAccount.title
msgid "No account configured yet"
msgstr "Դեռ մուտք չեք գործել"

#: accountManager.noAccount.description
msgid ""
"Click on the &accountManager.newAccount.label; button to let "
"&brandShortName; guide you through the process of configuring one."
msgstr ""
"Սեղմեք &accountManager.newAccount.label; կոճակը, որպեսզի &brandShortName;-ը "
"օգնի Ձեզ կարգավորելու հարցում։"

#: account.autoSignOn.label account.autoSignOn.accesskey
msgid "&Sign-on at startup"
msgstr "&Մուտք գործել բացելիս"

#: account.connect.label account.connect.accesskey
msgid "C&onnect"
msgstr "&Միանալ"

#: account.disconnect.label account.disconnect.accesskey
msgid "D&isconnect"
msgstr "Ան&ջատել"

#: account.edit.label account.edit.accesskey
msgid "&Properties"
msgstr "&Հատկություններ"

#: account.moveup.label
msgid "Move up"
msgstr "Վեր"

#: account.movedown.label
msgid "Move down"
msgstr "Վար"

#: account.cancelReconnection.label account.cancelReconnection.accesskey
msgid "C&ancel reconnection"
msgstr "&Չեղարկել վերամիացումը"

#: account.connecting
msgid "Connecting…"
msgstr "Միանում է..."

#: account.disconnecting
msgid "Disconnecting…"
msgstr "Անջատվում է..."

#: account.disconnected
msgid "Not Connected"
msgstr "Միացած չէ"
