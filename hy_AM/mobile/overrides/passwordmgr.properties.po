#. # 1st string is the username for the login, 2nd is the login's hostname.
#. # Note that long usernames may be truncated.
#: savePassword
msgid "Save password for \"%1$S\" on %2$S?"
msgstr "Պահպանե՞լ գաղտնաբառը \"%1$S\"-ի համար %2$S-ում:"

#. # String is the login's hostname
#: savePasswordNoUser
msgid "Save password on %S?"
msgstr "Պահպանե՞լ գաղտնաբառը %S-ում:"

#: saveButton
msgid "Save"
msgstr "Պահպանել"

#: dontSaveButton
msgid "Don't save"
msgstr "Չպահպանել"

#. # String is the login's hostname
#: updatePassword
msgid "Update saved password for %S?"
msgstr "Փոխե՞լ պահված գաղտնաբառը %S-ով:"

#: updatePasswordNoUser
msgid "Update saved password for this login?"
msgstr "Փոխե՞լ այս մուտքանվան գաղտնաբառը:"

#: updateButton
msgid "Update"
msgstr "Թարմացնել"

#: dontUpdateButton
msgid "Don't update"
msgstr "Չփոխել"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "Խնդրեմ նշեք թե որ օգտվողի գաղտնաբառն էք փոխում"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "Հավաստեք Գաղտնաբառի Փոփոխությունը"

#. # Strings used by PromptService.js
#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "Օգտագործել գաղտնաբառերի կառավարիչը այս գաղտնաբառը հիշելու համար:"

#: username
msgid "Username"
msgstr "Օգտվողի Անունը"

#: password
msgid "Password"
msgstr "Գաղտնաբառը"
