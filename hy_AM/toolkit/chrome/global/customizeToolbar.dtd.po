msgid "Icons and Text"
msgstr "Մանրանկարներ և Տեքստ"

#: icons.label
msgid "Icons"
msgstr "Մանրանկարներ"

#: text.label
msgid "Text"
msgstr "Տեքստ"

#: useSmallIcons.label
msgid "Use Small Icons"
msgstr "Փոքր Մանրանկարներ Գործածել"

#: restoreDefaultSet.label
msgid "Restore Default Set"
msgstr "Վերականգնել Նախնականը"

#: addNewToolbar.label
msgid "Add New Toolbar"
msgstr "Ավելացնել Նոր Վահանակ"

#: saveChanges.label
msgid "Done"
msgstr "Պատրաստ է"

#: undoChanges.label
msgid "Undo Changes"
msgstr "Փոփոխությունները Հետադարձել"
