#: options.connectionSecurity.opportunisticTLS
msgid "Use encryption if available"
msgstr "Gunakan enkripsi jika tersedia"

#: options.connectionSecurity.allowUnencryptedAuth
msgid "Allow sending the password unencrypted"
msgstr "Izinkan mengirimkan sandi tanpa enkripsi"

#: options.connectServer
msgid "Server"
msgstr "Server"

#: options.connectPort
msgid "Port"
msgstr "Port"

#: options.domain
#, fuzzy
msgid "Domain"
msgstr "Domain:"

