#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Sinkronisasi terakhir: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Sinkronisasi terakhir: sedang dilakukan…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Akun: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Tidak tersambung"

#: connecting.label
msgid "Connecting…"
msgstr "Menyambungkan…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Akun Firefox Sync Anda telah dihapus"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Batalkan"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 tidak kompatibel dengan versi terbaru Firefox Sync. Perbarui ke versi "
"terbaru."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 tidak kompatibel dengan versi lawas Firefox Sync. Perbarui Firefox "
"pada komputer Anda yang lain."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Pelajari Lebih Lanjut"

#: sync.update.close
msgid "Close"
msgstr "Tutup"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Tidak Dapat Menyiapkan Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Tidak ada sambungan internet tersedia"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S tidak dapat tersambung ke Sync. Ingin mencoba lagi?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Coba lagi"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Persiapan manual"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Tidak ada tab dari komputer Anda yang lain."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Kini dengan Sync yang baru dan lebih mudah. "

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Ketuk di sini untuk mempelajari lebih lanjut"
