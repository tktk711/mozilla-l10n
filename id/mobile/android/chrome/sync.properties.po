#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Sinkronisasi terakhir: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Sinkronisasi terakhir: sedang…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Akun: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Tidak tersambung"

#: connecting.label
msgid "Connecting…"
msgstr "Menyambungkan…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Akun Sync Firefox Anda telah dihapus"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Batalkan"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 tidak kompatibel dengan versi terbaru Firefox Sync. Perbarui ke versi "
"terbaru."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 tidak kompatibel dengan versi lama Firefox Sync. Perbarui Firefox di "
"semua komputer Anda yang lain."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Pelajari Lebih Lanjut"

#: sync.update.close
msgid "Close"
msgstr "Tutup"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Tidak Dapat Menyetel Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Tidak ada sambungan internet tersedia"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S tidak dapat tersambung ke Sync. Ingin coba lagi?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Coba lagi"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Setel manual"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr ""
