#: 1
msgid "Parsing an XSLT stylesheet failed."
msgstr "Þáttun mistókst fyrir XSLT stílblað."

#: 2
msgid "Parsing an XPath expression failed."
msgstr "Þáttun mistókst fyrir XPath segð."

#: 3
msgctxt "3"
msgid ""
msgstr ""

#: 4
msgid "XSLT transformation failed."
msgstr "XSLT vörpun mistókst."

#: 5
msgid "Invalid XSLT/XPath function."
msgstr "Ógilt XSLT/XPath fall."

#: 6
msgid "XSLT Stylesheet (possibly) contains a recursion."
msgstr "XSLT stílblað (hugsanlega) inniheldur sjálfkvaðningu."

#: 7
msgid "Attribute value illegal in XSLT 1.0."
msgstr "Gildi eigindis ógilt í XSLT 1.0."

#: 8
msgid "An XPath expression was expected to return a NodeSet."
msgstr "Bjóst við að XPath segð skilaði NodeSet."

#: 9
msgid "XSLT transformation was terminated by <xsl:message>."
msgstr "XSLT vörpun var hætt vegna <xsl:message>."

#: 10
msgid "A network error occurred loading an XSLT stylesheet:"
msgstr "Netvilla kom upp við niðurhal á XSLT stílblaði:"

#: 11
msgid "An XSLT stylesheet does not have an XML mimetype:"
msgstr "XSLT stílblað má ekki vera með XML gagnategund:"

#: 12
msgid "An XSLT stylesheet directly or indirectly imports or includes itself:"
msgstr "XSLT stílblað inniheldur sjálfa sig annaðhvort beint eða óbeint:"

#: 13
msgid "An XPath function was called with the wrong number of arguments."
msgstr "Kallað var á XPath fall með röngum fjölda færibreytna."

#: 14
msgid "An unknown XPath extension function was called."
msgstr "Kallað var á óþekkt XPath fall."

#: 15
msgid "XPath parse failure: ')' expected:"
msgstr "Villa í XPath þáttun: Bjóst við ')':"

#: 16
msgid "XPath parse failure: invalid axis:"
msgstr "Villa í XPath þáttun: Ógildur ás:"

#: 17
msgid "XPath parse failure: Name or Nodetype test expected:"
msgstr "Villa í XPath þáttun: Bjóst við nafni eða prófun á hnútategund:"

#: 18
msgid "XPath parse failure: ']' expected:"
msgstr "Villa í XPath þáttun: Bjóst við ']':"

#: 19
msgid "XPath parse failure: invalid variable name:"
msgstr "Villa í XPath þáttun: Ógilt breytu nafn:"

#: 20
msgid "XPath parse failure: unexpected end of expression:"
msgstr "Villa í XPath þáttun: Óvæntur endir á segð:"

#: 21
msgid "XPath parse failure: operator expected:"
msgstr "Villa í XPath þáttun: Bjóst við virkja:"

#: 22
msgid "XPath parse failure: unclosed literal:"
msgstr "Villa í XPath þáttun: opið lesgildi:"

#: 23
msgid "XPath parse failure: ':' unexpected:"
msgstr "Villa í XPath þáttun: Óvænt ':':"

#: 24
msgid "XPath parse failure: '!' unexpected, negation is not():"
msgstr "Villa í XPath þáttun: Óvænt '!', neitun er not():"

#: 25
msgid "XPath parse failure: illegal character found:"
msgstr "Villa í XPath þáttun: Ólöglegur stafur fannst:"

#: 26
msgid "XPath parse failure: binary operator expected:"
msgstr "Villa í XPath þáttun: Bjóst við tvíundavirkja:"

#: 27
msgid "An XSLT stylesheet load was blocked for security reasons."
msgstr "Lokað var á niðurhal á XSLT stílblaði vegna öryggistástæðna."

#: 28
msgid "Evaluating an invalid expression."
msgstr "Útreikningur leiðir til ógildrar segðar."

#: 29
msgid "Unbalanced curly brace."
msgstr "Misræmi í fjölda oddsviga."

#: 30
msgid "Creating an element with an invalid QName."
msgstr "Bjó til einindi með vitlaust QName."

#: 31
msgid "Variable binding shadows variable binding within the same template."
msgstr "Binding breytu bindir einnig breytur innan sama sniðmáts."

#: 32
msgid "Call to the key function not allowed."
msgstr "Að kalla á key fallið er ekki leyft."

#: LoadingError
msgid "Error loading stylesheet: %S"
msgstr "Villa við að hlaða inn stílblaði: %S"

#: TransformError
msgid "Error during XSLT transformation: %S"
msgstr "Villa í XSLT ummyndun: %S"
