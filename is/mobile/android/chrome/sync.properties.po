#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Síðasta samstilling: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Síðasta samstillingu: Í vinnslu..."

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Reikningur: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Ótengd(ur)"

#: connecting.label
msgid "Connecting…"
msgstr "Tengist..."

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Firefox samstillingar reikningurinn þinn hefur verið fjarlægður"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Afturkalla"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 er ekki samhæfur við nýjustu útgáfuna af Firefox samstillingu. "
"Vinsamlega uppfæra í nýjustu útgáfu."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 er ekki samhæfur við eldri útgáfur af Firefox Sync. Vinsamlega uppfæra "
"Firefox á hinni tölvunni þinni. "

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox samstilling"

#: sync.update.button
msgid "Learn More"
msgstr "Fræðast meira"

#: sync.update.close
msgid "Close"
msgstr "Loka"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Get ekki stillt samstillingu"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Engin internet tenging tiltæk"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%SGat ekki tengst við samstillingu. Viltu reyna aftur?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Reyndu aftur"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Handvirk uppsetning"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Engir flipar frá hinum tölvunum þínum."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Samstilling er splunkunýtt og auðveldara í notkun en áður."

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Snertu hér til að fræðast nánar"
