#. %brandDTD;
#: loadError.label
msgid "Problem loading page"
msgstr "Vandamál við að hlaða inn síðu"

#: retry.label
msgid "Try Again"
msgstr "Reyndu aftur"

#. Specific error messages
#: connectionFailure.title
msgid "Unable to connect"
msgstr "Gat ekki tengst"

#: connectionFailure.longDesc2
msgctxt "connectionFailure.longDesc2"
msgid "&sharedLongDesc3;"
msgstr "&sharedLongDesc3;"

#: deniedPortAccess.title
msgid "This address is restricted"
msgstr "Þetta vistfang er lokað"

#: deniedPortAccess.longDesc
msgctxt "deniedPortAccess.longDesc"
msgid ""
msgstr ""

#: dnsNotFound.title
msgid "Server not found"
msgstr "Netþjónn finnst ekki"

#: dnsNotFound.longDesc3
msgid ""
"<ul> \n"
"<li>Check the address for typing errors such as \n"
"<strong>ww</strong>.example.com instead of \n"
"<strong>www</strong>.example.com</li> \n"
"<li>If you are unable to load any pages, check your device's data or Wi-Fi "
"connection. \n"
"<button id='wifi'>Enable Wi-Fi</button> \n"
"</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Athugaði hvort það séu innsláttar villu í vistfanginu eins og \n"
"<strong>ww</strong>.daemi.is í staðinn fyrir \n"
"<strong>www</strong>.daemi.is</li> \n"
"<li>Ef þú getur ekki hlaðið inn neinum síðum, athugaðu þá gagna- eða WiFi "
"tenginguna á tækinu þínu. \n"
"</li>  \n"
"</ul>"

#: fileNotFound.title
msgid "File not found"
msgstr "Skrá fannst ekki"

#: fileNotFound.longDesc
msgid ""
"<ul> \n"
"<li>Check the file name for capitalization or other typing errors.</li> \n"
"<li>Check to see if the file was moved, renamed or deleted.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Athugaðu hvort það séu innsláttarvillur í skráarnafninu.</li> \n"
"<li>Athugaðu hvort skráin hafi verið færð, endurnefnd eða eytt.</li> \n"
"</ul>"

#: generic.title
msgid "Oops."
msgstr "Tja hérna."

#: generic.longDesc
msgid "<p>&brandShortName; can't load this page for some reason.</p>"
msgstr ""
"<p>&brandShortName; gat af einhverri ástæðu ekki hlaðið inn þessa síðu.</p>"

#: malformedURI.title
msgid "The address isn't valid"
msgstr "Vistfang er ógilt"

#: malformedURI.longDesc
msgid ""
"<ul> \n"
"<li>Web addresses are usually written like \n"
"<strong>http://www.example.com/</strong></li> \n"
"<li>Make sure that you're using forward slashes (i.e. \n"
"<strong>/</strong>).</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Vef vistföng eru vanalega skrifuð svona \n"
"<strong>http://www.daemi.is/</strong></li> \n"
"<li>Gátu hvort þú sért að nota rétt skástrik (sem sagt \n"
"<strong>/</strong>).</li> \n"
"</ul>"

#: netInterrupt.title
msgid "The connection was interrupted"
msgstr "Tengingin varð fyrir truflun"

#: netInterrupt.longDesc2
msgctxt "netInterrupt.longDesc2"
msgid "&sharedLongDesc3;"
msgstr "&sharedLongDesc3;"

#: notCached.title
msgid "Document Expired"
msgstr "Skjal útrunnið"

#: notCached.longDesc
msgid ""
"<p>The requested document is not available in &brandShortName;'s cache.</"
"p><ul><li>As a security precaution, &brandShortName; does not automatically "
"re-request sensitive documents.</li><li>Click Try Again to re-request the "
"document from the website.</li></ul>"
msgstr ""
"<p>Umbeðið skjal er ekki fáanlegt í &brandShortName;'s skyniminninu.</"
"p><ul><li>Sem hluti af varúðarráðstöfun þá biður &brandShortName; ekki "
"sjálfkrafa aftur um viðkvæm skjöl.</li><li>Smellið á Reynið aftur til að "
"biðja aftur um skjalið frá vefsvæðinu.</li></ul>"

#: netOffline.title
msgid "Offline mode"
msgstr "Aftengdur hamur"

#: netOffline.longDesc3
msgid ""
"<ul> \n"
"<li>Try again. &brandShortName; will attempt to open a connection and reload "
"the page. \n"
"<button id='wifi'>Enable Wi-Fi</button> \n"
"</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Reyndu aftur. &brandShortName; mun reyna að opna tengingu og enduhlaða "
"síðuna.  \n"
"<button id='wifi'>Virkja Wi-Fi</button> \n"
"</li> \n"
"</ul>"

#: contentEncodingError.title
msgid "Content Encoding Error"
msgstr "Villa við dulkóðun innihalds"

#: contentEncodingError.longDesc
msgctxt "contentEncodingError.longDesc"
msgid ""
"<ul> \n"
"<li>Please contact the website owners to inform them of this problem.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Vinsamlega hafið samband við eigandur þessa svæðis til að upplýsa þá um "
"þetta vandamál.</li> \n"
"</ul>"

#: unsafeContentType.title
msgid "Unsafe File Type"
msgstr "Óörugg skráartegund"

#: unsafeContentType.longDesc
msgctxt "unsafeContentType.longDesc"
msgid ""
"<ul> \n"
"<li>Please contact the website owners to inform them of this problem.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Vinsamlega hafið samband við eigandur þessa svæðis til að upplýsa þá um "
"þetta vandamál.</li> \n"
"</ul>"

#: netReset.title
msgid "The connection was reset"
msgstr "Tengingin var endursett"

#: netReset.longDesc2
msgctxt "netReset.longDesc2"
msgid "&sharedLongDesc3;"
msgstr "&sharedLongDesc3;"

#: netTimeout.title
msgid "The connection has timed out"
msgstr "Tengingin svaraði ekki tímanlega"

#: netTimeout.longDesc2
msgctxt "netTimeout.longDesc2"
msgid "&sharedLongDesc3;"
msgstr "&sharedLongDesc3;"

#: unknownProtocolFound.title
msgid "The address wasn't understood"
msgstr "Vistfangið skildist ekki"

#: unknownProtocolFound.longDesc
msgid ""
"<ul> \n"
"<li>You might need to install other software to open this address.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Vera kann að þú þurfir að setja upp annað forrit til að opna þetta "
"vistfang.</li> \n"
"</ul>"

#: proxyConnectFailure.title
msgid "The proxy server is refusing connections"
msgstr "Milliþjónn neitar tengingu"

#: proxyConnectFailure.longDesc
msgid ""
"<ul> \n"
"<li>Check the proxy settings to make sure that they are correct.</li> \n"
"<li>Contact your network administrator to make sure the proxy server is \n"
"working.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Athugaðu hvort milliþjóna stillingarnar séu réttar.</li> \n"
"<li>Hafðu samband við kerfisstjórann þinn til að tryggja að milliþjónninn "
"sé \n"
"að virka rétt.</li> \n"
"</ul>"

#: proxyResolveFailure.title
msgid "Unable to find the proxy server"
msgstr "Gat ekki fundið milliþjón"

#: proxyResolveFailure.longDesc3
msgid ""
"<ul> \n"
"<li>Check the proxy settings to make sure that they are correct.</li> \n"
"<li>Check to make sure your device has a working data or Wi-Fi connection. \n"
"<button id='wifi'>Enable Wi-Fi</button> \n"
"</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Athugaðu hvort milliþjóna stillingarnar séu réttar.</li> \n"
"<li>Athugaðu hvort tækið þitt sé með gagna- eða WiFi tengingu sem virkar. \n"
"<button id='wifi'>Virkja Wi-Fi</button> \n"
"</li> \n"
"</ul>"

#: redirectLoop.title
msgid "The page isn't redirecting properly"
msgstr "Síðan er ekki að beinast rétt"

#: redirectLoop.longDesc
msgid ""
"<ul> \n"
"<li>This problem can sometimes be caused by disabling or refusing to "
"accept \n"
"cookies.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Stundum er orsök þessa vandamáls ef neitað er að óvirkjuð meðtaka \n"
"smákökur.</li> \n"
"</ul>"

#: unknownSocketType.title
msgid "Unexpected response from server"
msgstr "Óvænt viðbrögð frá netþjóni"

#: unknownSocketType.longDesc
msgid ""
"<ul> \n"
"<li>Check to make sure your system has the Personal Security Manager \n"
"installed.</li> \n"
"<li>This might be due to a non-standard configuration on the server.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Athugaðu hvort kerfið þitt hafi Personal Security Manager \n"
"uppsettan.</li> \n"
"<li>Vera má að þetta sé vegna þess að netþjónninn er ekki stilltur eins og "
"venjulega.</li> \n"
"</ul>"

#: nssFailure2.title
msgctxt "nssFailure2.title"
msgid "Secure Connection Failed"
msgstr "Örugg tenging mistókst"

#: nssFailure2.longDesc2
msgid ""
"<ul> \n"
"<li>The page you are trying to view cannot be shown because the authenticity "
"of the received data could not be verified.</li> \n"
"<li>Please contact the website owners to inform them of this problem.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Ekki var hægt að sýna síðuna sem þú ert að reyna að skoða því ekki var "
"hægt að auðkenna móttekin gögn.</li> \n"
"<li>Vinsamlega hafið samband við vefsvæðis eigendur og látið þá vita af "
"þessu vandamáli.</li> \n"
"</ul>"

#: nssBadCert.title
msgctxt "nssBadCert.title"
msgid "Secure Connection Failed"
msgstr "Örugg tenging mistókst"

#: nssBadCert.longDesc2
msgid ""
"<ul> \n"
"<li>This could be a problem with the server's configuration, or it could "
"be \n"
"someone trying to impersonate the server.</li> \n"
"<li>If you have connected to this server successfully in the past, the error "
"may \n"
"be temporary, and you can try again later.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Það gæti verið vandamál með uppsetningu netþjónsins eða það gæti verið \n"
"einhver að reyna að þykjast vera þessi netþjónn.</li> \n"
"<li>Ef þú hefur náð að tengjast þessum netþjón áður þá gæti villan verið \n"
"tímabundin og þú getur reynt aftur síðar.</li> \n"
"</ul>"

#: sharedLongDesc3
msgid ""
"<ul> \n"
"<li>The site could be temporarily unavailable or too busy. Try again in a "
"few moments.</li> \n"
"<li>If you are unable to load any pages, check your mobile device's data or "
"Wi-Fi connection. \n"
"<button id='wifi'>Enable Wi-Fi</button> \n"
"</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Svæðið gæti verið ótiltækt tímabundið eða of upptekið til að svara. "
"Reyndu aftur eftir smá tíma.</li> \n"
"<li>Athugaðu gagna- eða WiFi tenginguna á tækinu þínu ef þú nærð ekki að "
"hlaða inn neinum síðum.   \n"
"<button id='wifi'>Virkja Wi-Fi</button>  \n"
"</li> \n"
"</ul>"

#: cspFrameAncestorBlocked.title
msgid "Blocked by Content Security Policy"
msgstr "Innihalds öryggisregla hefur lokað á þetta"

#: cspFrameAncestorBlocked.longDesc
msgid ""
"<p>&brandShortName; prevented this page from loading in this way because the "
"page has a content security policy that disallows it.</p>"
msgstr ""
"<p>&brandShortName; kom í veg fyrir að síðan hlóðst inn á þennan hátt vegna "
"þess að á síðunni er efni með öryggisreglu sem leyfir það ekki.</p>"

#: corruptedContentError.title
msgid "Corrupted Content Error"
msgstr "Brengluð innihalds villa"

#: corruptedContentError.longDesc
msgid ""
"<p>The page you are trying to view cannot be shown because an error in the "
"data transmission was detected.</p><ul><li>Please contact the website owners "
"to inform them of this problem.</li></ul>"
msgstr ""
"<p>Ekki er hægt að sýna síðuna sem þú ert að reyna að skoða því villa "
"greindist í gagna sendingu.</p><ul><li>Vinsamlega hafið samband við "
"vefsvæðis eigendur og látið þá vita af vandamálinu.</li></ul>"

#: securityOverride.linkText
msgid "Or you can add an exception…"
msgstr "Eða þú getur bætt við fráviki..."

#: securityOverride.getMeOutOfHereButton
msgid "Get me out of here!"
msgstr "Hjálpaðu mér út héðan!"

#: securityOverride.exceptionButtonLabel
msgid "Add Exception…"
msgstr "Bæta við fráviki..."

#. <!-- LOCALIZATION NOTE (securityOverride.warningContent) - Do not translate the
#. contents of the <xul:button> tags.  The only language content is the label= field,
#. which uses strings already defined above. The button is included here (instead of
#. netError.xhtml) because it exposes functionality specific to firefox.
#: securityOverride.warningContent
msgid ""
"<p>You should not add an exception if you are using an internet connection "
"that you do not trust completely or if you are not used to seeing a warning "
"for this server.</p> \n"
" \n"
"<button id='getMeOutOfHereButton'>&securityOverride.getMeOutOfHereButton;</"
"button> \n"
"<button id='exceptionDialogButton'>&securityOverride.exceptionButtonLabel;</"
"button>"
msgstr ""
"<p>Þú ættir ekki að bæta við fráviki ef þú ert að nota internets tengingu "
"sem þú treystir ekki fullkomlega eða ef þú sérð vanalega ekki aðvörun fyrir "
"þennan netþjón.</p> \n"
" \n"
"<button id='getMeOutOfHereButton'>&securityOverride.getMeOutOfHereButton;</"
"button> \n"
"<button id='exceptionDialogButton'>&securityOverride.exceptionButtonLabel;</"
"button>"

#: remoteXUL.title
msgid "Remote XUL"
msgstr "Fjartenging XUL"

#: remoteXUL.longDesc
msgid ""
"<p><ul><li>Please contact the website owners to inform them of this problem."
"</li></ul></p>"
msgstr ""
"<p><ul><li>Vinsamlega hafðu samband við eigendur þessa vefsvæðis og látið þá "
"vita af þessu vandamáli.</li></ul></p>"

