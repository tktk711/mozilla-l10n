#. # %1: the user name (Ed), %2: the app name (Firefox), %3: the operating system (Android)
#: client.name2
msgid "%1$S's %2$S on %3$S"
msgstr "%1$S's %2$S on %3$S"

#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Seinasta samstilling: %S"

#. # signInToSync.description is the tooltip for the Sync buttons when Sync is
#. # not configured.
#: signInToSync.description
msgid "Sign In To Sync"
msgstr ""

#: mobile.label
msgid "Mobile Bookmarks"
msgstr "Ytri bókamerki"

#: remote.pending.label
msgid "Remote tabs are being synced…"
msgstr "Verið er að samstilla ytri flipa…"

#: remote.missing2.label
msgid "Sync your other devices again to access their tabs"
msgstr "Samstilltu önnur tæki aftur til að fá aðgang að flipum þeirra"

#: remote.opened.label
msgid "All remote tabs are already open"
msgstr "Allir ytri flipar eru nú þegar opnir"

#: remote.notification.label
msgid "Recent desktop tabs will be available once they sync"
msgstr "Nýlegir flipar verða aðgengilegir um leið og þeir eru samstilltir"

#: error.login.title
msgid "Error While Signing In"
msgstr "Villa við innskráningu"

#: error.login.description
msgid "Sync encountered an error while connecting: %1$S.  Please try again."
msgstr "Sync fékk villu þegar reynt var að tengjast við: %1$S.  Reyndu aftur."

#: error.login.prefs.label error.login.prefs.accesskey
msgid "&Preferences…"
msgstr "Valk&ostir…"

#. # should decide if we're going to show this
#: error.logout.title
msgid "Error While Signing Out"
msgstr "Villa við að skrá út"

#: error.logout.description
msgid ""
"Sync encountered an error while connecting.  It's probably ok, and you don't "
"have to do anything about it."
msgstr ""
"Sync fékk villu þegar reynt var að tengjast.  Þetta er líklega í lagi, "
"þannig að þú þarft ekkert að gera í málinu."

#: error.sync.title
msgid "Error While Syncing"
msgstr "Villa við samstillingu"

#: error.sync.description
msgid ""
"Sync encountered an error while syncing: %1$S.  Sync will automatically "
"retry this action."
msgstr ""
"Sync fékk villu þegar reynt var að samstilla: %1$S.  Sync mun reyna að "
"endurtaka þessa aðgerð."

#: warning.sync.eol.label
msgid "Service Shutting Down"
msgstr "Þjónusta er að stöðvast"

#. # %1: the app name (Firefox)
#: warning.sync.eol.description
msgid ""
"Your Firefox Sync service is shutting down soon. Upgrade %1$S to keep "
"syncing."
msgstr ""
"Firefox Sync þjónustan þín er að hætta bráðlega. Uppfærðu %1$S til að halda "
"áfram að samstilla."

#: error.sync.eol.label
msgid "Service Unavailable"
msgstr "Þjónusta ótiltæk"

#. # %1: the app name (Firefox)
#: error.sync.eol.description
msgid ""
"Your Firefox Sync service is no longer available. You need to upgrade %1$S "
"to keep syncing."
msgstr ""
"Firefox Sync þjónustan þín er ekki lengur aðgengileg. Þú þarft að uppfæra "
"%1$S til að halda áfram að samstilla."

#: sync.eol.learnMore.label sync.eol.learnMore.accesskey
msgid "&Learn more"
msgstr "&Fræðast meira"

#~ msgid ""
#~ "Sync has not been able to complete during the last %1$S days. Please "
#~ "check your network settings."
#~ msgstr ""
#~ "Sync hefur ekki tekist að ljúka samstillingu seinustu %1$S daga. Athugaðu "
#~ "stillingar netkerfis."

#~ msgid "Ser&ver Status"
#~ msgstr "&Staða netþjóns"

#~ msgid "You need to update Firefox Sync to continue syncing your data."
#~ msgstr ""
#~ "Þú þarft að uppfæra Firefox Sync til að geta haldið áfram að samstilla "
#~ "gögn."

#~ msgid "&Update Firefox Sync"
#~ msgstr "&Uppfæra Firefox Sync"

#~ msgid "&Sync Now"
#~ msgstr "&Samstilla núna"

#~ msgid "Approaching Server Quota"
#~ msgstr "Er að nálgast hámark kvóta á netþjóninum"

#~ msgid ""
#~ "You are approaching the server quota. Please review which data to sync."
#~ msgstr ""
#~ "Þú ert að nálgast hámark kvóta á netþjóninum. Farðu yfir hvaða gögn á að "
#~ "samstilla."

#~ msgid "Server Quota Exceeded"
#~ msgstr "Kvóti á netþjóni yfir hámarki"

#~ msgid ""
#~ "Sync failed because it exceeded the server quota. Please review which "
#~ "data to sync."
#~ msgstr ""
#~ "Samstilling mistókst þar sem kvóti á netþjóni er yfir hámarki. Farðu yfir "
#~ "hvaða gögn á að samstilla."

#~ msgid "&View Quota"
#~ msgstr "&Skoða kvóta"
