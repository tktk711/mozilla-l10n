msgid "Mute"
msgstr "Hljóðlaus"

#: muteButton.unmuteLabel
msgid "Unmute"
msgstr "Virkja hljóð"

#: fullscreenButton.enterfullscreenlabel
msgid "Full Screen"
msgstr "Fylla skjá"

#: fullscreenButton.exitfullscreenlabel
msgid "Exit Full Screen"
msgstr "Hætta í fullum skjá"

#: castingButton.castingLabel
msgid "Cast to Screen"
msgstr "Senda á skjá"

#: closedCaption.off
msgid "Off"
msgstr "Óvirkt"

#: stats.media
msgid "Media"
msgstr "Gögn"

#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: stats.size
msgid "Size"
msgstr "Stærð"

#: stats.activity
msgid "Activity"
msgstr "Aðgerð"

#: stats.activityPaused
msgid "Paused"
msgstr "Í bið"

#: stats.activityPlaying
msgid "Playing"
msgstr "Spila"

#: stats.activityEnded
msgid "Ended"
msgstr "Lokið"

#: stats.activitySeeking
msgid "(seeking)"
msgstr "(leita)"

#: stats.volume
msgid "Volume"
msgstr "Bindi"

#: stats.framesParsed
msgid "Frames parsed"
msgstr "Þáttaðir rammar"

#: stats.framesDecoded
msgid "Frames decoded"
msgstr "Afkóðaðir rammar"

#: stats.framesPresented
msgid "Frames presented"
msgstr "Birtir rammar"

#: stats.framesPainted
msgid "Frames painted"
msgstr "Litaðir rammar"

#: error.aborted
msgid "Video loading stopped."
msgstr "Myndbandhleðsla stöðvuð."

#: error.network
msgid "Video playback aborted due to a network error."
msgstr "Ekki er hægt að spila myndband vegna villu í netkerfi."

#: error.decode
msgid "Video can’t be played because the file is corrupt."
msgstr "Ekki er hægt að spila myndband vegna þess að skráin er brengluð."

#: error.srcNotSupported
msgid "Video format or MIME type is not supported."
msgstr "Myndbandasnið eða MIME tegund er ekki með stuðning."

#: error.noSource2
msgid "No video with supported format and MIME type found."
msgstr "Fann ekki myndband sem er með stuðning og rétta MIME tegund."

#: error.generic
msgid "Video playback aborted due to an unknown error."
msgstr "Hætt við spilun á myndbandi vegna óþekktrar villu."

#. the #1 string is the current
#. media position, and the #2 string is the total duration. For example, when at
#. the 5 minute mark in a 6 hour long video, #1 would be "5:00" and #2 would be
#. "6:00:00", result string would be "5:00 of 6:00:00 elapsed".
#: scrubberScale.nameFormat
msgid "#1 of #2 elapsed"
msgstr "#1 af #2 búið"
