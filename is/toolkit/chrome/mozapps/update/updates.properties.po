#. # LOCALIZATION NOTE: The 1st %S is the update name and the 2nd %S is the build
#. # identifier from the local updates.xml for displaying update history
#. # example: MyApplication (20081022033543)
#: updateFullName
msgid "%S (%S)"
msgstr "%S (%S)"

#. # LOCALIZATION NOTE: The 1st %S is brandShortName and 2nd %S is update version
#. # where update version from the update xml
#. # example: MyApplication 10.0.5
#: updateName
msgid "%S %S"
msgstr "%S %S"

#: updateMoreInfoContentDownloading
msgid "Getting more details about %S %S…"
msgstr "Næ í meiri upplýsingar um %S %S…"

#. # LOCALIZATION NOTE: When present
#. # %1$S is the brandShortName. Ex: MyApplication
#. # %2$S is the update version - provided by the update xml. Ex: version 10.0.5
#. # %3$S is the build identifier - provided by the update xml. Ex: 20081022033543
#: updateNightlyName
msgid "%1$S %2$S %3$S nightly"
msgstr "%1$S %2$S %3$S næturútgáfa"

#: intro_major
msgid "Do you want to upgrade to %1$S %2$S now?"
msgstr "Viltu uppfæra í %1$S %2$S núna?"

#: intro_minor
msgid "A security and stability update for %1$S is available:"
msgstr "Öryggis og stöðugleikauppfærsla fyrir %1$S er fáanleg:"

#: updateType_major
msgid "New Version"
msgstr "Ný útgáfa"

#: updateType_minor
msgid "Security Update"
msgstr "Öryggisuppfærsla"

#. # LOCALIZATION NOTE: When present %S is brandShortName
#: verificationError
msgid "%S could not confirm the integrity of the update package."
msgstr "%S gat ekki sannreynt öryggislykil uppfærslunnar."

#: updateMoreInfoContentNotFound
msgid ""
"Additional details about this version could not be found. Please visit the "
"%S homepage for more information."
msgstr ""
"Ekki fundust meiri upplýsingar fyrir þessa útgáfu. Farðu á heimasíðu %S "
"fyrir meiri upplýsingar."

#: resumePausedAfterCloseTitle
msgid "Software Update"
msgstr "Hugbúnaðaruppfærsla"

#: resumePausedAfterCloseMsg
msgid ""
"You have paused downloading this update. Do you want to download the update "
"in the background while you continue to use %S?"
msgstr ""
"Þú hefur sett þessa uppfærslu í bið. Viltu niðurhala uppfærslunni í "
"bakgrunni á meðan þú heldur áfram að nota %S?"

#: updaterIOErrorTitle
msgid "Software Update Failed"
msgstr "Uppsetning hugbúnaðar mistókst"

#: updaterIOErrorMsg
msgid ""
"The update could not be installed. Please make sure there are no other "
"copies of %S running on your computer, and then restart %S to try again."
msgstr ""
"Ekki tókst að setja inn uppfærslu. Gakktu úr skugga um að engin fleiri "
"eintök af %S séu keyrandi, endurræstu svo %S til að reyna aftur."

#: okButton
msgid "OK"
msgstr "Í lagi"

#: okButton.accesskey
msgid "O"
msgstr "l"

#: askLaterButton
msgid "Ask Later"
msgstr "Spyrja seinna"

#: askLaterButton.accesskey
msgctxt "askLaterButton.accesskey"
msgid "A"
msgstr "S"

#: noThanksButton
msgid "No Thanks"
msgstr "Nei takk"

#: noThanksButton.accesskey
msgid "N"
msgstr "N"

#: updateButton_minor
msgid "Update %S"
msgstr "Uppfæra %S"

#: updateButton_minor.accesskey
msgid "U"
msgstr "U"

#: updateButton_major
msgid "Get the New Version"
msgstr "Ná í nýju útgáfuna"

#: updateButton_major.accesskey
msgid "G"
msgstr "g"

#: backButton
msgid "Back"
msgstr "Til baka"

#: backButton.accesskey
msgid "B"
msgstr "b"

#: acceptTermsButton
msgid "Accept Terms"
msgstr "Samþykkja skilmála"

#: acceptTermsButton.accesskey
msgctxt "acceptTermsButton.accesskey"
msgid "A"
msgstr "a"

#. # NOTE: The restartLaterButton string is also used in
#. # mozapps/extensions/content/blocklist.js
#: restartLaterButton
msgid "Restart Later"
msgstr "Endurræsa seinna"

#: restartLaterButton.accesskey
msgid "L"
msgstr "d"

#: restartNowButton
msgid "Restart %S"
msgstr "Endurræsa %S"

#: restartNowButton.accesskey
msgid "R"
msgstr "E"

#. # LOCALIZATION NOTE: %S is the date the update was installed from the local
#. # updates.xml for displaying update history
#: statusSucceededFormat
msgid "Installed on: %S"
msgstr "Sett upp: %S"

#: statusFailed
msgid "Install Failed"
msgstr "Uppsetning mistókst"

#: pauseButtonPause
msgid "Pause"
msgstr "Í bið"

#: pauseButtonResume
msgid "Resume"
msgstr "Halda áfram"

#: hideButton
msgid "Hide"
msgstr "Fela"

#: hideButton.accesskey
msgid "H"
msgstr "F"

#: applyingUpdate
msgid "Applying update…"
msgstr "Virkja uppfærslu…"

#: updatesfound_minor.title
msgid "Update Available"
msgstr "Fann uppfærslu"

#: updatesfound_major.title
msgid "New Version Available"
msgstr "Til er ný útgáfa"

#: installSuccess
msgid "The Update was successfully installed"
msgstr "Uppfærslan var sett upp"

#: installPending
msgid "Install Pending"
msgstr "Uppsetning bíður"

#: patchApplyFailure
msgid "The Update could not be installed (patch apply failed)"
msgstr "Ekki tókst að setja inn uppfærsluna (bót mistókst)"

#: elevationFailure
msgid ""
"You don’t have the permissions necessary to install this update. Please "
"contact your system administrator."
msgstr ""
"Þú hefur ekki nægjanleg réttindi til að setja inn þessa uppfærslu. Hafðu "
"samband við kerfisstjóra."

#. # LOCALIZATION NOTE: %S is the amount downloaded so far
#. # example: Paused —  879 KB of 2.1 MB
#: downloadPausedStatus
msgid "Paused —  %S"
msgstr "Í bið —  %S"

#: check_error-200
msgid "Update XML file malformed (200)"
msgstr "Uppfærslu XML skrá er gölluð (200)"

#: check_error-403
msgid "Access denied (403)"
msgstr "Aðgangur ekki leyfður (403)"

#: check_error-404
msgid "Update XML file not found (404)"
msgstr "Uppfærslu XML skrá fannst ekki (404)"

#: check_error-500
msgid "Internal server error (500)"
msgstr "Innri villa á netþjóni (500)"

#: check_error-2152398849
msgid "Failed (unknown reason)"
msgstr "Mistókst (Óþekkt ástæða)"

#: check_error-2152398861
msgid "Connection refused"
msgstr "Tengingu hafnað"

#: check_error-2152398862
msgid "Connection timed out"
msgstr "Tilraun til að tengjast féll á tíma"

#. # NS_ERROR_OFFLINE
#: check_error-2152398864
msgctxt "check_error-2152398864"
msgid "Network is offline (go online)"
msgstr "Netið er ekki tengt (Tengstu við netið)"

#: check_error-2152398867
msgid "Port not allowed"
msgstr "Þessi gátt er ekki leyfð"

#: check_error-2152398868
msgid "No data was received (please try again)"
msgstr "Engin gögn móttekin (reyndu aftur)"

#: check_error-2152398878
msgid "Update server not found (check your internet connection)"
msgstr "Uppfærsluþjónn fannst ekki (athugaðu nettengingu)"

#: check_error-2152398890
msgid "Proxy server not found (check your internet connection)"
msgstr "Milliþjónn fannst ekki (athugaðu nettengingu)"

#. # NS_ERROR_DOCUMENT_NOT_CACHED
#: check_error-2152398918
msgctxt "check_error-2152398918"
msgid "Network is offline (go online)"
msgstr "Netið er ekki tengt (Tengjast við netið)"

#: check_error-2152398919
msgid "Data transfer was interrupted (please try again)"
msgstr "Gagnaflutningur var truflaður (reyndu aftur)"

#: check_error-2152398920
msgid "Proxy server connection refused"
msgstr "Aðgangur að milliþjóni var hafnað"

#: check_error-2153390069
msgid ""
"Server certificate has expired (please adjust your system clock to the "
"correct date and time if it is incorrect)"
msgstr ""
"Skilríki er útrunnið (Ef þetta er ekki rétt stillið þá klukku tölvunnar til "
"að laga dagsetningu og tíma)"

#: check_error-verification_failed
msgid "The integrity of the update could not be verified"
msgstr "Ekki var hægt að staðfesta heilleika uppfærslunnar"

