#. # LOCALIZATION NOTE (commands):
#. #  %S is a comma separated list of command names.
#: commands
msgid ""
"Commands: %S.\n"
"Use /help &lt;command&gt; for more information."
msgstr ""
"Tiludna: %S.\n"
"Seqdec /help &lt;taladna&gt; i wugar n telɣut."

#. # LOCALIZATION NOTE (noCommand, noHelp):
#. #  %S is the command name the user typed.
#: noCommand
msgid "No '%S' command."
msgstr "Ulac taladna '%S'."

#: noHelp
msgid "No help message for the '%S' command, sorry!"
msgstr "Ulac izen n tallat i tladna '%S', suref iyi!"

#: sayHelpString
msgid "say &lt;message&gt;: send a message without processing commands."
msgstr "inid  &lt;izen&gt;: azen izen s war asenker n tludna."

#: rawHelpString
msgid "raw &lt;message&gt;: send a message without escaping HTML entities."
msgstr "izirig &lt;izen&gt;: azen izen s war asenser n tayunt HTML."

#: helpHelpString
msgid ""
"help &lt;name&gt;: show the help message for the &lt;name&gt; command, or "
"the list of possible commands when used without parameter."
msgstr ""
"mudd tallalt i &lt;isem&gt;: sken izen n tallalt i &lt;isem&gt; taladna, neɣ "
"tabdart n tiludna yellan ticki ittuseqdec s war aɣewwaṛ."

#. # LOCALIZATION NOTE (statusCommand):
#. #  %1$S is replaced with a status command name
#. #   (one of "back", "away", "busy", "dnd", or "offline").
#. #  %2$S is replaced with the localized version of that status type
#. #   (one of the 5 strings below).
#: statusCommand
msgid ""
"%1$S &lt;status message&gt;: set the status to %2$S with an optional status "
"message."
msgstr ""
"%1$S &lt;izen n waddad&gt;: sbadu addad ɣer %2$S s yizen n waddad afrayan.."

#: back
msgid "available"
msgstr "yella"

#: away
msgid "away"
msgstr "ulac-it"

#: busy
msgctxt "busy"
msgid "unavailable"
msgstr "ulac-it"

#: dnd
msgctxt "dnd"
msgid "unavailable"
msgstr "ulac-it"

#: offline
msgid "offline"
msgstr "aruqqin"
