#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Asemtawi aneggaru: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Asemtawi aneggaru: iteddu…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Amiḍan: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Ur iqqin ara"

#: connecting.label
msgid "Connecting…"
msgstr "Tuqqna…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Amiḍan inek Sync n Firefox ur yeqqin ara"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Sefsex"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 ur yemṣadi ara akked lqem aneggaru n Firefox Sync. Ma ulac aɣilif "
"leqqem s lqem aneggaru."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 ur imṣadi ara akked ileqman iqbuṛen n Firefox Sync. Ma ulac aɣilif "
"leqqem Firefox ɣef uselkim (i-en) nniḍen."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Issin ugar"

#: sync.update.close
msgid "Close"
msgstr "Mdel"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Ur izmir ara ad iswel Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Ulac tuqqna Internet"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S ur izmir ara ad yeqqen ɣer Sync. Tebɣiḍ ad tɛerḍeḍ tikelt nniḍen?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Ɛreḍ tikelt nniḍen"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Aswel s ufus"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Ulac iccaren n iselkimen inek nniḍen."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Sync akken d amaynut udiɣ d afessas. "

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Senned da iwakken ad tissineḍ ugar"
