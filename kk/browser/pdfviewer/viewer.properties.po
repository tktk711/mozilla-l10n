#: print.title
msgctxt "print.title"
msgid "Print"
msgstr "Баспаға шығару"

#: print_label
msgctxt "print_label"
msgid "Print"
msgstr "Баспаға шығару"

#: presentation_mode.title
msgid "Switch to Presentation Mode"
msgstr "Презентация режиміне ауысу"

#: presentation_mode_label
