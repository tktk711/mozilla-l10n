#: startup.label
msgid "Startup"
msgstr "ចាប់ផ្ដើម​ឡើង"

#: startupPage.label startupPage.accesskey
msgid "When &brandShortName; &starts:"
msgstr "ពេល &brandShortName; &ចាប់ផ្ដើម ៖"

#: startupHomePage.label
msgid "Show my home page"
msgstr "បង្ហាញ​គេហ​ទំព័រ​របស់​ខ្ញុំ"

#: startupBlankPage.label
msgid "Show a blank page"
msgstr "បង្ហាញ​ទំព័រ​ទទេ"

#: startupLastSession.label
msgid "Show my windows and tabs from last time"
msgstr "បង្ហាញ​បង្អួច និង​ផ្ទាំង​របស់​ខ្ញុំ​កាល​ពី​មុន"

#: homepage.label homepage.accesskey
msgid "Home &Page:"
msgstr "គេហ​&ទំព័រ ៖"

#: useCurrentPage.label useCurrentPage.accesskey
msgid "Use &Current Page"
msgstr "ប្រើ​ទំព័រ​&បច្ចុប្បន្ន"

#: useMultiple.label
msgid "Use Current Pages"
msgstr "ប្រើ​ទំព័រ​បច្ចុប្បន្ន"

#: chooseBookmark.label chooseBookmark.accesskey
msgid "Use &Bookmark…"
msgstr "ប្រើ​&ចំណាំ…"

#: restoreDefault.label restoreDefault.accesskey
msgid "&Restore to Default"
msgstr "&ស្ដារ​ទៅ​លំនាំ​ដើម"

#: downloads.label
msgid "Downloads"
msgstr "ទាញ​យក"

#: saveTo.label saveTo.accesskey
msgid "Sa&ve files to"
msgstr "រ&ក្សា​ទុក​ឯកសារ​ទៅ"

#: chooseFolderWin.label chooseFolderWin.accesskey
msgid "Br&owse…"
msgstr "រក​មើ&ល…"

#: chooseFolderMac.label chooseFolderMac.accesskey
msgid "Choos&e…"
msgstr "ជ្រើ&ស…"

#: alwaysAsk.label alwaysAsk.accesskey
msgid "&Always ask me where to save files"
msgstr "សួ&រ​ខ្ញុំ​ជានិច្ច​​អំពី​កន្លែង​ដែល​ត្រូវ​រក្សាទុក​ឯកសារ "

#: alwaysCheckDefault2.label alwaysCheckDefault2.accesskey
#, fuzzy
msgid "Alwa&ys check if &brandShortName; is your default browser"
msgstr "ពិនិត្យ&ជានិច្ច ថា &brandShortName; ជា​កម្មវិធី​អ៊ីនធឺណិត​លំនាំដើម"

#: setAsMyDefaultBrowser2.label setAsMyDefaultBrowser2.accesskey
msgid "Make &Default"
msgstr "ធ្វើឲ្យ&ជា​លំនាំដើម"

#: isDefault.label
msgid "&brandShortName; is currently your default browser"
msgstr "&brandShortName; បច្ចុប្បន្ន​ជា​កម្មវិធី​រុករក​លំនាំដើម​របស់​អ្នក"

#: isNotDefault.label
msgid "&brandShortName; is not your default browser"
msgstr "&brandShortName; វា​មិន​មែន​ជា​កម្មវិធី​រុករក​លំនាំដើម​របស់​អ្នក"

#: separateProfileMode.label
msgid "Allow &brandShortName; and Firefox to run at the same time"
msgstr "អនុញ្ញាត​ឲ្យ &brandShortName; និង Firefox ដំណើរការ​ក្នុង​ពេល​ដូចគ្នា"

#: useFirefoxSync.label
msgid "Tip: This uses separate profiles. Use Sync to share data between them."
msgstr ""
"ព័ត៌មាន​ជំនួយ៖ វា​ប្រើ​បវត្តិរូប​ដាច់ដោយឡែក។ ប្រើ​ការ​ធ្វើ​សមកាលកម្ម​ដើម្បី​ចែករំលែក​ទិន្នន័យ​រវាង​ពួកគេ។"

#: getStarted.label
msgid "Start using Sync…"
msgstr "ចាប់ផ្ដើម​ប្រើ​សមកាលកម្ម..."

#: e10sEnabled.label
msgid "Enable multi-process &brandShortName;"
msgstr "អនុញ្ញាត​ &brandShortName; ដំណើរការ​ច្រើន​ប្រភេទ​​"

#~ msgid ""
#~ "Always check to see if &brandShortName; is the default browser on startup"
#~ msgstr ""
#~ "ពិនិត្យមើល​ជានិច្ច ថា​តើ &brandShortName; គឺជា​កម្មវិធី​រុករក​លំនាំដើម នៅ​ពេល​ចាប់ផ្ដើម​ដែរ​ឬទេ"

#~ msgid "Make &brandShortName; My &Default Browser"
#~ msgstr "ធ្វើឲ្យ &brandShortName; ជា​កម្មវិធី​រុករក​លំនាំដើម"

#~ msgid "w"
#~ msgstr "w"

#~ msgid "Show the &Downloads window when downloading a file"
#~ msgstr "បង្ហាញ​បង្អួច​ទាញ​យក ពេល​កំពុង​ទាញ​យក​ឯកសារ"

#~ msgid "Close it &when all downloads are finished"
#~ msgstr "បិទ ពេល​ការ​ទាញយក​ទាំងអស់​ត្រូវ​បាន​បញ្ចប់"
