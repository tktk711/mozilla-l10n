#. # Main toolbar buttons (tooltips and alt text for images)
#: previous.title
msgid "Previous Page"
msgstr "ទំព័រ​មុន"

#: previous_label
msgctxt "previous_label"
msgid "Previous"
msgstr "មុន"

#: next.title
msgid "Next Page"
msgstr "ទំព័រ​បន្ទាប់"

#: next_label
msgctxt "next_label"
msgid "Next"
msgstr "បន្ទាប់"

#. # LOCALIZATION NOTE (page_label, page_of):
#. # These strings are concatenated to form the "Page: X of Y" string.
#. # Do not translate "{{pageCount}}", it will be substituted with a number
#. # representing the total number of pages.
#: page_label
msgid "Page:"
msgstr "ទំព័រ ៖"

#: page_of
msgid "of {{pageCount}}"
msgstr "នៃ {{pageCount}}"

#: zoom_out.title
msgctxt "zoom_out.title"
msgid "Zoom Out"
msgstr "​បង្រួម"

#: zoom_out_label
msgctxt "zoom_out_label"
msgid "Zoom Out"
msgstr "​បង្រួម"

#: zoom_in.title
msgctxt "zoom_in.title"
msgid "Zoom In"
msgstr "​ពង្រីក"

#: zoom_in_label
msgctxt "zoom_in_label"
msgid "Zoom In"
msgstr "​ពង្រីក"

#: zoom.title
msgid "Zoom"
msgstr "ពង្រីក"

#: print.title
msgctxt "print.title"
msgid "Print"
msgstr "បោះពុម្ព"

#: print_label
msgctxt "print_label"
msgid "Print"
msgstr "បោះពុម្ព"

#: presentation_mode.title
msgid "Switch to Presentation Mode"
msgstr "ប្ដូរ​ទៅ​របៀប​បទ​បង្ហាញ"

#: presentation_mode_label
msgid "Presentation Mode"
msgstr "របៀប​បទ​បង្ហាញ"

#: open_file.title
msgid "Open File"
msgstr "បើក​ឯកសារ"

#: open_file_label
msgid "Open"
msgstr "បើក"

#: download.title
msgctxt "download.title"
msgid "Download"
msgstr "ទាញ​យក"

#: download_label
msgctxt "download_label"
msgid "Download"
msgstr "ទាញ​យក"

#: bookmark.title
msgid "Current view (copy or open in new window)"
msgstr "ទិដ្ឋភាព​បច្ចុប្បន្ន (ចម្លង ឬ​បើក​នៅ​ក្នុង​បង្អួច​ថ្មី)"

#: bookmark_label
msgid "Current View"
msgstr "ទិដ្ឋភាព​បច្ចុប្បន្ន"

#. # Tooltips and alt text for side panel toolbar buttons
#. # (the _label strings are alt text for the buttons, the .title strings are
#. # tooltips)
#: toggle_slider.title
msgctxt "toggle_slider.title"
msgid "Toggle Slider"
msgstr "បិទ/បើក​គ្រាប់​រំកិល"

#: toggle_slider_label
msgctxt "toggle_slider_label"
msgid "Toggle Slider"
msgstr "បិទ/បើក​គ្រាប់​រំកិល"

#: outline.title
msgid "Show Document Outline"
msgstr "បង្ហាញ​គ្រោង​ឯកសារ"

#: outline_label
msgid "Document Outline"
msgstr "គ្រោង​ឯកសារ"

#: thumbs.title
msgid "Show Thumbnails"
msgstr "បង្ហាញ​រូបភាព​តូចៗ"

#: thumbs_label
msgid "Thumbnails"
msgstr "រួបភាព​តូចៗ"

#: findbar.title
msgid "Find in Document"
msgstr "រក​នៅ​ក្នុង​ឯកសារ"

#: findbar_label
msgid "Find"
msgstr "រក"

#. # Document outline messages
#: no_outline
msgid "No Outline Available"
msgstr "គ្មាន​គ្រោង​ទេ"

#. # Thumbnails panel item (tooltip and alt text for images)
#. # LOCALIZATION NOTE (thumb_page_title): "{{page}}" will be replaced by the page
#. # number.
#: thumb_page_title
msgid "Page {{page}}"
msgstr "ទំព័រ {{page}}"

#. # LOCALIZATION NOTE (thumb_page_canvas): "{{page}}" will be replaced by the page
#. # number.
#: thumb_page_canvas
msgid "Thumbnail of Page {{page}}"
msgstr "រូបភាព​តូច​របស់​ទំព័រ {{page}}"

#. # Context menu
#: first_page.label
msgid "Go to First Page"
msgstr "ទៅ​ទំព័រ​ដំបូង"

#: last_page.label
msgid "Go to Last Page"
msgstr "ទៅ​ទំព័រ​ចុងក្រោយ"

#: page_rotate_cw.label
msgid "Rotate Clockwise"
msgstr "បង្វិល​ស្រប​ទ្រនិច​នាឡិកា"

#: page_rotate_ccw.label
msgid "Rotate Counterclockwise"
msgstr "បង្វិល​ច្រាស់​ទ្រនិច​នាឡិកា​​"

#. # Find panel button title and messages
#: find_label
msgid "Find:"
msgstr "រក ៖"

#: find_previous.title
msgid "Find the previous occurrence of the phrase"
msgstr "រក​ពាក្យ ឬ​ឃ្លា​ដែល​បាន​ជួប​មុន"

#: find_previous_label
msgctxt "find_previous_label"
msgid "Previous"
msgstr "មុន"

#: find_next.title
msgid "Find the next occurrence of the phrase"
msgstr "រក​ពាក្យ ឬ​ឃ្លា​ដែល​បាន​ជួប​បន្ទាប់"

#: find_next_label
msgctxt "find_next_label"
msgid "Next"
msgstr "បន្ទាប់"

#: find_highlight
msgid "Highlight all"
msgstr "បន្លិច​ទាំងអស់"

#: find_match_case_label
msgid "Match case"
msgstr "ករណី​ដំណូច"

#: find_reached_top
msgid "Reached top of document, continued from bottom"
msgstr "បាន​បន្ត​ពី​ខាង​ក្រោម ទៅ​ដល់​ខាង​​លើ​នៃ​ឯកសារ"

#: find_reached_bottom
msgid "Reached end of document, continued from top"
msgstr "បាន​បន្ត​ពី​ខាងលើ ទៅដល់​ចុង​​នៃ​ឯកសារ"

#: find_not_found
msgid "Phrase not found"
msgstr "រក​មិន​ឃើញ​ពាក្យ ឬ​ឃ្លា"

#. # Error panel labels
#: error_more_info
msgid "More Information"
msgstr "ព័ត៌មាន​បន្ថែម"

#: error_less_info
msgid "Less Information"
msgstr "ព័ត៌មាន​តិចតួច"

#: error_close
msgid "Close"
msgstr "បិទ"

#. # LOCALIZATION NOTE (error_build): "{{build}}" will be replaced by the PDF.JS
#. # build ID.
#: error_build
msgid "PDF.JS Build: {{build}}"
msgstr "បង្កើត PDF.JS ៖ {{build}}"

#. # LOCALIZATION NOTE (error_message): "{{message}}" will be replaced by an
#. # english string describing the error.
#: error_message
msgid "Message: {{message}}"
msgstr "សារ ៖ {{message}}"

#. # LOCALIZATION NOTE (error_stack): "{{stack}}" will be replaced with a stack
#. # trace.
#: error_stack
msgid "Stack: {{stack}}"
msgstr "ជង់ ៖ {{stack}}"

#. # LOCALIZATION NOTE (error_file): "{{file}}" will be replaced with a filename
#: error_file
msgid "File: {{file}}"
msgstr "ឯកសារ ៖ {{file}}"

#. # LOCALIZATION NOTE (error_line): "{{line}}" will be replaced with a line number
#: error_line
msgid "Line: {{line}}"
msgstr "ជួរ ៖ {{line}}"

#: rendering_error
msgid "An error occurred while rendering the page."
msgstr "មាន​កំហុស​បាន​កើតឡើង​ពេល​បង្ហាញ​ទំព័រ ។"

#. # Predefined zoom values
#: page_scale_width
msgid "Page Width"
msgstr "ទទឹង​ទំព័រ"

#: page_scale_fit
msgid "Page Fit"
msgstr "សម​ទំព័រ"

#: page_scale_auto
msgid "Automatic Zoom"
msgstr "ពង្រីក​ស្វ័យប្រវត្តិ"

#: page_scale_actual
msgid "Actual Size"
msgstr "ទំហំ​ជាក់ស្ដែង"

#. # Loading indicator messages
#: loading_error_indicator
msgid "Error"
msgstr "កំហុស"

#: loading_error
msgid "An error occurred while loading the PDF."
msgstr "មាន​កំហុស​បាន​កើតឡើង​ពេល​កំពុង​ផ្ទុក PDF ។"

#: invalid_file_error
msgid "Invalid or corrupted PDF file."
msgstr "ឯកសារ PDF ខូច ឬ​មិន​ត្រឹមត្រូវ ។"

#. # LOCALIZATION NOTE (text_annotation_type): This is used as a tooltip.
#. # "{{type}}" will be replaced with an annotation type from a list defined in
#. # the PDF spec (32000-1:2008 Table 169 – Annotation types).
#. # Some common types are e.g.: "Check", "Text", "Comment", "Note"
#: text_annotation_type
msgid "[{{type}} Annotation]"
msgstr "[{{type}} ចំណារ​ពន្យល់]"

#: request_password
msgid "PDF is protected by a password:"
msgstr "PDF ត្រូវ​បាន​ការពារ​ដោយ​ពាក្យសម្ងាត់ ៖"

#: printing_not_supported
msgid "Warning: Printing is not fully supported by this browser."
msgstr "ការ​ព្រមាន ៖ កា​រ​បោះពុម្ព​មិន​ត្រូវ​បាន​គាំទ្រ​ពេញលេញ​ដោយ​កម្មវិធី​រុករក​នេះ​ទេ ។"
