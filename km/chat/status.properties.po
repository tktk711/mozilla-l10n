#: availableStatusType
msgid "Available"
msgstr "មាន"

#: awayStatusType
msgid "Away"
msgstr "ចាក​ឆ្ងាយ"

#: unavailableStatusType
msgid "Unavailable"
msgstr "ដែល​មិន​មាន​"

#: offlineStatusType
msgid "Offline"
msgstr "ក្រៅ​បណ្ដាញ"

#: invisibleStatusType
msgid "Invisible"
msgstr "ដែល​មិន​អាច​មើល​ឃើញ"

#: idleStatusType
msgid "Idle"
msgstr "ទំនេរ"

#: mobileStatusType
msgid "Mobile"
msgstr "ចល័ត"

#. # LOCALIZATION NOTE (unknownStatusType):
#. # the status of a buddy is unknown when it's in the list of a disconnected account
#: unknownStatusType
msgid "Unknown"
msgstr "មិន​ស្គាល់"

#. # LOCALIZATION NOTE (statusWithStatusMessage):
#. # Used to display the status of a buddy together with its status message.
#. # %1$S is the status type, %2$S is the status message text.
#: statusWithStatusMessage
msgid "%1$S - %2$S"
msgstr "%1$S - %2$S"

#. # LOCALIZATION NOTE (messenger.status.defaultIdleAwayMessage):
#. #  This will be the away message put automatically when the user is idle.
#: messenger.status.defaultIdleAwayMessage
msgid "I am currently away from the computer."
msgstr "បច្ចុប្បន្ន​ខ្ញុំ​ចាក​ឆ្ងាយ​ពី​កុំព្យូទ័រ។"
