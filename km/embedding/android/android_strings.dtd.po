#: splash_firstrun
msgid "Setting up &brandShortName;\\u2026"
msgstr "កំពុង​រៀបចំ &brandShortName;\\u2026"

#: no_space_to_start_error
msgid "There is not enough space available for &brandShortName; to start."
msgstr "មិន​មាន​ទំហំ​គ្រប់គ្រាន់​សម្រាប់​ឲ្យ &brandShortName; ចាប់ផ្ដើម​ទេ ។"

#: error_loading_file
msgid ""
"An error occurred when trying to load files required to run &brandShortName;"
msgstr "កំហុស​បាន​កើតឡើង​ពេល​ព្យាយាម​ផ្ទុក​ឯកសារ​ដែល​បាន​ទាមទារ​ដើម្បី​ដំណើរការ &brandShortName;"

#: crash_reporter_title
msgid "&brandShortName; Crash Reporter"
msgstr "កម្មវិធី​ផ្ដល់​របាយការណ៍​គាំង &brandShortName;"

#: crash_message
msgid ""
"&brandShortName; has crashed. Your tabs should be listed on the "
"&brandShortName; Start page when you restart."
msgstr ""
"&brandShortName; បាន​គាំង ។ ផ្ទាំង​របស់​អ្នក​គួរ​ត្រូវ​បាន​រាយ​នៅ​លើ​ទំព័រ​ចាប់ផ្ដើម "
"&brandShortName; ពេល​អ្នក​ចាប់ផ្ដើម​ឡើងវិញ ។"

#: crash_help_message
msgid "Please help us fix this problem!"
msgstr "សូម​ជួយ​ពួក​យើង​ដើម្បី​ដោះស្រាយ​បញ្ហា​នេះ !"

#: crash_send_report_message
msgid "Send Mozilla a crash report"
msgstr "ផ្ញើ​របាយការណ៍​គាំង​ទៅ Mozilla"

#: crash_include_url
msgid "Include page address"
msgstr "រួមបញ្ចូល​អាសយដ្ឋាន​ទំព័រ"

#: crash_close_label
msgid "Close"
msgstr "បិទ"

#: crash_restart_label
msgid "Restart &brandShortName;"
msgstr "ចាប់ផ្ដើម &brandShortName; ឡើង​វិញ"

#: sending_crash_report
msgid "Sending crash report\\u2026"
msgstr "កំពុង​ផ្ញើ​របាយការណ៍​គាំង\\u2026"

#: exit_label
msgid "Exit"
msgstr "ចេញ"

#: continue_label
msgid "Continue"
msgstr "បន្ត"

#: launcher_shortcuts_title
msgid "&brandShortName; Web Apps"
msgstr "&brandShortName; កម្មវិធី​បណ្ដាញ"

#: launcher_shortcuts_empty
msgid "No web apps were found"
msgstr "រក​មិន​ឃើញ​កម្មវិធី​បណ្ដាញ"

#: choose_file
msgid "Choose File"
msgstr "ជ្រើស​ឯកសារ"
