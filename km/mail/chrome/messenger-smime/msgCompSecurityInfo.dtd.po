#: view.label view.accesskey
msgid "&View"
msgstr "Տեսքը"

#: tree.recipient
msgid "Recipient"
msgstr "Ստացողը"

#: tree.status
msgid "Status"
msgstr "Վիճակը"

#: tree.issuedDate
msgid "Issued"
msgstr "Տրված"

#: tree.expiresDate
