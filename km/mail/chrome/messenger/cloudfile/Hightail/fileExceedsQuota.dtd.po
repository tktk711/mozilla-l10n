#: fileExceedsQuota.title
msgid "Hightail"
msgstr "ប្រញាប់ប្រញាល់"

#: fileExceedsQuota.storageLimitReached
msgid "Storage limit reached"
msgstr "បាន​ដល់​ដែន​កំណត់​ផ្ទុក"

#: fileExceedsQuota.description
msgid "You have reached your account storage limit of 2GB."
msgstr "អ្នក​បាន​ដល់​ដែន​កំណត់​ទំហំ​ផ្ទុក​គណនី​របស់​អ្នក​នៃ ២ ជីកាបៃ។"

#: fileExceedsQuota.description1
msgid "Your current storage usage is #XXX GB."
msgstr "ការ​ប្រើ​ទំហំ​ផ្ទុក​បច្ចុប្បន្ន​របស់​អ្នក​គឺ #XXX ជីកាបៃ។"

#~ msgid "Upgrade"
#~ msgstr "បង្កើន​ទំហំ"

#~ msgid "Cancel"
#~ msgstr "បោះបង់"

#~ msgid "Upgrade today to Pro Plus account and get more storage."
#~ msgstr "បង្កើន​ទំហំ​ទៅ​គណនី Pro Plus ថ្ងៃនេះ នឹង​ទទួល​បាន​ទំហំ​ផ្ទុក​បន្ថែម។"

#~ msgid ""
#~ "You can also choose to delete files in your account to create more space."
#~ msgstr "អ្នក​អាច​លុប​ឯកសារ​នៅ​ក្នុង​គណនី​របស់​អ្នក​ដើម្បី​បង្កើន​ទំហំ។"

#~ msgid "YouSendIt"
#~ msgstr "YouSendIt"
