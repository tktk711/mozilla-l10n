#. Mail Content
#: captionMailContent.label
msgid "Mail Content"
msgstr "មាតិកា​អ៊ីមែល"

#: acceptRemoteContent.label acceptRemoteContent.accesskey
msgid "Allow re&mote content in messages"
msgstr "អនុញ្ញាត​មាតិកា​ពី​ចម្ងាយ​ក្នុងសារ"

#: remoteContentExceptions.label remoteContentExceptions.accesskey
msgid "E&xceptions…"
msgstr "ករណី​លើកលែង…"

#: acceptRemoteContentInfo.label
msgid "Learn more about the privacy issues of remote content"
msgstr "ស្វែងយល់​បន្ថែម​អំពី​បញ្ហា​ភាព​ឯកជន​នៃ​មាតិកា​ពី​ចម្ងាយ"

#. Web Content
#: captionWebContent.label
msgid "Web Content"
msgstr "Web Content"

#: keepHistory.label keepHistory.accesskey
msgid "&Remember websites and links I've visited"
msgstr "ចងចាំ​វេបសាយ និង​តំណ​ដែលខ្ញុំ​បានមើល"

#: acceptCookies.label acceptCookies.accesskey
msgid "&Accept cookies from sites"
msgstr "ទទួលយក​ខូគី​ពី​តំបន់​បណ្ដាញ"

#: acceptThirdParty.pre.label acceptThirdParty.pre.accesskey
msgid "A&ccept third-party cookies:"
msgstr "ព្រម​ទទួល​ខូគី​ភាគី​ទីបី៖"

#: acceptThirdParty.always.label
msgid "Always"
msgstr "ជានិច្ច"

#: acceptThirdParty.never.label
msgid "Never"
msgstr "មិន"

#: acceptThirdParty.visited.label
msgid "From visited"
msgstr "ពី​អ្វី​ដែល​បាន​ទស្សនា"

#: keepUntil.label keepUntil.accesskey
msgid "&Keep until:"
msgstr "&Keep until:"

#: expire.label
msgid "they expire"
msgstr "they expire"

#: close.label
msgid "I close &brandShortName;"
msgstr "I close &brandShortName;"

#: askEachTime.label
msgid "ask me every time"
msgstr "ask me every time"

#: cookieExceptions.label cookieExceptions.accesskey
msgid "&Exceptions…"
msgstr "&Exceptions…"

#: showCookies.label showCookies.accesskey
msgid "&Show Cookies…"
msgstr "&Show Cookies…"

#: dntTrackingNopref.label2
msgid "Do not tell sites anything about my tracking preferences"
msgstr "កុំ​ប្រាប់​តំបន់បណ្ដាញ​អំពី​ចំណូលចិត្ត​ការ​តាមដាន​របស់​ខ្ញុំ"

#: dntTrackingNopref.accesskey
msgid "o"
msgstr "o"

#: dntTrackingNotOkay.label2
msgid "Tell sites that I do not want to be tracked"
msgstr "ប្រាប់​តំបន់បណ្ដាញ​ថា​អ្នក​មិន​ចង់​​ឲ្យ​គេ​តាមដាន"

#: dntTrackingNotOkay.accesskey
msgid "n"
msgstr "n"

#: dntTrackingOkay.label2
msgid "Tell sites that I want to be tracked"
msgstr "ប្រាប់​តំបន់​បណ្ដាញ​ថា​ខ្ញុំ​ចង់​ឲ្យ​គេ​តាមដាន"

#: dntTrackingOkay.accesskey
msgid "t"
msgstr "t"

#: doNotTrackInfo.label
msgid "Learn more about Do Not Track"
msgstr "ស្វែងយល់​បន្ថែម​អំពី Do Not Track"
