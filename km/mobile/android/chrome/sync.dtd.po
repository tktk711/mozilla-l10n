msgid "Not connected"
msgstr "មិន​បាន​តភ្ជាប់"

#: sync.connect
msgctxt "sync.connect"
msgid "Connect"
msgstr "តភ្ជាប់"

#: sync.connected
msgid "Connected"
msgstr "បាន​តភ្ជាប់"

#: sync.details
msgid "Details"
msgstr "សេចក្ដី​លម្អិត"

#: sync.deviceName
msgid "This device"
msgstr "ឧបរកណ៍​នេះ"

#: sync.disconnect
msgid "Disconnect"
msgstr "ផ្ដាច់"

#: sync.syncNow
msgid "Sync Now"
msgstr "ធ្វើ​សមកាលកម្ម​ឥឡូវ"

#: sync.setup.title
msgid "Connect to Sync"
msgstr "តភ្ជាប់​ទៅ​សមកាលកម្ម"

#: sync.setup.pair
msgid "To activate, select &#x0022;Pair a Device&#x0022; on your other device."
msgstr "ដើម្បី​ធ្វើ​ឲ្យ​សកម្ម ជ្រើស &#x0022;ផ្គូ​ឧបករណ៍&#x0022; នៅ​លើ​ឧបករណ៍​ផ្សេងទៀត​របស់​អ្នក ។"

#: sync.fallback
msgid "I'm not near my computer…"
msgstr "ខ្ញុំ​មិន​នៅ​ជិត​កុំព្យូទ័រ​ទេ..."

#: sync.setup.manual
msgid "Enter your Sync account information"
msgstr "បញ្ចូល​ព័ត៌មាន​គណនី​សមកាលកម្ម​របស់​អ្នក"

#: sync.account
msgid "Account Name"
msgstr "ឈ្មោះ​គណនី"

#: sync.password
msgid "Password"
msgstr "ពាក្យសម្ងាត់"

#: sync.recoveryKey
msgid "Recovery Key"
msgstr "កូនសោ​សង្គ្រោះ"

#: sync.customServer
msgid "Use custom server"
msgstr "ប្រើ​ម៉ាស៊ីន​មេ​ផ្ទាល់ខ្លួន"

#: sync.serverURL
msgid "Server URL"
msgstr "URL ម៉ាស៊ីន​មេ"

#: sync.setup.connect
msgctxt "sync.setup.connect"
msgid "Connect"
msgstr "តភ្ជាប់"

#: sync.setup.cancel
msgid "Cancel"
msgstr "បោះបង់"

#: sync.setup.tutorial
msgid "Show me how"
msgstr "បង្ហាញ​ខ្ញុំ​ពី​របៀប"

#: sync.setup.waiting2
msgid "Waiting for other device…"
msgstr "រង់ចាំ​ឧបករណ៍​ផ្សេងទៀត…"

#: sync.pair.title
msgid "Pair a Device"
msgstr "ផ្គូ​ឧបករណ៍"

#: sync.pair.description
msgid ""
"To activate your new device, select &#x0022;Set Up Sync&#x0022; on the "
"device."
msgstr ""
"ដើម្បី​ធ្វើ​ឲ្យ​ឧបករណ៍​ថ្មី​របស់​អ្នក​សកម្ម ជ្រើស &#x0022;រៀបចំ​សមកាលកម្ម&#x0022; នៅ​លើ​ឧបករណ៍ ។"

#: sync.setup.close
msgid "Close"
msgstr "បិទ"

#: sync.setup.waitingdownload
msgid ""
"Your data is now being downloaded in the background. You can close this "
"window at any time."
msgstr ""
"ទិន្នន័យ​របស់​អ្នក​កំពុង​ត្រូវ​បាន​ទាញ​យក​នៅ​ក្នុង​ផ្ទៃ​ខាងក្រោយ ។ អ្នក​អាច​បិទ​បង្អួច​នេះ​នៅ​ពេល​ណា​ក៏​បាន ។"

