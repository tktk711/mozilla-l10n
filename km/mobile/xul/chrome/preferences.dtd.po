#: prefsHeader.label
msgid "Preferences"
msgstr "ចំណូលចិត្ត"

#: about.title
msgid "About &brandShortName;"
msgstr "អំពី &brandShortName;"

#: about.button
msgid "Go to Page"
msgstr "ទៅកាន់​ទំព័រ"

#: content.title
msgid "Content"
msgstr "មាតិកា"

#: reflowZoom.title
msgid "Reformat text on zoom"
msgstr "ធ្វើ​ទ្រង់ទ្រាយ​អត្ថបទ​ឡើងវិញ​ពេល​ពង្រីក"

#: showImages.title
msgid "Show images"
msgstr "បង្ហាញ​រូបភាព"

#: enableJavaScript.title
msgid "Enable JavaScript"
msgstr "បើក &JavaScript"

#: privacy.title
msgid "Privacy &amp; Security"
msgstr "ភាព​ឯកជន &amp; ដែល​មាន​សុវត្ថិភាព"

#: allowCookies.title
msgid "Allow cookies"
msgstr "អនុញ្ញាត​ខូគី"

#: doNotTrack.title
msgid "Tell sites not to track me"
msgstr "ប្រាប់​តំបន់​បណ្ដាញ​នេះ​ថា​​កុំ​តាមដាន​ខ្ញុំ"

#: masterPassword.title
msgid "Use master password"
msgstr "ប្រើ​ពាក្យ​សម្ងាត់​មេ "

#: clearPrivateData2.title
msgid "Clear private data"
msgstr "សម្អាត​ទិន្នន័យ​ឯកជន"

#: clearPrivateData.button
msgid "Clear"
msgstr "សម្អាត"

#: rememberPasswords.title
msgid "Remember passwords"
msgstr "ចងចាំ​ពាក្យសម្ងាត់"

#: language.title
msgid "Language"
msgstr "ភាសា"

#: language.auto
msgid "Auto-detect"
msgstr "រក​ឃើញ​​ស្វ័យ​ប្រវត្តិ"

#: defaultBrowser.title
msgid "Default Browser"
msgstr "កម្មវិធី​រុករក​លំនាំដើម"

#: defaultBrowser.description
msgid "Make &brandShortName; your default browser"
msgstr "ធ្វើឲ្យ &brandShortName; ជា​កម្មវិធី​រុករក​លំនាំដើម​របស់​អ្នក"

#: homepage.title
msgid "Start page"
msgstr "ចាប់ផ្ដើម​ទំព័រ"

#: homepage.none
msgid "Blank Page"
msgstr "ទំព័រ​ទទេ"

#: homepage.default
msgid "&brandShortName; Start"
msgstr "ការ​ចាប់ផ្ដើម &brandShortName;"

#: homepage.currentpage
msgid "Use Current Page"
msgstr "ប្រើ​ទំព័រ​បច្ចុប្បន្ន"

#: showCharsetEncoding.title
msgid "Show character encoding"
msgstr "បង្ហាញ​ការ​អ៊ិនកូដ​តួអក្សរ"

#: actionbar.default
msgid "&brandShortName; Settings"
msgstr "ការ​កំណត់ &brandShortName;"

