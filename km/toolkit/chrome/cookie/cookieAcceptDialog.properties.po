#: hostColon
msgid "Host:"
msgstr "ម៉ាស៊ីន ៖"

#: domainColon
msgid "Domain:"
msgstr "ដែន ៖"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "ការ​តភ្ជាប់​ដែល​បាន​អ៊ិនគ្រីប​តែប៉ុណ្ណោះ"

#: forAnyConnection
msgid "Any type of connection"
msgstr "ប្រភេទ​តភ្ជាប់​ណាមួយ"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "នៅ​ចុង​បញ្ចប់​សម័យ"

#: showDetails
msgid "Show Details"
msgstr "បង្ហាញ​ព័ត៌មាន​លម្អិត"

#: hideDetails
msgid "Hide Details"
msgstr "លាក់​ព័ត៌មាន​លម្អិត"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "តំបន់​បណ្ដាញ %S ចង់​កំណត់​ខូគី ។"

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "តំបន់​បណ្ដាញ %S ចង់​កំណត់​ខូគី​ទី​ពីរ ។"

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"តំបន់​បណ្ដាញ %S ចង់​កំណត់​ខូគី​មួយ​ផ្សេងទៀត ។\n"
"អ្នក​មាន​ខូគី %S រួចហើយ ពី​តំបន់​បណ្ដាញ​នេះ ។"

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "តំបន់​បណ្ដាញ %S ចង់​កែប្រែ​ខូគី​ដែល​មាន​ស្រាប់ ។"
