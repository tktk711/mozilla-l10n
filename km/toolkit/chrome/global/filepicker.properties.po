msgstr "ឯកសារ HTML"

#: textTitle
msgid "Text Files"
msgstr "ឯកសារ​អត្ថបទ"

#: imageTitle
msgid "Image Files"
msgstr "ឯកសារ​រូបភាព"

#: xmlTitle
msgid "XML Files"
msgstr "ឯកសារ XML"

#: xulTitle
msgid "XUL Files"
msgstr "ឯកសារ XUL"

#: appsTitle
msgid "Applications"
msgstr "កម្មវិធី"

#: audioTitle
msgid "Audio Files"
msgstr "ឯកសារ​អូឌីយ៉ូ"

#: videoTitle
msgid "Video Files"
msgstr "ឯកសារ​វីដេអូ"

#: dirTextInputLabel
msgctxt "dirTextInputLabel"
msgid "Directory name:"
msgstr "ឈ្មោះ​ថត ៖"

#: dirTextInputAccesskey
msgid "n"
msgstr "Պ"

#: confirmTitle
msgid "Confirm"
msgstr "អះអាង"

#: confirmFileReplacing
msgid ""
"%S already exists.\n"
"Do you want to replace it?"
msgstr ""
"មាន %S រួចហើយ ។\n"
"តើ​អ្នក​ចង់​ជំនួស​វា​ឬទេ ?"

#: openButtonLabel
msgid "Open"
msgstr "បើក"

#: saveButtonLabel
msgid "Save"
msgstr "រក្សា​ទុក"

#: selectFolderButtonLabel
msgid "Select"
msgstr "ជ្រើស"

#: noButtonLabel
msgid "No"
msgstr "ទេ"

#: formatLabel
msgid "Format:"
msgstr "ទ្រង់ទ្រាយ ៖"

#: errorOpenFileDoesntExistTitle
msgctxt "errorOpenFileDoesntExistTitle"
msgid "Error opening %S"
msgstr "កំហុស​ក្នុង​ការ​បើក %S"

#: errorOpenFileDoesntExistMessage
msgid "File %S doesn't exist"
msgstr "មិន​មាន​ឯកសារ %S ទេ"

#: errorDirDoesntExistTitle
msgid "Error accessing %S"
msgstr "កំហុស​ក្នុង​ការ​ចូល​ដំណើរការ %S"

#: errorDirDoesntExistMessage
msgid "Directory %S doesn't exist"
msgstr "មិន​មាន​ថត %S ទេ"

#: errorOpeningFileTitle
msgctxt "errorOpeningFileTitle"
msgid "Error opening %S"
msgstr "កំហុស​ក្នុង​ការ​បើក %S"

#: openWithoutPermissionMessage_file
msgid "File %S is not readable"
msgstr "ឯកសារ %S មិន​អាច​អាន​បាន​ទេ"

#: errorSavingFileTitle
msgid "Error saving %S"
msgstr "កំហុស​ក្នុង​ការ​រក្សាទុក %S"

#: saveParentIsFileMessage
msgid "%S is a file, can't save %S"
msgstr "%S គឺជា​ឯកសារ មិន​អាច​រក្សាទុក %S បាន​ទេ"

#: saveParentDoesntExistMessage
msgid "Path %S doesn't exist, can't save %S"
msgstr "មិន​មាន​ផ្លូវ %S ទេ មិន​អាច​រក្សាទុក %S បាន​ទេ"

#: saveWithoutPermissionMessage_file
msgid "File %S is not writable."
msgstr "ឯកសារ %S មិន​អាច​សរសេរ​បាន​ទេ ។"

#: saveWithoutPermissionMessage_dir
msgid "Cannot create file. Directory %S is not writable."
msgstr "មិន​អាច​បង្កើត​ឯកសារ ។ ថត %S មិន​អាច​សរសេរ​បាន​ទេ ។"

#: errorNewDirDoesExistTitle
msgctxt "errorNewDirDoesExistTitle"
msgid "Error creating %S"
msgstr "កំហុស​ក្នុង​ការ​បង្កើត %S"

#: errorNewDirDoesExistMessage
msgid "A file named %S already exists, directory cannot be created."
msgstr "មាន​ឯកសារ​ដែល​មាន​ឈ្មោះ %S រួចហើយ ថត​មិន​អាច​ត្រូវ​បាន​បង្កើត​ទេ ។"

#: errorCreateNewDirTitle
msgctxt "errorCreateNewDirTitle"
msgid "Error creating %S"
msgstr "កំហុស​ក្នុង​ការ​បង្កើត %S"

#: errorCreateNewDirMessage
msgid "Directory %S could not be created"
msgstr "មិន​អាច​បង្កើត​ថត %S បាន​ទេ"

#: errorCreateNewDirIsFileMessage
msgid "Directory cannot be created, %S is a file"
msgstr "មិន​អាច​បង្កើត​ថត​បាន​ទេ %S គឺជា​ឯកសារ"

#: errorCreateNewDirPermissionMessage
msgid "Directory cannot be created, %S not writable"
msgstr "មិន​អាច​បង្កើត​ថត​បាន​ទេ %S មិន​អាច​សរសេរ​បាន"

#: promptNewDirTitle
msgid "Create new directory"
msgstr "បង្កើត​ថត​ថ្មី"

#: promptNewDirMessage
msgctxt "promptNewDirMessage"
msgid "Directory name:"
msgstr "ឈ្មោះ​ថត ៖"

#: errorPathProblemTitle
msgid "Unknown Error"
msgstr "មិន​ស្គាល់​កំហុស"

#: errorPathProblemMessage
msgid "An unknown error occured (path %S)"
msgstr "កំហុស​ដែល​មិន​ស្គាល់​មួយ​បាន​កើតឡើង (ផ្លូវ %S)"
