#: windowtitle.label
msgid "&brandShortName; - Choose User Profile"
msgstr "&brandShortName; - ជ្រើស​ទម្រង់​អ្នក​ប្រើ"

#: profilename.label
msgid "Profile Name:"
msgstr "ឈ្មោះ​ទម្រង់ ៖"

#: start.label
msgid "Start &brandShortName;"
msgstr "ចាប់ផ្តើម &brandShortName;"

#: exit.label
msgid "Exit"
msgstr "ចេញ"

#: availprofiles.label
msgid "Available Profiles"
msgstr "ទម្រង់​ដែល​មាន"

#: newButton.label newButton.accesskey
msgid "&Create Profile…"
msgstr "បង្កើត​ប្រវត្តិរូប…"

#: renameButton.label renameButton.accesskey
msgid "&Rename Profile…"
msgstr "ប្ដូរ​ឈ្មោះ​ប្រវត្តិរូប..."

#: deleteButton.label deleteButton.accesskey
msgid "&Delete Profile…"
msgstr "លុប​​ប្រវត្តិរូប…"

#. manager entities
#: pmDescription.label
msgid ""
"&brandShortName; stores information about your settings, preferences, and "
"other user items in your user profile."
msgstr ""
"&brandShortName; រក្សាទុក​ព័ត៌មាន​អំពី​ការ​កំណត់ ចំណូលចិត្ត និង​ធាតុ​របស់​អ្នកប្រើ​ផ្សេងទៀត​នៅ​ក្នុង​ទម្រង់​"
"អ្នកប្រើ​របស់​អ្នក ។"

#: offlineState.label offlineState.accesskey
msgid "W&ork offline"
msgstr "ធ្វើ​ការ​ដោយ​គ្មាន​អ៊ីនធឺណិត"

#: useSelected.label useSelected.accesskey
msgid "U&se the selected profile without asking at startup"
msgstr "ប្រើ​ជីវប្រវត្តិ​ដែល​បាន​ជ្រើស​ដោយ​មិន​ចាំបាច់​ស្នើ​ការ​រៀបចំ"

#~ msgid "Don't a&sk at startup"
#~ msgstr "កុំ​សួរ​នៅ​ពេល​ចាប់ផ្តើម"
