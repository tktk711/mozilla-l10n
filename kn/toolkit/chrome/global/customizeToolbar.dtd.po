#: dialog.title
msgid "Customize Toolbar"
msgstr "ಉಪಕರಣಪಟ್ಟಿಯನ್ನು ನನ್ನಿಚ್ಛೆಗೆ ತಕ್ಕಂತೆ ಹೊಂದಿಸು"

#: dialog.dimensions
msgid "width: 92ch; height: 36em;"
msgstr "width: 92ch; height: 36em;"

#: instructions.description
msgid "You can add or remove items by dragging to or from the toolbars."
msgstr ""
"ಉಪಕರಣ ಪಟ್ಟಿಗಳಿಂದ ಸೆಳೆಯುವುದು ಅಥವ  ಹಾಕುವುದರ ಮೂಲಕ ಅದಕ್ಕೆ ಸೇರ್ಪಡಿಸಬಹುದು ಅಥವ  ಅದರಿಂದ "
"ತೆಗೆದು ಹಾಕಬಹುದು."

#: show.label
msgid "Show:"
msgstr "ತೋರಿಸು:"

#: iconsAndText.label
msgid "Icons and Text"
msgstr "ಲಾಂಛನಗಳು ಹಾಗು ಪಠ್ಯ"

#: icons.label
msgid "Icons"
msgstr "ಲಾಂಛನಗಳು"

#: text.label
msgid "Text"
msgstr "ಪಠ್ಯ"

#: useSmallIcons.label
msgid "Use Small Icons"
msgstr "ಎಲ್ಲಾ ಚಿಕ್ಕ ಲಾಂಛನಗಳನ್ನ್ನು ಬಳಸು"

#: restoreDefaultSet.label
msgid "Restore Default Set"
msgstr "ಪೂರ್ವನಿಯೋಜಿತ ಸೆಟ್ಟ್‍ನ್ನು ಮತ್ತೊಮ್ಮೆ ಸ್ಥಾಪಿಸು"

#: addNewToolbar.label
msgid "Add New Toolbar"
msgstr "ಹೊಸ ಉಪಕರಣಪಟ್ಟಿಯನ್ನು ಸೇರಿಸು"

#: saveChanges.label
msgid "Done"
msgstr "ಆಗಿದೆ"

#: undoChanges.label
msgid "Undo Changes"
msgstr "ಬದಲಾವಣೆಗಳನ್ನು ರದ್ದು ಮಾಡು"
