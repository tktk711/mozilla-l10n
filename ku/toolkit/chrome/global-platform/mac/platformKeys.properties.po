#. #mac#this file defines the on screen display names for the various modifier keys#these are used in XP menus to show keyboard shortcuts
#. #the shift key - open up arrow symbol (ctrl-e)
#: VK_SHIFT
msgid "⇧"
msgstr "\\u0005"

#. #the command key - clover leaf symbol (ctrl-q)
#: VK_META
msgid "⌘"
msgstr "\\u0011"

#. #the win key - never generated by native key event
#: VK_WIN
msgid "win"
msgstr ""

#. #the option/alt key - splitting tracks symbol (ctrl-g)
#: VK_ALT
msgid "⌥"
msgstr "\\u0007"

#. #the control key. hat symbol (ctrl-f)
#: VK_CONTROL
msgid "⌃"
msgstr "\\u0006"

