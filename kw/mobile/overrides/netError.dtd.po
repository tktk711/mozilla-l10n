#. %brandDTD;
#: loadError.label
msgid "Problem loading page"
msgstr "Kudyn ow karga an folen"

#: retry.label
msgid "Try Again"
msgstr "Assaya arta"

#. Specific error messages
#: connectionFailure.title
msgid "Unable to connect"
msgstr "Ny yllir junya"

#: connectionFailure.longDesc
msgctxt "connectionFailure.longDesc"
msgid "&sharedLongDesc2;"
msgstr "&sharedLongDesc2;"

#: deniedPortAccess.title
msgid "This address is restricted"
msgstr "Fronnys yw an drigva-ma"

#: deniedPortAccess.longDesc
msgctxt "deniedPortAccess.longDesc"
msgid ""
msgstr ""

#: dnsNotFound.title
msgid "Server not found"
msgstr "Ny veu kevys an servyer"

#: dnsNotFound.longDesc2
msgid ""
"<ul> \n"
"<li>Check the address for typing errors such as \n"
"<strong>ww</strong>.example.com instead of \n"
"<strong>www</strong>.example.com</li> \n"
"<li>If you are unable to load any pages, check your device's data or Wi-Fi "
"connection.</li> \n"
"</ul>"
msgstr ""

#: fileNotFound.title
msgid "File not found"
msgstr "Ny veu kevys an restren"

#: fileNotFound.longDesc
msgid ""
"<ul> \n"
"<li>Check the file name for capitalization or other typing errors.</li> \n"
"<li>Check to see if the file was moved, renamed or deleted.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Checkyowgh hanow an restren rag gwallow jynnskrifa.</li> \n"
"<li>Checkyowgh mar peu an restren removys, dashenwys po diles.</li> \n"
"</ul>"

#: generic.title
msgid "Oops."
msgstr "Gwall"

#: generic.longDesc
msgid "<p>&brandShortName; can't load this page for some reason.</p>"
msgstr ""
"<p>Ny yll &brandShortName; karga an folen-ma drefen acheson ankoth.</p>"

#: malformedURI.title
msgid "The address isn't valid"
msgstr "Nyns yw da an drigva"

#: malformedURI.longDesc
msgid ""
"<ul> \n"
"<li>Web addresses are usually written like \n"
"<strong>http://www.example.com/</strong></li> \n"
"<li>Make sure that you're using forward slashes (i.e. \n"
"<strong>/</strong>).</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Skrifys yw trigvaow gwias yndella: \n"
"<strong>http://www.example.com/</strong></li> \n"
"<li>Surhowgh agas bos owth usya slashys war-rag (i.e. \n"
"<strong>/</strong>).</li> \n"
"</ul>"

#: netInterrupt.title
msgid "The connection was interrupted"
msgstr "Goderrys veu an junyans"

#: netInterrupt.longDesc
msgctxt "netInterrupt.longDesc"
msgid "&sharedLongDesc2;"
msgstr "&sharedLongDesc2;"

#: notCached.title
msgid "Document Expired"
msgstr "Termyn an skrifen a dhiwedhas"

#: notCached.longDesc
msgid ""
"<p>The requested document is not available in &brandShortName;'s cache.</"
"p><ul><li>As a security precaution, &brandShortName; does not automatically "
"re-request sensitive documents.</li><li>Click Try Again to re-request the "
"document from the website.</li></ul>"
msgstr ""
"<p>Nyns yw kavadow an skrifen wovynnys yn cache &brandShortName;.</"
"p><ul><li>Avel ragbreder sekerder, ny wra &brandShortName; daswovyn "
"skrifennow krohendanow yn awtomatek.</li><li>Klyckyowgh war Assaya arta rag "
"daswovyn an skrifen dhyworth an wiasva.</li></ul>"

#: netOffline.title
msgid "Offline mode"
msgstr "Modh meslinen"

#: netOffline.longDesc2
msgid ""
"<ul> \n"
"<li>Try again. &brandShortName; will attempt to open a connection and reload "
"the page.</li> \n"
"</ul>"
msgstr ""

#: contentEncodingError.title
msgid "Content Encoding Error"
msgstr ""

#: contentEncodingError.longDesc
#, fuzzy
msgctxt "contentEncodingError.longDesc"
msgid ""
"<ul> \n"
"<li>Please contact the website owners to inform them of this problem.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Gwrewgh kestava orth perhenegyon an wiasva rag derivas dhedhans an kudyn-"
"ma.</li> \n"
"</ul>"

#: unsafeContentType.title
msgid "Unsafe File Type"
msgstr "Ehen restren diantel"

#: unsafeContentType.longDesc
#, fuzzy
msgctxt "unsafeContentType.longDesc"
msgid ""
"<ul> \n"
"<li>Please contact the website owners to inform them of this problem.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Gwrewgh kestava orth perhenegyon an wiasva rag derivas dhedhans an kudyn-"
"ma.</li> \n"
"</ul>"

#: netReset.title
msgid "The connection was reset"
msgstr "Dassettyes veu an junyans"

#: netReset.longDesc
msgctxt "netReset.longDesc"
msgid "&sharedLongDesc2;"
msgstr "&sharedLongDesc2;"

#: netTimeout.title
msgid "The connection has timed out"
msgstr "Yma diwedh dhe dermyn gortos an junyans"

#: netTimeout.longDesc
msgctxt "netTimeout.longDesc"
msgid "&sharedLongDesc2;"
msgstr "&sharedLongDesc2;"

#: protocolNotFound.title
msgid "The address wasn't understood"
msgstr "Ny veu konvedhys an drigva"

#: protocolNotFound.longDesc
msgid ""
"<ul> \n"
"<li>You might need to install other software to open this address.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Res a alja bos dhywgh lea medhelweyth erel rag egeri an drigva-ma.</"
"li> \n"
"</ul>"

#: proxyConnectFailure.title
msgid "The proxy server is refusing connections"
msgstr "Yma an servyer proksi ow naha junyansow"

#: proxyConnectFailure.longDesc
msgid ""
"<ul> \n"
"<li>Check the proxy settings to make sure that they are correct.</li> \n"
"<li>Contact your network administrator to make sure the proxy server is \n"
"working.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Checkyowgh settyansow an proksi rag surhe aga bos ewn.</li> \n"
"<li>Kestevowgh orth menystrer agas rosweyth rag surhe bos an servyer "
"proksi \n"
"owth oberi.</li> \n"
"</ul>"

#: proxyResolveFailure.title
msgid "Unable to find the proxy server"
msgstr "Ny yllir kavos an servyer proksi"

#: proxyResolveFailure.longDesc2
msgid ""
"<ul> \n"
"<li>Check the proxy settings to make sure that they are correct.</li> \n"
"<li>Check to make sure your device has a working data or Wi-Fi connection.</"
"li> \n"
"</ul>"
msgstr ""

#: redirectLoop.title
msgid "The page isn't redirecting properly"
msgstr "Nyns usi an folen ow taskedya yn fas"

#: redirectLoop.longDesc
msgid ""
"<ul> \n"
"<li>This problem can sometimes be caused by disabling or refusing to "
"accept \n"
"cookies.</li> \n"
"</ul>"
msgstr ""

#: unknownSocketType.title
msgid "Unexpected response from server"
msgstr "Yth esa gorthyp nag o gortys dhyworth an servyer"

#: unknownSocketType.longDesc
msgid ""
"<ul> \n"
"<li>Check to make sure your system has the Personal Security Manager \n"
"installed.</li> \n"
"<li>This might be due to a non-standard configuration on the server.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Checkyowgh rag surhe bos les an Dyghtyer Sekerder Personel (PSM) \n"
" war agas jynn amontya.</li> \n"
"<li>Hemm a alja bos drefen restrans koynt war an servyer.</li> \n"
"</ul>"

#: nssFailure2.title
#, fuzzy
msgctxt "nssFailure2.title"
msgid "Secure Connection Failed"
msgstr "An junyans diogel a fyllas"

#: nssFailure2.longDesc
msgid ""
"<ul> \n"
"<li>The page you are trying to view cannot be shown because the authenticity "
"of the received data could not be verified.</li> \n"
"<li>Please contact the website owners to inform them of this problem. "
"Alternatively, use the command found in the help menu to report this broken "
"site.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Ny yllir diskwedhes an folen esowgh hwi owth assaya gweles drefen ny veu "
"possybyl gwirya an data recevys.</li> \n"
"<li>Gwrewgh kestava orth perhenogyon an wiasva rag derivas dhedhans an kudyn-"
"ma. Poken, usyowgh an arhadow kevys y'n rol gweres rag derivas an wiasva "
"terrys-ma.</li> \n"
"</ul>"

#: nssBadCert.title
#, fuzzy
msgctxt "nssBadCert.title"
msgid "Secure Connection Failed"
msgstr "An junyans diogel a fyllas"

#: nssBadCert.longDesc2
msgid ""
"<ul> \n"
"<li>This could be a problem with the server's configuration, or it could "
"be \n"
"someone trying to impersonate the server.</li> \n"
"<li>If you have connected to this server successfully in the past, the error "
"may \n"
"be temporary, and you can try again later.</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Hemm a alja bos kudyn gans restrans an servyer, po nebonan owth \n"
"assaya omwul dhe vos an servyer.</li> \n"
"<li>Mar kwrussowgh hwi junya gans an servyer-ma yn sowen y'n termyn eus \n"
"passyes, an gwall a alja bos anbarhus, ha hwi a yll assaya arta moy a-vres.</"
"li> \n"
"</ul>"

#: sharedLongDesc2
msgid ""
"<ul> \n"
"<li>The site could be temporarily unavailable or too busy. Try again in a "
"few moments.</li> \n"
"<li>If you are unable to load any pages, check your mobile device's data or "
"Wi-Fi connection.</li> \n"
"</ul>"
msgstr ""

#: cspFrameAncestorBlocked.title
msgid "Blocked by Content Security Policy"
msgstr "Lettyes gans polici sekerder dalgh"

#: cspFrameAncestorBlocked.longDesc
msgid ""
"<p>&brandShortName; prevented this page from loading in this way because the "
"page has a content security policy that disallows it.</p>"
msgstr ""
"<p>Lestys veu an folen-ma rag karga y'n fordh-ma gans &brandShortName; "
"drefen bos polici sekerder dalgh dhedhi na wra y avowa.</p>"

#: corruptedContentError.title
msgid "Corrupted Content Error"
msgstr "Gwall: Dalgh pedrys"

#: corruptedContentError.longDesc
msgid ""
"<p>The page you are trying to view cannot be shown because an error in the "
"data transmission was detected.</p><ul><li>Please contact the website owners "
"to inform them of this problem.</li></ul>"
msgstr ""
"<p>Ny yllir diskwedhes an folen esowgh hwi owth assaya gweles drefen bos "
"helerhys gwall yn treuskorrans an data.</p><ul><li>Gwrewgh kestava orth "
"perhenogyon an wiasva rag derivas dhedhans an kudyn-ma.</li></ul>"

#: securityOverride.linkText
msgid "Or you can add an exception…"
msgstr "Po hwi a yll keworra namm…"

#: securityOverride.getMeOutOfHereButton
msgid "Get me out of here!"
msgstr "Voydya alemma!"

#: securityOverride.exceptionButtonLabel
msgid "Add Exception…"
msgstr "Keworra namm…"

#. <!-- LOCALIZATION NOTE (securityOverride.warningContent) - Do not translate the
#. contents of the <xul:button> tags.  The only language content is the label= field,
#. which uses strings already defined above. The button is included here (instead of
#. netError.xhtml) because it exposes functionality specific to firefox.
#: securityOverride.warningContent
msgid ""
"<p>You should not add an exception if you are using an internet connection "
"that you do not trust completely or if you are not used to seeing a warning "
"for this server.</p> \n"
" \n"
"<button id='getMeOutOfHereButton'>&securityOverride.getMeOutOfHereButton;</"
"button> \n"
"<button id='exceptionDialogButton'>&securityOverride.exceptionButtonLabel;</"
"button>"
msgstr ""
"<p>Ny dal dhywgh keworra namm mars esowgh hwi owth usya junyans kesrosweyth "
"nag esowgh hwi ow trestya dhodho yn tien po mar nyns yw agas usadow gweles "
"gwarnyans rag an server-ma.</p> \n"
" \n"
"<button id='getMeOutOfHereButton'>&securityOverride.getMeOutOfHereButton;</"
"button> \n"
"<button id='exceptionDialogButton'>&securityOverride.exceptionButtonLabel;</"
"button>"

#: remoteXUL.title
msgid "Remote XUL"
msgstr "XUL pell"

#: remoteXUL.longDesc
msgid ""
"<p><ul><li>Please contact the website owners to inform them of this problem."
"</li></ul></p>"
msgstr ""
"<p><ul><li>Gwrewgh kestava orth perhenogyon an wiasva rag derivas dhedhans "
"an kudyn-ma.</li></ul></p>"
