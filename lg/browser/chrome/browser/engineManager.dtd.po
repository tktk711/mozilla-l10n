#: engineManager.title
msgid "Manage Search Engine List"
msgstr "Fuga Olukalala lwa Yingini Enoonya"

# min-obutono,width-obugazi
#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: engineManager.style
msgid "min-width: 35em;"
msgstr "min-width: 35em;"

# Olina yigin zino ezinonyereza ezitekedwamu
#: engineManager.intro
msgid "You have the following search engines installed:"
msgstr "Olina yingini ezinoonya ezitekeddwamu:"

#: columnLabel.name
msgid "Name"
msgstr "Erinnya"

# these can be used; Ekigambo ekyomugaso or Ekigambo ekyamakulu
#: columnLabel.keyword
msgid "Keyword"
msgstr "Ekigambo ekikulu"

# Use ne instead of (&)
#. Buttons
#: up.label up.accesskey
msgid "Move &Up"
msgstr "Dda &Waggulu"

# On English part & is not neccesary
#: dn.label dn.accesskey
msgid "Move &Down"
msgstr "Dda &Wansi"

# remove-gyako
#: remove.label remove.accesskey
msgid "&Remove"
msgstr "&Ggyako"

# There is an error on the word edit remove $ from it
#: edit.label edit.accesskey
msgid "Edi&t Keyword…"
msgstr "Terez&a Ekigambo Ekikulu…"

# remove & from the word search and yingini
#: addEngine.label addEngine.accesskey
msgid "Get more se&arch engines…"
msgstr "Funa yi&ngini ezinonya endala…"

#: enableSuggest.label enableSuggest.accesskey
msgid "&Show search suggestions"
msgstr "&Laga ebirowoozo by'okunoonya"

#: restoreDefaults.label restoreDefaults.accesskey
msgid "R&estore Defaults"
msgstr "Z&aayo Ebyenkalakkalira"
