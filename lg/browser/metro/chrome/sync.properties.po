#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Okutusa okwasembay: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Okatuusa okusembayo: kugenda mumaso…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Akaunti: %S"

#: notconnected.label
msgid "Not connected"
msgstr ""

#: connecting.label
msgid "Connecting…"
msgstr ""

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Akaunta yo Etuusa mu Firefox ejjiddwawo"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Ddayo"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 tekwatagana ne Firefox Sync esinga okuba empya. Osabibwa okutuusa ku "
"nfulumya esingayo obupya."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 tekwatagana ne Firefox Sync esinga okuba empya. Osabibwa okutuusa "
"Firefox ku Kompyuta yo(zo)."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Okutusaganya Firefox"

#: sync.update.button
msgid "Learn More"
msgstr "Manya Ebisingawo"

#: sync.update.close
msgid "Close"
msgstr "Ggalawo"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Tesoobola Kutegeka Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Tewaliwo ngeri yakuyungibwa ku yintaneti"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S tesoboode kutuka ku Sync. Osoobola okugezako nate?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Ddamu ogezeeko"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Okwetegekera"
