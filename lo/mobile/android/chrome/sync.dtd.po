#: sync.title
msgid "Sync"
msgstr "Sync"

#: sync.notconnected
msgid "Not connected"
msgstr "ບໍ່ໄດ້ເຊື່ອມຕໍ່"

#: sync.connect
msgctxt "sync.connect"
msgid "Connect"
msgstr "ເຊື່ອມຕໍ່"

#: sync.connected
msgid "Connected"
msgstr "ເຊື່ອມຕໍ່ຢູ່"

#: sync.details
msgid "Details"
msgstr "ລາຍລະອຽດ"

#: sync.deviceName
msgid "This device"
msgstr "ອຸປະກອນນີ້"

#: sync.disconnect
msgid "Disconnect"
msgstr "ຕັດການເຊື່ອມຕໍ່"

#: sync.syncNow
msgid "Sync Now"
msgstr "Sync ທັນທີ"

#: sync.setup.title
msgid "Connect to Sync"
msgstr "ເຊື່ອມຕໍ່ກັບ Sync"

#: sync.setup.pair
msgid "To activate, select \"Pair a Device\" on your other device."
msgstr "ເພື່ອເປີດໃຊ້ງານ, ເລືອກ “ເຊື່ອມໂຍງອຸປະກອນ” ໃນອຸປະກອນອື່ນໆຂອງທ່ານ."

#: sync.fallback
msgid "I'm not near my computer…"
msgstr "ຄອມພິວເຕີຂອງຂ້ອຍບໍ່ໄດ້ຢູ່ແຖວນີ້…"

#: sync.setup.manual
msgid "Enter your Sync account information"
msgstr "ປ້ອນລາຍລະອຽດບັນຊີຜູ້ໃຊ້ Sync ຂອງທ່ານ"

#: sync.account
msgid "Account Name"
msgstr "ຊື່ບັນຊີ"

#: sync.password
msgid "Password"
msgstr "ລະຫັດຜ່ານ"

#: sync.recoveryKey
msgid "Recovery Key"
msgstr "ຄີສຳລັບການກູ້ຄືນ"

#: sync.customServer
msgid "Use custom server"
msgstr "ໃຊ້ເຄື່ອງແມ່ຂ່າຍທີ່ກຳນົດເອງ"

#: sync.serverURL
msgid "Server URL"
msgstr "URL ຂອງເຄື່ອງແມ່ຂ່າຍ"

#: sync.setup.connect
msgctxt "sync.setup.connect"
msgid "Connect"
msgstr "ເຊື່ອມຕໍ່"

#: sync.setup.cancel
msgid "Cancel"
msgstr "ຍົກເລີກ"

#: sync.setup.tutorial
msgid "Show me how"
msgstr "ບອກວິທີເຮັດໃຫ້ຂ້ອຍຮູ້"

#: sync.setup.waiting2
msgid "Waiting for other device…"
msgstr "ກຳລັງລໍຖ້າອຸປະກອນອື່ນໆ…"

#: sync.pair.title
msgid "Pair a Device"
msgstr "ຈັບຄູ່ອຸປະກອນ"

#: sync.pair.description
msgid "To activate your new device, select \"Set Up Sync\" on the device."
msgstr "ເພື່ອເລີ່ມໃຊ້ງານອຸປະກອນໃຫມ່ຂອງທ່ານ, ໃຫ້ເລືອກ \"ຕິດຕັ້ງ Sync\" ໃນອຸປະກອນ."

#: sync.setup.close
msgid "Close"
msgstr "ອອກ"

#: sync.setup.waitingdownload
msgid ""
"Your data is now being downloaded in the background. You can close this "
"window at any time."
msgstr "ຂໍ້ມູນຂອງທ່ານກຳລັງຖືກດາວໂຫລດຢູ່ໃນເບື້ອງຫລັງ. ທ່ານສາມາດປິດຫນ້າຕ່າງນີ້ຕອນໃດກໍໄດ້."
