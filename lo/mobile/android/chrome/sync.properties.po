#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Sync ຄັ້ງຫລ່າສຸດ: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Sync ຄັ້ງຫລ່າສຸດ: ກຳລັງດຳເນີນການ…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "ບັນຊີ: %S"

#: notconnected.label
msgid "Not connected"
msgstr "ບໍ່ໄດ້ເຊື່ອມຕໍ່"

#: connecting.label
msgid "Connecting…"
msgstr "ກຳລັງເຊື່ອມຕໍ່…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "ບັນຊີ Firefox Sync ຂອງທ່ານໄດ້ຖືກລົບອອກແລ້ວ"

#: notificationDisconnect.button
msgid "Undo"
msgstr "ຍົກເລີກ"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr "#1 #2 ເຂົ້າກັນບໍ່ໄດ້ກັບ Firefox Sync ລຸ້ນຫລ້າສຸດ. ກະລຸນາອັບເດດໃຫ້ເປັນລຸ້ນຫລ້າສຸດ."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 ເຂົ້າກັນບໍ່ໄດ້ກັບ Firefox Sync ລຸ້ນເກົ່າ. ກະລຸນາອັບເດດໄຟຟັອກໃນຄອມພິວເຕີເຄື່ອງອື່ນໆຂອງທ່ານ."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Sync ໄຟຟັອກ"

#: sync.update.button
msgid "Learn More"
msgstr "ຮຽນຮູ້ເພີ່ມເຕີມ"

#: sync.update.close
msgid "Close"
msgstr "ອອກ"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "ບໍ່ສາມາດຕິດຕັ້ງ Sync ໄດ້"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "ບໍ່ພົບການເຊື່ອມຕໍ່ກັບອິນເຕີເນັດ"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S ບໍ່ສາມາດເຊື່ອມຕໍ່ກັບ Sync ໄດ້. ທ່ານຢາກຈະລອງອີກເທື່ອຫນຶ່ງບໍ່?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "ຫລອງໃຫມ່ອີກຄັ້ງ"

#: sync.setup.manual
msgid "Manual setup"
msgstr "ການຕິດຕັ້ງແບບກຳຫນົດເອງ"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "ບໍ່ມີແທັບຈາກຄອມພິວເຕີເຄື່ອງອື່ນ."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Sync ເປັນແບຣນໃຫມ່ ແລະ ໃຊ້ງານງ່າຍກ່ວາທີ່ຜ່ານມາ."

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "ແຕະທີ່ນີ້ເພື່ອຮຽນຮູ້ເພີ່ມເຕີມ"
