#. # String will be replaced by brandShortName.
#: saveLogin
msgid "Would you like %S to remember this login?"
msgstr "ທ່ານຕ້ອງການ %S ທີ່ຈະໃຫ້ຈືການລັອກອິນນີ້ໄວ້ບໍ?"

#: rememberButton
msgid "Remember"
msgstr "ຈື່"

#: neverButton
msgid "Never"
msgstr "ບໍ່ເຄີຍ"

#. # String is the login's hostname
#: updatePassword
msgid "Update saved password for %S?"
msgstr "ປັບປຸງລະຫັດຜ່ານທີ່ໄດ້ບັນທຶກໄວ້ສຳລັບ %S ບໍ່?"

#: updatePasswordNoUser
msgid "Update saved password for this login?"
msgstr "ປັບປຸງລະຫັດຜ່ານທີ່ໄດ້ບັນທຶກໄວ້ສຳລັບການລັອກອິນນີ້ບໍ່?"

#: updateButton
msgid "Update"
msgstr "ປັບປຸງ"

#: dontUpdateButton
msgid "Don't update"
msgstr "ບໍ່ຕ້ອງປັບປຸງ"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "ກະລຸນາຢືນຢັນວ່າຜູ້ໃຊ້ໃດທີ່ທ່ານກຳລັງມີການປ່ຽນແປງລະຫັດຜ່ານເພື່ອ"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "ຢືນຢັນການປ່ຽນແປງລະຫັດຜ່ານ"

#. # Strings used by PromptService.js
#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "ນໍາໃຊ້ຕົວຈັດການລະຫັດຜ່ານເພື່ອຈື່ຈໍາລະຫັດຜ່ານນີ້."

#: username
msgid "Username"
msgstr "ຊື່ຜູ້ໃຊ້"

#: password
msgid "Password"
msgstr "ລະຫັດຜ່ານ"
