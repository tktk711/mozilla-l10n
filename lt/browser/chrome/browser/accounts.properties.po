#~ msgid "&Sign In"
#~ msgstr "Pri&sijungti"

#~ msgid "%S not verified"
#~ msgstr "%S nepatvirtintas"

#~ msgid "Please click the verification link in the email sent to %S"
#~ msgstr "Prašome spustelėti į %S nusiųstame laiške esantį patvirtinimo saitą"

#~ msgid "&Resend"
#~ msgstr "&Siųsti iš naujo"

#~ msgid "&Forget"
#~ msgstr "&Pamiršti"

