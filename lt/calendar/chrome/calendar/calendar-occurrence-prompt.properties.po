#: buttons.occurrence.edit.label
msgid "Edit just this occurrence"
msgstr "Taisyti tik šį įvykį"

#: buttons.allfollowing.delete.label
msgid "Delete this and all future occurrences"
msgstr "Šalinti šį įvykį ir visus jo pasikartojimus ateityje"

#: buttons.allfollowing.edit.label
msgid "Edit this and all future occurrences"
msgstr "Taisyti šį įvykį ir visus jo pasikartojimus ateityje"

#: buttons.parent.delete.label
msgid "Delete all occurrences"
msgstr "Šalinti visus šio įvykio pasikartojimus"

#: buttons.parent.edit.label
msgid "Edit all occurrences"
msgstr "Taisyti visus šio įvykio pasikartojimus"
