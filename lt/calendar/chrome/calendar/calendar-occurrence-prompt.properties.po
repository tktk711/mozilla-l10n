#: header.isrepeating.event.label
msgid "is a repeating event"
msgstr "yra pasikartojantis įvykis"

#: header.isrepeating.task.label
msgid "is a repeating task "
msgstr "yra pasikartojantis darbas"

#: windowtitle.event.delete
msgid "Delete Repeating Event"
msgstr "Pasikartojančio įvykio šalinimas"

#: windowtitle.task.delete
msgid "Delete Repeating Task"
msgstr "Pasikartojančio darbo šalinimas"

#: windowtitle.event.edit
msgid "Edit Repeating Event"
msgstr "Pasikartojančio įvykio taisymas"

#: windowtitle.task.edit
msgid "Edit Repeating Task"
msgstr "Pasikartojančio darbo taisymas"

#: buttons.occurrence.delete.label
