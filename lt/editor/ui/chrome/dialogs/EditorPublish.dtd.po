#. Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "Tinklalapio publikavimas"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "Publikavimo nuostatos"

#: publishTab.label
msgctxt "publishTab.label"
msgid "Publish"
msgstr "Publikavimas"

#: settingsTab.label
msgid "Settings"
msgstr "Nuostatos"

#: publishButton.label
msgctxt "publishButton.label"
msgid "Publish"
msgstr "Publikuoti"

#. Publish Tab Panel
#: siteList.label siteList.accesskey
msgctxt "siteList.label siteList.accesskey"
msgid "Sit&e Name:"
msgstr "&Tinklavietės pavadinimas:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "Pasirinkite tinklavietę, kurioje ketinate publikuoti"

#: newSiteButton.label newSiteButton.accesskey
msgid "&New Site"
msgstr "Apr&ašyti naują tinklavietę"

#: docDirList.label docDirList.accesskey
msgid "&Site subdirectory for this page:"
msgstr "Tinklala&pio poaplankis:"

#: docDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory for this page"
msgstr "Parinkite arba įveskite tinklalapiui skirto serverio poaplankio vardą"

#: publishImgCheckbox.label publishImgCheckbox.accesskey
msgid "Include images and &other files"
msgstr "Įtrau&kti grafinius ir kitus failus"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr ""
"Publikuoti paveikslus ir kitus failus, į kuriuos eina saitai iš šio "
"tinklalapio"

#: sameLocationRadio.label sameLocationRadio.accesskey
msgid "&Use same location as page"
msgstr "&Failų vieta sutampa su tinklalapio vieta"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "Failai publikuojami toje pat tinklavietėje, kur ir tinklalapis"

#: useSubdirRadio.label useSubdirRadio.accesskey
msgid "Use this site sub&directory:"
msgstr "&Įrašyti į šį tinklavietės poaplankį:"

#: useSubdirRadio.tooltip
msgid "Publish files to the selected remote subdirectory"
msgstr "Failus publikuoti į parinktą serverio poaplankį"

#: otherDirList.tooltip
msgid ""
"Choose or enter name of remote subdirectory where files will be published"
msgstr ""
"Parinkite arba įveskite tinklavietės poaplankio, į kurį norite publikuoti "
"failus, vardą"

#: pageTitle.label pageTitle.accesskey
msgid "Page &Title:"
msgstr "Tinkl&alapio antraštė:"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr ""
"Tinklalapio antraštė bus matoma naršyklės lango antraštės juostoje ir "
"adresyne"

#: pageTitleExample.label
msgid "e.g.: \"My Web Page\""
msgstr "pvz., „V. Pavardenio tinklalapis“"

#: filename.label filename.accesskey
msgid "&Filename:"
msgstr "Failo &vardas:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr "Surinkite šio tinklalapio failo vardą ir prievardį „.html“"

#: filenameExample.label
msgid "e.g,: \"mypage.html\""
msgstr "pvz., „Jonas.html“"

#: setDefaultButton.label setDefaultButton.accesskey
msgid "Set as &Default"
msgstr "Laikyti &numatytąja"

#: removeButton.label removeButton.accesskey
msgid "&Remove Site"
msgstr "Pa&šalinti tinklavietės aprašą"

#. Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "Aprašytos tinklavietės"

#: serverInfo.label
msgid "Server Information"
msgstr "Serverio informacija"

#: loginInfo.label
msgid "Login Information"
msgstr "Seanso pradžios informacija"

#: siteName.label siteName.accesskey
msgctxt "siteName.label siteName.accesskey"
msgid "Sit&e Name:"
msgstr "Tinklavietės &pavadinimas:"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr ""
"Alternatyvus (vietinis) pavadinimas tinklavietės, kurioje publikuojate "
"(pvz., „Rašiniai“)"

#: siteUrl.label siteUrl.accesskey
msgid "Publishing &address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr "Publik&avimo adresas (pvz., ftp://ftp.tiekejas.lt/abonentas):"

#: siteUrl.tooltip
msgid ""
"The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr ""
"Adresas prasideda „ftp://“ (http://). Jį galima gauti iš IPT arba prieglobos "
"teikėjo"

#: browseUrl.label browseUrl.accesskey
msgid ""
"H&TTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr ""
"Pradžios &tinklalapio adresas (pvz., http://www.tiekejas.lt/abonentas):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr ""
"Jūsų svetainės pradžios tinklalapio aplanko adresas. Jis prasideda „http://“ "
"ir baigiasi aplanko (ne failo) vardu."

#: username.label username.accesskey
msgid "&User name:"
msgstr "&Abonento vardas:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr ""
"Abonento vardas, su kuriuo prisijungiate prie paslaugas teikiančios tarnybos"

#: password.label password.accesskey
msgid "Pass&word:"
msgstr "&Slaptažodis:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "Slaptažodis, naudojamas kartu su abonento vardu."

#: savePassword.label savePassword.accesskey
msgid "&Save Password"
msgstr "Į&rašyti slaptažodį"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr ""
"Slaptažodį galima išsaugoti slaptažodžių tvarkytuvėje, kad nereikėtų "
"kiekvieną kartą jo rinkti."
