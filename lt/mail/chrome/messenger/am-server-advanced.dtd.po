#: serverAdvanced.label
msgid "Advanced Account Settings"
msgstr "Papildomos paskyros nuostatos"

#. Do not translate "IMAP"
#: serverDirectory.label serverDirectory.accesskey
msgid "IMAP server &directory:"
msgstr "IMAP serverio &šakninis aplankas:"

#: usingSubscription.label usingSubscription.accesskey
msgid "Sho&w only subscribed folders"
msgstr "&Rodyti tik prenumeruotus aplankus"

#: dualUseFolders.label dualUseFolders.accesskey
msgid "Server supports &folders that contain sub-folders and messages"
msgstr "&Serveris turi darbo su poaplankiais priemones"

#: maximumConnectionsNumber.label maximumConnectionsNumber.accesskey
msgid "&Maximum number of server connections to cache"
msgstr "&Didžiausias leistinas lygiagrečių prisijungimų prie serverio skaičius"

#. Do not translate "IMAP"
#: namespaceDesc.label
msgid "These preferences specify the namespaces on your IMAP server"
msgstr "Šios nuostatos apibrėžia IMAP serverio vardų sritis"

#: personalNamespace.label personalNamespace.accesskey
msgid "&Personal namespace:"
msgstr "&Asmeninė vardų sritis:"

#: publicNamespace.label publicNamespace.accesskey
msgid "P&ublic (shared):"
msgstr "&Viešoji (bendro naudojimo):"

#: otherUsersNamespace.label otherUsersNamespace.accesskey
msgid "&Other Users:"
msgstr "&Kitų naudotojų:"

#: overrideNamespaces.label overrideNamespaces.accesskey
msgid "&Allow server to override these namespaces"
msgstr "&Leisti serveriui keisti šias vardų sritis"

#: pop3DeferringDesc.label
msgid ""
"When downloading mail from this account's server, use the following folder "
"to store new messages:"
msgstr "Laiškus, parsiųstus iš šios paskyros serverio, saugokite aplanke:"

