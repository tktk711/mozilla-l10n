#: loadError.label
msgid "Problem loading page"
msgstr "Nepavyko įkelti tinklalapio"

#: retry.label
msgid "Try Again"
msgstr "Bandyti dar kartą"

#. Specific error messages
#: connectionFailure.title
msgid "Unable to connect"
msgstr "Nepavyko užmegzti ryšio"

#: connectionFailure.longDesc
msgctxt "connectionFailure.longDesc"
msgid "&sharedLongDesc;"
msgstr "&sharedLongDesc;"

#: deniedPortAccess.title
msgid "This address is restricted"
msgstr "Prieiga prie šio adreso apribota"

#: deniedPortAccess.longDesc
msgctxt "deniedPortAccess.longDesc"
msgid ""
msgstr " "

#: dnsNotFound.title
msgid "Server not found"
msgstr "Nerastas serveris"

#: dnsNotFound.longDesc
msgid ""
"<ul> <li>Check the address for typing errors such as <strong>ww</strong>."
"example.com instead of <strong>www</strong>.example.com</li> <li>If you are "
"unable to load any pages, check your computer's network connection.</li> "
"<li>If your computer or network is protected by a firewall or proxy, make "
"sure that &brandShortName; is permitted to access the Web.</li> </ul>"
msgstr ""
"<ul> <li>Patikrinkite, ar rinkdami adresą nepadarėte klaidų, pavyzdžiui, "
"<strong>ww</strong>.pavyzdys.lt vietoje <strong>www</strong>.pavyzdys.lt</"
"li> <li>Jei nepavyksta įkelti ir kitų tinklalapių, patikrinkite kompiuterio "
"ryšį su tinklu.</li> <li>Jei jūsų kompiuteris ar tinklas apsaugotas užkarda "
"arba jungiamasi per įgaliotąjį serverį, įsitikinkite, jog programai "
"„&brandShortName;“ leidžiama pasiekti saityną.</li> </ul>"

#: fileNotFound.title
msgid "File not found"
msgstr "Nerastas failas"

#: fileNotFound.longDesc
msgid ""
"<ul> <li>Check the file name for capitalization or other typing errors.</li> "
"<li>Check to see if the file was moved, renamed or deleted.</li> </ul>"
msgstr ""
"<ul> <li>Patikrinkite, ar failo varde nėra rinkimo klaidų, pvz., didžiosios "
"raidės pakeistos mažosiomis.</li> <li>Patikrinkite, ar failas nebuvo "
"perkeltas, pervardytas ar pašalintas.</li> </ul>"

#: generic.title
msgid "Oops."
msgstr "Klaida"

#: generic.longDesc
msgid "<p>&brandShortName; can't load this page for some reason.</p>"
msgstr ""
"<p>Programai „&brandShortName;“ nepavyko įkelti šio tinklalapio dėl "
"nežinomos priežasties.</p>"

#: malformedURI.title
msgid "The address isn't valid"
msgstr "Neteisingas adresas"

#: malformedURI.longDesc
msgid ""
"<ul> <li>Web addresses are usually written like <strong>http://www.example."
"com/</strong></li> <li>Make sure that you're using forward slashes (i.e. "
"<strong>/</strong>).</li> </ul>"
msgstr ""
"<ul> <li>Saityno adresai paprastai turi tokį pavidalą: <strong>http://www."
"pavyzdys/lt/</strong></li> <li>Įsitikinkite, kad vartojate tinkamą adreso "
"komponentų skirtuką (t. y. dešininį brūkšnį <strong>/</strong>).</li> </ul>"

#: netInterrupt.title
msgid "The connection was interrupted"
msgstr "Ryšys buvo nutrauktas"

#: netInterrupt.longDesc
msgctxt "netInterrupt.longDesc"
msgid "&sharedLongDesc;"
msgstr "&sharedLongDesc;"

#: notCached.title
msgid "Document Expired"
msgstr "Dokumento galiojimas baigėsi"

#: notCached.longDesc
msgid ""
"<p>The requested document is not available in the cache.</p><ul><li>As a "
"security precaution, &brandShortName; does not automatically re-request "
"sensitive documents.</li><li>Click Try Again to re-request the document from "
"the website.</li></ul>"
msgstr ""
"<p>Prašomo dokumento programos podėlyje nėra.</p><ul><li>Saugumo sumetimais "
"„&brandShortName;“ jautrių dokumentų automatiškai iš naujo neatsiunčia.</"
"li><li>Jeigu norite dokumentą iš svetainės atsiųsti iš naujo, spustelėkite "
"„Bandyti dar kartą“.</li></ul>"

#: netOffline.title
msgid "Offline mode"
msgstr "Dirbama neprisijungus prie tinklo"

#: netOffline.longDesc2
msgid ""
"<ul> <li>Press \"Try Again\" to switch to online mode and reload the page.</"
"li> </ul>"
msgstr ""
"<ul> <li>Spustelėkite „Bandyti dar kartą“, kad būtų prisijungta prie tinklo "
"ir pabandyta įkelti tinklalapį iš naujo.</li> </ul>"

#: contentEncodingError.title
msgid "Content encoding error"
msgstr "Kodavimo klaida"

#: contentEncodingError.longDesc
msgctxt "contentEncodingError.longDesc"
msgid ""
"<ul> <li>Please contact the website owners to inform them of this problem.</"
"li> </ul>"
msgstr ""
"<ul> <li>Prašome pranešti svetainės savininkams apie šią problemą.</li> </ul>"

#: unsafeContentType.title
msgid "Unsafe file type"
msgstr "Nesaugus failo tipas"

#: unsafeContentType.longDesc
msgctxt "unsafeContentType.longDesc"
msgid ""
"<ul> <li>Please contact the website owners to inform them of this problem.</"
"li> </ul>"
msgstr ""
"<ul> <li>Prašome pranešti svetainės savininkams apie šią problemą.</li> </ul>"

#: netReset.title
msgid "The connection was reset"
msgstr "Ryšys nutrūko"

#: netReset.longDesc
msgctxt "netReset.longDesc"
msgid "&sharedLongDesc;"
msgstr "&sharedLongDesc;"

#: netTimeout.title
msgid "The connection has timed out"
msgstr "Baigėsi prisijungimui skirtas laikas"

#: netTimeout.longDesc
msgctxt "netTimeout.longDesc"
msgid "&sharedLongDesc;"
msgstr "&sharedLongDesc;"

#: unknownProtocolFound.title
msgid "The address wasn't understood"
msgstr "Neatpažintas adresas"

#: unknownProtocolFound.longDesc
msgid ""
"<ul> <li>You might need to install other software to open this address.</li> "
"</ul>"
msgstr ""
"<ul> <li>Gali būti, kad šiam adresui atverti reikia įdiegti papildomą "
"programinę įrangą.</li> </ul>"

#: proxyConnectFailure.title
msgid "The proxy server is refusing connections"
msgstr "Įgaliotasis serveris atmeta ryšį"

#: proxyConnectFailure.longDesc
msgid ""
"<ul> <li>Check the proxy settings to make sure that they are correct.</li> "
"<li>Contact your network administrator to make sure the proxy server is "
"working.</li> </ul>"
msgstr ""
"<ul> <li>Patikrinkite, ar programoje nurodytos teisingos įgaliotojo serverio "
"nuostatos.</li> <li>Jei norite sužinoti, ar įgaliotasis serveris veikia "
"korektiškai, kreipkitės į sistemos administratorių.</li> </ul>"

#: proxyResolveFailure.title
msgid "Unable to find the proxy server"
msgstr "Nerastas įgaliotasis serveris"

#: proxyResolveFailure.longDesc
msgid ""
"<ul> <li>Check the proxy settings to make sure that they are correct.</li> "
"<li>Check to make sure your computer has a working network connection.</li> "
"<li>If your computer or network is protected by a firewall or proxy, make "
"sure that &brandShortName; is permitted to access the Web.</li> </ul>"
msgstr ""
"<ul> <li>Patikrinkite, ar programoje nurodytos teisingos įgaliotojo serverio "
"nuostatos.</li> <li>Patikrinkite, ar yra ryšys tarp jūsų kompiuterio ir "
"tinklo.</li> <li>Jei jūsų kompiuteris ar tinklas apsaugotas užkarda arba "
"jungiamasi per įgaliotąjį serverį, tai įsitikinkite, kad programai "
"„&brandShortName;“ leidžiama pasiekti saityną.</li> </ul>"

#: redirectLoop.title
msgid "The page isn't redirecting properly"
msgstr "Neteisingas tinklalapio peradresavimas"

#: redirectLoop.longDesc
msgid ""
"<ul> <li>This problem can sometimes be caused by disabling or refusing to "
"accept cookies.</li> </ul>"
msgstr ""
"<ul> <li>Ši klaida galėjo įvykti dėl to, kad programa nepriima svetainės "
"slapukų.</li> </ul>"

#: unknownSocketType.title
msgid "Unexpected response from server"
msgstr "Netikėtas serverio atsakas"

#: unknownSocketType.longDesc
msgid ""
"<ul> <li>Check to make sure your system has the Personal Security Manager "
"installed.</li> <li>This might be due to a non-standard configuration on the "
"server.</li> </ul>"
msgstr ""
"<ul> <li>Įsitikinkite, kad į jūsų sistemą yra įdiegta asmeninio saugumo "
"tvarkytuvė.</li> <li>Ši klaida galėjo įvykti dėl nestandartinės serverio "
"konfigūracijos.</li> </ul>"

#: nssFailure2.title
msgctxt "nssFailure2.title"
msgid "Secure connection failed"
msgstr "Saugaus ryšio užmegzti nepavyko"

#: nssFailure2.longDesc2
msgid ""
"<ul> <li>The page you are trying to view can not be shown because the "
"authenticity of the received data could not be verified.</li> <li>Please "
"contact the website owners to inform them of this problem.</li> </ul>"
msgstr ""
"<ul> <li>Tinklalapis, kurį bandote atverti, negali būti parodytas, nes "
"nepavyko patikrinti gautų duomenų autentiškumo.</li> <li>Prašom pranešti "
"apie šią problemą svetainės savininkams.</li> </ul>"

#: nssBadCert.title
msgctxt "nssBadCert.title"
msgid "Secure connection failed"
msgstr "Saugaus ryšio užmegzti nepavyko"

#: nssBadCert.longDesc2
msgid ""
"<ul> <li>This could be a problem with the server's configuration, or it "
"could be someone trying to impersonate the server.</li> <li>If you have "
"connected to this server successfully in the past, the error may be "
"temporary, and you can try again later.</li> </ul>"
msgstr ""
"<ul> <li>Ši problema gali kilti dėl blogai parinktų serverio nuostatų arba "
"dėl bandymo apsimesti kitu serveriu.</li> <li>Jei prie šio serverio "
"sėkmingai jungėtės anksčiau, šis nesklandumas gali būti laikinas. Bandykite "
"prisijungti vėliau.</li> </ul>"

#: sharedLongDesc
msgid ""
"<ul> <li>The site could be temporarily unavailable or too busy. Try again in "
"a few moments.</li> <li>If you are unable to load any pages, check your "
"computer's network connection.</li> <li>If your computer or network is "
"protected by a firewall or proxy, make sure that &brandShortName; is "
"permitted to access the Web.</li> </ul>"
msgstr ""
"<ul> <li>Ši svetainė laikinai nepasiekiama arba yra per daug apkrauta. Šiek "
"tiek palaukite ir bandykite prisijungti iš naujo.</li> <li>Jei nepavyksta "
"įkelti ir kitų tinklalapių, patikrinkite kompiuterio ryšį su tinklu.</li> "
"<li>Jei jūsų kompiuteris ar tinklas apsaugotas užkarda arba jungiamasi per "
"įgaliotąjį serverį, įsitikinkite, jog programai „&brandShortName;“ leidžiama "
"pasiekti saityną.</li> </ul>"

#: malwareBlocked.title
msgid "Suspected attack site!"
msgstr "Svetainė įtariama kenkėjiškumu!"

#: malwareBlocked.longDesc
msgid ""
"<p>Attack sites try to install programs that steal private information, use "
"your computer to attack others, or damage your system.</p> <p>Website owners "
"who believe their site has been reported as an attack site in error may <a "
"href='http://www.stopbadware.org/home/reviewinfo'>request a review</a>.</p>"
msgstr ""
"<p>Kenkėjiškos svetainės siekia įdiegti programas, vagiančias asmeninius "
"duomenis, naudojančias kompiuterį kitų atakoms ir kenkiančias pačiai "
"sistemai.</p> <p>Svetainių, klaidingai palaikytų kenkėjiškomis, savininkai "
"gali <a href='http://www.stopbadware.org/home/reviewinfo'>paprašyti "
"peržiūros</a>.</p>"

#: unwantedBlocked.title
msgid "Suspected Unwanted Software Site!"
msgstr "Įtartina nepageidaujamos programinės įrangos svetainė!"

# Angliškai yra apibūdinama, kas yra nepageidaujamos programinės įrangos svetainės. Manau ir lietuviškai reikėtų parašyti kažką panašaus, nes dabartinis vertimas gal kiek per daug užtikrintai sako, kad lankomas tinklalapis bando kenkti, o juk tai _gali_ būti klaida.
#: unwantedBlocked.longDesc
msgid ""
"<p>Unwanted software pages try to install software that can be deceptive and "
"affect your system in unexpected ways.</p>"
msgstr ""
"<p>Nepageidaujamos programinės įrangos svetainės bando įdiegti programinę "
"įrangą, kuri gali būti žalinga jūsų kompiuteriui.</p>"

#: phishingBlocked.title
msgid "Suspected web forgery!"
msgstr "Svetainė įtariama apgaule!"

#: phishingBlocked.longDesc
msgid ""
"<p>Entering any personal information on this page may result in identity "
"theft or other fraud.</p> <p>These types of web forgeries are used in scams "
"known as phishing attacks, in which fraudulent web pages and emails are used "
"to imitate sources you may trust.</p>"
msgstr ""
"<p>Bet kokių asmeninių duomenų įvedimas šiame tinklalapyje gali tapti "
"tapatybės duomenų vagystės ar kitos apgaulės priežastimi.</p> <p>Tokio tipo "
"apgavystės dažniausiai pasitaiko atakose, kuriose, siekiant sudaryti "
"patikimo šaltinio įvaizdį, pasitelkiami apgaulingi tinklalapiai ir "
"elektroniniai laiškai.</p>"

#: forbiddenBlocked.title
msgid "Forbidden Site"
msgstr "Uždrausta svetainė"

#: forbiddenBlocked.longDesc
msgid ""
"<p>&brandShortName; prevented this page from loading because it is "
"configured to block it.</p>"
msgstr ""
"<p>&brandShortName; neleido įkelti šio tinklalapio, nes taip nurodyta "
"programos nustatymuose.</p>"

#: cspBlocked.title
msgid "Blocked by Content Security Policy"
msgstr "Užblokuota dėl saugumo politikos"

#: cspBlocked.longDesc
msgid ""
"<p>&brandShortName; prevented this page from loading in this way because the "
"page has a content security policy that disallows it.</p>"
msgstr ""
"<p>„&brandShortName;“ neleido įkelti šio tinklalapio, nes jo saugumo "
"politika neleidžia jo turinio įterpti nurodytuoju būdu.</p>"

#: corruptedContentError.title
msgid "Corrupted Content Error"
msgstr "Klaida: duomenys pažeisti"

#: corruptedContentError.longDesc
msgid ""
"<p>The content you are trying to view cannot be shown because an error in "
"the data transmission was detected.</p><ul><li>Please contact the owners to "
"inform them of this problem.</li></ul>"
msgstr ""
"<p>Turinio, kurį bandote atverti, parodyti negalima, nes, perduodant "
"duomenis, įvyko klaida.</p><ul><li>Siūlome susisiekti su jo savininkais ar "
"valdytojais ir pranešti jiems apie šią problemą.</li></ul>"

#: securityOverride.linkText
msgid "Or you can add an exception…"
msgstr "Galite sukurti išimtį…"

#: securityOverride.getMeOutOfHereButton
msgid "Get me out of here!"
msgstr "Išeiti"

#: securityOverride.exceptionButtonLabel
msgid "Add Exception…"
msgstr "Sukurti išimtį…"

#. <!-- LOCALIZATION NOTE (securityOverride.warningContent) - Do not translate the
#. contents of the <button> tags. It uses strings already defined above. The
#. button is included here (instead of netError.xhtml) because it exposes
#. functionality specific to thunderbird.
#: securityOverride.warningContent
msgid ""
"<p>You should not add an exception if you are using an internet connection "
"that you do not trust completely or if you are not used to seeing a warning "
"for this server.</p>  <button id='getMeOutOfHereButton'>&securityOverride."
"getMeOutOfHereButton;</button> <button "
"id='exceptionDialogButton'>&securityOverride.exceptionButtonLabel;</button>"
msgstr ""
"<p>Išimties kurti neturėtumėte, jei naudojatės interneto ryšiu, kuriuo "
"nepasitikite pilnai, arba jei anksčiau nematėte įspėjimo apie šį serverį.</"
"p>  <button id='getMeOutOfHereButton'>&securityOverride.getMeOutOfHereButton;"
"</button> <button id='exceptionDialogButton'>&securityOverride."
"exceptionButtonLabel;</button>"

#: remoteXUL.title
msgid "Remote XUL"
msgstr "XUL iš tinklo"

#: remoteXUL.longDesc
msgid ""
"<p><ul><li>Please contact the website owners to inform them of this problem."
"</li></ul></p>"
msgstr ""
"<p><ul><li>Siūlome susisiekti su svetainės savininkais ar valdytojais ir "
"pranešti jiems apie šią problemą.</li></ul></p>"
