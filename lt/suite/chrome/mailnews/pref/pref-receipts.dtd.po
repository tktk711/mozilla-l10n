#: pref.receipts.title
msgctxt "pref.receipts.title"
msgid "Return Receipts"
msgstr "Laiškų pristatymo pažymos"

#: prefReceipts.caption
msgctxt "prefReceipts.caption"
msgid "Return Receipts"
msgstr "Laiškų pristatymo pažymos"

#: requestReceipt.label requestReceipt.accesskey
msgid "&When sending messages, always request a return receipt"
msgstr "&Kai išsiunčiamas laiškas, paprašyti pranešti apie jo pristatymą"

#: receiptArrive.label
msgid "When a receipt arrives:"
msgstr "Kai gaunamas pranešimas apie išsiųstą laišką:"

#: leaveIt.label leaveIt.accesskey
msgid "&Leave it in my Inbox"
msgstr "pa&likti jį aplanke „Gauti laiškai“"

#: moveToSent.label moveToSent.accesskey
msgid "&Move it to my \"Sent\" folder"
msgstr "į&dėti į išsiųstų laiškų aplanką"

#: requestMDN.label
msgid "When I receive a request for a return receipt:"
msgstr "Kai siuntėjas prašo pranešti apie laiško gavimą:"

#: returnSome.label returnSome.accesskey
msgid "&Allow return receipts for some messages"
msgstr "išsiųsti pranešimus šiais atvejais (parin&kite nuostatas):"

#: never.label never.accesskey
msgid "&Never send a return receipt"
msgstr "pra&nešimo nesiųsti"

#: notInToCc.label notInToCc.accesskey
msgid "If I'm not in the &To or Cc of the message:"
msgstr "jei nesate įrašytas į laiško „Ka&m“ arba „Kopija“ sąrašus:"

#: outsideDomain.label outsideDomain.accesskey
msgid "If the &sender is outside my domain:"
msgstr "jei siun&tėjas yra iš kitos interneto srities:"

#: otherCases.label otherCases.accesskey
msgid "In all &other cases:"
msgstr "&kitais atvejais:"

#: askMe.label
msgid "Ask me"
msgstr "Paklausti"

#: alwaysSend.label
msgid "Always send"
msgstr "Išsiųsti"

#: neverSend.label
msgid "Never send"
msgstr "Neišsiųsti"
