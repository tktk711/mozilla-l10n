#: hostColon
msgid "Host:"
msgstr "Serveris:"

#: domainColon
msgid "Domain:"
msgstr "Sritis:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Tik šifruotasis ryšys"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Bet koks ryšys"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "Seanso pabaigoje"

#: showDetails
msgid "Show Details"
msgstr "Rodyti išsamiau"

#: hideDetails
msgid "Hide Details"
msgstr "Rodyti glausčiau"

