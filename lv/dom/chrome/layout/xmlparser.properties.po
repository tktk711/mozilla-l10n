#. # Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "pietrūkst atmiņas"

#: 2
msgid "syntax error"
msgstr "sintakses kļūda"

#: 3
msgid "no element found"
msgstr "elements nav atrasts"

#: 4
msgid "not well-formed"
msgstr "nav pareizs"

#: 5
msgid "unclosed token"
msgstr "neaizvērts elements"

#: 6
msgid "partial character"
msgstr "daļēja rakstzīme"

#: 7
msgid "mismatched tag"
msgstr "neatbilstošs tags"

#: 8
msgid "duplicate attribute"
msgstr "dubultots atribūts"

#: 9
msgid "junk after document element"
msgstr "muļķības pēc dokumenta elementa"

#: 10
msgid "illegal parameter entity reference"
msgstr "nekorekta norāde uz parametra entītiju"

#: 11
msgid "undefined entity"
msgstr "nedefinēta entītija"

#: 12
msgid "recursive entity reference"
msgstr "rekursīva entītiju norāde"

#: 13
msgid "asynchronous entity"
msgstr "asinhrona entītija"

#: 14
msgid "reference to invalid character number"
msgstr "norāde uz nederīgu rakstzīmes skaitli"

#: 15
msgid "reference to binary entity"
msgstr "norāde uz bināru entītiju"

#: 16
msgid "reference to external entity in attribute"
msgstr "norāde uz ārēju entītiju atribūtā"

#: 17
msgid "XML or text declaration not at start of entity"
msgstr "XML vai teksta deklarācija nav entītijas sākumā"

#: 18
msgid "unknown encoding"
msgstr "nezināms kodējums"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "XML deklarācijā norādītais kodējums ir nekorekts"

#: 20
msgid "unclosed CDATA section"
msgstr "neaizvērta CDATA sadaļa"

#: 21
msgid "error in processing external entity reference"
msgstr "kļūda, apstrādājot ārējās entītijas norādi"

#: 22
msgid "document is not standalone"
msgstr "dokuments nav neatkarīgs"

#: 23
msgid "unexpected parser state"
msgstr "negaidīts apstrādātāja stāvoklis"

#: 24
msgid "entity declared in parameter entity"
msgstr "entītija deklarēta parametra entītijā"

#: 27
msgid "prefix not bound to a namespace"
msgstr "prefikss nav piesaistīts vārdtelpai"

#: 28
msgid "must not undeclare prefix"
msgstr "nevajag atdeklarēt prefiksu"

#: 29
msgid "incomplete markup in parameter entity"
msgstr "nepabeigts iezīmējums parametra entītijā"

#: 30
msgid "XML declaration not well-formed"
msgstr "XML deklarācija nav korekti noformatēta"

#: 31
msgid "text declaration not well-formed"
msgstr "teksta deklarācija nav korekti noformatēta"

#: 32
msgid "illegal character(s) in public id"
msgstr "neatļauts simbols(-i) publiskajā id"

#: 38
msgid ""
"reserved prefix (xml) must not be undeclared or bound to another namespace "
"name"
msgstr ""
"rezervēto prefiksu (xml) nevar atdeklarēt vai piesaistīt citas vārdtelpas "
"nosaukumam"

#: 39
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr "rezervēto prefiksu (xml) nevar deklarēt vai atdeklarēt"

#: 40
msgid "prefix must not be bound to one of the reserved namespace names"
msgstr ""
"prefiksu nevar piesaistīt nevienam no rezervētajām vārdtelpu nosaukumiem"

#. # %1$S is replaced by the Expat error string, may be followed by Expected (see below)
#. # %2$S is replaced by URL
#. # %3$u is replaced by line number
#. # %4$u is replaced by column number
#: XMLParsingError
msgid ""
"XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$u, Column %4$u:"
msgstr ""
"XML apstrādes kļūda: %1$S\n"
"Atrašanās vieta: %2$S\n"
"Rindas numurs %3$u, kolona %4$u:"

#. # %S is replaced by a tag name.
#. # This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". Gaidīts: </%S>."
