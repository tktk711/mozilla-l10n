#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Pēdējā sinhronizācija: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Pēdējā sinhronizācija: šobrīd notiek…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Konts: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Nav savienots"

#: connecting.label
msgid "Connecting…"
msgstr "Savienojas…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Jūsu Firefox Sync konts ir atslēgts"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Atcelt"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 nav savietojama ar jaunāko Firefox Sync versiju. Lūdzu, atjauniniet uz "
"jaunāko versiju."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 nav savietojama ar vecākām Firefox Sync versijām. Lūdzu, atjauniniet "
"Firefox otrā datorā vai datoros."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Uzzināt vairāk"

#: sync.update.close
msgid "Close"
msgstr "Aizvērt"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Nevar iestatīt sinhronizāciju"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Nav savienojuma ar internetu"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S nevar savienoties ar Sync. Vai vēlaties mēģināt vēlreiz?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Mēģināt vēlreiz"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Iestatīt manuāli"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Nav nevienas cilnes no citām ierīcēm."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Sinhronizācija ir pavisam jauna un ērtāka kā jebkad. "

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Vēlaties uzzināt vairāk?"
