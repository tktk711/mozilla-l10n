#: muteButton.unmuteLabel
msgid "Unmute"
msgstr "Ieslēgt skaņu"

#: fullscreenButton.enterfullscreenlabel
msgid "Full Screen"
msgstr "Pa visu ekrānu"

#: fullscreenButton.exitfullscreenlabel
msgid "Exit Full Screen"
msgstr "Aizvērt pilnekrāna režīmu"

#: castingButton.castingLabel
msgid "Cast to Screen"
