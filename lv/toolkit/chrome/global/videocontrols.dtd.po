#: stats.size
msgid "Size"
msgstr "Izmērs"

#: stats.activity
msgid "Activity"
msgstr "Aktivitāte"

#: stats.activityPaused
msgid "Paused"
msgstr "Apturēts"

#: stats.activityPlaying
msgid "Playing"
msgstr "Atskaņo"

#: stats.activityEnded
msgid "Ended"
msgstr "Atskaņots"

#: stats.activitySeeking
msgid "(seeking)"
msgstr "(meklē)"

#: stats.volume
msgid "Volume"
msgstr "Skaļums"

#: stats.framesParsed
msgid "Frames parsed"
msgstr "Apstrādāti kadri"

#: stats.framesDecoded
msgid "Frames decoded"
msgstr "Dekodēti kadri"

#: stats.framesPresented
msgid "Frames presented"
msgstr "Prezentētie kadri"

#: stats.framesPainted
msgid "Frames painted"
msgstr "Uzzīmētie kadri"

#: error.aborted
msgid "Video loading stopped."
msgstr "Video ielāde apturēta."

#: error.network
msgid "Video playback aborted due to a network error."
msgstr "Video atskaņošana apturēta tīkla kļūmes dēļ."

#: error.decode
msgid "Video can't be played because the file is corrupt."
msgstr "Video nevar atskaņot, jo tā fails ir bojāts."

#: error.srcNotSupported
msgid "Video format or MIME type is not supported."
msgstr "Video formāts vai MIME tips nav atbalstīs."

#: error.noSource2
msgid "No video with supported format and MIME type found."
msgstr "Netika atrasts atbalstīta formāta vai MIME tipa video."

#: error.generic
msgid "Video playback aborted due to an unknown error."
msgstr "Video atskaņošana apturēta nezināmas kļūmes dēļ."

#. the #1 string is the current
#. media position, and the #2 string is the total duration. For example, when at
#. the 5 minute mark in a 6 hour long video, #1 would be "5:00" and #2 would be
#. "6:00:00", result string would be "5:00 of 6:00:00 elapsed".
#: scrubberScale.nameFormat
msgid "#1 of #2 elapsed"
msgstr "atskaņotas #1 no #2"
