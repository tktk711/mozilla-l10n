#. # Panel Strings
#: share_link_header_text
msgid "Share this link to invite someone to talk:"
msgstr "ആരെയെങ്കിലും സംസാരിയ്ക്കുവാന്‍ ക്ഷണിയ്ക്കുന്നതിന് ഈ കണ്ണി പങ്കിടുക:"

#. # Status text
#: display_name_dnd_status
msgid "Do Not Disturb"
msgstr "ശല്ല്യം ചെയ്യരുതു്"

#: display_name_available_status
msgid "Available"
msgstr "ലഭ്യം"

#: unable_retrieve_url
msgid "Sorry, we were unable to retrieve a call url."
msgstr "ക്ഷമിക്കണം, കോളിനുള്ള യുആല്‍എല്‍ ലഭ്യമാക്കുവാനായില്ല."

#. # Conversation Window Strings
#: incoming_call_title
msgid "Incoming Call…"
msgstr "വരുന്ന കോള്‍..."

#: incoming_call
msgid "Incoming call"
msgstr "വരുന്ന കോള്‍"

#: incoming_call_answer_button
msgid "Answer"
msgstr "കോളെടുക്കുക"

#: incoming_call_decline_button
msgid "Decline"
msgstr "വേണ്ട"

#: incoming_call_ignore_button
msgid "Ignore"
msgstr "ഉപേക്ഷിയ്ക്കുക"

#: incoming_call_block_button
msgid "Block"
msgstr "തടയുക "

#: hangup_button_title
msgid "Hangup"
msgstr "നിര്‍ത്തുക"

#: mute_local_audio_button_title
msgid "Mute your audio"
msgstr "ഓഡിയോ നിശബ്ദമാക്കുക"

#: unmute_local_audio_button_title
msgid "Unmute your audio"
msgstr "ഓഡിയോ മ്യൂട്ട് മാറ്റുക"

#: mute_local_video_button_title
msgid "Mute your video"
msgstr "വീഡിയോ നിശബ്ദമാക്കുക"

#: unmute_local_video_button_title
msgid "Unmute your video"
msgstr "വീഡിയോ മ്യൂട്ട് മാറ്റുക"

#: peer_ended_conversation
msgid "Your peer ended the conversation."
msgstr "മറ്റേ വ്യക്തി സംസാരം നിര്‍ത്തി."

#: call_has_ended
msgid "Your call has ended."
msgstr "കോള്‍ അവസാനിച്ചു."

#: close_window
msgid "Close this window"
msgstr "ജാലകം അടയ്ക്കുക"

#: cannot_start_call_session_not_ready
msgid "Can't start call, session is not ready."
msgstr "കോള്‍ ആരംഭിയ്ക്കുവാന്‍ സാധ്യമല്ല. സെഷന്‍ തയ്യാറാല്ല."

#: network_disconnected
msgid "The network connection terminated abruptly."
msgstr "നെറ്റ്‌വര്‍ക്ക് കണക്ഷന്‍ പെട്ടെന്ന് നിന്നു."

#: connection_error_see_console_notification
msgid "Call failed; see console for details."
msgstr "കോള്‍ നിന്നുപോയി; വിവരങ്ങള്‍ക്കായി കണ്‍സോള്‍ കാണുക."

