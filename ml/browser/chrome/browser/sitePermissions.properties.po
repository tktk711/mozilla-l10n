#: allow
msgid "Allow"
msgstr "അനുവദിക്കുക"

#: allowForSession
msgid "Allow for Session"
msgstr "ഈ സെഷന്‍ അനുവദിക്കുക"

#: block
msgid "Block"
msgstr "തടയുക "

#: alwaysAsk
msgid "Always Ask"
msgstr "എപ്പോഴും ചോദിയ്ക്കുക"

#: permission.cookie.label
msgid "Set Cookies"
msgstr "കുക്കികള്‍ സജ്ജമാക്കുക"

#: permission.desktop-notification2.label
msgid "Receive Notifications"
msgstr ""

#: permission.image.label
msgid "Load Images"
msgstr "ഇമേജുകള്‍ ലഭ്യമാക്കുക"

#: permission.camera.label
msgid "Use the Camera"
msgstr "ക്യാമറാ ഉപയോഗിയ്ക്കുക"

#: permission.microphone.label
msgid "Use the Microphone"
