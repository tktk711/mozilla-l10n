msgid "Done"
msgstr "പൂര്‍ത്തിയായി"

#: button.syncOptionsCancel.label
msgid "Cancel"
msgstr "റദ്ദാക്കുക"

#: invalidEmail.label
msgid "Invalid email address"
msgstr "തെറ്റായ ഈമെയില്‍ വിലാസം"

#: serverInvalid.label
msgid "Please enter a valid server URL"
msgstr "ശരിയായൊരു സര്‍വര്‍ യുആര്‍എല്‍ നല്‍കുക"

#: usernameNotAvailable.label
