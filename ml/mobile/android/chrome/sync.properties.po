#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "അവസാന സിന്‍ക്: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "അവസാന സിന്‍ക്: പുരോഗതിയില്‍..."

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "അക്കൌണ്ട്: %S"

#: notconnected.label
msgid "Not connected"
msgstr "കണക്ട് ചെയ്തിട്ടില്ല"

#: connecting.label
msgid "Connecting…"
msgstr "കണക്ട് ചെയ്യുന്നു…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "നിങ്ങളുടെ ഫയര്‍ഫോക്സ് സിന്‍ക് നീക്കം ചെയ്തിരിയ്ക്കുന്നു"

#: notificationDisconnect.button
msgid "Undo"
msgstr "വേണ്ട"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"ഫയര്‍ഫോക്സ് സിന്‍കിന്റെ ഏറ്റവും പുതിയ പതിപ്പുമായി #1 #2 പൊരുത്തപ്പെടുന്നില്ല. ദയവായി ഏറ്റവും "
"പുതിയ പതിപ്പിലേക്കു് പുതുക്കുക."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"ഫയര്‍ഫോക്സ് സിന്‍കിന്റെ പഴയ പതിപ്പുകളുമായി #1 #2 പൊരുത്തപ്പെടുന്നില്ല. ദയവായി മറ്റു് സിസ്റ്റങ്ങള്‍ "
"ഏറ്റവും പുതിയ പതിപ്പിലേക്കു് പുതുക്കുക. "

#: sync.update.title
msgid "Firefox Sync"
msgstr "ഫയര്‍ഫോക്സ് സിന്‍ക്"

#: sync.update.button
msgid "Learn More"
msgstr "കൂടുതല്‍ അറിയുക"

#: sync.update.close
msgid "Close"
msgstr "അടക്കുക"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "സിന്‍ക് സജ്ജമാക്കുവാനായില്ല"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "ഇന്റർനെറ്റ് കണക്ഷന്‍ ലഭ്യമല്ല"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S-നു് സിന്‍കിലേക്കു് കണക്ട് ചെയ്യുവാന്‍ സാധ്യമായില്ല. വീണ്ടും ശ്രമിയ്ക്കണോ?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "വീണ്ടും ശ്രമിയ്ക്കുക"

#: sync.setup.manual
msgid "Manual setup"
msgstr "സ്വമേതയുള്ള സജ്ജീകരണം"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "മറ്റു കംമ്പ്യൂട്ടറുകളിൽ നിന്നും ഒരു ടാബുമില്ല."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "സിന്‍ക് ഏറ്റവും പുതിയതും എളുപ്പത്തിലുള്ളതുമാകുന്നു. "

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "കൂടുതലറിയാൻ ഇവിടെ തട്ടുക"
