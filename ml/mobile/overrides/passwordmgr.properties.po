#. # 1st string is the username for the login, 2nd is the login's hostname.
#. # Note that long usernames may be truncated.
#: savePassword
msgid "Save password for \"%1$S\" on %2$S?"
msgstr "%2$S-ല്‍ %1$S-നുള്ള രഹസ്യവാക്ക് സംരക്ഷിക്കണോ?"

#. # String is the login's hostname
#: savePasswordNoUser
msgid "Save password on %S?"
msgstr "%S-ല്‍ രഹസ്യവാക്ക് സംരക്ഷിക്കണോ?"

#: saveButton
msgid "Save"
msgstr "സംരക്ഷിക്കുക"

#: dontSaveButton
msgid "Don't save"
msgstr ""

#. # String is the login's hostname
#: updatePassword
msgid "Update saved password for %S?"
msgstr "%S-ന് സംരക്ഷിച്ച രഹസ്യവാക്ക് പുതുക്കണമോ?"

#: updatePasswordNoUser
msgid "Update saved password for this login?"
msgstr "ഈ ലോഗിനായി സംരക്ഷിച്ച രഹസ്യവാക്ക് പുതുക്കണമോ?"

#: updateButton
msgid "Update"
msgstr "പരിഷ്കരിയ്ക്കുക"

#: dontUpdateButton
msgid "Don't update"
msgstr ""

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "ഏത്‌ ഉപയോക്താവിന്റെ രഹസ്യവാക്കാണ് മാറ്റുന്നത്‌ എന്നു് ഉറപ്പുവരുത്തുക"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "രഹസ്യവാക്ക് മാറ്റം തീര്‍ച്ചപ്പെടുത്തുക"

#. # Strings used by PromptService.js
#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "ഈ രഹസ്യവാക്ക് ഓര്‍ത്തിരിക്കുന്നതിനായി പാസ്‌വേര്‍ഡ് മാനേജര്‍ ഉപയോഗിക്കുക "

#: username
msgid "Username"
msgstr "ഉപയോക്തൃനാമം "

#: password
msgid "Password"
msgstr "രഹസ്യവാക്ക്"
