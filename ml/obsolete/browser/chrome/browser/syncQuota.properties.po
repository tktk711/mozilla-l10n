#: collection.addons.label
msgid "Add-ons"
msgstr "ആഡ്-ഓണുകള്‍"

#: collection.bookmarks.label
msgid "Bookmarks"
msgstr "അടയാളക്കുറിപ്പുകള്‍"

#: collection.history.label
msgid "History"
msgstr "നാള്‍വഴി"

#: collection.passwords.label
msgid "Passwords"
msgstr "രഹസ്യവാക്കുകള്‍"

#: collection.prefs.label
msgid "Preferences"
msgstr "മുന്‍ഗണനകള്‍"

#: collection.tabs.label
msgid "Tabs"
msgstr "റ്റാബുകള്‍"

#. # LOCALIZATION NOTE (quota.usageNoQuota.label): %1$S and %2$S are numeric value
#. # and unit (as defined in the download manager) of the amount of space occupied
#. # on the server
#: quota.usageNoQuota.label
msgid "You are currently using %1$S %2$S."
msgstr "നിങ്ങള്‍ ഇപ്പോള്‍ %1$S %2$S ഉപയോഗിയ്ക്കുന്നു."

#. # LOCALIZATION NOTE (quota.usagePercentage.label):
#. # %1$S is the percentage of space used,
#. # %2$S and %3$S numeric value and unit (as defined in the download manager)
#. # of the amount of space used,
#. # %3$S and %4$S numeric value and unit (as defined in the download manager)
#. # of the total space available.
#: quota.usagePercentage.label
msgid "You are using %1$S%% (%2$S %3$S) of your allowed %4$S %5$S."
msgstr "നിങ്ങള്‍ %1$S%% (%2$S %3$S) ഉപയോഗിയ്ക്കുന്നു. അനുവദിച്ചിരിയ്ക്കുന്നതു് %4$S %5$S."

#: quota.usageError.label
msgid "Could not retrieve quota information."
msgstr "കോട്ടാ വിവരം ലഭ്യമാക്കുവാന്‍ സാധ്യമല്ല."

#: quota.retrieving.label
msgid "Retrieving…"
msgstr "ലഭ്യമാക്കുന്നു…"

#. # LOCALIZATION NOTE (quota.sizeValueUnit.label): %1$S is the amount of space
#. # occupied by the engine, %2$K the corresponding unit (e.g. kB) as defined in
#. # the download manager.
#: quota.sizeValueUnit.label
msgid "%1$S %2$S"
msgstr "%1$S %2$S"

#: quota.remove.label
msgid "Remove"
msgstr "നീക്കം ചെയ്യുക"

#: quota.treeCaption.label
msgid "Uncheck items to stop syncing them and free up space on the server."
msgstr ""
"വസ്തുക്കള്‍ സിന്‍ക് ചെയ്യാതിരിയ്ക്കുന്നതിനായി, തെരഞ്ഞെടുത്തവ വേണ്ടെന്നു് വയ്ക്കുക. അങ്ങനെ സര്‍വറില്‍ "
"കാലി സ്ഥലം ലഭ്യമാക്കുക."

#. # LOCALIZATION NOTE (quota.removal.label): %S is a list of engines that will be
#. # disabled and whose data will be removed once the user confirms.
#: quota.removal.label
msgid "Firefox Sync will remove the following data: %S."
msgstr "ഫയര്‍ഫോക്സ് സിന്‍ക് ഈ ഡേറ്റാ നീക്കം ചെയ്യുന്നു: %S."

#. # LOCALIZATION NOTE (quota.list.separator): This is the separator string used
#. # for the list of engines (incl. spaces where appropriate)
#: quota.list.separator
msgid ", "
msgstr ", "

#. # LOCALIZATION NOTE (quota.freeup.label): %1$S and %2$S are numeric value
#. # and unit (as defined in the download manager) of the amount of space freed
#. # up by disabling the unchecked engines.  If displayed this string is
#. # concatenated directly to quota.removal.label and may need to start off with
#. # whitespace.
#: quota.freeup.label
msgid " This will free up %1$S %2$S."
msgstr " ഇതു് %1$S %2$S കാലിയാക്കുന്നു."
