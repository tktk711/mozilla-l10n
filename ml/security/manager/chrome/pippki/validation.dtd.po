msgid "Name"
msgstr "പേരു്"

#: validation.crllastupdate.label
msgid "Last Update"
msgstr "ഏറ്റവും ഒടുവിലുള്ള പരിഷ്കാരണം"

#: validation.crlnextupdate.label
msgid "Next Update"
