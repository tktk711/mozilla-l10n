#: hostColon
msgid "Host:"
msgstr "ഹോസ്റ്റ്‌:"

#: domainColon
msgid "Domain:"
msgstr "ഡൊമെയിന്‍"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "എന്‍ക്രിപ്റ്റ്‌ ചെയ്ത കണക്ഷനുകള്‍ മാത്രം "

#: forAnyConnection
msgid "Any type of connection"
msgstr "ഏതു തരത്തിലുമുള്ള കണക്ഷന്‍ "

#: expireAtEndOfSession
msgid "At end of session"
msgstr "സെഷനവസാനം"

#: showDetails
msgid "Show Details"
msgstr "വിവരങ്ങള്‍ കാണിക്കുക"

#: hideDetails
msgid "Hide Details"
msgstr "വിവരങ്ങള്‍ ഒളിപ്പിക്കുക "

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "%S എന്ന സൈറ്റിന്  ഒരു കുക്കി സെറ്റ് ‌ ചെയ്യുവാന്‍ ശ്രമിക്കുന്നു"

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "%S എന്ന സൈറ്റിന് രണ്ടാമത് ഒരു കുക്കി ക്രമികരണം."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"%S എന്ന സൈറ്റിനു് മറ്റൊരു കുക്കി ക്റമികരിക്കണം.\n"
" ഈ സൈറ്റില്‍ നിന്നും നിങ്ങള്‍ക്ക് നിലവില്‍ %S കുക്കികള്‍ ലഭ്യമാണ്."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "%S എന്ന സൈറ്റിന് നിലവിലുള്ള ഒരു കുക്കിയില്‍ മാറ്റം വരുത്തണം."
