#~ msgid "&Disable all add-ons"
#~ msgstr "Бүх нэмэгдлүүдийг хор&ь"

#~ msgid "&Reset toolbars and controls"
#~ msgstr "Бага&жит самбар ба хяналтыг шинээр сэргээ"

#~ msgid "Reset &bookmarks to &brandShortName; defaults"
#~ msgstr "Ха&ягнуудыг &brandShortName;-н анхны хаягаар сэргээ"

#~ msgid "Reset all user &preferences to &brandShortName; defaults"
#~ msgstr ""
#~ "Бүх хэрэглэгч&ийн тохируулгыг &brandShortName;-н анхных руу шинээр сэргээ"

#~ msgid "Re&store default search engines"
#~ msgstr "Анхны хайгчдыг &сэргээ"

