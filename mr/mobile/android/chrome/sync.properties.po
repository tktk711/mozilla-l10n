#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "शेवटचे ताळमेळ: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "शेवटचे ताळमेळ: चालू…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "खाते: %S"

#: notconnected.label
msgid "Not connected"
msgstr "जोडलेले नाही"

#: connecting.label
msgid "Connecting…"
msgstr "जोडणी करत आहे…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "आपले Firefox ताळमेळ खाते काढले गेले"

#: notificationDisconnect.button
msgid "Undo"
msgstr "आत्ताची कृती रद्द करा"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr "#1 #2 हे Firefox ताळमेळीच्या नवीनतम आवृत्तीशी सुसंगत नाही. कृपया अद्यावत करा."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 हे Firefox ताळमेळीच्या जुन्या आवृत्तीशी सुसंगत नाही. कृपया तुमच्या दुसऱ्या संगणकांवर "
"Firefox अद्ययावत करा."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox ताळमेळ"

#: sync.update.button
msgid "Learn More"
msgstr "अधिक जाणून घ्या"

#: sync.update.close
msgid "Close"
msgstr "बंद करा"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "ताळमेळ सेटअप करू शकत नाही"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "इंटरनेट जोडणी उपलब्ध नाही"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S ताळमेळशी जोडणी करू शकले नाही. आपण पुनःप्रयत्न करू इच्छिता?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "पुन्हा प्रयत्न करा"

#: sync.setup.manual
msgid "Manual setup"
msgstr "मॅन्युअल सेटअप"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "आपल्या दुसऱ्या संगणकावरून टॅब्स नाहीत."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "ताळमेळ नवीनतम आणि पहिलेपेक्षा अधिक सोपे आहे. "

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "अधिक जाणण्यासाठी येथे टिचकी मारा"
