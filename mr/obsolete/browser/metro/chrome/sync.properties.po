#. # LOCALIZATION NOTE: Used in the default os description when a new account
#. # is being set up. Should be unique to Metro, so that it does not conflict
#. # with Desktop. See /services/sync/modules/engines/clients.js locaName.
#: sync.defaultAccountApplication
msgid "Metro %S"
msgstr "मेट्रो %S"

#. # %S is the date and time at which the last sync successfully completed
#: sync.flyout.connected.lastSynced
msgid "Last sync: %S"
msgstr "शेवटचे सिंक: %S"

#. # %S is the username logged in
#: sync.flyout.connected.account
msgid "Account: %S"
msgstr "खाते : %S"

#. # %S is the name that the user has chosen for this device
#: sync.flyout.connected.device
msgid "Your device: %S"
msgstr "तुमचे साधन: %S"

#~ msgid "%1$S's %2$S on %3$S"
#~ msgstr "%1$S चे %2$S, %3$S वरील"

#~ msgid "Mobile Bookmarks"
#~ msgstr "मोबाइल पुस्तखुणा"

#~ msgid "Remote tabs are being synced…"
#~ msgstr "दूरस्थ टॅब्स् सिंक केले जात आहे…"

#~ msgid "Sync your other devices again to access their tabs"
#~ msgstr "टॅब्सकरिता प्रवेश प्राप्त करण्यासाठी इतर साधनांना पुन्हा सिंक करा"

#~ msgid "All remote tabs are already open"
#~ msgstr "सर्व दूरस्थ टॅब्स् आधिपासूनच उघडे आहेत"

#~ msgid "Recent desktop tabs will be available once they sync"
#~ msgstr "एकदाचे सिंक झाल्यावर नुकतेच डेस्कटॉप टॅब्स् उपलब्ध होतील"

#~ msgid "Error While Signing In"
#~ msgstr "सिंक करतेवेळी त्रुटी आढळली"

#~ msgid "Sync encountered an error while connecting: %1$S.  Please try again."
#~ msgstr "जोडणीकरतेवेळी सिंकला त्रुटी आढळली: %1$S.  कृपया पुन्हा प्रयत्न करा."

#~ msgid "Preferences…"
#~ msgstr "पसंती…"

#~ msgid "P"
#~ msgstr "P"

#~ msgid "Error While Signing Out"
#~ msgstr "प्रवेश अशक्य करतेवेळी त्रुटी आढळली"

#~ msgid ""
#~ "Sync encountered an error while connecting.  It's probably ok, and you "
#~ "don't have to do anything about it."
#~ msgstr ""
#~ "जोडणीकरतेवेळी सिंकला त्रुटी आढळली.  हे संभाव्यतया ठिक आहे, व तुम्हाला याविषयी काहिच "
#~ "करायचे नाही."

#~ msgid "Error While Syncing"
#~ msgstr "ताळमेळवेळी त्रुटी आढळली"

#~ msgid ""
#~ "Sync encountered an error while syncing: %1$S.  Sync will automatically "
#~ "retry this action."
#~ msgstr ""
#~ "ताळमेळवेळी सिंकला त्रुटी आढळली: %1$S.  सिंक स्वयं हि कृती पुन्हा करायचा प्रयत्न करेल."

#~ msgid ""
#~ "Sync has not been able to complete during the last %1$S days. Please "
#~ "check your network settings."
#~ msgstr ""
#~ "शेटवच्या %1$S दिवसांपासून सिंक पूर्ण झाले नाही. कृपया नेटवर्क सेटिंग्स्ची तपासणी करा."

#~ msgid ""
#~ "The Sync server is a little busy right now, but you don't need to do "
#~ "anything about it.  We'll start syncing your data as soon as we can!"
#~ msgstr ""
#~ "सिंक सर्व्हर आत्ता जरा व्यस्त आहे, परंतु तुम्हाला याविषयी काहिच करायचे नाही.  शक्य "
#~ "तेव्हा लवकरच डाटा सिंक केले जाईल!"

#~ msgid "Sync Delay"
#~ msgstr "सिंक विलंब"

#~ msgid "Server Status"
#~ msgstr "सर्व्हर स्थिती"

