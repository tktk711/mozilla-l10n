#. # Panel Strings
#: share_link_header_text
msgid "Share this link to invite someone to talk:"
msgstr "Kongsi pautan ini untuk menjemput seseorang untuk bercakap:"

#. # Status text
#: display_name_dnd_status
msgid "Do Not Disturb"
msgstr "Jangan Ganggu"

#: display_name_available_status
msgid "Available"
msgstr "Tersedia"

#: unable_retrieve_url
msgid "Sorry, we were unable to retrieve a call url."
msgstr "Maaf, kami tidak dapat mendapatkan kembali url panggilan."

#. # Conversation Window Strings
#: incoming_call_title
msgid "Incoming Call…"
msgstr "Panggilan Masuk…"

#: incoming_call
msgid "Incoming call"
msgstr "Panggilan masuk"

#: incoming_call_answer_button
msgid "Answer"
msgstr "Jawab"

#: incoming_call_decline_button
msgid "Decline"
msgstr "Tolak"

#: incoming_call_ignore_button
msgid "Ignore"
msgstr "Abai"

#: incoming_call_block_button
msgid "Block"
msgstr "Sekat"

#: hangup_button_title
msgid "Hangup"
msgstr "Letak"

#: mute_local_audio_button_title
msgid "Mute your audio"
msgstr "Bisukan audio anda"

#: unmute_local_audio_button_title
msgid "Unmute your audio"
msgstr "Nyahbisukan audio anda"

#: mute_local_video_button_title
msgid "Mute your video"
msgstr "Bisukan video anda"

#: unmute_local_video_button_title
msgid "Unmute your video"
msgstr "Nyahbisukan video anda"

#: peer_ended_conversation
msgid "Your peer ended the conversation."
msgstr "Rakan anda telah menamatkan perbualan."

#: call_has_ended
msgid "Your call has ended."
msgstr "Panggilan anda telah tamat."

#: close_window
msgid "Close this window"
msgstr "Tutup tetingkap ini"

#: cannot_start_call_session_not_ready
msgid "Can't start call, session is not ready."
msgstr "Tidak dapat memulakan panggilan, sesi belum bersedia."

#: network_disconnected
msgid "The network connection terminated abruptly."
msgstr "Sambungan rangkaian ditamatkan tiba-tiba."

#: connection_error_see_console_notification
msgid "Call failed; see console for details."
msgstr "Panggilan gagal; lihat konsol untuk perincian."

