#. # Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "kehabisan memori"

#: 2
msgid "syntax error"
msgstr "ralat sintaks"

#: 3
msgid "no element found"
msgstr "tiada elemen ditemui"

#: 4
msgid "not well-formed"
msgstr "tidak berbaik-bentuk"

#: 5
msgid "unclosed token"
msgstr "token tidak ditutup"

#: 6
msgid "partial character"
msgstr "sebahagian aksara"

#: 7
msgid "mismatched tag"
msgstr "tag tidak sepadan"

#: 8
msgid "duplicate attribute"
msgstr "atribut pendua"

#: 9
msgid "junk after document element"
msgstr "sampah selepas elemen dokumen"

#: 10
msgid "illegal parameter entity reference"
msgstr "rujukan entiti parameter tidak sah"

#: 11
msgid "undefined entity"
msgstr "entiti tidak ditentukan"

#: 12
msgid "recursive entity reference"
msgstr "rujukan entiti rekursif"

#: 13
msgid "asynchronous entity"
msgstr "penyegerakan entiti"

#: 14
msgid "reference to invalid character number"
msgstr "rujukan dakan nombor aksara tidak sah"

#: 15
msgid "reference to binary entity"
msgstr "rujukan kepada entiti binari"

#: 16
msgid "reference to external entity in attribute"
msgstr "rujukan kepada entiti luaran dalam atribut"

#: 17
msgid "XML or text declaration not at start of entity"
msgstr "deklarasi XML atau teks tidak pada permulaan entiti"

#: 18
msgid "unknown encoding"
msgstr "pengkodan tidak diketahui"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "pengkodan ditentukan dalam deklarasi XML tidak betul"

#: 20
msgid "unclosed CDATA section"
msgstr "seksyen CDATA tidak ditutup"

#: 21
msgid "error in processing external entity reference"
msgstr "ralat dalam memproses rujukan entiti luaran"

#: 22
msgid "document is not standalone"
msgstr "dokumen bukanlah tersendiri"

#: 23
msgid "unexpected parser state"
msgstr "keadaan penghurai tidak dijangka"

#: 24
msgid "entity declared in parameter entity"
msgstr "entiti diisytihar dalam entiti parameter"

#: 27
msgid "prefix not bound to a namespace"
msgstr "awalan tidak dijilid kepada ruang nama"

#: 28
msgid "must not undeclare prefix"
msgstr "hendaklah tidak mengisytihar prefix"

#: 29
msgid "incomplete markup in parameter entity"
msgstr ""
"awalan rizab (xml) mestilah tidak diisytiharkan atau dijilid pada ruang nama "
"lain URI"

#: 30
msgid "XML declaration not well-formed"
msgstr ""
"awalan rizab (xmlns) mestilah tidak diisytiharkan atau belum diisytihar"

#: 31
msgid "text declaration not well-formed"
msgstr ""
"awalan mestilah tidak dijilid kepada salah satu dari ruang nama rizab URI"

#: 32
msgid "illegal character(s) in public id"
msgstr "aksara(-aksara) tak sah dalam id awam"

#: 38
msgid ""
"reserved prefix (xml) must not be undeclared or bound to another namespace "
"name"
msgstr ""
"awalan terpelihara (xml) tidak perlu tidak diisytiharkan atau terikat kepada "
"nama ruang nama yang lain"

#: 39
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr ""
"awalan yang dipinjam (xmlns) tidak perlu diisytiharkan atau tidak "
"diisytiharkan"

#: 40
msgid "prefix must not be bound to one of the reserved namespace names"
msgstr ""
"awalan tidak perlu terikat kepada salah satu daripada nama-nama ruang nama "
"yang dipinjam"

#. # %1$S is replaced by the Expat error string, may be followed by Expected (see below)
#. # %2$S is replaced by URL
#. # %3$u is replaced by line number
#. # %4$u is replaced by column number
#: XMLParsingError
msgid ""
"XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$u, Column %4$u:"
msgstr ""
"Ralat Penghurai XML:  %1$S\n"
"Lokasi:  %2$S\n"
"Nombor Baris %3$u, Kolum %4$u:"

#. # %S is replaced by a tag name.
#. # This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". Dijangka: </%S>"
