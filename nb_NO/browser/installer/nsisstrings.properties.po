msgstr "Avbryt"

#: OPTIONS_BUTTON
msgid "&Options"
msgstr "&Innstillinger"

#: MAKE_DEFAULT
msgid "&Make $BrandShortName my default browser"
msgstr "&Gjør $BrandShortName til standard nettleser"

#: CREATE_SHORTCUTS
msgid "Create Shortcuts for $BrandShortName:"
msgstr "Opprett snarveier for $BrandShortName:"

#: ADD_SC_TASKBAR
msgid "On my &Task bar"
msgstr "På &oppgavelinjen"

#: ADD_SC_QUICKLAUNCHBAR
msgid "On my &Quick Launch bar"
msgstr "På &hurtigstartlinjen"

#: ADD_CheckboxShortcutInStartMenu
msgid "In my &Start Menu Programs Folder"
msgstr "I s&tartmenyen"

#: ADD_CheckboxShortcutOnDesktop
msgid "On my &Desktop"
msgstr "På &skrivebordet"

#: SPACE_REQUIRED
msgid "Space Required:"
msgstr "Diskplass som trengs:"

#: SPACE_AVAILABLE
msgid "Space Available:"
msgstr "Tilgjengelig diskplass:"

#: ONE_MOMENT_INSTALL
msgid ""
"One moment, $BrandShortName will launch as soon as the install is complete…"
msgstr ""
"Et øyeblikk, $BrandShortName vil startes så snart installeringen er fedig …"

#: ONE_MOMENT_UPGRADE
msgid ""
"One moment, $BrandShortName will launch as soon as the upgrade is complete…"
msgstr ""
"Et øyeblikk, $BrandShortName vil startes så snart oppgraderingen er ferdig …"

#: INSTALL_MAINT_SERVICE
msgid "&Install the $BrandShortName background update service"
msgstr "&Installer oppdateringstjenesten for $BrandShortName"

#: SEND_PING
msgid "S&end information about this installation to Mozilla"
msgstr "S&end informasjon om denne installeringen til Mozilla"

#: BROWSE_BUTTON
msgid "B&rowse…"
msgstr "&Bla gjennom …"

#: DEST_FOLDER
msgid "Destination Folder"
msgstr "Målmappe"

#: DOWNLOADING_LABEL
msgid "Downloading $BrandShortName…"
msgstr "Laster ned $BrandShortName …"

#: INSTALLING_LABEL
msgid "Installing $BrandShortName…"
msgstr "Installerer $BrandShortName …"

#: UPGRADING_LABEL
msgid "Upgrading $BrandShortName…"
msgstr "Oppgraderer $BrandShortName …"

#: SELECT_FOLDER_TEXT
msgid "Select the folder to install $BrandShortName in."
msgstr "Velg mappen å installere $BrandShortName til."

