#. # CORS
#. # LOCALIZATION NOTE: Do not translate "Access-Control-Allow-Origin", Access-Control-Allow-Credentials, Access-Control-Allow-Methods, Access-Control-Allow-Headers
#: CORSDisabled
msgid ""
"Cross-Origin Request Blocked: The Same Origin Policy disallows reading the "
"remote resource at %1$S. (Reason: CORS disabled)."
msgstr ""
"Forespørsel med kryssende opprinnelse ble blokkert: Same Origin-policyen "
"tillater ikke lesing av den eksterne ressursen på %1$S. (Årsak: CORS "
"avslått)."

#: CORSRequestNotHttp
msgid ""
"Cross-Origin Request Blocked: The Same Origin Policy disallows reading the "
"remote resource at %1$S. (Reason: CORS request not http)."
msgstr ""
"Forespørsel med kryssende opprinnelse ble blokkert: Same Origin-policyen "
"tillater ikke lesing av den eksterne ressursen på %1$S. (Årsak: CORS-"
"forespørsel ikke http)."

#: CORSMissingAllowOrigin
#, fuzzy
msgid ""
"Cross-Origin Request Blocked: The Same Origin Policy disallows reading the "
"remote resource at %1$S. (Reason: CORS header ‘Access-Control-Allow-Origin’ "
"missing)."
msgstr ""
"Forespørsel med kryssende opprinnelse ble blokkert: Same Origin-policyen "
"tillater ikke lesing av den eksterne ressursen på %1$S. (Årsak: CORS-header "
"'Access-Control-Allow-Origin' mangler)."

#: CORSAllowOriginNotMatchingOrigin
#, fuzzy
msgid ""
"Cross-Origin Request Blocked: The Same Origin Policy disallows reading the "
"remote resource at %1$S. (Reason: CORS header ‘Access-Control-Allow-Origin’ "
"does not match ‘%2$S’)."
msgstr ""
"Forespørsel med kryssende opprinnelse ble blokkert: Same Origin-policyen "
"tillater ikke lesing av den eksterne ressursen på %1$S. (Årsak: CORS-header "
"'Access-Control-Allow-Origin' er ikke '%2$S')."

#: CORSMethodNotFound
#, fuzzy
msgid ""
"Cross-Origin Request Blocked: The Same Origin Policy disallows reading the "
"remote resource at %1$S. (Reason: Did not find method in CORS header ‘Access-"
"Control-Allow-Methods’)."
msgstr ""
"Forespørsel med kryssende opprinnelse ble blokkert: Same Origin-policyen "
"tillater ikke lesing av den eksterne ressursen på %1$S. (Årsak: Fant ikke "
"metoden i CORS-headeren 'Access-Control-Allow-Methods')."

#: CORSMissingAllowCredentials
#, fuzzy
msgid ""
"Cross-Origin Request Blocked: The Same Origin Policy disallows reading the "
"remote resource at %1$S. (Reason: expected ‘true’ in CORS header ‘Access-"
"Control-Allow-Credentials’)."
msgstr ""
"Forespørsel med kryssende opprinnelse ble blokkert: Same Origin-policyen "
"tillater ikke lesing av den eksterne ressursen på %1$S. (Årsak: forventet "
"'true' i CORS-headeren 'Access-Control-Allow-Credentials')."

#: CORSPreflightDidNotSucceed
msgid ""
"Cross-Origin Request Blocked: The Same Origin Policy disallows reading the "
"remote resource at %1$S. (Reason: CORS preflight channel did not succeed)."
msgstr ""
"Forespørsel med kryssende opprinnelse ble blokkert: Same Origin-policyen "
"tillater ikke lesing av den eksterne ressursen på %1$S. (Årsak: CORS "
"preflight-kanal var ikke vellykket)."

#: CORSInvalidAllowMethod
#, fuzzy
msgid ""
"Cross-Origin Request Blocked: The Same Origin Policy disallows reading the "
"remote resource at %1$S. (Reason: invalid token ‘%2$S’ in CORS header "
"‘Access-Control-Allow-Methods’)."
msgstr ""
"Forespørsel med kryssende opprinnelse ble blokkert: Same Origin-policyen "
"tillater ikke lesing av den eksterne ressursen på %1$S. (Årsak: ugyldig "
"verdi '%2$S' i CORS-headeren 'Access-Control-Allow-Methods')."

#: CORSInvalidAllowHeader
#, fuzzy
msgid ""
"Cross-Origin Request Blocked: The Same Origin Policy disallows reading the "
"remote resource at %1$S. (Reason: invalid token ‘%2$S’ in CORS header "
"‘Access-Control-Allow-Headers’)."
msgstr ""
"Forespørsel med kryssende opprinnelse ble blokkert: Same Origin-policyen "
"tillater ikke lesing av den eksterne ressursen på %1$S. (Årsak: ugyldig "
"verdi '%2$S' i CORS-headeren 'Access-Control-Allow-Headers')."

#: CORSMissingAllowHeaderFromPreflight
#, fuzzy
msgid ""
"Cross-Origin Request Blocked: The Same Origin Policy disallows reading the "
"remote resource at %1$S. (Reason: missing token ‘%2$S’ in CORS header "
"‘Access-Control-Allow-Headers’ from CORS preflight channel)."
msgstr ""
"Forespørsel med kryssende opprinnelse ble blokkert: Same Origin-policyen "
"tillater ikke lesing av den eksterne ressursen på %1$S. (Årsak: manglende "
"verdi '%2$S' i CORS-headeren 'Access-Control-Allow-Headers' fra CORS "
"preflight-kanal)."

#. # LOCALIZATION NOTE: Do not translate "Strict-Transport-Security", "HSTS", "max-age" or "includeSubDomains"
#: STSUnknownError
msgid ""
"Strict-Transport-Security: An unknown error occurred processing the header "
"specified by the site."
msgstr ""
"Strict-Transport-Security: En ukjent feil oppstod ved behandling av headeren "
"oppgitt av nettstedet."

#: STSUntrustworthyConnection
msgid ""
"Strict-Transport-Security: The connection to the site is untrustworthy, so "
"the specified header was ignored."
msgstr ""
"Strict-Transport-Security: Tilkoblingen til nettstedet kan ikke stoles på, "
"så headeren ble ignorert."

#: STSCouldNotParseHeader
msgid ""
"Strict-Transport-Security: The site specified a header that could not be "
"parsed successfully."
msgstr ""
"Strict-Transport-Security: Nettstedet oppgav en header som ikke kunne bli "
"tolket."

#: STSNoMaxAge
#, fuzzy
msgid ""
"Strict-Transport-Security: The site specified a header that did not include "
"a ‘max-age’ directive."
msgstr ""
"Strict-Transport-Security: Nettstedet oppgav en header som ikke inneholdt et "
"'max-age'-direktiv."

#: STSMultipleMaxAges
#, fuzzy
msgid ""
"Strict-Transport-Security: The site specified a header that included "
"multiple ‘max-age’ directives."
msgstr ""
"Strict-Transport-Security: Nettstedet oppgav en header som inneholdt flere "
"'max-age'-direktiver."

#: STSInvalidMaxAge
#, fuzzy
msgid ""
"Strict-Transport-Security: The site specified a header that included an "
"invalid ‘max-age’ directive."
msgstr ""
"Strict-Transport-Security: Nettstedet oppgav en header som inneholdt et "
"ugyldig 'max-age'-direktiv."

#: STSMultipleIncludeSubdomains
#, fuzzy
msgid ""
"Strict-Transport-Security: The site specified a header that included "
"multiple ‘includeSubDomains’ directives."
msgstr ""
"Strict-Transport-Security: Nettstedet oppgav en header som inneholdt flere "
"'includeSubDomains'-direktiver."

#: STSInvalidIncludeSubdomains
#, fuzzy
msgid ""
"Strict-Transport-Security: The site specified a header that included an "
"invalid ‘includeSubDomains’ directive."
msgstr ""
"Strict-Transport-Security: Nettstedet oppgav en header som inneholdt et "
"ugyldig 'includeSubDomains'-direktiv."

#: STSCouldNotSaveState
msgid ""
"Strict-Transport-Security: An error occurred noting the site as a Strict-"
"Transport-Security host."
msgstr ""
"Strict-Transport-Security: En feil oppstod ved markering av nettstedet som "
"en Strict-Transport-Security-vert."

