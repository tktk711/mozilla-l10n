#. # Download Messages Prompt
#: downloadMessagesWindowTitle1
msgid "Download Messages"
msgstr "Last ned meldinger"

#: downloadMessagesLabel1
msgid "Do you want to download messages for offline use before going offline?"
msgstr "Vil du laste ned meldinger for frakoblet bruk før du kobler fra?"

#: downloadMessagesCheckboxLabel1
msgid "Always ask me when I go offline"
msgstr "Spør alltid før jeg kobler fra"

#: downloadMessagesNow2
msgid "&Download Now"
msgstr "&Last ned nå"

#. # Send Messages Prompt
#: sendMessagesWindowTitle1
msgctxt "sendMessagesWindowTitle1"
msgid "Unsent Messages"
msgstr "Usendte meldinger"

#: sendMessagesLabel2
msgid "Do you want to send your unsent messages now?"
msgstr "Vil du sende meldingene som venter på å bli sendt nå?"

#: sendMessagesCheckboxLabel1
msgid "Always ask me when I go online"
msgstr "Alltid spør meg når jeg kobler til"

#: sendMessagesNow2
msgid "&Send Now"
msgstr "&Send nå"

#: processMessagesLater2
msgid "&Later"
msgstr "S&enere"

#. # GetMessages While Offline Prompt
#: getMessagesOfflineWindowTitle1
msgid "Get Messages"
msgstr "Last ned meldinger"

#: getMessagesOfflineLabel1
msgid ""
"You are currently offline. Do you want to go online to get new messages?"
msgstr ""
"Du er i frakoblet modus. Vil du koble til for å laste ned nye meldinger?"

#. # Send Messages Offline Prompt
#: sendMessagesOfflineWindowTitle1
msgctxt "sendMessagesOfflineWindowTitle1"
msgid "Unsent Messages"
msgstr "Usendte meldinger"

#: sendMessagesOfflineLabel1
msgid ""
"You are currently offline. Do you want to go online to send unsent messages?"
msgstr ""
"Du er i frakoblet modus. Vil du koble til for å sende usendte meldinger?"

#: offlineTooltip
msgid "You are currently offline."
msgstr "Du er i frakoblet modus."

#: onlineTooltip
msgid "You are currently online."
msgstr "Du er i tilkoblet modus."
