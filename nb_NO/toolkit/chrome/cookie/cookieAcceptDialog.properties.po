#: hostColon
msgid "Host:"
msgstr "Server:"

#: domainColon
msgid "Domain:"
msgstr "Domene:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Bare krypterte tilkoblinger"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Alle typer tilkoblinger"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "Ved avslutning av økten"

#: showDetails
msgid "Show Details"
msgstr "Vis detaljer"

#: hideDetails
msgid "Hide Details"
msgstr "Skjul detaljer"

#: detailsAccessKey
msgid "T"
msgstr "d"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Nettstedet %S ønsker å sette en infokapsel."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "Nettstedet %S ønsker å sette enda en infokapsel."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"Nettstedet %S ønsker til å sette enda en infokapsel.\n"
"Du har allerede %S infokapsler fra dette nettstedet."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "Nettstedet %S ønsker å endre en allerede opprettet infokapsel."
