#. # strings used by the Find bar, split from browser.properties
#: NotFound
msgid "Phrase not found"
msgstr "Finner ikke uttrykket"

#: WrappedToTop
msgid "Reached end of page, continued from top"
msgstr "Nådde bunnen av siden, fortsetter fra toppen"

#: WrappedToBottom
msgid "Reached top of page, continued from bottom"
msgstr "Nådde toppen av siden, fortsetter fra bunnen"

#: NormalFind
msgid "Find in page"
msgstr "Finn på siden"

#: FastFind
msgid "Quick find"
msgstr "Hurtigsøk"

#: FastFindLinks
msgid "Quick find (links only)"
msgstr "Hurtigsøk (bare lenker)"

#: CaseSensitive
msgid "(Case sensitive)"
msgstr "(Skill mellom store/små bokstaver)"

#: EntireWord
msgid "(Whole words only)"
msgstr "(Bare hele ord)"

