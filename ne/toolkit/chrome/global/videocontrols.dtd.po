#: playButton.playLabel
msgid "Play"
msgstr "प्ले"

#: playButton.pauseLabel
msgid "Pause"
msgstr "रोक्नुहोस्"

#: muteButton.muteLabel
msgid "Mute"
msgstr "म्यूट"

#: muteButton.unmuteLabel
msgid "Unmute"
msgstr "अन म्यूट"

#: fullscreenButton.enterfullscreenlabel
msgid "Full Screen"
msgstr "पूरा पर्दा"

#: fullscreenButton.exitfullscreenlabel
msgid "Exit Full Screen"
msgstr "पूरा पर्दाबाट बाहिर जानुहोस्"

#: castingButton.castingLabel
msgid "Cast to Screen"
msgstr "स्क्रिनमा कास्ट गर्नुहोस्"

#: closedCaption.off
msgid "Off"
msgstr "बन्द गर्नुहोस"

#: stats.media
msgid "Media"
msgstr "मिडिया"

#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: stats.size
msgid "Size"
msgstr "आकार"

#: stats.activity
msgid "Activity"
msgstr "क्रियाकलाप"

#: stats.activityPaused
msgid "Paused"
msgstr "रोकिएको"

#: stats.activityPlaying
msgid "Playing"
msgstr "बजाउदै"

#: stats.activityEnded
msgid "Ended"
msgstr "सकियो"

#: stats.activitySeeking
msgid "(seeking)"
msgstr "(खोज्दै)"

#: stats.volume
msgid "Volume"
msgstr "आवाज"

#: stats.framesParsed
msgid "Frames parsed"
msgstr "फ्रेमहरू पद वर्णन गरियो"

#: stats.framesDecoded
msgid "Frames decoded"
msgstr "फ्रेमहरू सङ्केत भयो"

#: stats.framesPresented
msgid "Frames presented"
msgstr "फ्रेमहरू प्रस्तुत"

#: stats.framesPainted
msgid "Frames painted"
msgstr "फ्रेमहरू चित्रित"

#: error.aborted
msgid "Video loading stopped."
msgstr "भिडियो लोड हुँदै गर्दा रोकियो।"

#: error.network
msgid "Video playback aborted due to a network error."
msgstr "सञ्जालको त्रुटिले गर्दा भिडियो प्लेब्याक परित्याग हुन पुग्यो।"

#: error.decode
msgid "Video can’t be played because the file is corrupt."
msgstr "फाइलको खराबीले गर्दा भिडियो प्ले गर्न सकिएन।"

#: error.srcNotSupported
msgid "Video format or MIME type is not supported."
msgstr "भिडियो ढाँचा वा MIME प्रकार असमर्थित।"

#: error.noSource2
msgid "No video with supported format and MIME type found."
msgstr "कुनै पनि समर्थित भिडियो ढाँचा र MIME प्रकार भेटिएन।"

#: error.generic
msgid "Video playback aborted due to an unknown error."
msgstr "अज्ञात त्रुटिले गर्दा भिडियो प्लेब्याक परित्याग हुन पुग्यो।"

#. the #1 string is the current
#. media position, and the #2 string is the total duration. For example, when at
#. the 5 minute mark in a 6 hour long video, #1 would be "5:00" and #2 would be
#. "6:00:00", result string would be "5:00 of 6:00:00 elapsed".
#: scrubberScale.nameFormat
msgid "#1 of #2 elapsed"
msgstr "#1 को #2 व्यतीत भयो"

