#. # LOCALIZATION NOTE (commands):
#. #  %S is a comma separated list of command names.
#: commands
msgid ""
"Commands: %S.\n"
"Use /help &lt;command&gt; for more information."
msgstr ""
"Kommandoar: %S.\n"
"Bruk /help &lt;command&gt; for meir informasjon."

#. # LOCALIZATION NOTE (noCommand, noHelp):
#. #  %S is the command name the user typed.
#: noCommand
msgid "No '%S' command."
msgstr "Ingen kommando «%S» finst."

#: noHelp
msgid "No help message for the '%S' command, sorry!"
msgstr "Ingen hjelpetekst for kommandoen «%S» finst, beklagar!"

#: sayHelpString
msgid "say &lt;message&gt;: send a message without processing commands."
msgstr "say &lt;message&gt;: send ei melding utan å behandla kommandoar."

#: rawHelpString
msgid "raw &lt;message&gt;: send a message without escaping HTML entities."
msgstr "raw &lt;message&gt;: send ei melding utan å verna HTML-entitetar."

#: helpHelpString
msgid ""
"help &lt;name&gt;: show the help message for the &lt;name&gt; command, or "
"the list of possible commands when used without parameter."
msgstr ""
"help &lt;name&gt;: vis hjelpetekst om &lt;name&gt;-kommandoen, eller ei "
"liste over tilgjengelege kommandoar når ingen parameter er spesifiserte."

#. # LOCALIZATION NOTE (statusCommand):
#. #  %1$S is replaced with a status command name
#. #   (one of "back", "away", "busy", "dnd", or "offline").
#. #  %2$S is replaced with the localized version of that status type
#. #   (one of the 5 strings below).
#: statusCommand
msgid ""
"%1$S &lt;status message&gt;: set the status to %2$S with an optional status "
"message."
msgstr ""
"%1$S &lt;status melding&gt;: sett statusen til %2$S, med ei valfri "
"statusmelding."

#: back
msgid "available"
msgstr "tilgjengeleg"

#: away
msgid "away"
msgstr "borte"

#: busy
msgctxt "busy"
msgid "unavailable"
msgstr "utilgjengeleg"

#: dnd
msgctxt "dnd"
msgid "unavailable"
msgstr "utiljengeleg"

#: offline
msgid "offline"
msgstr "fråkopla"
