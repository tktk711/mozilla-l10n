#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Siste synkronisering: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Siste synkronisering: I framdrift …"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Konto: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Ikkje tilkopla"

#: connecting.label
msgid "Connecting…"
msgstr "Koplar til …"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Firefox Sync-kontoen din er fjerna"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Angre"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 er ikkje kompatibel med den nyaste versjonen av Firefox Sync. Oppdater "
"til den nyaste versjonen."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 er ikkje kompatibel med eldre versjonar av Firefox Sync. Oppdater "
"Firefox på dei andre datamaskinene dine."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Les meir"

#: sync.update.close
msgid "Close"
msgstr "Lukk"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Kan ikkje konfigurera Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Ikkje noko Internett-samband funne"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S klarte ikkje å kopla til Sync. Vil du prøva igjen?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Prøv igjen"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Manuell konfigurasjon"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Ingen faner frå andre datamaskiner."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Sync er heilt ny og mykje enklare å bruka enn før. "

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Trykk her for å lesa meir"
