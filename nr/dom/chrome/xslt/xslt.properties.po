#: 1
msgid "Parsing an XSLT stylesheet failed."
msgstr ""
"Ukwakha XSLT okusisetjenziswa sokuhlathulula ilimi njengekhasi kuhlulekile."

#: 2
msgid "Parsing an XPath expression failed."
msgstr "Ukwakha XPath kuhlulekile."

#: 4
msgid "XSLT transformation failed."
msgstr "Ukutjhugulula XSLT kuhlulekile."

#: 5
msgid "Invalid XSLT/XPath function."
msgstr "Isisetjenziswa seXpath esingakalungi."

#: 6
msgid "XSLT Stylesheet (possibly) contains a recursion."
msgstr "Ikhasi lendlela ye XSLT (mhlambe) kunokuphindaphinda."

#: 7
msgid "Attribute value illegal in XSLT 1.0."
msgstr "Isabelo senani asikavumeleki ku XSLT 1.0."

#: 8
msgid "An XPath expression was expected to return a NodeSet."
msgstr "Indlela yokusebenza ngeXPath beyilindeleke ukubuyisa Isedeyedzimelelo."

#: 9
msgid "XSLT transformation was terminated by <xsl:message>."
msgstr "Ukutjhugululwa kweXSLT kujanyiswe yi <xsl:message>."

#: 10
msgid "A network error occured loading an XSLT stylesheet:"
msgstr "Kwenzeke iphoso yehlanganiso nakulayitjhwa ikhasi lendlela le XSLT:"

#: 11
msgid "An XSLT stylesheet does not have an XML mimetype:"
msgstr "Ikhasi lendlela ye- XSLT alinakufanisa kwe-XML:"

#: 12
msgid "An XSLT stylesheet directly or indirectly imports or includes itself:"
msgstr ""
"Indlela yekhasi leXSLT lingenise ngaphakathi bunqopha namkha ingasi bunqopha "
"namkha lizibandakanyile:"

#: 13
msgid "An XPath function was called with the wrong number of arguments."
msgstr "Kubizwe isisetjenziswa seXPath ngenomboro engakalungi yokuphikisana."

#: 14
msgid "An unknown XPath extension function was called."
msgstr "Kubizwe isisetjenziswa esingeezelelako seXPath esingaziwa."

#: 15
msgid "XPath parse failure: ')' expected:"
msgstr "Ukuhluleka kwesisetjenziswa seXPath: ')' kulindelekile:"

#: 16
msgid "XPath parse failure: invalid axis:"
msgstr "Ukuhluleka kwesisetjenziswa seXPath: akunalayini elungileko:"

#: 17
msgid "XPath parse failure: Name or Nodetype test expected:"
msgstr ""
"Ukuhluleka kwesisetjenziswa seXPath: Igama namkha Umhlobo wedzimelela "
"ulindelekile:"

#: 18
msgid "XPath parse failure: ']' expected:"
msgstr "Ukuhluleka kwesisetjenziswa seXPath: ']' kulindelekile:"

#: 19
msgid "XPath parse failure: invalid variable name:"
msgstr ""
"Ukuhluleka kwesisetjenziswa seXPath: igama elitjhugululwako elingasebenziko:"

#: 20
msgid "XPath parse failure: unexpected end of expression:"
msgstr ""
"Ukuhlupheka kwesisetjenziswa seXPath ukuhluleka kwesisetjenziswa sokulungisa "
"ilimi: ukuphela kokubonakalisa okungakalindeleki:"

#: 21
msgid "XPath parse failure: operator expected:"
msgstr "Ukuhluleka kwesisetjenziswa seXPath: umphathi ulindelekile:"

#: 22
msgid "XPath parse failure: unclosed literal:"
msgstr "Ukuhlukela kwesisetjenziswa seXPath: umtlolo ongavalwa:"

#: 23
msgid "XPath parse failure: ':' unexpected:"
msgstr "Ukuhluleka kwesisetjenziswa seXPath: ':' akukalindeleki:"

#: 24
msgid "XPath parse failure: '!' unexpected, negation is not():"
msgstr ""
"Ukuhluleka kwesisetjenziswa seXPath: '!' akukalindeleki, ukuphikisa akusi():"

#: 25
msgid "XPath parse failure: illegal character found:"
msgstr ""
"Ukuhluleka kwesisetjenziswa seXPath: kufunyenwe ukwakheka "
"okungasisemthethweni:"

#: 26
msgid "XPath parse failure: binary operator expected:"
msgstr ""
"Ukuhluleka kwesisetjenziswa seXPath: kulindelwe isisetjenziswa esikabili:"

#: 27
msgid "An XSLT stylesheet load was blocked for security reasons."
msgstr ""
"Ukulayitjhwa komhlobo wekhasi leXSLT kuvaliwe ngencayenzathu zevikeleko."

#: LoadingError
msgid "Error loading stylesheet: %S"
msgstr "Kubenephoso ukulayitjha umhlobo wekhasi: %S"

#: TransformError
msgid "Error during XSLT transformation: %S"
msgstr "Kubenephoso ngesikhathi sokutjhugulwa kwe XSLT: %S"
