#. # Tooltips and alt text for side panel toolbar buttons
#. # (the _label strings are alt text for the buttons, the .title strings are
#. # tooltips)
#: toggle_sidebar.title
msgctxt "toggle_sidebar.title"
msgid "Toggle Sidebar"
msgstr "Šielanya para ya ka thoko"

#: toggle_sidebar_label
msgctxt "toggle_sidebar_label"
msgid "Toggle Sidebar"
msgstr "Šielanya para ya ka thoko"

#: outline.title
msgid "Show Document Outline"
msgstr "Laetša kakaretšo ya tokumente"

#: outline_label
msgid "Document Outline"
msgstr "Kakaretšo ya tokumente"

#: thumbs.title
msgid "Show Thumbnails"
msgstr "Laetša dikhutšofatšo"

#: thumbs_label
msgid "Thumbnails"
msgstr "Dikhutšofatšo"

#: findbar.title
msgid "Find in Document"
msgstr "Hwetša go tokumente"

#: findbar_label
msgid "Find"
msgstr "Hwetša"

#. # Document outline messages
#: no_outline
msgid "No Outline Available"
msgstr "Ga go na kakaretšo"

#. # Thumbnails panel item (tooltip and alt text for images)
#. # LOCALIZATION NOTE (thumb_page_title): "{{page}}" will be replaced by the page
#. # number.
#: thumb_page_title
msgid "Page {{page}}"
msgstr "Letlakala {{page}}"

#. # LOCALIZATION NOTE (thumb_page_canvas): "{{page}}" will be replaced by the page
#. # number.
#: thumb_page_canvas
msgid "Thumbnail of Page {{page}}"
msgstr "Khutšofatšo ya letlakala {{page}}"

#. # Context menu
#: first_page.label
msgid "Go to First Page"
msgstr "Eya letlakaleng la mathomo"

#: last_page.label
msgid "Go to Last Page"
msgstr "Eya letlakaleng la mafelelo"

#: page_rotate_cw.label
msgid "Rotate Clockwise"
msgstr "Dikološa go ya ka go la go ja"

#: page_rotate_ccw.label
msgid "Rotate Counterclockwise"
msgstr "Dikološa go ya go la ntsogošo"

#. # Find panel button title and messages
#: find_label
msgid "Find:"
msgstr "Hwetša:"

#: find_previous.title
msgid "Find the previous occurrence of the phrase"
msgstr "Hwetša tiragalo e fetilego ya sekafoko"

#: find_previous_label
msgctxt "find_previous_label"
msgid "Previous"
msgstr "Fetilego"

#: find_next.title
msgid "Find the next occurrence of the phrase"
msgstr "Hwetša tiragalo e latelago ya sekafoko"

#: find_next_label
msgctxt "find_next_label"
msgid "Next"
msgstr "Latelago"

#: find_highlight
msgid "Highlight all"
msgstr "Bonagatša tšohle"

#: find_match_case_label
msgid "Match case"
msgstr "Swantšha kheisi"

#: find_reached_top
msgid "Reached top of document, continued from bottom"
msgstr "Fihlile godimo ga tokumente, go tšwetšwe pele go tloga tlase"

#: find_reached_bottom
msgid "Reached end of document, continued from top"
msgstr "Fihlile mafelelong a tokumente, go tšwetšwe pele go tloga godimo"

#: find_not_found
msgid "Phrase not found"
msgstr "Sekafoko ga sa hwetšwa"

#. # Error panel labels
#: error_more_info
msgid "More Information"
msgstr "Tshedimošo e oketšegilego"

#: error_less_info
msgid "Less Information"
msgstr "Tshedimošo ya tlasana"

#: error_close
msgid "Close"
msgstr "Tswalela"

#. # LOCALIZATION NOTE (error_version_info): "{{version}}" and "{{build}}" will be
#. # replaced by the PDF.JS version and build ID.
#: error_version_info
msgid "PDF.js v{{version}} (build: {{build}})"
msgstr "PDF.js v{{version}} (build: {{build}})"

#. # LOCALIZATION NOTE (error_message): "{{message}}" will be replaced by an
#. # english string describing the error.
#: error_message
msgid "Message: {{message}}"
msgstr "Molaetša: {{message}}"

#. # LOCALIZATION NOTE (error_stack): "{{stack}}" will be replaced with a stack
#. # trace.
#: error_stack
msgid "Stack: {{stack}}"
msgstr "Mokgobo: {{stack}}"

#. # LOCALIZATION NOTE (error_file): "{{file}}" will be replaced with a filename
#: error_file
msgid "File: {{file}}"
msgstr "Faele: {{file}}"

#. # LOCALIZATION NOTE (error_line): "{{line}}" will be replaced with a line number
#: error_line
msgid "Line: {{line}}"
msgstr "Mothaladi: {{line}}"

#: rendering_error
msgid "An error occurred while rendering the page."
msgstr "Go diregile phošo ge go be go gafelwa letlakala."

#. # Predefined zoom values
#: page_scale_width
msgid "Page Width"
msgstr "Bophara bja letlakala"

#: page_scale_fit
msgid "Page Fit"
msgstr "Go lekana ga letlakala"

#: page_scale_auto
msgid "Automatic Zoom"
msgstr "Kgodišo ya maitirišo"

#: page_scale_actual
msgid "Actual Size"
msgstr "Bogolo bja kgonthe"

#. # Loading indicator messages
#: loading_error_indicator
msgid "Error"
msgstr "Phošo"

#: loading_error
msgid "An error occurred while loading the PDF."
msgstr "Go diregile phošo ge go hlahlelwa PDF."

#: invalid_file_error
msgid "Invalid or corrupted PDF file."
msgstr "Faele ye e sa šomego goba e senyegilego ya PDF."

#: missing_file_error
msgid "Missing PDF file."
msgstr "Faele yeo e sego gona ya PDF."

#. # LOCALIZATION NOTE (text_annotation_type): This is used as a tooltip.
#. # "{{type}}" will be replaced with an annotation type from a list defined in
#. # the PDF spec (32000-1:2008 Table 169 – Annotation types).
#. # Some common types are e.g.: "Check", "Text", "Comment", "Note"
#: text_annotation_type
msgid "[{{type}} Annotation]"
msgstr "[{{type}} Tlhaloso]"

#: request_password
msgid "PDF is protected by a password:"
msgstr "PDF e šireleditšwe ka lentšuphetišo:"

#: printing_not_supported
msgid "Warning: Printing is not fully supported by this browser."
msgstr "Temošo: Go gatiša ga go thekgwe ke praosara ye ka botlalo."

#: printing_not_ready
msgid "Warning: The PDF is not fully loaded for printing."
msgstr "Temošo: PDF ga ya hlahlelwa ka botlalo bakeng sa go gatišwa."

#: web_fonts_disabled
msgid "Web fonts are disabled: unable to use embedded PDF fonts."
msgstr ""
"Difonte tša wepe di šitišitšwe: ga e kgone go diriša difonte tša PDF tše "
"khutišitšwego."

