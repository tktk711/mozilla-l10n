#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Darrièra sincronizacion : %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Sincronizacion : en cors…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Compte : %S"

#: notconnected.label
msgid "Not connected"
msgstr "Pas connectat"

#: connecting.label
msgid "Connecting…"
msgstr "Connexion…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Vòstre compte Firefox Sync es estat suprimit"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Anullar"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 es pas compatible amb la darrièra version de Firefox Sync. Metètz a "
"jorn."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 es pas compatible amb d'ancianas versions de Firefox Sync. Metètz "
"Firefox a jorn sus vòstres autres ordenadors."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Ne saber mai…"

#: sync.update.close
msgid "Close"
msgstr "Tampar"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Impossible d'inicializar Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Cap de connexion Internet pas disponibla"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S a pas pogut se connectar a Sync. Tornar ensajar ?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Ensajatz tornarmai"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Configuracion manuala"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Pas cap d'onglet de vòstres autres ordenadors."
