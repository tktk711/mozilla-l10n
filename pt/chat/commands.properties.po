#. # LOCALIZATION NOTE (commands):
#. #  %S is a comma separated list of command names.
#: commands
msgid ""
"Commands: %S.\n"
"Use /help &lt;command&gt; for more information."
msgstr ""
"Comandos: %S.\n"
"Utilize /help &lt;comando&gt; para mais informação."

#. # LOCALIZATION NOTE (noCommand, noHelp):
#. #  %S is the command name the user typed.
#: noCommand
msgid "No '%S' command."
msgstr "Nenhum comando '%S'."

#: noHelp
msgid "No help message for the '%S' command, sorry!"
msgstr "Desculpe, nenhuma mensagem de ajuda para o comando '%S'!"

#: sayHelpString
msgid "say &lt;message&gt;: send a message without processing commands."
msgstr ""
"diga &lt;mensagem&gt;: enviar uma mensagem sem comandos de processamento."

#: rawHelpString
msgid "raw &lt;message&gt;: send a message without escaping HTML entities."
msgstr "raw &lt;mensagem&gt;: enviar uma mensagem sem entidades HTML."

#: helpHelpString
msgid ""
"help &lt;name&gt;: show the help message for the &lt;name&gt; command, or "
"the list of possible commands when used without parameter."
msgstr ""
"ajuda &lt;nome&gt;: mostrar a mensagem de ajuda para o comando &lt;nome&gt; "
"ou a lista de comandos possíveis quando utilizado sem parâmetro."

#. # LOCALIZATION NOTE (statusCommand):
#. #  %1$S is replaced with a status command name
#. #   (one of "back", "away", "busy", "dnd", or "offline").
#. #  %2$S is replaced with the localized version of that status type
#. #   (one of the 5 strings below).
#: statusCommand
msgid ""
"%1$S &lt;status message&gt;: set the status to %2$S with an optional status "
"message."
msgstr ""
"%1$S &lt;mensgame de estado&gt;: define o estado para %2$S com uma mensagem "
"de estado facultativa."

#: back
msgid "available"
msgstr "disponível"

#: away
msgid "away"
msgstr "ausente"

#: busy
msgctxt "busy"
msgid "unavailable"
msgstr "indisponível"

#: dnd
msgctxt "dnd"
msgid "unavailable"
msgstr "indisponível"

#: offline
msgid "offline"
msgstr "offline"
