#: toolboxDockButtons.bottom.tooltip
msgid "Dock to bottom of browser window"
msgstr "Fixar no fundo da janela do navegador"

#: toolboxDockButtons.side.tooltip
msgid "Dock to side of browser window"
msgstr "Fixar na lateral da janela do navegador"

#: toolboxDockButtons.window.tooltip
msgid "Show in separate window"
msgstr "Mostrar em janela separada"

#. # LOCALIZATION NOTE (toolboxDockButtons.bottom.minimize): This string is shown
#. # as a tooltip that appears in the toolbox when it is in "bottom host" mode and
#. # when hovering over the minimize button in the toolbar. When clicked, the
#. # button minimizes the toolbox so that just the toolbar is visible at the
#. # bottom.
#: toolboxDockButtons.bottom.minimize
msgid "Minimize the toolbox"
msgstr "Minimizar caixa de ferramentas"

#. # LOCALIZATION NOTE (toolboxDockButtons.bottom.maximize): This string is shown
#. # as a tooltip that appears in the toolbox when it is in "bottom host" mode and
#. # when hovering over the maximize button in the toolbar. When clicked, the
#. # button maximizes the toolbox again (if it had been minimized before) so that
#. # the whole toolbox is visible again.
#: toolboxDockButtons.bottom.maximize
msgid "Maximize the toolbox"
msgstr "Maximizar caixa de ferramentas"

#. # LOCALIZATION NOTE (toolboxToggleButton.errors): Semi-colon list of plural
#. # forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 number of errors in the current web page
#: toolboxToggleButton.errors
msgid "#1 error;#1 errors"
msgstr "#1 erro;#1 erros"

#. # LOCALIZATION NOTE (toolboxToggleButton.warnings): Semi-colon list of plural
#. # forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 number of warnings in the current web page
#: toolboxToggleButton.warnings
msgid "#1 warning;#1 warnings"
msgstr "#1 aviso;#1 avisos"

#. # LOCALIZATION NOTE (toolboxToggleButton.tooltip): This string is shown
#. # as tooltip in the developer toolbar to open/close the developer tools.
#. # It's using toolboxToggleButton.errors as first and
#. # toolboxToggleButton.warnings as second argument to show the number of errors
#. # and warnings.
#: toolboxToggleButton.tooltip
msgid ""
"%1$S, %2$S\n"
"Click to toggle the developer tools."
msgstr ""
"%1$S, %2$S\n"
"Clique para mostrar as ferramentas de programador."

#. # LOCALIZATION NOTE (toolbar.closeButton.tooltip)
#. # Used as a message in tooltip when overing the close button of the Developer
#. # Toolbar.
#: toolbar.closeButton.tooltip
msgid "Close Developer Toolbar"
msgstr "Fechar barra de ferramentas de programador"

#. # LOCALIZATION NOTE (toolbar.toolsButton.tooltip)
#. # Used as a message in tooltip when overing the wrench icon of the Developer
#. # Toolbar, which toggle the developer toolbox.
#: toolbar.toolsButton.tooltip
msgid "Toggle developer tools"
msgstr "Alternar ferramentas de programador"

#. # LOCALIZATION NOTE (toolbox.titleTemplate1): This is the template
#. # used to format the title of the toolbox.
#. # The URL of the page being targeted: %1$S.
#: toolbox.titleTemplate1
msgid "Developer Tools - %1$S"
msgstr "Ferramentas de programador - %1$S"

#. # LOCALIZATION NOTE (toolbox.titleTemplate2): This is the template
#. # used to format the title of the toolbox.
#. # The page title or other name for the thing being targeted: %1$S
#. # The URL of the page being targeted: %2$S.
#: toolbox.titleTemplate2
msgid "Developer Tools - %1$S - %2$S"
msgstr "Ferramentas de programador - %1$S - %2$S"

#. # LOCALIZATION NOTE (toolbox.defaultTitle): This is used as the tool
#. # name when no tool is selected.
#: toolbox.defaultTitle
msgctxt "toolbox.defaultTitle"
msgid "Developer Tools"
msgstr "Ferramentas de programador"

#. # LOCALIZATION NOTE (toolbox.label): This is used as the label for the
#. # toolbox as a whole
#: toolbox.label
msgctxt "toolbox.label"
msgid "Developer Tools"
msgstr "Ferramentas de programador"

#. # LOCALIZATION NOTE (options.toolNotSupported): This is the template
#. # used to add a * marker to the label for the Options Panel tool checkbox for the
#. # tool which is not supported for the current toolbox target.
#. # The name of the tool: %1$S.
#: options.toolNotSupportedMarker
msgid "%1$S *"
msgstr "%1$S *"

#. # LOCALIZATION NOTE (scratchpad.keycode)
#. # Used for opening scratchpad from the detached toolbox window
#. # Needs to match scratchpad.keycode from browser.dtd
#: scratchpad.keycode
msgid "VK_F4"
msgstr "VK_F4"

#. # LOCALIZATION NOTE (browserConsoleCmd.commandkey)
#. # Used for toggling the browser console from the detached toolbox window
#. # Needs to match browserConsoleCmd.commandkey from browser.dtd
#: browserConsoleCmd.commandkey
msgid "j"
msgstr "j"

#. # LOCALIZATION NOTE (pickButton.tooltip)
#. # This is the tooltip of the pick button in the toolbox toolbar
#: pickButton.tooltip
msgid "Pick an element from the page"
msgstr "Escolher um elemento da página"

#. # LOCALIZATION NOTE (sidebar.showAllTabs.tooltip)
#. # This is the tooltip shown when hover over the '…' button in the tabbed side
#. # bar, when there's no enough space to show all tabs at once
#: sidebar.showAllTabs.tooltip
msgid "All tabs"
msgstr "Todos os separadores"

#. # LOCALIZATION NOTE (toolbox.noContentProcessForTab.message)
#. # Used as a message in the alert displayed when trying to open a browser
#. # content toolbox and there is no content process running for the current tab
#: toolbox.noContentProcessForTab.message
msgid "No content process for this tab."
msgstr "Nenhum processo de conteúdo para este separador."

#. # LOCALIZATION NOTE (toolbox.viewCssSourceInStyleEditor.label)
#. # Used as a message in either tooltips or contextual menu items to open the
#. # corresponding URL as a css file in the Style-Editor tool.
#. # DEV NOTE: Mostly used wherever toolbox.viewSourceInStyleEditor is used.
#: toolbox.viewCssSourceInStyleEditor.label
msgid "Open File in Style-Editor"
msgstr "Abrir ficheiro no editor de estilos"

#. # LOCALIZATION NOTE (toolbox.viewJsSourceInDebugger.label)
#. # Used as a message in either tooltips or contextual menu items to open the
#. # corresponding URL as a js file in the Debugger tool.
#. # DEV NOTE: Mostly used wherever toolbox.viewSourceInDebugger is used.
#: toolbox.viewJsSourceInDebugger.label
msgid "Open File in Debugger"
msgstr "Abrir ficheiro no Depurador"

#: toolbox.resumeOrderWarning
msgid ""
"Page did not resume after the debugger was attached. To fix this, please "
"close and re-open the toolbox."
msgstr ""
"A página não retomou depois do depurador se ligar. Para reparar isto, por "
"favor feche e reabra a caixa de ferramentas."

#. # LOCALIZATION NOTE (toolbox.options.key)
#. # Key shortcut used to open the options panel
#: toolbox.options.key
msgid "CmdOrCtrl+Shift+O"
msgstr "CmdOrCtrl+Shift+O"

#. # LOCALIZATION NOTE (toolbox.help.key)
#. # Key shortcut used to open the options panel
#: toolbox.help.key
msgid "F1"
msgstr "F1"

#. # LOCALIZATION NOTE (toolbox.nextTool.key)
#. # Key shortcut used to select the next tool
#: toolbox.nextTool.key
msgid "CmdOrCtrl+]"
msgstr "CmdOrCtrl+]"

#. # LOCALIZATION NOTE (toolbox.previousTool.key)
#. # Key shortcut used to select the previous tool
#: toolbox.previousTool.key
msgid "CmdOrCtrl+["
msgstr "CmdOrCtrl+["

#. # LOCALIZATION NOTE (toolbox.zoom*.key)
#. # Key shortcuts used to zomm in/out or reset the toolbox
#. # Should match fullZoom*Cmd.commandkey values from browser.dtd
#: toolbox.zoomIn.key
msgid "CmdOrCtrl+Plus"
msgstr "CmdOrCtrl+Plus"

#: toolbox.zoomIn2.key
msgid "CmdOrCtrl+="
msgstr "CmdOrCtrl+="

#: toolbox.zoomIn3.key
msgctxt "toolbox.zoomIn3.key"
msgid ""
msgstr ""

#: toolbox.zoomOut.key
msgid "CmdOrCtrl+-"
msgstr "CmdOrCtrl+-"

#: toolbox.zoomOut2.key
msgctxt "toolbox.zoomOut2.key"
msgid ""
msgstr ""

#: toolbox.zoomReset.key
msgid "CmdOrCtrl+0"
msgstr "CmdOrCtrl+0"

#: toolbox.zoomReset2.key
msgctxt "toolbox.zoomReset2.key"
msgid ""
msgstr ""

#. # LOCALIZATION NOTE (toolbox.reload*.key)
#. # Key shortcuts used to reload the page
#: toolbox.reload.key
msgid "CmdOrCtrl+R"
msgstr "CmdOrCtrl+R"

#: toolbox.reload2.key
msgid "F5"
msgstr "F5"

#. # LOCALIZATION NOTE (toolbox.forceReload*.key)
#. # Key shortcuts used to force reload of the page by bypassing caches
#: toolbox.forceReload.key
msgid "CmdOrCtrl+Shift+R"
msgstr "CmdOrCtrl+Shift+R"

#: toolbox.forceReload2.key
msgid "CmdOrCtrl+F5"
msgstr "CmdOrCtrl+F5"

#. # LOCALIZATION NOTE (toolbox.minimize.key)
#. # Key shortcut used to minimize the toolbox
#: toolbox.minimize.key
msgid "CmdOrCtrl+Shift+U"
msgstr "CmdOrCtrl+Shift+U"

#. # LOCALIZATION NOTE (toolbox.toggleHost.key)
#. # Key shortcut used to move the toolbox in bottom or side of the browser window
#: toolbox.toggleHost.key
msgid "CmdOrCtrl+Shift+D"
msgstr "CmdOrCtrl+Shift+D"

#. # LOCALIZATION NOTE (toolbox.frames.tooltip): This is the label for
#. # the iframes menu list that appears only when the document has some.
#. # It allows you to switch the context of the whole toolbox.
#: toolbox.frames.tooltip
msgid "Select an iframe as the currently targeted document"
msgstr "Selecionar um iframe como o documento alvo atual"

#. # LOCALIZATION NOTE (toolbox.noautohide.tooltip): This is the label for
#. # the button to force the popups/panels to stay visible on blur.
#. # This is only visible in the browser toolbox as it is meant for
#. # addon developers and Firefox contributors.
#: toolbox.noautohide.tooltip
msgid "Disable popup auto hide"
msgstr "Desativar ocultação automática de popups"

#. # LOCALIZATION NOTE (toolbox.closebutton.tooltip): This is the tooltip for
#. # the close button the developer tools toolbox.
#: toolbox.closebutton.tooltip
msgid "Close Developer Tools"
msgstr "Fechar ferramentas de programador"

#. # LOCALIZATION NOTE (toolbox.allToolsButton.tooltip): This is the tooltip for the
#. # "all tools" button displayed when some tools are hidden by overflow of the toolbar.
#: toolbox.allToolsButton.tooltip
msgid "Select another tool"
msgstr "Selecionar outra ferramenta"

