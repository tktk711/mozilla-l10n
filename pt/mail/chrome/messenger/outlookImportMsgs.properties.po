#. #
#. # The following are used by the outlook express import code to display status/error 
#. # and informational messages
#. #
#. # Description of import module
#. ## @name OUTLOOKIMPORT_DESCRIPTION
#. ## @loc None
#. ## LOCALIZATION NOTE (2010): In this item, don't translate "Outlook"
#: 2010
msgid "Outlook mail, address books, and settings"
msgstr "Correio, livros de endereços e definições do Outlook"

#. # Success message
#. ## @name OUTLOOKIMPORT_MAILBOX_SUCCESS
#. ## @loc None
#. ## LOCALIZATION NOTE (2002): In this item, don't translate "%S" or "%d"
#. ## The variable %S will receive the name of the mailbox
#. ## The variable %d will receive the number of messages
#: 2002
msgid "Mailbox %S, imported %d messages"
msgstr "Caixa de correio %S, importadas %d mensagens"

#. # Error message
#. ## @name OUTLOOKIMPORT_MAILBOX_BADPARAM
#. ## @loc None
#: 2003
msgid "Bad parameter passed to import mailbox."
msgstr "Parâmetro inválido ao importar a caixa de correio."

#. # Error message
#. ## @name OUTLOOKIMPORT_MAILBOX_CONVERTERROR
#. ## @loc None
#. ## LOCALIZATION NOTE (2004): In this item, don't translate "%S"
#. ## The variable %S will receive the name of the mailbox
#: 2004
msgid ""
"Error importing mailbox %S, all messages may not be imported from this "
"mailbox."
msgstr ""
"Erro ao importar a caixa de correio %S. É possível que algumas mensagens não "
"tenham sido importadas."

#. # Address book name
#. ## @name OUTLOOKIMPORT_ADDRNAME
#. ## @loc None
#. ## LOCALIZATION NOTE (2005): In this item, don't translate "Outlook"
#: 2005
msgid "Outlook address books"
msgstr "Livro de endereços do Outlook"

#. # Description
#. ## @name OUTLOOKIMPORT_ADDRESS_SUCCESS
#. ## @loc None
#. ## LOCALIZATION NOTE (2006): In this item, don't translate "%S"
#. ## The variable %S will receive the name of the address book
#: 2006
msgid "Imported address book %S"
msgstr "Livro de endereços %S importado"

#. # Error message
#. ## @name OUTLOOKIMPORT_ADDRESS_BADPARAM
#. ## @loc None
#: 2007
msgid "Bad parameter passed to import address book."
msgstr "Parâmetro inválido ao importar o livre de endereços."

#. # Error message
#. ## @name OUTLOOKIMPORT_ADDRESS_BADSOURCEFILE
#. ## @loc None
#. ## LOCALIZATION NOTE (2008): In this item, don't translate "%S"
#. ## The variable %S will receive the name of the address book
#: 2008
msgid "Error accessing file for address book %S."
msgstr "Erro ao aceder ao ficheiro do livro de endereços %S."

#. # Error message
#. ## @name OUTLOOKIMPORT_ADDRESS_CONVERTERROR
#. ## @loc None
#. ## LOCALIZATION NOTE (2009): In this item, don't translate "%S"
#. ## The variable %S will receive the name of the address book
#: 2009
msgid ""
"Error importing address book %S, all addresses may not have been imported."
msgstr ""
"Erro ao importar o livro de endereços %S. É possível que alguns endereços "
"não tenham sido importados."
