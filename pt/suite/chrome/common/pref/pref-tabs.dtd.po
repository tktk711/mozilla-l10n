#: tabHeader.label
msgid "Tabbed Browsing"
msgstr "Navegação por separadores"

#: tabDisplay.label
msgid "Tab Display"
msgstr "Exibição de separadores"

#: autoHide.label
msgid "Hide the tab bar when only one tab is open"
msgstr "Ocultar a barra de separadores se apenas existir um separador"

#: autoHide.accesskey2
msgid "e"
msgstr "e"

#: background.label background.accesskey
msgid "&Switch to new tabs opened from links"
msgstr "Mudar para novo&s separadores abertos a partir de ligações"

#: warnOnClose.label warnOnClose.accesskey
msgid "&Warn me when closing a window with multiple tabs"
msgstr "Avisar-me qua&ndo fechar uma janela com múltiplos separadores"

#: relatedAfterCurrent.label relatedAfterCurrent.accesskey
msgid "&Open related tabs after current tab"
msgstr "Abrir separad&ores relacionados depois do separador atual"

#: loadGroup.label
msgid "When opening a bookmark group"
msgstr "Ao abrir um grupo de marcadores"

#: loadGroupAppend.label loadGroupAppend.accesskey
msgid "&Add tabs"
msgstr "&Adicionar separadores"

#: loadGroupReplace.label loadGroupReplace.accesskey
msgid "&Replace existing tabs"
msgstr "Substitui&r separadores existentes"

#: openTabs.label
msgid "Open tabs instead of windows for"
msgstr "Abrir separadores em vez de janelas"

#: openManagers.label
msgid "Open in a new tab instead of a stand-alone window"
msgstr "Abrir em novo separador em vez de nova janela"

#: openDataManager.label openDataManager.accesskey
msgid "&Data Manager"
msgstr "Gestor de &dados"

#: openAddOnsManager.label openAddOnsManager.accesskey
msgid "Add-o&ns Manager"
msgstr "Gestor de e&xtras"
