#. # Strings for the Quit, Restart, and Close-browser dialogs
#. # used by _onQuitRequest() in nsSuiteGlue.js
#. # LOCALIZATION NOTE:
#. # %S (also in the messages further down)
#. # is the application's short name (e.g. SeaMonkey)
#. # from the brand.properties file
#: quitDialogTitle
msgid "Quit %S"
msgstr "Sair do %S"

#: lastwindowDialogTitle
msgid "Close %S Browser"
msgstr "Fechar navegador %S"

#: restartDialogTitle
msgid "Restart %S"
msgstr "Reiniciar o %S"

#: restartNowTitle
msgid "&Restart Now"
msgstr "&Reiniciar agora"

#: restartLaterTitle
msgid "Restart &Later"
msgstr "Reiniciar &depois"

#: quitTitle
msgid "&Quit"
msgstr "&Sair"

#: lastwindowTitle
msgid "Close &Browser"
msgstr "Fechar &navegador"

#: cancelTitle
msgid "&Cancel"
msgstr "&Cancelar"

#. # LOCALIZATION NOTE:
#. # The following two strings are labels for the same button, depending on
#. # whether we are quitting the whole Suite, or only the Browser
#: saveTitle
msgid "&Save and Quit"
msgstr "Guardar e &sair"

#: savelastwindowTitle
msgid "&Save and Close"
msgstr "Guardar e &fechar"

#: neverAsk
msgid "&Do not ask next time"
msgstr "&Não perguntar novamente"

#: message
msgid ""
"Do you want %S to save your tabs and windows for the next time it starts?"
msgstr ""
"Pretende que o %S guarde os separadores e janelas para uma sessão posterior?"

#: messageNoWindows
msgid "Do you want %S to save your tabs for the next time it starts?"
msgstr "Pretende que o %S guarde os separadores para uma sessão posterior?"

#: messageRestart
msgid "%S will try to restore your tabs and windows when it restarts."
msgstr "O %S irá tentar restaurar os separadores e janelas na próxima sessão."
