#: dropped_frames_label
msgid "Dropped frames"
msgstr "Frames perdidos"

#: discarded_packets_label
msgid "Discarded packets"
msgstr "Pacotes descartados"

#: decoder_label
msgid "Decoder"
msgstr "Descodificador"

#: encoder_label
msgid "Encoder"
msgstr "Codificado"

#: received_label
