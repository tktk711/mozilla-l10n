#: profilename.label
msgid "Profile Name:"
msgstr "Nome do perfil:"

#: start.label
msgid "Start &brandShortName;"
msgstr "Iniciar o &brandShortName;"

#: exit.label
msgid "Exit"
msgstr "Sair"

#: availprofiles.label
msgid "Available Profiles"
msgstr "Perfis disponíveis"

