#. # Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "sem memória"

#: 2
msgid "syntax error"
msgstr "erro de sintaxe"

#: 3
msgid "no element found"
msgstr "nenhum elemento encontrado"

#: 4
msgid "not well-formed"
msgstr "formatação incorreta"

#: 5
msgid "unclosed token"
msgstr "token não fechado"

#: 6
msgid "partial character"
msgstr "caractere parcial"

#: 7
msgid "mismatched tag"
msgstr "tag sem correspondência"

#: 8
msgid "duplicate attribute"
msgstr "atributo duplicado"

#: 9
msgid "junk after document element"
msgstr "caracteres sem utilidade após um elemento do documento"

#: 10
msgid "illegal parameter entity reference"
msgstr "referência ilegal a entidade parâmetro"

#: 11
msgid "undefined entity"
msgstr "entidade não definida"

#: 12
msgid "recursive entity reference"
msgstr "referência recursiva a entidade"

#: 13
msgid "asynchronous entity"
msgstr "entidade assíncrona"

#: 14
msgid "reference to invalid character number"
msgstr "referência para um número de caractere inválido"

#: 15
msgid "reference to binary entity"
msgstr "referência a entidade binária"

#: 16
msgid "reference to external entity in attribute"
msgstr "referência a entidade externa em atributo"

#: 17
msgid "XML or text declaration not at start of entity"
msgstr "declaração xml ou de texto fora do início da entidade"

#: 18
msgid "unknown encoding"
msgstr "codificação desconhecida"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "codificação especificada na declaração XML é incorreta"

#: 20
msgid "unclosed CDATA section"
msgstr "seção CDATA não fechada"

#: 21
msgid "error in processing external entity reference"
msgstr "erro ao processar uma referência a entidade externa"

#: 22
msgid "document is not standalone"
msgstr "documento não é independente"

#: 23
msgid "unexpected parser state"
msgstr "estado não esperado do processador"

#: 24
msgid "entity declared in parameter entity"
msgstr "entidade declarada em entidade parâmetro"

#: 27
msgid "prefix not bound to a namespace"
msgstr "prefixo não associado a um namespace"

#: 28
msgid "must not undeclare prefix"
msgstr "não deve anular a declaração de prefixo"

#: 29
msgid "incomplete markup in parameter entity"
msgstr "markup incompleto em entidade parâmetro"

#: 30
msgid "XML declaration not well-formed"
msgstr "declaração XML formatada incorretamente"

#: 31
msgid "text declaration not well-formed"
msgstr "declaração de texto formatada incorretamente"

#: 32
msgid "illegal character(s) in public id"
msgstr "caractere(s) ilegais em id público"

#: 38
msgid ""
"reserved prefix (xml) must not be undeclared or bound to another namespace "
"name"
msgstr ""
"prefixo reservado (xml) não deve ter a declaração anulada ou ser associado a "
"outro nome de namespace"

#: 39
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr ""
"prefixo reservado (xmlns) não deve ser declarado ou ter a declaração anulada"

#: 40
msgid "prefix must not be bound to one of the reserved namespace names"
msgstr "prefixo não deve ser associado a um dos nomes de namespace reservado"

#. # %1$S is replaced by the Expat error string, may be followed by Expected (see below)
#. # %2$S is replaced by URL
#. # %3$u is replaced by line number
#. # %4$u is replaced by column number
#: XMLParsingError
msgid ""
"XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$u, Column %4$u:"
msgstr ""
"Erro no processamento de XML: %1$S\n"
"Posição: %2$S\n"
"Número da linha %3$u, coluna %4$u:"

#. # %S is replaced by a tag name.
#. # This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". Esperada: </%S>."
