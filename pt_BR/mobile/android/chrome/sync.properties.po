#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Última sincronização: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Última sincronização: em andamento…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Conta: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Não conectado"

#: connecting.label
msgid "Connecting…"
msgstr "Conectando…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "A sua conta do Firefox Sync foi removida"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Desfazer"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"O #1 #2 não é compatível com a versão mais recente do Firefox Sync. Por "
"favor, atualize."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 não é compatível com a versões anteriores do Firefox Sync. Por favor, "
"atualize o Firefox em seu(s) outro(s) computador(es)."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Saiba mais"

#: sync.update.close
msgid "Close"
msgstr "Fechar"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Não foi possível configurar o Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Sem conexão com a internet"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "O %S não conseguiu conectar-se ao Sync. Deseja tentar de novo?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Tentar de novo"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Configuração manual"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Nenhuma aba de outros computadores."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "O Sync está completamente novo e mais fácil que nunca. "

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Toque aqui para saber mais"
