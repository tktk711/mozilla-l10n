#: checkingForUpdatesTitle
msgid "Checking for updates…"
msgstr "Verificando atualização…"

#: checkingForUpdatesMessage
msgid "Checking for updates to your apps"
msgstr "Verificando atualizações de seus aplicativos"

#: noUpdatesTitle
msgid "No updates available"
msgstr "Nenhuma atualização disponível."

#: noUpdatesMessage
msgid "There are no updates to your apps"
msgstr "Nenhuma atualização de aplicativos disponível."

#. # LOCALIZATION NOTE (retrieveUpdateTitle): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 is the number of updates.
#. # example: 3 new updates available
#: retrieveUpdateTitle
msgid "#1 new update available;#1 new updates available"
msgstr "#1 nova atualização disponível;#1 novas atualizações disponíveis"

#. # LOCALIZATION NOTE (retrieveUpdateMessage): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # %1$S is a comma-separated list of apps for which to retrieve an update.
#. # example: Touch to retrieve updates for Foo, Bar, Baz
#: retrieveUpdateMessage
msgid "Touch to retrieve update for %1$S;Touch to retrieve updates for %1$S"
msgstr ""
"Toque para receber a atualização de %1$S;Toque para receber as atualizações "
"de %1$S"

#. # LOCALIZATION NOTE (retrievingUpdateTitle): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 is the number of updates.
#. # example: Retrieving 3 updates…
#: retrievingUpdateTitle
msgid "Retrieving #1 update…;Retrieving #1 updates…"
msgstr "Recebendo #1 atualização…; Recebendo #1 atualizações…"

#. # LOCALIZATION NOTE (retrievingUpdateMessage): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # %1$S is a comma-separated list of apps for which we're retrieving updates.
#. # example: Retrieving updates for Foo, Bar, Baz
#: retrievingUpdateMessage
msgid "Retrieving update for %1$S; Retrieving updates for %1$S"
msgstr "Recebendo atualização de %1$S; Recebendo atualizações de %1$S"

#. # LOCALIZATION NOTE (installUpdateTitle): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 is the number of updates.
#. # example: 3 updates downloaded
#: installUpdateTitle
msgid "#1 update downloaded;#1 updates downloaded"
msgstr "#1 atualização recebida;#1 atualizações recebidas"

#. # LOCALIZATION NOTE (installUpdateMessage2): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # %1$S is a comma-separated list of apps for which to install an update.
#. # example: Touch to install updates for Foo, Bar, Baz
#: installUpdateMessage2
msgid "Touch to install update for %1$S;Touch to install updates for %1$S"
msgstr ""
"Toque para instalar a atualização de %1$S;Toque para instalar as "
"atualizações de %1$S"

#. # LOCALIZATION NOTE (retrievalFailedTitle): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # #1 is the number of updates.
#. # example: 3 updates failed
#: retrievalFailedTitle
msgid "#1 update failed;#1 updates failed"
msgstr "Falha em #1 atualização;Falha em #1 atualizações"

#. # LOCALIZATION NOTE (retrievalFailedMessage): Semi-colon list of plural forms.
#. # See: http://developer.mozilla.org/en/docs/Localization_and_Plurals
#. # %1$S is a comma-separated list of apps for which retrieval failed.
#. # example: Failed to retrieve updates for Foo, Bar, Baz
#: retrievalFailedMessage
msgid "Failed to retrieve update for %1$S;Failed to retrieve updates for %1$S"
msgstr ""
"Falha no recebimento da atualização de %1$S;Falha no recebimento das "
"atualizações de %1$S"

#: webappsDisabled
msgid "Installing apps is disabled"
msgstr "A instalação de aplicativos está desativada"
