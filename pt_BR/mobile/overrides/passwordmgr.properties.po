#. # 1st string is the username for the login, 2nd is the login's hostname.
#. # Note that long usernames may be truncated.
#: savePassword
msgid "Save password for \"%1$S\" on %2$S?"
msgstr "Memorizar senha do usuário “%1$S” para %2$S?"

#. # String is the login's hostname
#: savePasswordNoUser
msgid "Save password on %S?"
msgstr "Memorizar senha para %S?"

#: saveButton
msgid "Save"
msgstr "Memorizar"

#: dontSaveButton
msgid "Don't save"
msgstr "Não memorizar"

#. # String is the login's hostname
#: updatePassword
msgid "Update saved password for %S?"
msgstr "Atualizar senha memorizada para %S?"

#: updatePasswordNoUser
msgid "Update saved password for this login?"
msgstr "Atualizar senha memorizada deste usuário?"

#: updateButton
msgid "Update"
msgstr "Atualizar"

#: dontUpdateButton
msgid "Don't update"
msgstr "Não atualizar"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "Confirme o usuário do qual você está alterando a senha:"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "Confirmar nova senha"

#. # Strings used by PromptService.js
#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "Memorizar esta senha"

#: username
msgid "Username"
msgstr "Nome de usuário"

#: password
msgid "Password"
msgstr "Senha"
