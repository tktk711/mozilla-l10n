#: pref.scripts.title
msgid "Scripts & Plugins"
msgstr "Scripts & Plug-ins"

#: navigator.label navigator.accesskey
msgid "&Browser"
msgstr "&Navigator"

#. 'JavaScript' should never be translated
#: enableJavaScript.label
msgid "Enable JavaScript for"
msgstr "Permitir JavaScript no"

#: allowScripts.label allowScripts.accesskey
msgid "Allow &scripts to:"
msgstr "Script&s têm permissão para:"

#: allowWindowMoveResize.label
msgid "Move or resize existing windows"
msgstr "Mover ou alterar o tamanho de janelas existentes"

#: allowWindowFlip.label
msgid "Raise or lower windows"
msgstr "Alterar a disposição entre as janelas"

#: allowWindowStatusChange.label
msgid "Change status bar text"
msgstr "Alterar o texto da barra de status"

#: allowWindowImageSrcChange.label
msgid "Change images"
msgstr "Alterar imagens"

#: allowContextmenuDisable.label
msgid "Disable or replace context menus"
msgstr "Desativar ou substituir menus de contexto"

#: allowHideStatusBar.label
msgid "Hide the status bar"
msgstr "Ocultar a barra de status "

#: enablePlugins.label
msgid "Enable Plugins for"
msgstr "Permitir plugins para"

#: enablePluginForSuite.label enablePluginForSuite.accesskey
msgid "S&uite"
msgstr "S&uite"

#: whenPageRequiresPlugins.label
msgid "When a page requires plugins"
msgstr "Quando a página necessitar usar plugins"

#: activateAllPlugins.label activateAllPlugins.accesskey
msgid "Activate all plugins by &default"
msgstr "Ativar todos plugins por pa&drão"

#: warnPluginsRequired.label warnPluginsRequired.accesskey
msgid "&Warn me if additional plugins need to be installed"
msgstr "&Alertar se plugins adicionais precisam ser instalados"
