#: errorCloudFileAuth.title
msgid "Authentication Error"
msgstr "Eroare de autentificare"

#. ## LOCALIZATION NOTE(errorCloudFileAuth.message):
#. ## %1$S is the name of the online storage service that authentication failed against.
#: errorCloudFileAuth.message
msgid "Unable to authenticate to %1$S."
msgstr "Nu se poate autentifica către %1$S."

#: errorCloudFileUpload.title
msgid "Upload Error"
msgstr "Eroare la încărcare"

#. ## LOCALIZATION NOTE(errorCloudFileUpload.message):
#. ## %1$S is the name of the online storage service that uploading failed against.
#. ## %2$S is the name of the file that failed to upload.
#: errorCloudFileUpload.message
msgid "Unable to upload %2$S to %1$S."
msgstr "Nu pot încărca %2$S la %1$S."

#: errorCloudFileQuota.title
msgid "Quota Error"
msgstr "Eroare de cotă"

#. ## LOCALIZATION NOTE(errorCloudFileQuota.message):
#. ## %1$S is the name of the online storage service being uploaded to.
#. ## %2$S is the name of the file that could not be uploaded due to exceeding the storage limit.
#: errorCloudFileQuota.message
msgid "Uploading %2$S to %1$S would exceed your space quota."
msgstr "Încărcarea %2$S în %1$S va depăși cota dumneavoastră de spațiu."

#: errorCloudFileNameLimit.title
msgid "File Name Error"
msgstr "Eroare la numele fișierului"

#. ## LOCALIZATION NOTE(errorCloudFileNameLimit.message):
#. ## %1$S is the name of the online storage service being uploaded to.
#. ## %2$S is the name of the file that could not be uploaded due to the excess file name length
#: errorCloudFileNameLimit.message
msgid ""
"Uploading %2$S to %1$S contains has more than 120 characters in its name. "
"Please rename the file to have 120 characters or less in its name and upload "
"again."
msgstr ""
"Uploading %2$S to %1$S contains has more than 120 characters in its name. "
"Please rename the file to have 120 characters or less in its name and upload "
"again."

#: errorCloudFileLimit.title
msgid "File Size Error"
msgstr "Eroare la dimensiunea fișierului"

#. ## LOCALIZATION NOTE(errorCloudFileLimit.message):
#. ## %1$S is the name of the online storage service being uploaded to.
#. ## %2$S is the name of the file that could not be uploaded due to size restrictions.
#: errorCloudFileLimit.message
msgid "%2$S exceeds the maximum size for %1$S."
msgstr "%2$S depășește dimensiunea maximă pentru %1$S."

#: errorCloudFileOther.title
msgid "Unknown Error"
msgstr "Eroare necunoscută"

#. ## LOCALIZATION NOTE(errorCloudFileOther.message):
#. ## %1$S is the name of the online storage service that cannot be communicated with.
#: errorCloudFileOther.message
msgid "An unknown error occurred when communicating with %1$S."
msgstr "O eroare necunoscută s-a produs în timpul comunicării cu %1$S."

#: errorCloudFileDeletion.title
msgid "Deletion Error"
msgstr "Eroare la ștergere"

#. ## LOCALIZATION NOTE(errorCloudFileDeletion.message):
#. ## %1$S is the name of the online storage service that the file is to be deleted from.
#. ## %2$S is the name of the file that failed to be deleted.
#: errorCloudFileDeletion.message
msgid "There was a problem deleting %2$S from %1$S."
msgstr "A apărut o problemă la ștergerea %2$S de pe %1$S."

#: errorCloudFileUpgrade.label
msgid "Upgrade"
msgstr "Actualizează"

