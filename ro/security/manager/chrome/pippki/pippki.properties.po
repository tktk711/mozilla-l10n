msgid "File Error"
msgstr "Eroare de fișier"

#: writeFileFailed
msgid ""
"Can’t write to file %S:\n"
"%S."
msgstr ""
"Nu se poate scrie în fișierul %S:\n"
"%S."

#: writeFileAccessDenied
msgid "Access denied"
msgstr "Acces refuzat"

#: writeFileIsLocked
msgid "File is locked"
msgstr "Fișierul este blocat"

#: writeFileNoDeviceSpace
