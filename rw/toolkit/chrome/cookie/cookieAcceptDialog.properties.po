#: hostColon
msgid "Host:"
msgstr "Ubuturo:"

#: domainColon
msgid "Domain:"
msgstr "Indangarubuga:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Ukwihuza gusobetse gusa"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Ukwihuza uko ariko kose"

#: expireAtEndOfSession
msgid "At end of session"
msgstr ""

#: showDetails
msgid "Show Details"
msgstr "Kwerekana Ibisobanuro"

#: hideDetails
msgid "Hide Details"
msgstr "Guhisha Ibisobanuro"

#: detailsAccessKey
msgid "T"
msgstr "I"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Urubuga %S rurashaka gushyiraho Dosiyenyibutsa."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "Urubuga %S rurashaka gushyiraho Dosiyenyibutsa ya kabiri."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"Urubuga %S rurashaka gushyiraho iyindi Dosiyenyibutsa.\n"
"Usanzwe ufite Dosiyenyibutsa %S zatanzwe n'uru rubuga."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "Urubuga %S rurashaka guhindura Dosiyenyibutsa isanzwe iriho."
