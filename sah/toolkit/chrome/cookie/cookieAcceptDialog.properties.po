#: hostColon
msgid "Host:"
msgstr "Хост:"

#: domainColon
msgid "Domain:"
msgstr "Домен:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Шифрдаммыт эрэ холбонууга"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Ханнык баҕарар холбонууга"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "Сиэссийэ бүтүүтүгэр"

#: showDetails
msgid "Show Details"
msgstr "Сиһилии"

#: hideDetails
msgid "Hide Details"
msgstr "Сиһилиитин кистээ"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "%S ситим-сир кууканы хаалларыан баҕарар."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "%S ситим-сир иккис куукатын хаалларыан баҕарар."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"%S ситим-сир өссө биир кууканы хаалларыан баҕарар.\n"
"Бу иннинэ %S кууканы хаалларбыт."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "%S ситим-сир баар кууканы уларытыан баҕарар."
