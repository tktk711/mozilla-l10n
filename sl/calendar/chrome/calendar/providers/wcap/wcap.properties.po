#. # args: host
#: accessingServerFailedError.text
msgid "Cannot access server %1$S!"
msgstr "Dostop do strežnika %1$S ni možen!"

#: loginFailed.text
msgid "Login failed or invalid session Id."
msgstr "Prijava ni uspela ali pa Id seje ni veljaven."

#: accessDenied.text
msgid "The user is denied access."
msgstr "Uporabniku dostop ni dovoljen."

#. # args: host
#: noHttpsConfirmation.text
msgid ""
"Insecure login on %1$S!\n"
"Continue?"
msgstr ""
"Nezaščitena prijava v %1$S!\n"
"Želite nadaljevati?"

#: noHttpsConfirmation.check.text
msgid "Don't ask again."
msgstr "Ne sprašuj več."

#: noHttpsConfirmation.label
msgid "Warning!"
msgstr "Opozorilo!"

#. # args: host, prodId, serverVersion, wcapVersion
#: insufficientWcapVersionConfirmation.text
msgid ""
"Server %1$S (%2$S, v%3$S, WCAP v%4$S) doesn't support a sufficient WCAP "
"version! The required version is at least 3.0.0.\n"
"Continue?"
msgstr ""
"Strežnik %1$S (%2$S, v%3$S, WCAP v%4$S) ne podpira zadostne različice WCAP! "
"Zahtevana različica je vsaj 3.0.0.\n"
"Želite nadaljevati?"

#: insufficientWcapVersionConfirmation.label
msgid "Insufficient WCAP version!"
msgstr "Nezadostna različica WCAP!"

#: loginDialog.label
msgid "Calendar Server Password Required"
msgstr "Potrebno je geslo za strežnik koledarjev"

#: privateItem.title.text
msgid "Private"
msgstr "Zasebno"

#: confidentialItem.title.text
msgid "Confidential"
msgstr "Zaupno"

#: busyItem.title.text
msgid "Busy"
msgstr "Zasedeno"
