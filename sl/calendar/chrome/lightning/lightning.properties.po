#. # Html event display in message
#: imipHtml.header
msgid "Event Invitation"
msgstr "Povabilo na dogodek"

#: imipHtml.summary
msgid "Title:"
msgstr "Naziv:"

#: imipHtml.location
msgid "Location:"
msgstr "Kje:"

#: imipHtml.when
msgid "When:"
msgstr "Kdaj:"

#: imipHtml.organizer
msgid "Organizer:"
msgstr "Organizator:"

#: imipHtml.description
msgid "Description:"
msgstr "Opis:"

#: imipHtml.comment
msgid "Comment:"
msgstr "Komentar:"

#: imipHtml.attendees
msgid "Attendees:"
msgstr "Udeleženci:"

#: imipHtml.canceledOccurrences
msgid "Canceled Occurrences:"
msgstr "Preklicane ponovitve:"

#: imipHtml.modifiedOccurrences
msgid "Modified Occurrences:"
msgstr "Spremenjene ponovitve:"

#: imipHtml.newLocation
msgid "New Location: %1$S"
msgstr "Nov kraj: %1$S"

#: imipAddToCalendar.label
msgid "Add To Calendar"
msgstr "Dodaj v koledar"

#: imipAddedItemToCal
msgid "Event Added to Calendar"
msgstr "Dogodek dodan v koledar"

#: imipCanceledItem
msgid "Event has been deleted"
msgstr "Dogodek je bil izbrisan"

#: imipUpdatedItem
msgid "Event has been updated"
msgstr "Dogodek je bil posodobljen"

#: imipBarCancelText
msgid "This message contains an event cancellation."
msgstr "To sporočilo vsebuje odpoved dogodka."

#: imipBarRefreshText
msgid "This message asks for an event update."
msgstr "To sporočilo sprašuje po posodobitvi dogodka."

#: imipBarPublishText
msgid "This message contains an event."
msgstr "To sporočilo vsebuje dogodek."

#: imipBarRequestText
msgid "This message contains an invitation to an event."
msgstr "To sporočilo vsebuje povabilo na dogodek."

#: imipBarUpdateText
msgid "This message contains an update to an existing event."
msgstr "To sporočilo vsebuje posodobitev obstoječega dogodka."

#: imipBarAlreadyProcessedText
msgid "This message contains an event that has already been processed."
msgstr "To sporočilo vsebuje dogodek, ki je že obdelan."

#: imipBarProcessedNeedsAction
msgid "This message contains an event that you have not yet responded to."
msgstr "To sporočilo vsebuje dogodek, na katerega še niste odgovorili."

#: imipBarReplyText
msgid "This message contains a reply to an invitation."
msgstr "To sporočilo vsebuje odgovor na povabilo."

#: imipBarUnsupportedText
msgid ""
"This message contains an event that this version of Lightning cannot process."
msgstr ""
"To sporočilo vsebuje dogodek, ki ga ta različica dodatka Lightning ne zmore "
"obdelati."

#: imipBarProcessingFailed
msgid "Processing message failed. Status: %1$S."
msgstr "Obdelava sporočila ni uspela. Stanje: %1$S."

#: imipBarNotWritable
msgid ""
"No writable calendars are configured for invitations, please check the "
"calendar properties."
msgstr ""
"Noben zapisljiv koledar ni nastavljen za povabila, preverite lastnosti "
"koledarja."

#: imipSendMail.title
msgid "E-Mail Notification"
msgstr "Obvesti udeležence"

#: imipSendMail.text
msgid "Would you like to send out notification E-Mail now?"
msgstr "Želite zdaj odposlati obvestila po e-pošti?"

#: imipSendMail.Outlook2000CompatMode.text
msgid "Support Outlook 2000 and Outlook 2002/XP"
msgstr "Podpora za Outlook 2000 in Outlook 2002/XP"

#: imipNoIdentity
msgid "None"
msgstr "Brez"

#: imipNoCalendarAvailable
msgid "There are no writable calendars available."
msgstr "Na voljo ni koledarjev za zapisovanje."

#: itipReplySubject
msgid "Event Invitation Reply: %1$S"
msgstr "Odgovor na povabilo na dogodek: %1$S"

#: itipReplyBodyAccept
msgid "%1$S has accepted your event invitation."
msgstr "%1$S je sprejel(a) vaše povabilo na dogodek."

#: itipReplyBodyDecline
msgid "%1$S has declined your event invitation."
msgstr "%1$S je odklonil(a) vaše povabilo na dogodek."

#: itipReplySubjectAccept
msgid "Event Invitation Reply (Accepted): %1$S"
msgstr "Odgovor na povabilo na dogodek (prejeto): %1$S"

#: itipReplySubjectDecline
msgid "Event Invitation Reply (Declined): %1$S"
msgstr "Odgovor na povabilo na dogodek (odklonjeno): %1$S"

#: itipReplySubjectTentative
msgid "Event Invitation Reply (Tentative): %1$S"
msgstr "Odgovor na povabilo na dogodek (neobvezno): %1$S"

#: itipRequestSubject
msgid "Event Invitation: %1$S"
msgstr "Povabilo na dogodek: %1$S"

#: itipRequestUpdatedSubject
msgid "Updated Event Invitation: %1$S"
msgstr "Povabilo na dogodek - posodobljeno: %1$S"

#: itipRequestBody
msgid "%1$S has invited you to %2$S"
msgstr "%1$S vas je povabil(a) na %2$S"

#: itipCancelSubject
msgid "Event Canceled: %1$S"
msgstr "Dogodek preklican: %1$S"

#: itipCancelBody
msgid "%1$S has canceled this event: « %2$S »"
msgstr "%1$S je preklical(a) naslednji dogodek: « %2$S »"

#: confirmProcessInvitation
msgid ""
"You have recently deleted this item, are you sure you want to process this "
"invitation?"
msgstr ""
"Pred kratkim ste izbrisali ta predmet, ali res želite obdelati povabilo?"

#: confirmProcessInvitationTitle
msgid "Process Invitation?"
msgstr "Obdelaj povabilo?"

#: invitationsLink.label
msgid "Invitations: %1$S"
msgstr "Povabila: %1$S"
