#. # LOCALIZATION NOTE (commands):
#. #  %S is a comma separated list of command names.
#: commands
msgid ""
"Commands: %S.\n"
"Use /help &lt;command&gt; for more information."
msgstr ""
"Ukazi:%S.\n"
"Uporabite /help &lt;ukaz&gt; za več informacij."

#. # LOCALIZATION NOTE (noCommand, noHelp):
#. #  %S is the command name the user typed.
#: noCommand
msgid "No '%S' command."
msgstr "Ukaz '%S' ne obstaja."

#: noHelp
msgid "No help message for the '%S' command, sorry!"
msgstr "Žal za ukaz '%S' ni pomoči!"

#: sayHelpString
msgid "say &lt;message&gt;: send a message without processing commands."
msgstr "say &lt;sporoćilo&gt;: pošlje sporočilo brez ukazov za obdelavo."

#: rawHelpString
msgid "raw &lt;message&gt;: send a message without escaping HTML entities."
msgstr ""
"raw &lt;sporočilo&gt;: pošlji sporočilo, ne da bi ubežal HTML postavke."

#: helpHelpString
msgid ""
"help &lt;name&gt;: show the help message for the &lt;name&gt; command, or "
"the list of possible commands when used without parameter."
msgstr ""
"help &lt;ime&gt;: pokaže pomoč za ukaz &lt;ime&gt; ali pa seznam možnih "
"ukazov, če ga vtipkate brez parametra."

#. # LOCALIZATION NOTE (statusCommand):
#. #  %1$S is replaced with a status command name
#. #   (one of "back", "away", "busy", "dnd", or "offline").
#. #  %2$S is replaced with the localized version of that status type
#. #   (one of the 5 strings below).
#: statusCommand
msgid ""
"%1$S &lt;status message&gt;: set the status to %2$S with an optional status "
"message."
msgstr ""
"%1$S &lt;statusno sporočilo&gt;: nastavi status na %2$S, doda neobvezno "
"sporočilo o stanju"

#: back
msgid "available"
msgstr "na razpolago"

#: away
msgid "away"
msgstr "odsoten"

#: busy
msgctxt "busy"
msgid "unavailable"
msgstr "ni dostopen"

#: dnd
msgctxt "dnd"
msgid "unavailable"
msgstr "ni dostopen"

#: offline
msgid "offline"
msgstr "ni na spletu"
