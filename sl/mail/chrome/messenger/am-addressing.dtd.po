#: addressing.label
msgid "Composition & Addressing"
msgstr "Sestavljanje in naslavljanje"

#: addressingGroupTitle.label
msgid "Addressing"
msgstr "Naslavljanje"

#: addressingText.label
msgid "When looking up addresses:"
msgstr "Med iskanjem naslovov:"

#. LOCALIZATION NOTE (override.label) : do not translate "LDAP" in below line
#: useGlobal.label useGlobal.accesskey
msgid "&Use my global LDAP server preferences for this account"
msgstr "&Uporabi splošne nastavitve strežnika LDAP za ta račun"

#: editDirectories.label editDirectories.accesskey
msgid "&Edit Directories…"
msgstr "&Uredi imenike …"

#. do not translate "LDAP" in below line
#: directories.label directories.accesskey
msgid "Use a &different LDAP server:"
msgstr "Uporabi drug strežnik LDA&P:"

#: directoriesNone.label
msgid "None"
msgstr "Brez"

#. am-addressing.xul
#: compositionGroupTitle.label
msgid "Composition"
msgstr "Sestavljanje"

#. do not translate "html" in below line
#: useHtml.label useHtml.accesskey
msgid "&Compose messages in HTML format"
msgstr "&Sporočila sestavljaj v obliki HTML"

#: autoQuote.label autoQuote.accesskey
msgid "Auto&matically quote the original message when replying"
msgstr "V odgovoru &samodejno navedi izvirno sporočilo"

#. This will concatenate with the 4 strings that follow.
#: then.label then.accesskey
msgid "&Then,"
msgstr "&Potem"

#: aboveQuote.label
msgid "start my reply above the quote"
msgstr "začni moj odgovor nad navedenim besedilom"

#: belowQuote.label
msgid "start my reply below the quote"
msgstr "začni moj odgovor pod navedenim besedilom"

#: selectAndQuote.label
msgid "select the quote"
msgstr "izberi navedeno besedilo"

#: place.label place.accesskey
msgid "and place my &signature"
msgstr "in vstavi moj &podpis"

#: belowText.label
msgid "below the quote (recommended)"
msgstr "pod navedeno besedilo (priporočeno)"

#: aboveText.label
msgid "below my reply (above the quote)"
msgstr "pod moj odgovor (nad navedeno besedilo)"

#: includeSigOnReply.label includeSigOnReply.accesskey
msgid "&Include signature for replies"
msgstr "Dodaj podpi&s odgovorom"

#: includeSigOnForward.label includeSigOnForward.accesskey
msgid "Include signature for for&wards"
msgstr "Dodaj podpis pos&redovanim sporočilom"

#: globalComposingPrefs.label globalComposingPrefs.accesskey
msgid "&Global Composing Preferences…"
msgstr "&Splošne nastavitve za sestavljanje …"

#: globalAddressingPrefs.label globalAddressingPrefs.accesskey
msgid "Global Addressing &Preferences…"
msgstr "Splošne &nastavitve za naslavljanje …"
