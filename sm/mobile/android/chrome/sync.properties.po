#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Tu'ufa'atasia mulimuli: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Tu'ufa'atasia mulimuli: i au'iluma…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Account: %S"

#: notconnected.label
msgid "Not connected"
msgstr "E lē feso'ota'iga"

#: connecting.label
msgid "Connecting…"
msgstr "Feso'ota'iga…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Lou Firefox tu'ufa'atasia account e ave'esega"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Toe 'ave'ese"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 e le'i talafeagai ma le liliuga fou o Firefox Tu'ufa'atasia. Toe "
"fa'afouina i le liliuga aupito fou."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 e le'i talafeagai ma le liliuga mātua o Firefox Tu'ufa'atasia. Toe "
"fa'afouina Firefox i lou isi komepiuta."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Tu'ufa'atasia"

#: sync.update.button
msgid "Learn More"
msgstr "Mālamalama Atili"

#: sync.update.close
msgid "Close"
msgstr "Tāpuni"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "E lē mafai ona fa'atūlagaga tu'ufa'atasia"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Leai se internet feso'ota'iga avanoa"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "E lē mafai ona feso'ota'i %S i le Tu'ufa'atasia. E fia toe taumafai?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Toe taumafai"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Fa'atūlagaga fai-lima"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Leai se tama'i-ufitaga mai lou isi komepiuta."
