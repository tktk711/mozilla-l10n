#. # LOCALIZATION NOTE: Used in the default os description when a new account
#. # is being set up. Should be unique to Metro, so that it does not conflict
#. # with Desktop. See /services/sync/modules/engines/clients.js locaName.
#: sync.defaultAccountApplication
msgid "Metro %S"
msgstr "Metro %S"

#. # %S is the date and time at which the last sync successfully completed
#: sync.flyout.connected.lastSynced
msgid "Last sync: %S"
msgstr "Tu'ufa'atasia mulimuli: %S"

#. # %S is the username logged in
#: sync.flyout.connected.account
msgid "Account: %S"
msgstr "Account: %S"

#. # %S is the name that the user has chosen for this device
#: sync.flyout.connected.device
msgid "Your device: %S"
msgstr "Lou togafiti: %S"

#~ msgid "%1$S's %2$S on %3$S"
