msgstr " ma toe taumafai."

#. This and the other report.* strings should be as short as possible, ideally 2-3 words.
#: report.please
msgid "Send crash report"
msgstr "'Ave līpoti o le palasi"

#: report.submitting
msgid "Sending report…"
msgstr "Lafoina līpoti…"

#: report.submitted
msgid "Crash report sent."
msgstr "Līpoti o le palasi 'aveina."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "E lē mafai ona līpoti o le palasi."

#: report.failed
msgid "Submission failed."
msgstr "Gāua'i lē taunu'u."

#: report.unavailable
msgid "No report available."
msgstr "Leai se līpoti avanoa."

#: plugin.file
msgid "File"
msgstr "Faila"

#: plugin.mimeTypes
msgid "MIME Types"
msgstr "MIME Itu'āiga"

