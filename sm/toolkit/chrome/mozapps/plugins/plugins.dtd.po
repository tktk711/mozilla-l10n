msgid ""
msgstr ""

#: missingPlugin
msgid "A plugin is needed to display this content."
msgstr "O lo'o sulu-totonu e te mana'omia ona fa'aali le meatotonu."

#. Mobile (used for touch interfaces) only has one type of plugin possible.
#: tapToPlayPlugin
msgid "Tap here to activate plugin."
msgstr "'O'omi i ma fa'agāoioi le sulu-totonu."

#: clickToPlayPlugin
msgid "Click here to activate plugin."
msgstr "'O'omi i ma fa'agāoioi le sulu-totonu."

#: checkForUpdates
msgid "Check for updates…"
msgstr "Siati mo fa'afouina…"

#: disabledPlugin
msgid "This plugin is disabled."
msgstr "Le sulu-totonu lea e lē mafai."

#: blockedPlugin.label
msgid "This plugin has been blocked for your protection."
msgstr "Le sulu-totonu lea e punitiaga mo lou fa'amamalu."

#: installPlugin
msgid "Install plugin…"
msgstr "Fa'anofo sulu-totonu…"

#: managePlugins
msgid "Manage plugins…"
msgstr "Pūlea sulu-totonu…"

