#~ msgstr "Ordinater jerey nor lokey"

#~ msgid "Last Update: %S"
#~ msgstr "Taagandiri koraa: %S"

#~ msgid "Last Update: in progress…"
#~ msgstr "Taagandiri koraa: goo ma tee…"

#~ msgid "Connected: %S"
#~ msgstr "Goo ciyari ra: %S"

#~ msgid "Disconnected"
#~ msgstr "Ciyaroo dunbu"

#~ msgid "Connecting…"
#~ msgstr "Goo ma ciya…"
