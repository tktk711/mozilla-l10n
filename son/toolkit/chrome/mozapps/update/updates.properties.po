#. # updates.xml for displaying update history
#: statusSucceededFormat
msgid "Installed on: %S"
msgstr "Sinjiyan han: %S"

#: statusFailed
msgid "Install Failed"
msgstr "Sinjiyan kay"

#: pauseButtonPause
msgid "Pause"
msgstr "Hunanzam"

#: pauseButtonResume
msgid "Resume"
msgstr "Sintin taaga"

#: hideButton
msgid "Hide"
msgstr "Tugu"

#: hideButton.accesskey
