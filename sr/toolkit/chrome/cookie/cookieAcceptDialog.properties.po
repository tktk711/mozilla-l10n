#: hostColon
msgid "Host:"
msgstr "Рачунар:"

#: domainColon
msgid "Domain:"
msgstr "Домен:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Само шифроване везе"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Било која врста везе"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "На крају сесије"

#: showDetails
msgid "Show Details"
msgstr "Прикажи детаље"

#: hideDetails
msgid "Hide Details"
msgstr "Сакриј детаље"

