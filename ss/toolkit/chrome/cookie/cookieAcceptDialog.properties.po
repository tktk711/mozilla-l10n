#: hostColon
msgid "Host:"
msgstr "Umvakashelwa:"

#: domainColon
msgid "Domain:"
msgstr "Idomeyini:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Letiphawuliwe kuphela tihlanganisi"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Nome nguyiphi inhlobo yekuhlanganisa"

#: expireAtEndOfSession
#, fuzzy
msgid "At end of session"
msgstr "ekugcineni kweseshini"

#: showDetails
msgid "Show Details"
msgstr "Khombisa imininingwane"

#: hideDetails
msgid "Hide Details"
msgstr "Fihla imininingwane"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Lisayithi %S lifuna kuhlela likuku."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "Lisayithi %S lifuna kuhlela likuku lesibili."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"Lisayithi %S lifuna kusetha lelinye likukisi.\n"
"Sewuvele unawo emakukisi %S lasuka kulesayithi."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "Lisayithi %S lifuna kulungisa likuku leselivele likhona."

#~ msgid "The site %S wants to load an image."
#~ msgstr "Lisayithi %S lifuna kulayisha umfanekiso."

#~ msgid "Remember this decision"
#~ msgstr "Sikhumbule lesincumo"
