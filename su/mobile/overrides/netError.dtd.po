#: dnsNotFound.title
msgid "Server not found"
msgstr "Server teu kapanggih"

#: dnsNotFound.longDesc2
msgid ""
"<ul> \n"
"<li>Check the address for typing errors such as \n"
"<strong>ww</strong>.example.com instead of \n"
"<strong>www</strong>.example.com</li> \n"
"<li>If you are unable to load any pages, check your device's data or Wi-Fi "
"connection.</li> \n"
"</ul>"
msgstr ""

#: fileNotFound.title
msgid "File not found"
msgstr "Berkas teu kapanggih"

#: fileNotFound.longDesc
msgid ""
"<ul> <li>Check the file name for capitalization or other typing errors.</li> "
"<li>Check to see if the file was moved, renamed or deleted.</li> </ul>"
msgstr ""

#: generic.title
msgid "Oops."
msgstr "Ups."

#: generic.longDesc
msgid "<p>&brandShortName; can't load this page for some reason.</p>"
msgstr ""
"<p>&brandShortName; teu tiasa ngamuat ieu kaca kusabab sawatara ékol.</p>"

#: malformedURI.title
msgid "The address isn't valid"
msgstr "Alamat katut henteu sah"

#: malformedURI.longDesc
msgid ""
"<ul> \n"
"<li>Web addresses are usually written like \n"
"<strong>http://www.example.com/</strong></li> \n"
"<li>Make sure that you're using forward slashes (i.e. \n"
"<strong>/</strong>).</li> \n"
"</ul>"
msgstr ""
"<ul> \n"
"<li>Umumnya alamat ramat ditulis kawas ieu \n"
"<strong>http://www.example.com/</strong></li> \n"
"<li>Pastikeun Anjeun ngagunakeun gurat doyong hareup, nyaéta \n"
"<strong>/</strong>).</li> \n"
"</ul>"

