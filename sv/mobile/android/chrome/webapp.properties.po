#~ msgstr "#1 ny uppdatering;#1 nya uppdateringar"

#~ msgid "Touch to download %S"
#~ msgstr "Tryck för att hämta %S"

#~ msgid "Downloading #1 update…;Downloading #1 updates…"
#~ msgstr "Hämtar #1 uppdatering…;Hämtar #1 uppdateringar…"

#~ msgid "Downloading %S"
#~ msgstr "Hämtar %S"

#~ msgid "Touch to install %S"
#~ msgstr "Tryck för att installera %S"

#~ msgid "#1 download failed;#1 downloads failed"
#~ msgstr "#1 hämtning misslyckades;#1 hämtningar misslyckades"

#~ msgid "Failed to download %S"
#~ msgstr "Kunde inte hämta %S"
