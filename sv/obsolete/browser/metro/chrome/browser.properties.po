msgstr "Hjälp (online)"

#. # General
#. # LOCALIZATION NOTE (browserForSaveLocation): Title for the "Save..." file picker dialog
#: browserForSaveLocation
msgid "Save Location"
msgstr "Spara plats"

#. # LOCALIZATION NOTE (browserForSaveLocation): Title for the "Open..." file picker dialog
#: browserForOpenLocation
msgid "Open Location"
msgstr "Öppna plats"

#. # Download Manager
#: downloadsUnknownSize
msgid "Unknown size"
msgstr "Okänd storlek"

#: downloadOpen
msgid "Open"
msgstr "Öppna"

#: downloadSave
msgid "Save"
msgstr "Spara"

#: downloadCancel
msgid "Cancel"
msgstr "Avbryt"

#: downloadTryAgain
msgid "Try Again"
msgstr "Försök igen"

#: downloadOpenNow
msgid "Open it now"
msgstr "Öppna nu"

