#: quitDialogTitle
msgid "Quit %S"
msgstr "Toka %S"

#: quitTitle
msgid "&Quit"
msgstr "&Toka"

#: cancelTitle
msgid "&Cancel"
msgstr "&Ghairi"

#: saveTitle
msgid "&Save and Quit"
msgstr "&Hifadhi na Kutoka"

#: neverAsk
msgid "Do not ask next time"
msgstr "Usiniulize wakati ujao"

#: message
msgid ""
"Do you want %S to save your tabs and windows for the next time it starts?"
msgstr ""
"Ungetaka %S kuihifadhi vichupo vyako na dirisha kwa wakati ujao ikianza?"

#: messageNoWindows
msgid "Do you want %S to save your tabs for the next time it starts?"
msgstr "Ungetaka %S kuhifadhi vichupo vyako kwa wakati ujao ikianza?"

#~ msgid "Restart %S"
#~ msgstr "Washa upya %S"

#~ msgid "&Restart"
#~ msgstr "&Washa upya"

#~ msgid "%S will try to restore your tabs and windows when it restarts."
#~ msgstr ""
#~ "%S itajaribu kurejesha vichupo na dirisha zako wakati itawashwa upya."
