#: hostColon
msgid "Host:"
msgstr "Mwenyeji:"

#: domainColon
msgid "Domain:"
msgstr "Kikoa:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Miunganisho iliyofichika pekee"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Aina yoyote ya muunganisho"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "Mwisho wa kipindi"

#: showDetails
msgid "Show Details"
msgstr "Onyesha Maelezo"

#: hideDetails
msgid "Hide Details"
msgstr "Ficha Maelezo"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Tovuti %S inataka kuseti kidakuzi."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "Tovuti %S inataka kuseti kidakuzi cha pili."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"Tovuti %S inataka kuseti kidakuzi kingine.\n"
"Tayari una vidakuzi %S kutoka kwenye tovuti hii."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "Tovuti %S inataka kurekebisha kidakuzi kilichoko."
