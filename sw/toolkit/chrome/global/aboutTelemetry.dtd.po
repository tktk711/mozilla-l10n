#: aboutTelemetry.pageTitle
msgid "Telemetry Data"
msgstr "Data ya Telemeta"

#: aboutTelemetry.telemetryEnabled
msgid "  Telemetry is <span>enabled</span>."
msgstr "      Telemeta <span>imewezeshwa</span>."

#: aboutTelemetry.telemetryDisabled
msgid "  Telemetry is <span>disabled</span>."
msgstr "       Telemeta <span>imelemazwa</span>."

#: aboutTelemetry.slowSqlSection
msgid "  Slow SQL Statements"
msgstr "      Chelewesha Kauli za SQL"

#: aboutTelemetry.chromeHangsSection
msgid "  Browser Hangs"
msgstr "       Vikwamaji vya Kivinjari"

#: aboutTelemetry.histogramsSection
msgid "  Histograms"
msgstr "  Histogramia"

#: aboutTelemetry.simpleMeasurementsSection
msgid "  Simple Measurements"
msgstr "       Vipimo Rahisi"

#: aboutTelemetry.lateWritesSection
msgid "  Late Writes"
msgstr "       Viandiko Vilivyochelewa"

#: aboutTelemetry.systemInfoSection
msgid "  System Information"
msgstr "       Maelezo ya Mfumo"

#: aboutTelemetry.addonHistogramsSection
msgid "  Histograms Collected by Add-ons"
msgstr "       Histogramia Zilizokusanywa na vifaa vya nyongesa"

#: aboutTelemetry.toggleOn
msgid "  Click to expand section"
msgstr "       Bonyeza ili kupanua sehemu"

#: aboutTelemetry.toggleOff
msgid "  Click to collapse section"
msgstr "       Bonyeza ili kupoteza sehemu"

#: aboutTelemetry.emptySection
msgid "  (No data collected)"
msgstr "  (Hakuna data iliyokusanywa)"

#: aboutTelemetry.fullSqlWarning
msgid ""
"  NOTE: Slow SQL debugging is enabled. Full SQL strings may be displayed "
"below but they will not be submitted to Telemetry."
msgstr ""
"  DOKEZO: Ueuaji wa SQL ya ucheleweshaji umewezeshwa. Tungo za SQL nzima "
"zinaweza onyeshwa hapa chini lakini hazitawasilishwa kwa Telemeta."

#: aboutTelemetry.fetchSymbols
msgid "  Fetch function names for hang stacks"
msgstr "  Leta majina ya fomula saidizi za kwama ya omekezo"

#: aboutTelemetry.hideSymbols
msgid "  Show raw data from hangs"
msgstr "  Onyesha data ghafi kutoka kwa vikwamaji"
