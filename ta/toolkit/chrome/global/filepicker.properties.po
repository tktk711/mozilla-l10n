msgstr "HTML கோப்புகள்"

#: textTitle
msgid "Text Files"
msgstr "உரை கோப்புகள்"

#: imageTitle
msgid "Image Files"
msgstr "பட கோப்புகள்"

#: xmlTitle
msgid "XML Files"
msgstr "XML கோப்புகள்"

#: xulTitle
msgid "XUL Files"
msgstr "XUL கோப்புகள்"

#: appsTitle
msgid "Applications"
msgstr "பயன்பாடுகள்"

#: audioTitle
msgid "Audio Files"
msgstr "ஆடியோ கோப்புகள்"

#: videoTitle
msgid "Video Files"
msgstr "வீடியோ கோப்புகள்"

#: dirTextInputLabel
msgctxt "dirTextInputLabel"
msgid "Directory name:"
msgstr "கோப்பக பெயர்:"

#: dirTextInputAccesskey
msgid "n"
msgstr "n"

#: confirmTitle
msgid "Confirm"
msgstr "உறுதிசெய்"

#: confirmFileReplacing
msgid ""
"%S already exists.\n"
"Do you want to replace it?"
msgstr ""
"%S ஏற்கனவே உள்ளது.\n"
"இதை மாற்ற வேண்டுமா?"

#: openButtonLabel
msgid "Open"
msgstr "திற"

#: saveButtonLabel
msgid "Save"
msgstr "சேமி"

#: selectFolderButtonLabel
msgid "Select"
msgstr "தேர்வு செய்"

#: noButtonLabel
msgid "No"
msgstr "இல்லை"

#: formatLabel
msgid "Format:"
msgstr "வடிவமைப்பு:"

#: errorOpenFileDoesntExistTitle
msgctxt "errorOpenFileDoesntExistTitle"
msgid "Error opening %S"
msgstr "%Sஐ திறப்பதில் பிழை"

#: errorOpenFileDoesntExistMessage
msgid "File %S doesn't exist"
msgstr "கோப்பு %S இல்லை"

#: errorDirDoesntExistTitle
msgid "Error accessing %S"
msgstr "அணுகல் பிழை %S"

#: errorDirDoesntExistMessage
msgid "Directory %S doesn't exist"
msgstr "அடைவு %S இல்லை"

#: errorOpeningFileTitle
msgctxt "errorOpeningFileTitle"
msgid "Error opening %S"
msgstr "%Sஐ திறப்பதில் பிழை"

#: openWithoutPermissionMessage_file
msgid "File %S is not readable"
msgstr "கோப்பு %S படிக்ககூடிய வடிவில் இல்லை"

#: errorSavingFileTitle
msgid "Error saving %S"
msgstr "சேமிக்கும் போது பிழை %S"

#: saveParentIsFileMessage
msgid "%S is a file, can't save %S"
msgstr "%S ஒரு கோப்பு, %Sஐ சேமிக்க முடியவில்லை"

#: saveParentDoesntExistMessage
msgid "Path %S doesn't exist, can't save %S"
msgstr "பாதை %S இல்லை, சேமிக்க முடியாது %S"

#: saveWithoutPermissionMessage_file
msgid "File %S is not writable."
msgstr "கோப்பு %S இல் எழுத முடியாது."

#: saveWithoutPermissionMessage_dir
msgid "Cannot create file. Directory %S is not writable."
msgstr "கோப்பை உருவாக்க முடியாது. அடைவு %S இல் எழுத முடியாது."

#: errorNewDirDoesExistTitle
msgctxt "errorNewDirDoesExistTitle"
msgid "Error creating %S"
msgstr "%Sஐ உருவாக்குவதில் பிழை"

#: errorNewDirDoesExistMessage
msgid "A file named %S already exists, directory cannot be created."
msgstr "%S என்ற பெயருள்ள கோப்பு ஏற்கெனவே உள்ளது, அடைவை உருவாக்க முடியாது."

#: errorCreateNewDirTitle
msgctxt "errorCreateNewDirTitle"
msgid "Error creating %S"
msgstr "%Sஐ உருவாக்குவதில் பிழை"

#: errorCreateNewDirMessage
msgid "Directory %S could not be created"
msgstr "அடைவு %S ஐ உருவாக்க முடியவில்லை"

#: errorCreateNewDirIsFileMessage
msgid "Directory cannot be created, %S is a file"
msgstr "அடைவை உருவாக்க முடியவில்லை, %S ஒரு கோப்பாகும்"

#: errorCreateNewDirPermissionMessage
msgid "Directory cannot be created, %S not writable"
msgstr "அடைவை உருவாக்க முடியவில்லை %S இல் எழுத முடியாது"

#: promptNewDirTitle
msgid "Create new directory"
msgstr "புதிய அடைவை உருவாக்கு"

#: promptNewDirMessage
msgctxt "promptNewDirMessage"
msgid "Directory name:"
msgstr "அடைவு பெயர்:"

#: errorPathProblemTitle
msgid "Unknown Error"
msgstr "தெரியாத பிழை"

#: errorPathProblemMessage
msgid "An unknown error occured (path %S)"
msgstr "தெரியாத பிழை நேர்ந்தது (path %S)"
