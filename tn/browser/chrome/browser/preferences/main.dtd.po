#: startup.label
msgid "Startup"
msgstr "Tshimololosešwa"

#: startupPage.label startupPage.accesskey
msgid "When &brandShortName; &starts:"
msgstr "Fa &brandShortName; & e simolola:"

#: startupHomePage.label
msgid "Show my home page"
msgstr "Bontsha Tsebegae yame"

#: startupBlankPage.label
msgid "Show a blank page"
msgstr "Bontsha tsebe e lolea"

#: startupLastSession.label
msgid "Show my windows and tabs from last time"
msgstr "Supa di window le dithebe tsame go tsweng mo nakong e fetileng"

#: homepage.label homepage.accesskey
msgid "Home &Page:"
msgstr "Gae &Tsebe:"

#: useCurrentPage.label useCurrentPage.accesskey
msgid "Use &Current Page"
msgstr "Dirisa &Tsebe ya Ga jaana"

#: useMultiple.label
msgid "Use Current Pages"
msgstr "Dirisa Ditsebe tsa Ga jaana"

#: chooseBookmark.label chooseBookmark.accesskey
msgid "Use &Bookmark…"
msgstr "Dirisa &Letshwaotsebe…"

#: restoreDefault.label restoreDefault.accesskey
msgid "&Restore to Default"
msgstr "&Busetsa ko Thulaganyetsoruri"

#: downloads.label
msgid "Downloads"
msgstr "Dikopololo"

#: saveTo.label saveTo.accesskey
msgid "Sa&ve files to"
msgstr "&Bolokela difaele ko"

#: chooseFolderWin.label chooseFolderWin.accesskey
msgid "Br&owse…"
msgstr "&Batla…"

#: chooseFolderMac.label chooseFolderMac.accesskey
msgid "Choos&e…"
msgstr "Tlhopha…"

#: alwaysAsk.label alwaysAsk.accesskey
msgid "&Always ask me where to save files"
msgstr "&Nna o mpotsa ka nako tsotlhe gore di faele di bolokwe kae"

#: alwaysCheckDefault2.label alwaysCheckDefault2.accesskey
msgid "Al&ways check if &brandShortName; is your default browser"
msgstr ""
"&Ka nako tsotlhe tlhola gore &brandShortName; ke sebatli sa thulaganyetsoruri"

#: setAsMyDefaultBrowser2.label setAsMyDefaultBrowser2.accesskey
msgid "Make &Default"
msgstr "E dire &Thulaganyetsoruri"

#: isDefault.label
msgid "&brandShortName; is currently your default browser"
msgstr ""
"&brandShortName; ga jaana ke sebatli sebatli sa gago sa thulaganyetsoruri"

#: isNotDefault.label
msgid "&brandShortName; is not your default browser"
msgstr "&brandShortName; ga se sebatli sa gago sa thulaganyetsoruri"

#: separateProfileMode.label
msgid "Allow &brandShortName; and Firefox to run at the same time"
msgstr "Letlelela &brandShortName; le Firefox go dira ka nako e le nngwe"

#: useFirefoxSync.label
msgid "Tip: This uses separate profiles. Use Sync to share data between them."
msgstr ""
"Tlhababotlhale: Se se dirisa diporofaele tse di kgaoganeng. Dirisa "
"thulaganyo ka tsamaiso go aba tshedimosetso fa gare ga tsone."

#: getStarted.label
msgid "Start using Sync…"
msgstr "Simolola go dirisa Thulaganyo ka tsamaiso.."

#: e10sEnabled.label
msgid "Enable multi-process &brandShortName;"
msgstr "Kgontsha Thulaganyo-bontsi &brandShortName;"

#~ msgid "3em"
#~ msgstr "3em"
