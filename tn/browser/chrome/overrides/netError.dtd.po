msgstr "Ithute thata..."

#: remoteXUL.title
msgid "Remote XUL"
msgstr ""

#: remoteXUL.longDesc
msgid ""
"<p><ul><li>Please contact the website owners to inform them of this problem."
"</li></ul></p>"
msgstr ""
"<p><ul><li>Tswêê-tswêê itshwaraganye le beng ba webosaete go ba itsese ka "
"bothaka jo.</li></ul></p>"

#: sslv3Used.title
msgid "Unable to Connect Securely"
msgstr "Ga go kgonege go Golagana mo go Sireletsegileng"

#. <!-- LOCALIZATION NOTE (sslv3Used.longDesc2) - Do not translate
#. "SSL_ERROR_UNSUPPORTED_VERSION".
#: sslv3Used.longDesc2
msgid "Advanced info: SSL_ERROR_UNSUPPORTED_VERSION"
