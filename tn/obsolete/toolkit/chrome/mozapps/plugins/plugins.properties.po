#: pluginLicenseAgreement.label
msgid "To install %S, you need to agree to the following:"
msgstr "Go tlhoma %S, o tlhoka go dumelana le tse di latelang:"

#: pluginInstallation.download.start
msgid "Downloading %S…"
msgstr "E kopolola %S…"

#: pluginInstallation.download.finish
msgid "Finished downloading %S."
msgstr "E feditse go kopolola %S."

#: pluginInstallation.install.start
msgid "Installing %S…"
msgstr "E tlhoma %S…"

#: pluginInstallation.install.finish
msgid "Successfully installed %S."
msgstr "E tlhomile %S ka katlego."

#: pluginInstallation.install.error
msgid "Failed to install %S (%S)."
msgstr "E reteletswe ke go tlhoma %S (%S)."

#: pluginInstallation.complete
msgid "Finished installing plugins."
msgstr "E feditse go tlhoma dipolaka."

#: pluginInstallationSummary.success
msgid "Installed"
msgstr "Tlhomilwe"

#: pluginInstallationSummary.failed
msgid "Failed"
msgstr "Reteletswe"

#: pluginInstallationSummary.licenseNotAccepted
msgid "License not accepted"
msgstr "Laesense ga e a amogelwa"

#: pluginInstallationSummary.notAvailable
msgid "Not Available"
msgstr "Ga e gona"

#: pluginInstallationSummary.manualInstall.label
msgid "Manual Install"
msgstr "Tlhoma ka go dira"

#: pluginInstallationSummary.manualInstall.tooltip
msgid "Manually install the plugin."
msgstr "Tlhoma polaka ka go dira."

#: pluginInstallation.noPluginsFound
msgid "No suitable plugins were found."
msgstr "Ga go a fitlhelwa dipolaka dipe tse di maleba."

#: pluginInstallation.noPluginsInstalled
msgid "No plugins were installed."
msgstr "Ga go na dipolaka dipe tse di tlhomilweng."

#: pluginInstallation.unknownPlugin
msgid "Unknown Plugin (%S)"
msgstr ""

#: pluginInstallation.restart.label pluginInstallation.restart.accesskey
#, fuzzy
msgid "&Restart %S"
msgstr "Simolola %S sešwa"

#: pluginInstallation.close.label pluginInstallation.close.accesskey
#, fuzzy
msgid "&Close"
msgstr "Tswala"

#~ msgid "Unknown Plugin"
#~ msgstr "Polaka e e sa Itsiweng"

#~ msgid "Click here to download plugin."
#~ msgstr "Tobetsa fano go kopolola polaka."
