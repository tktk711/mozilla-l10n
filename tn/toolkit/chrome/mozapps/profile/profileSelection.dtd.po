#: profilename.label
msgid "Profile Name:"
msgstr "Leina la Porofaele:"

#: start.label
msgid "Start &brandShortName;"
msgstr "Simolola &brandShortName;"

#: exit.label
msgid "Exit"
msgstr "Tswa"

#: availprofiles.label
msgid "Available Profiles"
msgstr "Diporofaele tse di Gona"

#: newButton.label newButton.accesskey
msgid "&Create Profile…"
msgstr "&Tlhama Porofaele…"

