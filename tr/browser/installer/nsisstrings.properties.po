msgstr "$BrandShortName &tarayıcısını varsayılan tarayıcım yap"

#: CREATE_SHORTCUTS
msgid "Create Shortcuts for $BrandShortName:"
msgstr "$BrandShortName için şu konumlarda kısayol oluştur:"

#: ADD_SC_TASKBAR
msgid "On my &Task bar"
msgstr "&Görev çubuğuma"

#: ADD_SC_QUICKLAUNCHBAR
msgid "On my &Quick Launch bar"
msgstr "&Hızlı Başlangıç çubuğuma"

#: ADD_CheckboxShortcutInStartMenu
msgid "In my &Start Menu Programs Folder"
msgstr "&Başlat menüsündeki Programlar klasörüme"

#: ADD_CheckboxShortcutOnDesktop
msgid "On my &Desktop"
msgstr "&Masaüstüne"

#: SPACE_REQUIRED
msgid "Space Required:"
msgstr "Gerekli alan:"

#: SPACE_AVAILABLE
msgid "Space Available:"
msgstr "Mevcut alan:"

#: ONE_MOMENT_INSTALL
msgid ""
"One moment, $BrandShortName will launch as soon as the install is complete…"
msgstr ""
"Lütfen bekleyin. $BrandShortName yükleme tamamlanır tamamlanmaz açılacaktır…"

#: ONE_MOMENT_UPGRADE
msgid ""
"One moment, $BrandShortName will launch as soon as the upgrade is complete…"
msgstr "Lütfen bekleyin. Yükseltme tamamlanınca $BrandShortName açılacaktır…"

#: INSTALL_MAINT_SERVICE
msgid "&Install the $BrandShortName background update service"
msgstr "$BrandShortName &arka planda güncelleme hizmetini yükle"

#: SEND_PING
msgid "S&end information about this installation to Mozilla"
msgstr "&Mozilla'ya bu kurulum hakkında bilgi gönder"

#: BROWSE_BUTTON
msgid "B&rowse…"
msgstr "&Gözat…"

#: DEST_FOLDER
msgid "Destination Folder"
msgstr "Hedef Dizin"

#: DOWNLOADING_LABEL
msgid "Downloading $BrandShortName…"
msgstr "$BrandShortName indiriliyor…"

#: INSTALLING_LABEL
msgid "Installing $BrandShortName…"
msgstr "$BrandShortName Kuruluyor…"

#: UPGRADING_LABEL
msgid "Upgrading $BrandShortName…"
msgstr "$BrandShortName yükseltiliyor…"

#: SELECT_FOLDER_TEXT
msgid "Select the folder to install $BrandShortName in."
msgstr "$BrandShortName uygulamasının yükleneceği dizini seçin."

