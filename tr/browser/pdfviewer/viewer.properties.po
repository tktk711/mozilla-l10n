msgid "Automatic Zoom"
msgstr "Otomatik yakınlaştır"

#: page_scale_actual
msgid "Actual Size"
msgstr "Gerçek boyut"

#. # Loading indicator messages
#: loading_error_indicator
msgid "Error"
msgstr "Hata"

#: loading_error
msgid "An error occurred while loading the PDF."
msgstr "PDF yüklenirken bir hata oluştu."

#: invalid_file_error
msgid "Invalid or corrupted PDF file."
msgstr "Geçersiz veya bozulmuş PDF dosyası."

#: missing_file_error
msgid "Missing PDF file."
msgstr "PDF dosyası eksik."

#. # LOCALIZATION NOTE (text_annotation_type.alt): This is used as a tooltip.
#. # "{{type}}" will be replaced with an annotation type from a list defined in
#. # the PDF spec (32000-1:2008 Table 169 – Annotation types).
#. # Some common types are e.g.: "Check", "Text", "Comment", "Note"
#: text_annotation_type.alt
msgid "[{{type}} Annotation]"
msgstr "[{{type}} işareti]"

#: request_password
msgid "PDF is protected by a password:"
msgstr "Bu PDF parola korumalı:"

#: invalid_password
msgid "Invalid Password."
msgstr "Geçersiz Parola."

