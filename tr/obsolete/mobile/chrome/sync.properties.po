#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Son eşleşme: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Son güncelleme: sürüyor..."

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Hesap: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Bağlı değil."

#: connecting.label
msgid "Connecting…"
msgstr "Bağlanıyor..."

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Firefox Eşitleme hesabınız silindi"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Geri al"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 Firefox Sync'in son sürümüyle uyumlu değil. Lütfen son sürüme "
"güncelleme yapın."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 Firefox Sync'in eski sürümleriyle uyumlu değil. Lütfen bilgisayar(lar)"
"ınızdaki Firefox'u güncelleyin."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Daha fazla bilgi"

#: sync.update.close
msgid "Close"
msgstr "Kapat"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Sync kurulamıyor"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "İnternet bağlantısı yok"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S Sync'e bağlanamadı. Yeniden denemek ister misiniz?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Yeniden dene"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Elle ayarla"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "No tabs from your other computers."
