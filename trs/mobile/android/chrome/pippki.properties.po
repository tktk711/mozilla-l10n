#~ msgstr "Si yugui rasuun (CN)"

#~ msgid "Organization (O)"
#~ msgstr "Yi'nï'ïn (O)"

#~ msgid "Serial Number"
#~ msgstr "Número nikò' dugui'i"

#~ msgid "SHA1 Fingerprint"
#~ msgstr "Da'nga' ra'a SHA1"

#~ msgid "Begins On"
#~ msgstr "Gayi'ì ngà"

#~ msgid "Expires On"
#~ msgstr "Girè'ej gui"

#~ msgid "Certificate Exists"
#~ msgstr "Ngà hua certificado"

#~ msgid "The Certificate already exists."
#~ msgstr "Ngà hua certificado ma."
