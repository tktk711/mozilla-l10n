#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Sa nahuin guña rukù nï'ïnj: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Hìaj nagi'iaj nàkaj: sa nagi'iaj guñaan..."

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Cuenta: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Nitaj si hua conectadoj"

#: connecting.label
msgid "Connecting…"
msgstr "Hìaj 'iaj conectandoj..."

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Cuenta nu 'iát riña Firefox Sync gà' nare' ma"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Nadurê'"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 Nitaj si ara' dugui'ij ngà nej sa nakàa araj sun Firefox Sync. Gi'iaj "
"'ngo sunnuj nu, nagi'iaj nakàt man an."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 Nitaj si aran'anj nga Firefox Sync go araj sunt. Gi'iaj 'ngo sunnuj "
"ni, nagi'iaj nakàt Firefox Sync riña a'ngô nej si aga't."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Gahuin chrun' doj"

#: sync.update.close
msgid "Close"
msgstr "Ganarun"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Na'ue nahuin sà' Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Nitaj si hua gatu' riñan internet"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S na'ue gatu riña Sync. ¿Ni'iaj si ga'ue gatu ñû raj?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "A'ngo ñun"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Naduna mu'ûn'"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Nitaj rakïj ñanj hua riña a'ngô ne si aga't aga' sikà' ràa ma."

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Sync nga naguin naká ni nitaj si chí' hua man"

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "Gahui' ñunan da' guini'in doj"
