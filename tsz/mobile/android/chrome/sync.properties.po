#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "terukutarukuni:%S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "terukutantskuani jatantaxati"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "miyukorhentskua"

#: notconnected.label
msgid "Not connected"
msgstr "no uxati kutuparakuni"

#: connecting.label
msgid "Connecting…"
msgstr "kutuparakuxati"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "cheeti patsakatarhu Firefox Sync xerekuarhintastia"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Xerentani"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr "#1#2 no jintesti majku jasïsti, éxe sáni"

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 no jintesti majku jasï yontki anapuni jinkoni Firefox Sync, weenta "
"mentehu éxeni materhu Firefox Sync urakuechani"

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Sánteru mítini"

#: sync.update.close
msgid "Close"
msgstr "mikani"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "no usïnti motakukuarhini"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "no jarasti kuturakua internet"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S no usïnti kuturakoreni Sync.  wekasïnti ménteru wentaani ?"

#: sync.setup.tryagain
msgid "Try again"
msgstr " menteru wentaani"

#: sync.setup.manual
msgid "Manual setup"
msgstr "menteru untani sïrantani"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "nómpe xarantasïnti jirinanskuecha materuecheri uratarakwecha"

#. # LOCALIZATION NOTE (promoBanner.message.text): First part of the message displayed in a
#. # banner on about:home. The final space separates this text from the link.
#: promoBanner.message.text
msgid "Sync is brand new and easier than ever. "
msgstr "Sync jimpanintasti ka sani sesi urakuarasïnti"

#. # LOCALIZATION NOTE (promoBanner.message.link): Second part of the message, styled as a link.
#: promoBanner.message.link
msgid "Tap here to learn more"
msgstr "p`arhi ixu ka sani mitiakarhi"
