#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "ئاخىرقى قەدەمداش: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "ئاخىرقى قەدەمداش: ئىجرا قىلىنىۋاتىدۇ…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "ھېسابات: %S"

#: notconnected.label
msgid "Not connected"
msgstr "باغلانمىدى"

#: connecting.label
msgid "Connecting…"
msgstr "باغلىنىۋاتىدۇ…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "سىزنىڭ Firefox قەدەمداش ھېساباتىڭىز چىقىرىۋېتىلىدۇ"

#: notificationDisconnect.button
msgid "Undo"
msgstr "يېنىۋال"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 ئەڭ يېڭى نەشرىدىكى Firefox قەدەمداش بىلەن مەسلاشمايدۇ. ئەڭ يېڭى "
"نەشرىگە يېڭىلاڭ."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 كونا نەشرىدىكى Firefox قەدەمداش بىلەن ماسلاشمايدۇ. باشقا "
"كومپيۇتېرىڭىزدىكى Firefox نى يېڭىلاڭ."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox قەدەمداش"

#: sync.update.button
msgid "Learn More"
msgstr "تېخىمۇ كۆپ بىلدۈرگۈ"

#: sync.update.close
msgid "Close"
msgstr "ياپ"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "قەدەمداشنى ئورنىتالمايدۇ"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "ئىشلەتكىلى بولىدىغان ئىنتېرنېت باغلىنىشى يوق"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S قەدەمداشقا باغلىنالمىدى. قايتا سىنامسىز؟"

#: sync.setup.tryagain
msgid "Try again"
msgstr "قايتا سىناڭ"

#: sync.setup.manual
msgid "Manual setup"
msgstr "قولدا ئورنىتىش"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "باشقا كومپيۇتېرنىڭ بەتكۈچلىرى يوق."
