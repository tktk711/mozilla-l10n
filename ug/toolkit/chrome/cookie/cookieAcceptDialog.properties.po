#: hostColon
msgid "Host:"
msgstr "كومپيۇتېر:"

#: domainColon
msgid "Domain:"
msgstr "دائىرە:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "شىفىرلانغان باغلىنىشلا"

#: forAnyConnection
msgid "Any type of connection"
msgstr "خالىغان باغلىنىش"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "سۆزلىشىش ئاخىرلاشقاندا"

#: showDetails
msgid "Show Details"
msgstr "تەپسىلاتىنى كۆرسەت"

#: hideDetails
msgid "Hide Details"
msgstr "تەپسىلاتىنى يوشۇر"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "%S بېكەتكە بىر cookie بەلگىلەش لازىم."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "%S بېكەتكە ئىككى cookie بەلگىلەش لازىم."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"%S بېكەتكە باشقا cookie بەلگىلەش لازىم.\n"
"سىز بۇ بېكەتتىن %S دانە cookie غا ئېرىشتىڭىز."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "%S بېكەت نۆۋەتتىكى cookie نى ئۆزگەرتىش لازىم."
