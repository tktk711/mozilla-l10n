#: offlineAppsList.height
msgid "7em"
msgstr "7em"

#: offlineAppsListRemove.label offlineAppsListRemove.accesskey
msgid "&Remove…"
msgstr "&ہٹائیں"

#: offlineAppRemove.confirm
msgid "Remove offline data"
msgstr "آف لائن کوائف ہٹائیں"

#: encryptionTab.label
msgid "Encryption"
msgstr "خفیہ کاری"

#: protocols.label
msgid "Protocols"
msgstr "پروٹوکول"

#: useSSL3.label useSSL3.accesskey
msgid "Use SSL &3.0"
msgstr "SSL &3.0 استعمال کریں"

#: useTLS1.label useTLS1.accesskey
msgid "Use TLS &1.0"
msgstr "TLS &1.0 استعمال کریں"

#: certificates.label
msgid "Certificates"
msgstr "تصدیق نامے"

#: certSelection.description
msgid "When a server requests my personal certificate:"
msgstr "جب پیش کار میرے ذاتی تصدیق نامہ کی درخواست کرے:"

#: certs.auto
msgid "Select one automatically"
msgstr "خودبخود ایک منتخب کریں"

#: certs.auto.accesskey
msgid "l"
msgstr "ت"

#: certs.ask
msgid "Ask me every time"
msgstr "مجھے ہر بار پوچھیں"

#: certs.ask.accesskey
msgid "i"
msgstr "پ"

#: viewCerts.label viewCerts.accesskey
msgid "View Certificate&s"
msgstr "&تصدیق نامے نظارہ کریں"

#: viewCRLs.label viewCRLs.accesskey
msgid "&Revocation Lists"
msgstr "&باطلفہرستیں"

#: verify2.label verify2.accesskey
msgid "&Validation"
msgstr "&جواز دہی"

#: viewSecurityDevices.label viewSecurityDevices.accesskey
msgid "Securit&y Devices"
msgstr "&سلامتی آلات"

