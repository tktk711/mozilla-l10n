msgid "Use &Bookmark…"
msgstr "&Xatchoʻplardan foydalanish"

#: restoreDefault.label restoreDefault.accesskey
msgid "&Restore to Default"
msgstr "&Standart holatiga tikalsh"

#: downloads.label
msgid "Downloads"
msgstr "Yuklab olishlar"

#: saveTo.label saveTo.accesskey
msgid "Sa&ve files to"
msgstr "Fayllarni &saqlash:"

#: chooseFolderWin.label chooseFolderWin.accesskey
msgid "Br&owse…"
msgstr "&Koʻrish"

#: chooseFolderMac.label chooseFolderMac.accesskey
msgid "Choos&e…"
msgstr "&Tanlash"

