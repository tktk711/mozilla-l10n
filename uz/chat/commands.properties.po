#. # LOCALIZATION NOTE (commands):
#. #  %S is a comma separated list of command names.
#: commands
msgid ""
"Commands: %S.\n"
"Use /help &lt;command&gt; for more information."
msgstr ""
"Buyruqlar: %S.\n"
"Ko‘proq ma’lumot olish uchun /help &lt;buyruq&gt; buyrug‘idan foydalaning."

#. # LOCALIZATION NOTE (noCommand, noHelp):
#. #  %S is the command name the user typed.
#: noCommand
msgid "No '%S' command."
msgstr "'%S' buyrug‘i yo‘q."

#: noHelp
msgid "No help message for the '%S' command, sorry!"
msgstr "'%S' buyrug‘i uchun yordam xabari yo‘q, kechirasiz!"

#: sayHelpString
msgid "say &lt;message&gt;: send a message without processing commands."
msgstr ""
"Buyruqlarni klaviaturadan termasdan xabar jo‘natish uchun &lt;message&gt;: "
"deb ayting."

#: rawHelpString
msgid "raw &lt;message&gt;: send a message without escaping HTML entities."
msgstr "raw &lt;message&gt;: HTML nomlardan qochishsiz xabarni jo‘natish."

#: helpHelpString
msgid ""
"help &lt;name&gt;: show the help message for the &lt;name&gt; command, or "
"the list of possible commands when used without parameter."
msgstr ""
"help &lt;name&gt;: &lt;name&gt; buyrug‘i yoki parametersiz foydalanilgan "
"mavjud buyurqlar ro‘yxati uchun yordam xabarini ko‘rsatish."

#. # LOCALIZATION NOTE (statusCommand):
#. #  %1$S is replaced with a status command name
#. #   (one of "back", "away", "busy", "dnd", or "offline").
#. #  %2$S is replaced with the localized version of that status type
#. #   (one of the 5 strings below).
#: statusCommand
msgid ""
"%1$S &lt;status message&gt;: set the status to %2$S with an optional status "
"message."
msgstr ""
"%1$S &lt;status message&gt;: ixtiyoriy holat xabari bilan birga %2$S "
"holatini o‘rnatish."

#: back
msgid "available"
msgstr "mavjud"

#: away
msgid "away"
msgstr "tashqarida"

#: busy
msgctxt "busy"
msgid "unavailable"
msgstr "mavjud emas"

#: dnd
msgctxt "dnd"
msgid "unavailable"
msgstr "mavjud emas"

#: offline
msgid "offline"
msgstr "oflayn"
