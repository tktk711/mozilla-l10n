msgstr "Profil ID raqami"

#. # LOCALZIATION NOTE (command.*):
#. #  These are the help messages for each command.
#: command.join3
msgid ""
"%S [&lt;room&gt;[@&lt;server&gt;][/&lt;nick&gt;]] [&lt;password&gt;]: Join a "
"room, optionally providing a different server, or nickname, or the room "
"password."
msgstr ""
"%S [&lt;room&gt;[@&lt;server&gt;][/&lt;nick&gt;]] [&lt;password&gt;]: Xonaga "
"qo‘shiling, ixtiyriy boshqa server yoki taxallu yoki parolni ni ko‘rsatmoqda."

#: command.part2
msgid "%S [&lt;message&gt;]: Leave the current room with an optional message."
msgstr "%S [&lt;message&gt;]: Joriy xonani ixtiyoriy xabarsiz tashlab chiqish."

#: command.topic
msgid "%S [&lt;new topic&gt;]: Set this room's topic."
msgstr "%S [&lt;new topic&gt;]: Ushbu xonaning suhbat mavzusini o‘rnating."

#: command.ban
msgid ""
"%S &lt;nick&gt;[&lt;message&gt;]: Ban someone from the room. You must be a "
"room administrator to do this."
msgstr ""
"%S &lt;nick&gt;[&lt;message&gt;]: Xonadagi biror kishini bloklash. Buni "
"amalga oshirish uchun xona adminsitratori bo‘lishingiz kerak."

#: command.kick
msgid ""
"%S &lt;nick&gt;[&lt;message&gt;]: Remove someone from the room. You must be "
"a room moderator to do this."
msgstr ""
"%S &lt;nick&gt;[&lt;message&gt;]: Xonadagi biror kishini bloklash. Buni "
"amalga oshirish uchun xona moderatori bo‘lishingiz kerak."

#: command.invite
msgid ""
"%S &lt;jid&gt;[&lt;message&gt;]: Invite a user to join the current room with "
"an optional message."
msgstr ""
"%S &lt;jid&gt;[&lt;message&gt;]:Majburiy bo‘lmagan havola yordamida "
"foydalanuvchini joriy xonaga qo‘shilish uchun taklif qiling."

#: command.me
msgid "%S &lt;action to perform&gt;: Perform an action."
msgstr "%S &lt;bajarish uchun amal&gt;: Amalni bajarish."

#: command.nick
msgid "%S &lt;new nickname&gt;: Change your nickname."
msgstr "%S &lt;yangi taxallus&gt;: Taxallusingizni o‘zgartirish"

#: command.msg
msgid ""
"%S &lt;nick&gt; &lt;message&gt;: Send a private message to a participant in "
"the room."
msgstr ""
"%S &lt;nick&gt; &lt;message&gt;: Maxfiy xabarni xonadagi ishtirokchiga "
"jo‘nating."

#~ msgid "Unknown error"
#~ msgstr "Noma’lum xatolik"
