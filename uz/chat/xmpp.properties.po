#: tooltip.subscription
msgid "Subscription"
msgstr "Obuna"

#: tooltip.fullName
msgid "Full Name"
msgstr "To‘liq ismi"

#: tooltip.nickname
msgid "Nickname"
msgstr "Taxallusi"

#: tooltip.email
msgid "Email"
msgstr "E-pochta"

#: tooltip.birthday
msgid "Birthday"
msgstr "Tug‘ilgan kun"

#: tooltip.userName
msgid "Username"
msgstr "Foydalanuvchi"

#: tooltip.title
msgid "Title"
msgstr "Sarlavha"

#: tooltip.organization
msgid "Organization"
msgstr "Tashkilot"

#: tooltip.locality
msgid "Locality"
msgstr "Joylashuvi"

#: tooltip.country
msgid "Country"
