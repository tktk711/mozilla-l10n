#. Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "E’lon qilish sahifasi"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "E’lon qilish sozlamalari"

#: publishTab.label
msgctxt "publishTab.label"
msgid "Publish"
msgstr "E’lon qilish"

#: settingsTab.label
msgid "Settings"
msgstr "Sozlamalar"

#: publishButton.label
msgctxt "publishButton.label"
msgid "Publish"
msgstr "E’lon qilish"

#. Publish Tab Panel
#: siteList.label siteList.accesskey
msgctxt "siteList.label siteList.accesskey"
msgid "Sit&e Name:"
msgstr "&Sayt nomi:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "E’lon qilish uchun saytni tanlang:"

#: newSiteButton.label newSiteButton.accesskey
msgid "&New Site"
msgstr "&Yangi sayt"

#: docDirList.label docDirList.accesskey
msgid "&Site subdirectory for this page:"
msgstr "Ushbu sahifa uchun &sayt quyi direktoriyasi:"

#: docDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory for this page"
msgstr ""
"Ushbu sayt uchun masofadagi quyi direktoriya nomini kiriting yoki tanlang"

#: publishImgCheckbox.label publishImgCheckbox.accesskey
msgid "Include images and &other files"
msgstr "Rasmlar va &boshqa fayllar bilan birga"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr ""
"Ushbu sahifa tomonidan taqdim etilgan boshqa fayllar va rasmlarni e’lon "
"qilish"

#: sameLocationRadio.label sameLocationRadio.accesskey
msgid "&Use same location as page"
msgstr "Ayni shu manzildan sahifa sifatida &foydalanish"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "Fayllarni aynni shu manzilga sahifa sifatida e’lon qilish"

#: useSubdirRadio.label useSubdirRadio.accesskey
msgid "Use this site sub&directory:"
msgstr "Ushbu sayt quyi &direktoriyasidan foydalaning:"

#: useSubdirRadio.tooltip
msgid "Publish files to the selected remote subdirectory"
msgstr "Tanlangan masofadagi quyi direktoriyaga fayllarni e’lon qilish"

#: otherDirList.tooltip
msgid ""
"Choose or enter name of remote subdirectory where files will be published"
msgstr ""
"Fayllar e’lon qilinandigan masofadagi quyi direktoriya nomini tanlang yoki "
"kiriting"

#: pageTitle.label pageTitle.accesskey
msgid "Page &Title:"
msgstr "Sahifa &nomi:"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr "E’lon qilish sahifasi"

#: pageTitleExample.label
msgid "e.g.: \"My Web Page\""
msgstr "Masalan: \"Mening veb sahifam\""

#: filename.label filename.accesskey
msgid "&Filename:"
msgstr "Fayl nomi:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr "Veb sahifalar uchun \".html\" bilan birga ushbu fayl nomini kiriting"

#: filenameExample.label
msgid "e.g,: \"mypage.html\""
msgstr "masalan: \"mypage.html\""

#: setDefaultButton.label setDefaultButton.accesskey
msgid "Set as &Default"
msgstr "&Standart sifatida o‘rnatish"

#: removeButton.label removeButton.accesskey
msgid "&Remove Site"
msgstr "Saytni &olib tashlash"

#. Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "E’lon qilinadigan sayt"

#: serverInfo.label
msgid "Server Information"
msgstr "Server ma’lumoti"

#: loginInfo.label
msgid "Login Information"
msgstr "Kirish ma’lumotlari"

#: siteName.label siteName.accesskey
msgctxt "siteName.label siteName.accesskey"
msgid "Sit&e Name:"
msgstr "&Sayt nomi:"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr ""
"Ushbu e’lon qilinadigan saytni tadiqdan o‘tkazish uchun taxallus (masalan: "
"\"MySite\")"

#: siteUrl.label siteUrl.accesskey
msgid "Publishing &address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr "&address e’lon qilinmoqda (masalan: 'ftp://ftp.myisp.com/myusername'):"

#: siteUrl.tooltip
msgid ""
"The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr ""
"The FTP:// yoki HTTP:// manzil ISP yoki veb hosting xizmati tomonidan "
"beriladi"

#: browseUrl.label browseUrl.accesskey
msgid ""
"H&TTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr ""
"Bosh sahifangiz uchun H&TTP manzili (masalan: 'http://www.myisp.com/"
"myusername'):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr "Uy direktoriyangiz uchun HTTP:// manzil (fayl nomi qo‘shilmasin)"

#: username.label username.accesskey
msgid "&User name:"
msgstr "&Foydalanuvchi nomi:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr ""
"ISP yoki veb hosting xizmatiga kirishda foydalanish uchun foydalanuvchi nomi"

#: password.label password.accesskey
msgid "Pass&word:"
msgstr "&Parol:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "Parol sizning foydalanish uchun nomingiz bilan bog‘liq"

#: savePassword.label savePassword.accesskey
msgid "&Save Password"
msgstr "Parolni &saqlash"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr ""
"\"Parol menejeri\"dan foydalanib parolingizni xavfsiz saqlash uchu uni "
"tanlang"
