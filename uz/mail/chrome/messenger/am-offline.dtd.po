msgid "Keep messages f&or this account on this computer"
msgstr "&Bu hisob uchun xabarlar kompyuterga saqlansin"

#: offlineNotDownload.label offlineNotDownload.accesskey
msgid "&Messages larger than"
msgstr "Ushbudan katta &xabarlar"

#: autosyncNotDownload.label autosyncNotDownload.accesskey
msgid "Don't download &messages larger than"
msgstr "Ushbudan katta &xabarlar yuklab olinmasin"

