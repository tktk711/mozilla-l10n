#. General Settings
#: enableGlodaSearch.label enableGlodaSearch.accesskey
msgid "&Enable Global Search and Indexer"
msgstr "Global izlash va indekslashni yoqib &qo‘yish"

#: allowHWAccel.label allowHWAccel.accesskey
msgid "Use &hardware acceleration when available"
msgstr "Mavjud bo‘lganda &qurilmani tezlatishdan foydalanilsin"

#: storeType.label storeType.accesskey
msgid "Message Store &Type for new accounts:"
msgstr "Yangi hisoblar uchun xabarlarni saqlash &turi:"

#: mboxStore2.label
msgid "File per folder (mbox)"
msgstr "Har bir jilddagi fayl (mbox)"

#: maildirStore.label
msgid "File per message (maildir)"
msgstr "Har bir xabardagi fayl (maildir)"

#: scrolling.label
msgid "Scrolling"
msgstr "Siljitish"

#: useAutoScroll.label useAutoScroll.accesskey
msgid "&Use autoscrolling"
msgstr "Avtosiljitishdan &foydalanish"

#: useSmoothScrolling.label useSmoothScrolling.accesskey
msgid "Use s&mooth scrolling"
msgstr "Bir t&ekisda siljitishdan foydalanish"

#: systemIntegration.label
msgid "System Integration"
msgstr "Tizimni integratsiyalash"

#: alwaysCheckDefault.label alwaysCheckDefault.accesskey
msgid ""
"&Always check to see if &brandShortName; is the default mail client on "
"startup"
msgstr ""
"Ishga tushirilganda doimo &brandShortName; standart e-pochta mijozi ekanligi "
"tekshirilsin"

#: searchIntegration.label searchIntegration.accesskey
msgid "Allow &searchIntegration.engineName; to &search messages"
msgstr "Xabarlarni qidirishda &searchIntegration.engineName;ga ruxsat berilsin"

#: checkDefaultsNow.label checkDefaultsNow.accesskey
msgid "Check &Now…"
msgstr "&Hozir tekshirish…"

#: configEditDesc.label
msgid "Advanced Configuration"
msgstr "Qoʻshimcha moslash"

#: configEdit.label configEdit.accesskey
msgid "&Config Editor…"
msgstr "&Tahrirlagichni moslash…"

#: returnReceiptsInfo.label
msgid "Determine how &brandShortName; handles return receipts"
msgstr ""
"&brandShortName; qabul qilinganlik haqidagi xabarlar bilan qanday ishlashini "
"aniqlash"

#: showReturnReceipts.label showReturnReceipts.accesskey
msgid "&Return Receipts…"
msgstr "&Qabul qilinganligi haqida xabar berish…"

#. Data Choices
#: telemetrySection.label
msgid "Telemetry"
msgstr "Telemetry"

#: telemetryDesc.label
msgid ""
"Shares performance, usage, hardware and customization data about your e-mail "
"client with &vendorShortName; to help us make &brandShortName; better"
msgstr ""
"&vendorShortName; bilan &brandShortName;ni yaxshilashimizga yordam "
"berishingiz uchun e-pochta mijozingiz ishlashi, foydalanish, qurilma va "
"moslash ma’lumotlarini boʻlishadi"

#: enableTelemetry.label enableTelemetry.accesskey
msgid "Enable &Telemetry"
msgstr "&Telemetry xizmatini yoqish"

#: telemetryLearnMore.label
msgctxt "telemetryLearnMore.label"
msgid "Learn More"
msgstr "Batafsil ma’lumot"

#: crashReporterSection.label
msgid "Crash Reporter"
msgstr "Nosozlik haqida xabar bergich"

#: crashReporterDesc.label
msgid ""
"&brandShortName; submits crash reports to help &vendorShortName; make your e-"
"mail client more stable and secure"
msgstr ""
"&brandShortName; &vendorShortName;ga e-pochta mijozini yaxshi ishlashi va "
"xavfsiz qilishda yordam berish uchun nosozlik ma’lumotlarni jo‘natadi"

#: enableCrashReporter.label enableCrashReporter.accesskey
msgid "Enable &Crash Reporter"
msgstr "&Nosozlik haqida xabar bergichni yoqib qo‘yish"

#: crashReporterLearnMore.label
msgctxt "crashReporterLearnMore.label"
msgid "Learn More"
msgstr "Batafsil ma’lumot"

#. Update
#: updateApp.label
msgid "&brandShortName; updates:"
msgstr "&brandShortName; yangilanishlari:"

#: updateAuto.label updateAuto.accesskey
msgid "&Automatically install updates (recommended: improved security)"
msgstr ""
"Yangilanishlarni &avtomatik o‘rnatish (tavsiya qilinadi: yaxshilangan "
"xavfsizlik)"

#: updateCheck.label updateCheck.accesskey
msgid "&Check for updates, but let me choose whether to install them"
msgstr ""
"Yangilanishlar uchun &tekshirilsin, ammo menga tanlab o‘rnatish imkoni "
"berilsin"

#: updateManual.label updateManual.accesskey
msgid "&Never check for updates (not recommended: security risk)"
msgstr ""
"Yangilanishlar uchun &hech qachon tekshirilmasin (tavsiya qilinmaydi: "
"xavfsizlik tahdidi)"

#: updateHistory.label updateHistory.accesskey
msgid "Show U&pdate History"
msgstr "Y&angilash tarixini ko‘rsatish"

#: useService.label useService.accesskey
msgid "Use a &background service to install updates"
msgstr ""
"Yangilanishlarni o‘rnatish uchun &orqa fonda yangilash xizmatidan foydalaning"

#. Networking and Disk Space
#: showSettings.label showSettings.accesskey
msgid "&Settings…"
msgstr "&Sozlamalar…"

#: proxiesConfigure.label
msgid "Configure how &brandShortName; connects to the Internet"
msgstr "&brandShortName; dasturini internetga ulanishini moslash"

#: connectionsInfo.caption
msgid "Connection"
msgstr "Ulanish"

#: offlineInfo.caption
msgid "Offline"
msgstr "Oflayn"

#: offlineInfo.label
msgid "Configure offline settings"
msgstr "Oflayn sozlamalarini to‘g‘rilash"

#: showOffline.label showOffline.accesskey
msgid "&Offline…"
msgstr "&Oflayn…"

#: Diskspace
msgid "Disk Space"
msgstr "Diskdagi joy"

#: offlineCompactFolders.label offlineCompactFolders.accesskey
msgid "Comp&act all folders when it will save over"
msgstr "Barcha jildlar saqlanganda ularni yig‘ish"

#: offlineCompactFoldersMB.label
msgid "MB in total"
msgstr "Jami: MB"

#. LOCALIZATION NOTE:
#. The entities useCacheBefore.label and useCacheAfter.label appear on a single
#. line in preferences as follows:
#. &useCacheBefore.label  [ textbox for cache size in MB ]   &useCacheAfter.label;
#: useCacheBefore.label useCacheBefore.accesskey
msgid "&Use up to"
msgstr "Kesh uchun"

#: useCacheAfter.label
msgid "MB of space for the cache"
msgstr "MB joydan foydalaning"

#: clearCacheNow.label clearCacheNow.accesskey
msgid "&Clear Now"
msgstr "Hozir &tozalash"

#. Certificates
#: certSelection.description
msgid "When a server requests my personal certificate:"
msgstr "Server shaxsiy sertifikatimni so‘raganida:"

#: certs.auto
msgid "Select one automatically"
msgstr "Avtomatik tarzda bittasini tanlash"

#: certs.auto.accesskey
msgid "S"
msgstr "S"

#: certs.ask
msgid "Ask me every time"
msgstr "Har safar mendan so‘ralsin"

#: certs.ask.accesskey
msgid "A"
msgstr "A"

#: enableOCSP.label enableOCSP.accesskey
msgid ""
"&Query OCSP responder servers to confirm the current validity of certificates"
msgstr ""
"OCSP javob berish serverlari sertifikatlarining joriy yaroqliligini "
"tasdiqlash uchun &so‘rov jo‘natish"

#: manageCertificates.label manageCertificates.accesskey
msgid "&Manage Certificates"
msgstr "Sertifikatlarni &boshqarish"

#: viewSecurityDevices.label viewSecurityDevices.accesskey
msgid "Security &Devices"
msgstr "Xavfsizlik &qurilmalari"

