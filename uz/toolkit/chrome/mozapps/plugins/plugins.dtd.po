#: reloadPlugin.middle
msgid "Reload the page"
msgstr "Sahifani qayta yuklash"

#: reloadPlugin.post
msgid " to try again."
msgstr "yana urinib koʻring."

#. This and the other report.* strings should be as short as possible, ideally 2-3 words.
#: report.please
msgid "Send crash report"
msgstr "Nosozlik ma`lumotini joʻnatish"

#: report.submitting
msgid "Sending report…"
msgstr "Ma`lumot joʻnatish"

#: report.submitted
msgid "Crash report sent."
msgstr "Nosozlik ma`lumoti joʻnatildi."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "Nosozlik ma`lumoti aniqlandi"

#: report.failed
msgid "Submission failed."
msgstr "Joʻnatish amalga oshmadi."

#: report.unavailable
msgid "No report available."
msgstr "Ma`lumot mavjud emas"

#: report.comment
msgid "Add a comment (comments are publicly visible)"
msgstr "Mulohaza bildiring (mulohazalar hammaga koʻrinadi)"

#: report.pageURL
msgid "Include the page's URL"
msgstr "Sahifa manzilini qoʻshish"

#: plugin.file
msgid "File"
msgstr "Fayl"

#: plugin.mimeTypes
msgid "MIME Types"
msgstr "MIME turlari"
