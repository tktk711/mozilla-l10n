#: 1
msgid "Parsing an XSLT stylesheet failed."
msgstr "Thất bại khi phân tích một stylesheet XSLT."

#: 2
msgid "Parsing an XPath expression failed."
msgstr "Thất bại khi phân tích một biểu thức XPath."

#: 3
msgctxt "3"
msgid ""
msgstr ""

#: 4
msgid "XSLT transformation failed."
msgstr "Thất bại khi chuyển đổi XSLT."

#: 5
msgid "Invalid XSLT/XPath function."
msgstr "Sai hàm XSLT/XPath."

#: 6
msgid "XSLT Stylesheet (possibly) contains a recursion."
msgstr "Stylesheet XSLT (có thể) chứa đệ qui."

#: 7
msgid "Attribute value illegal in XSLT 1.0."
msgstr "Giá trị thuộc tính không hợp lệ trong XSLT 1.0."

#: 8
msgid "An XPath expression was expected to return a NodeSet."
msgstr "Cần một biểu thức XPath để trả lại một NodeSet."

#: 9
msgid "XSLT transformation was terminated by <xsl:message>."
msgstr "Chuyển đổi XSLT bị hủy bỏ bởi <xsl:message>."

#: 10
msgid "A network error occurred loading an XSLT stylesheet:"
msgstr "Một lỗi mạng đã xảy ra khi tải một stylesheet XSLT:"

#: 11
msgid "An XSLT stylesheet does not have an XML mimetype:"
msgstr "Một stylesheet XSLT không chứa kiểu mime XML:"

#: 12
msgid "An XSLT stylesheet directly or indirectly imports or includes itself:"
msgstr "Một stylesheet XSLT nhập trực tiếp hoặc gián tiếp hoặc chứa chính nó:"

#: 13
msgid "An XPath function was called with the wrong number of arguments."
msgstr "Nột hàm XPath được gọi với số đối số sai."

#: 14
msgid "An unknown XPath extension function was called."
msgstr "Một hàm mở rộng XPath được gọi."

#: 15
msgid "XPath parse failure: ‘)’ expected:"
msgstr "Phân tích XPath thất bại: cần ')':"

#: 16
msgid "XPath parse failure: invalid axis:"
msgstr "Phân tích XPath thất bại: sai trục:"

#: 17
msgid "XPath parse failure: Name or Nodetype test expected:"
msgstr "Phân tích XPath thất bại: cần kiểm tra Tên hoặc Nodetype:"

#: 18
msgid "XPath parse failure: ‘]’ expected:"
msgstr "Phân tích XPath thất bại: cần ']':"

#: 19
msgid "XPath parse failure: invalid variable name:"
msgstr "Phân tích XPath thất bại: sai tên biến:"

#: 20
msgid "XPath parse failure: unexpected end of expression:"
msgstr "Phân tích XPath thất bại: bất ngờ kết thúc biểu thức:"

#: 21
msgid "XPath parse failure: operator expected:"
msgstr "Phân tích XPath thất bại: cần toán tử:"

#: 22
msgid "XPath parse failure: unclosed literal:"
msgstr "Phân tích XPath thất bại: chưa đóng kí tự sai:"

#: 23
msgid "XPath parse failure: ‘:’ unexpected:"
msgstr "Phân tích XPath thất bại: thừa ':':"

#: 24
msgid "XPath parse failure: ‘!’ unexpected, negation is not():"
msgstr "Phân tích XPath thất bại: thừa '!', phủ định là not():"

#: 25
msgid "XPath parse failure: illegal character found:"
msgstr "Phân tích XPath thất bại: tìm thấy kí tự không hợp lệ:"

#: 26
msgid "XPath parse failure: binary operator expected:"
msgstr "Phân tích XPath thất bại: cần toán tử nhị phân:"

#: 27
msgid "An XSLT stylesheet load was blocked for security reasons."
msgstr "Một stylesheet XSLT đã bị chặn vì lí do bảo mật."

#: 28
msgid "Evaluating an invalid expression."
msgstr "Định lượng biểu thức bất hợp lệ."

#: 29
msgid "Unbalanced curly brace."
msgstr "Dấu ngoặc ôm không cân bằng."

#: 30
msgid "Creating an element with an invalid QName."
msgstr "Đang tạo một thành tố với QName bất hợp lệ."

#: 31
msgid "Variable binding shadows variable binding within the same template."
msgstr "Gắn kết biến che đậy gắn kết biến trong cùng khuôn mẫu."

#: 32
msgid "Call to the key function not allowed."
msgstr ""

#: LoadingError
msgid "Error loading stylesheet: %S"
msgstr "Gặp lỗi khi tải stylesheet: %S"

#: TransformError
msgid "Error during XSLT transformation: %S"
msgstr "Gặp lỗi khi chuyển đổi XSLT: %S"
