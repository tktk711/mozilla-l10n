#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Lần đồng bộ cuối: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr ""

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Tài khoản: %S"

#: notconnected.label
msgid "Not connected"
msgstr "Không kết nối"

#: connecting.label
msgid "Connecting…"
msgstr "Đang kết nối…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Tài khoản Firefox Sync của bạn đã được xóa"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Hoàn tác"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 không tương thích với phiên bản Firefox Sync mới nhất. Vui lòng cập "
"nhật lên phiên bản mới nhất."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 không tương thích với phiên bản cũ của Firefox Sync. Vui lòng cập nhật "
"Firefox trên máy tính khác của bạn."

#: sync.update.title
msgid "Firefox Sync"
msgstr ""

#: sync.update.button
msgid "Learn More"
msgstr "Tìm hiểu Thêm"

#: sync.update.close
msgid "Close"
msgstr "Đóng"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Không thể Cài đặt Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Không có kết nối internet"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S không thể kết nối tới Sync. Bạn có muốn thử lại không?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Thử lại"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Cài đặt thủ công"

#: sync.message.notabs
#, fuzzy
msgid "No tabs from your other computers."
msgstr "Thẻ từ máy tính khác của bạn"
