#: hostColon
msgid "Host:"
msgstr "Máy chủ:"

#: domainColon
msgid "Domain:"
msgstr "Tên miền:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Chỉ những kết nối đã mã hóa"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Mọi loại kết nối"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "Khi kết thúc phiên"

#: showDetails
msgid "Show Details"
msgstr "Hiện các Chi tiết"

#: hideDetails
msgid "Hide Details"
msgstr "Ẩn các Chi tiết"

#: detailsAccessKey
msgid "T"
msgstr "n"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Trang %S muốn đặt một cookie."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "Trang %S muốn đặt cookie thứ hai."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"Trang %S muốn đặt một cookie khác.\n"
"Bạn đã có %S cookie từ trang này."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "Trang %S muốn sửa đổi một cookie đã có trong máy."
