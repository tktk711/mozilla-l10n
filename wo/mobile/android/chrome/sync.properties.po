#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Jappante bi mujj : %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Jappante bi : mi ngi ci yoon…"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Koont : %S"

#: notconnected.label
msgid "Not connected"
msgstr "Jokkoowul"

#: connecting.label
msgid "Connecting…"
msgstr "Ci yoonu jokkoo…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "Sa koont Firefox Sync dindi nañu ko"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Neenal"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr "#1 #2 andul ak sumbu Firefox Sync bi mujj. Yeesalal."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 andul ak sumb yu yagg yu Firefox Sync . Yeesalal Firefox ci sa yeneen "
"ordinatëër."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox jàppante"

#: sync.update.button
msgid "Learn More"
msgstr "Gëna xam"

#: sync.update.close
msgid "Close"
msgstr "Tëj"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "Mêneesula sàjjaat Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "Amul benn jokkoo internet bu feex"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "%S mënula jokkoo ak Sync. Jéemaatal?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Jéemaatal"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Tabb ak loxo"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Amul benn koñ bu joge ci sa yeneen ordinatëër."

