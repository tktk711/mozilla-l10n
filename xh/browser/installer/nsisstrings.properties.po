msgstr "Kwi&thaskhbha yam"

#: ADD_SC_QUICKLAUNCHBAR
msgid "On my &Quick Launch bar"
msgstr "Kwibha yam yeLontshi &eKhawulezayo"

#: ADD_CheckboxShortcutInStartMenu
msgid "In my &Start Menu Programs Folder"
msgstr "Kwifolda yam yeeNkqubo zoku&Qalisa iMenyu"

#: ADD_CheckboxShortcutOnDesktop
msgid "On my &Desktop"
msgstr "Kwi&Desktophu yam"

#: SPACE_REQUIRED
