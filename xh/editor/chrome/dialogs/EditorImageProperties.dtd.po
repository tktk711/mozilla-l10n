#: windowTitle.label
msgid "Image Properties"
msgstr "Iinkcazelo Ngeempawu Zomfuziselo"

#: pixelsPopup.value
msgid "pixels"
msgstr "amachokoza eskrinini"

#: percentPopup.value
msgid "percent"
msgstr "ipesenti"

#: locationEditField.label locationEditField.accessKey
msgid "Image &Location:"
msgstr "Indawo Okuyo &Umfuziselo:"

#: locationEditField.tooltip
msgid "Type the image's filename or location"
msgstr "Chwetheza igama lefayili yomfuziselo okanye indawo ekuyo"

#: title.label title.accessKey
msgid "&Tooltip:"
msgstr "&Icebiso ngesixhobo somyalelo:"

#: title.tooltip
msgid "The html 'title' attribute that displays as a tooltip"
msgstr ""
"Uphawu loyelelwano 'title' lwe-html olubonisa njengecebiso ngesixhobo "
"somyalelo"

#: altText.label altText.accessKey
msgid "&Alternate text:"
msgstr "&Isiqendu esisesinye:"

#: altTextEditField.tooltip
msgid "Type text to display in place of the image"
msgstr "Chwetheza isiqendu ukubonisa indawo yomfuziselo"

#: noAltText.label noAltText.accessKey
msgid "&Don't use alternate text"
msgstr "&Musa ukusebenzisa isiqendu esisesinye"

#: previewBox.label
msgid "Image Preview"
msgstr "Jonga Phambi Koshicilelo Lomfuziselo"

#: MoreFewerButton.tooltip
msgid "Display more or fewer properties to edit"
msgstr ""
"Bonisa iinkcazelo ngeempawu ezithe chatha okanye ezimbalwa ukuze kuhlelwe"

#: actualSize.label
msgid "Actual Size:"
msgstr "Ubukhulu Obububo:"

#: actualSizeRadio.label actualSizeRadio.accessKey
msgid "&Actual Size"
msgstr "&Ubukhulu Obububo"

#: actualSizeRadio.tooltip
msgid "Revert to the image's actual size"
msgstr "Buyela umva kubukhulu obububo bomfuziselo"

#: customSizeRadio.label customSizeRadio.accessKey
msgid "Custom &Size"
msgstr "Ubukhulu &Bokulungiselela"

#: customSizeRadio.tooltip
msgid "Change the image's size as displayed in the page"
msgstr "Guqula ubukhulu bomfuziselo njengoko bubonisiwe ekhasini"

#: heightEditField.label heightEditField.accessKey
msgid "Hei&ght:"
msgstr "Uku&phakama:"

#: widthEditField.label widthEditField.accessKey
msgid "&Width:"
msgstr "&Ububanzi:"

#: constrainCheckbox.label constrainCheckbox.accessKey
msgid "&Constrain"
msgstr "&Isithintelo"

#: constrainCheckbox.tooltip
msgid "Maintain the image's aspect ratio"
msgstr "Londoloza ulwalamano lomba womfuziselo"

#: imagemapBox.label
msgid "Image Map"
msgstr "Imephu Yomfuziselo"

#: removeImageMapButton.label removeImageMapButton.accessKey
msgid "&Remove"
msgstr "&Susa"

#: editImageMapButton.label
msgid "Edit..."
msgstr "Hlela..."

#: editImageMapButton.tooltip
msgid "Create clickable hotspots for this image"
msgstr "Yila iindawo ezingumtsalane ekunokunqomfwa kuzo ngalo mfuziselo"

#: alignment.label
msgid "Align Text to Image"
msgstr "Lungelelanisa Isiqendu Kumfuziselo"

#: bottomPopup.value
msgid "At the bottom"
msgstr "Emazantsi"

#: topPopup.value
msgid "At the top"
msgstr "Ngaphezulu"

#: centerPopup.value
msgid "In the center"
msgstr "Embindini"

#: wrapRightPopup.value
msgid "Wrap to the right"
msgstr "Gqithela kumgca olandelayo ekunene"

#: wrapLeftPopup.value
msgid "Wrap to the left"
msgstr "Gqithela kumgca olandelayo ekhohlo"

#: spacingBox.label
msgid "Spacing"
msgstr "Izikhewu ezahlukanisayo"

#: spacingBox.tooltip
msgid "Type a number for the amount of space around image"
msgstr "Chwetheza inani lesixa sesithuba esijikeleze umfuziselo"

#: leftRightEditField.label leftRightEditField.accessKey
msgid "&Left and Right:"
msgstr "&Ekhohlo Nasekunene:"

#: topBottomEditField.label topBottomEditField.accessKey
msgid "&Top and Bottom:"
msgstr "&Ngaphezulu Nasemazantsi:"

#: borderEditField.label borderEditField.accessKey
msgid "Solid &Border:"
msgstr "Umda &Osisiqina:"

#: showImageLinkBorder.label showImageLinkBorder.accessKey
msgid "Show &border around linked image"
msgstr "Bonisa &umda malunga nomfuziselo onxulunyanisiweyo"

#: makePageBackgroundCheckbox.label
msgid "Make Page Background"
msgstr "Yenza Okungasemva Eskrinini Kwekhasi"

#: makePageBackgroundCheckbox.tooltip
msgid "Use image as background for your page"
msgstr "Sebenzisa umfuziselo njengokungasemva eskrinini malunga nekhasi lakho"

#: editImageButton.label
msgid "Edit Image"
msgstr "Hlela Umfuziselo"

#: editImageButton.tooltip
msgid "Edit image file in another program"
msgstr "Hlela ifayili yomfuziselo kwenye inkqubo"

#: imageInputTab.label
msgid "Form"
msgstr "Ifom"

#: imageLocationTab.label
msgid "Location"
msgstr "Indawo"

#: imageDimensionsTab.label
msgid "Dimensions"
msgstr "Amacala okuthile"

#: imageAppearanceTab.label
msgid "Appearance"
msgstr "Imbonakalo"

#: imageLinkTab.label
msgid "Link"
msgstr "Unxulumano"
