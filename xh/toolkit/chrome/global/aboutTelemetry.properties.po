#. # Note to translators:
#. # - %1$S will be replaced by brandFullName
#. # - %2$S will be replaced with the value of the toolkit.telemetry.server_owner preference
#: pageSubtitle
msgid ""
"This page shows the information about performance, hardware, usage and "
"customizations collected by Telemetry. This information is submitted to %1$S "
"to help improve %2$S."
msgstr ""
"Eli khasi libonisa inkcazelo engomsebenzi, ihadiwe, ukusetyenziswa "
"noqhelaniso eziqokelelwe yiThelemetri. Le nkcazelo ithunyelwa kwi-%1$S "
"ukunceda ekuphuculeni i-%2$S."

#: slowSqlMain
msgid "Slow SQL Statements on Main Thread"
msgstr "Iingxelo ze-SQL ezicothayo kwithredi ephambili"

#: slowSqlOther
msgid "Slow SQL Statements on Helper Threads"
msgstr "Iingxelo ze-SQL ezicothayo kwiithredi ezincedayo"

#: slowSqlHits
msgid "Hits"
msgstr "Abatyeleli-webhu"

#: slowSqlAverage
msgid "Avg. Time (ms)"
msgstr "I-Avg Ixesha (ms)"

#: slowSqlStatement
msgid "Statement"
msgstr "Uniko-ngxelo"

#. # Note to translators:
#. # - The %1$S will be replaced with the number of the hang
#. # - The %2$S will be replaced with the duration of the hang
