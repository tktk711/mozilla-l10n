msgid ""
msgstr ""

#: reloadPlugin.middle
msgid "Reload the page"
msgstr "Layisha kwakhona ikhasi"

#: reloadPlugin.post
msgid " to try again."
msgstr " ukuzama kwakhona."

#. This and the other report.* strings should be as short as possible, ideally 2-3 words.
#: report.please
msgid "Send crash report"
msgstr "Thumela ingxelo yokonakala"

#: report.submitting
msgid "Sending report…"
msgstr "Ithumela ingxelo…"

#: report.submitted
msgid "Crash report sent."
msgstr "Ingxelo yokonakala ithunyelwe."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "Ukunika ingxelo yokonakala kuqhwalelisiwe."

#: report.failed
msgid "Submission failed."
msgstr "Ukuthunyelwa kwayo akuphumelelanga."

#: report.unavailable
msgid "No report available."
msgstr "Akukho ngxelo ifumanekayo."

#: report.comment
msgid "Add a comment (comments are publicly visible)"
msgstr "Yongeza ikhomenti (iikhomenti zibonakala ngokuphandle)"

#: report.pageURL
msgid "Include the page's URL"
msgstr "Quka i-URL yephepha"

#: plugin.file
msgid "File"
msgstr "Ifayile"

#: plugin.mimeTypes
msgid "MIME Types"
msgstr "Iintlobo ze-MIME"

