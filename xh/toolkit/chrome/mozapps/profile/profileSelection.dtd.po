#: windowtitle.label
msgid "&brandShortName; - Choose User Profile"
msgstr "I-&brandShortName; - Khetha iprofayile Yomsebenzisi"

#: profilename.label
msgid "Profile Name:"
msgstr "Igama leprofayile:"

#: start.label
msgid "Start &brandShortName;"
msgstr "Qalisa i-&brandShortName;"

#: exit.label
msgid "Exit"
msgstr "Phuma"

#: availprofiles.label
msgid "Available Profiles"
msgstr "Iiprofayile ezifumanekayo"

#: newButton.label newButton.accesskey
msgid "&Create Profile…"
msgstr "&Yila iprofayile…"

#: renameButton.label renameButton.accesskey
msgid "&Rename Profile…"
msgstr "&Thiya ngokutsha iprofayile…"

#: deleteButton.label deleteButton.accesskey
msgid "&Delete Profile…"
msgstr "&Cima iprofayile…"

#. manager entities
#: pmDescription.label
msgid ""
"&brandShortName; stores information about your settings, preferences, and "
"other user items in your user profile."
msgstr ""
"I-&brandShortName; igcina inkcazelo ngeesethingi, iipriferensi nezinye "
"iiayithem zomsebenzisi kwiprofayile yomsebenzisi."

#: offlineState.label offlineState.accesskey
msgid "W&ork offline"
msgstr "Um&sebenzi kwimo engeyo yakusebenza"

#: useSelected.label useSelected.accesskey
msgid "U&se the selected profile without asking at startup"
msgstr "S&ebenzisa iprofayile ekhethiweyo ngaphandle kokucela ukuqalisa"

#~ msgid "Don't a&sk at startup"
#~ msgstr "Musa ukuc&ela ngexesha lokuqalisa"
