msgstr "Ifakelwe kwi-: %S"

#: statusFailed
msgid "Install Failed"
msgstr "Ufakelo aluPhumelelanga"

#: pauseButtonPause
msgid "Pause"
msgstr "Isiqabu"

#: pauseButtonResume
msgid "Resume"
msgstr "Buyela enkomeni"

#: hideButton
msgid "Hide"
msgstr "Fihla"

#: hideButton.accesskey
msgid "H"
msgstr "H"

#: applyingUpdate
