#: hostColon
msgid "Host:"
msgstr "Baálé:"

#: domainColon
msgid "Domain:"
msgstr "Sàkání"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Is̀olùpọ̀ tí kò lu jáde nìkan"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Èyíkéyìí ìs̀olùpọ̀"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "Ní òpin sáà kan"

#: showDetails
msgid "Show Details"
msgstr "Fi Kókó Hàn"

#: hideDetails
msgid "Hide Details"
msgstr "Fi Kókó Pamọ́"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Ojúbọ %S fẹ́ fi àmì ìtọ́ni lélẹ̀."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "Ojúbọ %S fẹ́ fi àmì ìtọ́ni kejì lélẹ̀."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"Ojúbọ %S fẹ́ fi àmì ìtọ́ni míràn lélẹ̀.\n"
"O ti ní àwọn ìtọ́ni %S lati ojúbọ yìí."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "Ojúbọ %S fẹ́ ṣe àyípadà sí àmì ìtọ́ni kan."
