#. # 1st string is the username for the login, 2nd is the login's hostname.
#. # Note that long usernames may be truncated.
#: savePassword
msgid "Save password for \"%1$S\" on %2$S?"
msgstr "Taak wáa a ta'akik mulukts'íib \"%1$S\" ti' %2$S?"

#. # String is the login's hostname
#: savePasswordNoUser
msgid "Save password on %S?"
msgstr "Taak wáa a ta'akik le mukults'íib ichil %S"

#: saveButton
msgid "Save"
msgstr "Ta'ak"

#: dontSaveButton
msgid "Don't save"
msgstr "Ma' ta'akik"

#. # String is the login's hostname
#: updatePassword
msgid "Update saved password for %S?"
msgstr "Taak wáa a túumbenkuunsik mukults'íib tia'al %S"

#: updatePasswordNoUser
msgid "Update saved password for this login?"
msgstr "Taak wáa a túumbenkuunsik mukults'íib tia'al le kúuchila'"

#: updateButton
msgid "Update"
msgstr "Túumbenkuunsaj"

#: dontUpdateButton
msgid "Don't update"
msgstr "Ma' túumbenkuunsik"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "Jets' u yuumile' taak u k'exik le mukults'íibo'"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "Jets' u k'éexpajal le mukults'íibo'"

#. # Strings used by PromptService.js
#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr ""
"K'abéetkuuns áantajil muk'ults'íib tia'al u k'a'asajil le mukults'íiba'"

#: username
msgid "Username"
msgstr "K'aaba'"

#: password
msgid "Password"
msgstr "Mukults'íib"

