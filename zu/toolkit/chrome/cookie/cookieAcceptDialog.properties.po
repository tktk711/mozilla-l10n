#: hostColon
msgid "Host:"
msgstr "Umxhumi:"

#: domainColon
msgid "Domain:"
msgstr "Indawo Elawulayo:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Izindlela zokuxhumana ngamakhodi kuphela"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Noma iluphi uhlobo lokuxhuma"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "ekupheleni kwenkathi"

#: showDetails
msgid "Show Details"
msgstr "Bonisa Imininingwane"

#: hideDetails
msgid "Hide Details"
msgstr "Fihla Imininingwane"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Ingosi %S ifuna ukuthumela ukwaziswa nge-Inthaneti."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr ""
"Ingosi %S ifuna ukuba nengosi yesibili yokuthumela ukwaziswa nge-Inthaneti."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"Ingosi %S ifuna ukuba nenye ingosi yokuthumela ukwaziswa nge-Inthaneti.\n"
"Usunayo kakade I%S yezingosi zokuthumela ukwaziswa nge-Inthaneti."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr ""
"Ingosi %S ifuna ukulungisa ingosi ekhona yokuthumela ukwaziswa nge-Inthaneti."

#~ msgid "The site %S wants to load an image."
#~ msgstr "Ingosi %S ifuna ukuthumela isithombe."

#~ msgid "Remember this decision"
#~ msgstr "Khumbula lesi sinqumo"
